{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 603,
      "verse_key": "109:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قُلۡ يَٰٓأَيُّهَا ٱلۡكَٰفِرُونَ",
      "translation": "Thuaj: “O ju jobesimtarë!",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 603,
      "verse_key": "109:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَآ أَعۡبُدُ مَا تَعۡبُدُونَ",
      "translation": "Unë nuk adhuroj çfarë ju adhuroni",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 603,
      "verse_key": "109:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَآ أَنتُمۡ عَٰبِدُونَ مَآ أَعۡبُدُ",
      "translation": "dhe ju nuk jeni adhurues të Atij që unë adhuroj.",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 603,
      "verse_key": "109:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَآ أَنَا۠ عَابِدٞ مَّا عَبَدتُّمۡ",
      "translation": "Unë nuk do të jem kurrë adhurues i atyre që ju i adhuroni",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 603,
      "verse_key": "109:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَآ أَنتُمۡ عَٰبِدُونَ مَآ أَعۡبُدُ",
      "translation": "dhe as ju nuk do të jeni adhurues të Atij që unë adhuroj![436]",
      "footnotes": "[436] Vargu flet për ata jobesimtarë që Allahu e di se nuk do të besojnë asnjëherë."
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 603,
      "verse_key": "109:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَكُمۡ دِينُكُمۡ وَلِيَ دِينِ",
      "translation": "Ju keni fenë tuaj, ndërsa unë kam fenë time!”",
      "footnotes": ""
    }
  ]
}