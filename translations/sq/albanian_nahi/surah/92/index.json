{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 595,
      "verse_key": "92:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّيۡلِ إِذَا يَغۡشَىٰ",
      "translation": "Për natën që mbulon (me terr),",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 595,
      "verse_key": "92:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلنَّهَارِ إِذَا تَجَلَّىٰ",
      "translation": "për ditën që agon",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 595,
      "verse_key": "92:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا خَلَقَ ٱلذَّكَرَ وَٱلۡأُنثَىٰٓ",
      "translation": "dhe për Atë që ka krijuar mashkullin dhe femrën!",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 595,
      "verse_key": "92:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ سَعۡيَكُمۡ لَشَتَّىٰ",
      "translation": "Vërtet, veprat tuaja janë të ndryshme.",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 595,
      "verse_key": "92:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَمَّا مَنۡ أَعۡطَىٰ وَٱتَّقَىٰ",
      "translation": "Sa për atë që jep (për hir të Allahut), ka frikë (nga Ai)",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 595,
      "verse_key": "92:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَصَدَّقَ بِٱلۡحُسۡنَىٰ",
      "translation": "dhe dëshmon për vërtetësinë e më të bukurës (besimit),",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 595,
      "verse_key": "92:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَسَنُيَسِّرُهُۥ لِلۡيُسۡرَىٰ",
      "translation": "Ne do t’ia lehtësojmë atij rrugën drejt shpëtimit.",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 595,
      "verse_key": "92:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَمَّا مَنۢ بَخِلَ وَٱسۡتَغۡنَىٰ",
      "translation": "Kurse atij që është koprrac, ndihet i vetëmjaftueshëm,",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 595,
      "verse_key": "92:9",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَكَذَّبَ بِٱلۡحُسۡنَىٰ",
      "translation": "dhe e quan gënjeshtër më të bukurën (besimin),",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 595,
      "verse_key": "92:10",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَسَنُيَسِّرُهُۥ لِلۡعُسۡرَىٰ",
      "translation": "Ne do t’ia lehtësojmë rrugën drejt dënimit,",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 595,
      "verse_key": "92:11",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا يُغۡنِي عَنۡهُ مَالُهُۥٓ إِذَا تَرَدَّىٰٓ",
      "translation": "e, kur të hidhet (në zjarr), pasuria e tij nuk do t’i bëjë dobi.",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 595,
      "verse_key": "92:12",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ عَلَيۡنَا لَلۡهُدَىٰ",
      "translation": "Neve na përket që t’ua tregojmë rrugën e drejtë,",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 595,
      "verse_key": "92:13",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِنَّ لَنَا لَلۡأٓخِرَةَ وَٱلۡأُولَىٰ",
      "translation": "dhe, me të vërtetë, e Jona është ajo botë dhe kjo botë!",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 595,
      "verse_key": "92:14",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَنذَرۡتُكُمۡ نَارٗا تَلَظَّىٰ",
      "translation": "Prandaj Unë ju kam tërhequr vërejtjen me një zjarr që flakëron,",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 596,
      "verse_key": "92:15",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَا يَصۡلَىٰهَآ إِلَّا ٱلۡأَشۡقَى",
      "translation": "ku do të hidhet vetëm më i keqi,",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 596,
      "verse_key": "92:16",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِي كَذَّبَ وَتَوَلَّىٰ",
      "translation": "që ka mohuar dhe është larguar nga besimi.",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 596,
      "verse_key": "92:17",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَسَيُجَنَّبُهَا ٱلۡأَتۡقَى",
      "translation": "E do të mbahet larg tij (zjarrit) më i devotshmi (që ruhet nga gjynahet),",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 596,
      "verse_key": "92:18",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِي يُؤۡتِي مَالَهُۥ يَتَزَكَّىٰ",
      "translation": "që e jep pasurinë e tij, duke pastruar veten",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 596,
      "verse_key": "92:19",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا لِأَحَدٍ عِندَهُۥ مِن نِّعۡمَةٖ تُجۡزَىٰٓ",
      "translation": "dhe duke mos pritur shpërblim prej dikujt tjetër për këtë,",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 596,
      "verse_key": "92:20",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَّا ٱبۡتِغَآءَ وَجۡهِ رَبِّهِ ٱلۡأَعۡلَىٰ",
      "translation": "por vetëm për të kërkuar Fytyrën (kënaqësinë) e Zotit të tij, më të Lartit.",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 596,
      "verse_key": "92:21",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَسَوۡفَ يَرۡضَىٰ",
      "translation": "Dhe ai, me siguri, do të jetë i kënaqur!",
      "footnotes": ""
    }
  ]
}