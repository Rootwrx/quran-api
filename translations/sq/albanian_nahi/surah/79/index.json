{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 583,
      "verse_key": "79:1",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلنَّٰزِعَٰتِ غَرۡقٗا",
      "translation": "Për ata (engjëj) që marrin me forcë,[389]",
      "footnotes": "[389] D.m.th. shpirtrat e jobesimtarëve."
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 583,
      "verse_key": "79:2",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلنَّٰشِطَٰتِ نَشۡطٗا",
      "translation": "për ata që marrin butësisht,[390]",
      "footnotes": "[390] D.m.th. shpirtrat e besimtarëve."
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 583,
      "verse_key": "79:3",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلسَّٰبِحَٰتِ سَبۡحٗا",
      "translation": "për ata që notojnë në hapësirë,",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 583,
      "verse_key": "79:4",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَٱلسَّٰبِقَٰتِ سَبۡقٗا",
      "translation": "për ata që nxitojnë,[391]",
      "footnotes": "[391] D.m.th. të fusin në Xhenet shpirtrat e mirë."
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 583,
      "verse_key": "79:5",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَٱلۡمُدَبِّرَٰتِ أَمۡرٗا",
      "translation": "dhe për ata që i rregullojnë punët![392]",
      "footnotes": "[392] D.m.th. zbatojnë urdhrat e Allahut në univers."
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 583,
      "verse_key": "79:6",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَوۡمَ تَرۡجُفُ ٱلرَّاجِفَةُ",
      "translation": "Ditën kur do të dridhet gjithçka nga ushtima (që lajmëron Kiametin),",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 583,
      "verse_key": "79:7",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تَتۡبَعُهَا ٱلرَّادِفَةُ",
      "translation": "e cila do ta ndiqet nga një e dytë,",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 583,
      "verse_key": "79:8",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قُلُوبٞ يَوۡمَئِذٖ وَاجِفَةٌ",
      "translation": "disa zemra do të jenë të mbushura me tmerr",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 583,
      "verse_key": "79:9",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَبۡصَٰرُهَا خَٰشِعَةٞ",
      "translation": "e shikimet e tyre do të bien përdhe.",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 583,
      "verse_key": "79:10",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَقُولُونَ أَءِنَّا لَمَرۡدُودُونَ فِي ٱلۡحَافِرَةِ",
      "translation": "Ata thonë: “Vallë, vërtet do të rikthehemi në jetë?!",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 583,
      "verse_key": "79:11",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَءِذَا كُنَّا عِظَٰمٗا نَّخِرَةٗ",
      "translation": "A thua, edhe kur të bëhemi eshtra të thërrmuara?!”",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 583,
      "verse_key": "79:12",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قَالُواْ تِلۡكَ إِذٗا كَرَّةٌ خَاسِرَةٞ",
      "translation": "Dhe thonë: “Po qe kështu, atëherë ky kthim do të jetë humbje (për ne)!”",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 583,
      "verse_key": "79:13",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِنَّمَا هِيَ زَجۡرَةٞ وَٰحِدَةٞ",
      "translation": "Por, ajo do të jetë veç një fryrje e vetme (e Surit)",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 583,
      "verse_key": "79:14",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِذَا هُم بِٱلسَّاهِرَةِ",
      "translation": "dhe ata përnjëherësh do të brofin (të ringjallur).",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 583,
      "verse_key": "79:15",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "هَلۡ أَتَىٰكَ حَدِيثُ مُوسَىٰٓ",
      "translation": "A e ke dëgjuar (o Muhamed) historinë e Musait?",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 584,
      "verse_key": "79:16",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذۡ نَادَىٰهُ رَبُّهُۥ بِٱلۡوَادِ ٱلۡمُقَدَّسِ طُوًى",
      "translation": "Zoti i tij e thirri në luginën e shenjtë të Tuvas dhe i tha:",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 584,
      "verse_key": "79:17",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱذۡهَبۡ إِلَىٰ فِرۡعَوۡنَ إِنَّهُۥ طَغَىٰ",
      "translation": "“Shko te Faraoni, se ai, me të vërtetë, ka kaluar çdo kufi;",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 584,
      "verse_key": "79:18",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَقُلۡ هَل لَّكَ إِلَىٰٓ أَن تَزَكَّىٰ",
      "translation": "dhe thuaji: “A do ta pastrosh veten,",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 584,
      "verse_key": "79:19",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَهۡدِيَكَ إِلَىٰ رَبِّكَ فَتَخۡشَىٰ",
      "translation": "dhe të të udhëzoj te Zoti yt, që t’i frikësohesh?”",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 584,
      "verse_key": "79:20",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَرَىٰهُ ٱلۡأٓيَةَ ٱلۡكُبۡرَىٰ",
      "translation": "Musai ia tregoi mrekullinë më të madhe,",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 584,
      "verse_key": "79:21",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَكَذَّبَ وَعَصَىٰ",
      "translation": "por ai (Faraoni) e mohoi dhe e kundërshtoi.",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 584,
      "verse_key": "79:22",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ أَدۡبَرَ يَسۡعَىٰ",
      "translation": "Pastaj i ktheu shpinën, duke u përpjekur (kundër së vërtetës)",
      "footnotes": ""
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 584,
      "verse_key": "79:23",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَحَشَرَ فَنَادَىٰ",
      "translation": "dhe tuboi njerëzit e u deklaroi:",
      "footnotes": ""
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 584,
      "verse_key": "79:24",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَقَالَ أَنَا۠ رَبُّكُمُ ٱلۡأَعۡلَىٰ",
      "translation": "“Unë jam zoti juaj më i lartë!”",
      "footnotes": ""
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 584,
      "verse_key": "79:25",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَخَذَهُ ٱللَّهُ نَكَالَ ٱلۡأٓخِرَةِ وَٱلۡأُولَىٰٓ",
      "translation": "Prandaj Allahu e goditi atë me dënim në jetën tjetër dhe në këtë jetë.",
      "footnotes": ""
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 584,
      "verse_key": "79:26",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ فِي ذَٰلِكَ لَعِبۡرَةٗ لِّمَن يَخۡشَىٰٓ",
      "translation": "Kjo është këshillë për ata që i frikësohen Allahut.",
      "footnotes": ""
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 584,
      "verse_key": "79:27",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ءَأَنتُمۡ أَشَدُّ خَلۡقًا أَمِ ٱلسَّمَآءُۚ بَنَىٰهَا",
      "translation": "Cili është më i vështirë: krijimi i juaj apo i qiellit, të cilin Ai e ndërtoi,",
      "footnotes": ""
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 584,
      "verse_key": "79:28",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "رَفَعَ سَمۡكَهَا فَسَوَّىٰهَا",
      "translation": "ia ngriti lart kupën dhe e përsosi.",
      "footnotes": ""
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 584,
      "verse_key": "79:29",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَغۡطَشَ لَيۡلَهَا وَأَخۡرَجَ ضُحَىٰهَا",
      "translation": "Netët i bëri të errëta, kurse ditët të ndritshme.",
      "footnotes": ""
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 584,
      "verse_key": "79:30",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡأَرۡضَ بَعۡدَ ذَٰلِكَ دَحَىٰهَآ",
      "translation": "Pastaj e shtriu rrafsh Tokën,",
      "footnotes": ""
    },
    {
      "id": 31,
      "verse_number": 31,
      "page_number": 584,
      "verse_key": "79:31",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَخۡرَجَ مِنۡهَا مَآءَهَا وَمَرۡعَىٰهَا",
      "translation": "dhe, nga ajo nxori ujërat dhe bëri kullotat,",
      "footnotes": ""
    },
    {
      "id": 32,
      "verse_number": 32,
      "page_number": 584,
      "verse_key": "79:32",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡجِبَالَ أَرۡسَىٰهَا",
      "translation": "kurse malet i bëri të patundshme,",
      "footnotes": ""
    },
    {
      "id": 33,
      "verse_number": 33,
      "page_number": 584,
      "verse_key": "79:33",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَتَٰعٗا لَّكُمۡ وَلِأَنۡعَٰمِكُمۡ",
      "translation": "që t’i gëzoni ju dhe bagëtitë tuaja.",
      "footnotes": ""
    },
    {
      "id": 34,
      "verse_number": 34,
      "page_number": 584,
      "verse_key": "79:34",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِذَا جَآءَتِ ٱلطَّآمَّةُ ٱلۡكُبۡرَىٰ",
      "translation": "Kur të arrijë gjëma më e madhe,",
      "footnotes": ""
    },
    {
      "id": 35,
      "verse_number": 35,
      "page_number": 584,
      "verse_key": "79:35",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَوۡمَ يَتَذَكَّرُ ٱلۡإِنسَٰنُ مَا سَعَىٰ",
      "translation": "atë Ditë njeriu do të kujtojë gjithçka që ka bërë",
      "footnotes": ""
    },
    {
      "id": 36,
      "verse_number": 36,
      "page_number": 584,
      "verse_key": "79:36",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَبُرِّزَتِ ٱلۡجَحِيمُ لِمَن يَرَىٰ",
      "translation": "dhe Zjarri flakërues do t’i tregohet çdokujt që sheh.",
      "footnotes": ""
    },
    {
      "id": 37,
      "verse_number": 37,
      "page_number": 584,
      "verse_key": "79:37",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَمَّا مَن طَغَىٰ",
      "translation": "Atëherë, ai që ka shkelur gjithçka",
      "footnotes": ""
    },
    {
      "id": 38,
      "verse_number": 38,
      "page_number": 584,
      "verse_key": "79:38",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَءَاثَرَ ٱلۡحَيَوٰةَ ٱلدُّنۡيَا",
      "translation": "dhe ka parapëlqyer jetën e kësaj bote,",
      "footnotes": ""
    },
    {
      "id": 39,
      "verse_number": 39,
      "page_number": 584,
      "verse_key": "79:39",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِنَّ ٱلۡجَحِيمَ هِيَ ٱلۡمَأۡوَىٰ",
      "translation": "sigurisht që do të ketë për strehë Xhehenemin.",
      "footnotes": ""
    },
    {
      "id": 40,
      "verse_number": 40,
      "page_number": 584,
      "verse_key": "79:40",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَمَّا مَنۡ خَافَ مَقَامَ رَبِّهِۦ وَنَهَى ٱلنَّفۡسَ عَنِ ٱلۡهَوَىٰ",
      "translation": "Kurse ai që i është frikësuar madhërisë së Zotit të tij dhe e ka frenuar veten nga dëshirat,",
      "footnotes": ""
    },
    {
      "id": 41,
      "verse_number": 41,
      "page_number": 584,
      "verse_key": "79:41",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِنَّ ٱلۡجَنَّةَ هِيَ ٱلۡمَأۡوَىٰ",
      "translation": "pa dyshim që do të ketë për strehë Xhenetin.",
      "footnotes": ""
    },
    {
      "id": 42,
      "verse_number": 42,
      "page_number": 584,
      "verse_key": "79:42",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَسۡـَٔلُونَكَ عَنِ ٱلسَّاعَةِ أَيَّانَ مُرۡسَىٰهَا",
      "translation": "Të pyesin ty për Orën (e Kiametit) se kur do të ndodhë.",
      "footnotes": ""
    },
    {
      "id": 43,
      "verse_number": 43,
      "page_number": 584,
      "verse_key": "79:43",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِيمَ أَنتَ مِن ذِكۡرَىٰهَآ",
      "translation": "Çfarë rëndësie ka për ty që ta përmendësh atë?",
      "footnotes": ""
    },
    {
      "id": 44,
      "verse_number": 44,
      "page_number": 584,
      "verse_key": "79:44",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَىٰ رَبِّكَ مُنتَهَىٰهَآ",
      "translation": "Tek Zoti yt është fundi i (dijenisë rreth) saj.",
      "footnotes": ""
    },
    {
      "id": 45,
      "verse_number": 45,
      "page_number": 584,
      "verse_key": "79:45",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّمَآ أَنتَ مُنذِرُ مَن يَخۡشَىٰهَا",
      "translation": "Ti je vetëm paralajmërues i atyre që i frikësohen asaj.",
      "footnotes": ""
    },
    {
      "id": 46,
      "verse_number": 46,
      "page_number": 584,
      "verse_key": "79:46",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَأَنَّهُمۡ يَوۡمَ يَرَوۡنَهَا لَمۡ يَلۡبَثُوٓاْ إِلَّا عَشِيَّةً أَوۡ ضُحَىٰهَا",
      "translation": "Ditën kur do ta shohin atë, (njerëzve) do t’u duket se kanë qëndruar (në Tokë) veç një mbrëmje apo një mëngjes.",
      "footnotes": ""
    }
  ]
}