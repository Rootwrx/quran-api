{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 586,
      "verse_key": "81:1",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذَا ٱلشَّمۡسُ كُوِّرَتۡ",
      "translation": "Kur Dielli të mblidhet e të humbë shkëlqimin;",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 586,
      "verse_key": "81:2",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلنُّجُومُ ٱنكَدَرَتۡ",
      "translation": "kur yjet të rrëzohen",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 586,
      "verse_key": "81:3",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡجِبَالُ سُيِّرَتۡ",
      "translation": "dhe malet të lëvizin tutje;",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 586,
      "verse_key": "81:4",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡعِشَارُ عُطِّلَتۡ",
      "translation": "kur devetë barrse të braktisen",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 586,
      "verse_key": "81:5",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡوُحُوشُ حُشِرَتۡ",
      "translation": "dhe egërsirat të tubohen;",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 586,
      "verse_key": "81:6",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡبِحَارُ سُجِّرَتۡ",
      "translation": "kur detet të vlojnë",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 586,
      "verse_key": "81:7",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلنُّفُوسُ زُوِّجَتۡ",
      "translation": "dhe shpirtrat të bashkohen (me trupat);",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 586,
      "verse_key": "81:8",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡمَوۡءُۥدَةُ سُئِلَتۡ",
      "translation": "kur foshnja e varrosur për së gjalli të pyetet",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 586,
      "verse_key": "81:9",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بِأَيِّ ذَنۢبٖ قُتِلَتۡ",
      "translation": "për çfarë faji ishte vrarë;",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 586,
      "verse_key": "81:10",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلصُّحُفُ نُشِرَتۡ",
      "translation": "kur fletët të shpërndahen[395]",
      "footnotes": "[395] D.m.th. regjistrat e veprave të njerëzve."
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 586,
      "verse_key": "81:11",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلسَّمَآءُ كُشِطَتۡ",
      "translation": "dhe qielli të zhvishet;",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 586,
      "verse_key": "81:12",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡجَحِيمُ سُعِّرَتۡ",
      "translation": "kur Xhehenemi të flakërojë",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 586,
      "verse_key": "81:13",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡجَنَّةُ أُزۡلِفَتۡ",
      "translation": "dhe Xheneti të afrohet:",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 586,
      "verse_key": "81:14",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلِمَتۡ نَفۡسٞ مَّآ أَحۡضَرَتۡ",
      "translation": "atëherë çdokush do të marrë vesh çfarë kishte përgatitur (për këtë Ditë).",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 586,
      "verse_key": "81:15",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلَآ أُقۡسِمُ بِٱلۡخُنَّسِ",
      "translation": "Betohem për ata (planetë) që tërhiqen,",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 586,
      "verse_key": "81:16",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلۡجَوَارِ ٱلۡكُنَّسِ",
      "translation": "që lindin e perëndojnë,",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 586,
      "verse_key": "81:17",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّيۡلِ إِذَا عَسۡعَسَ",
      "translation": "për natën kur bie terri",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 586,
      "verse_key": "81:18",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلصُّبۡحِ إِذَا تَنَفَّسَ",
      "translation": "e për agimin kur nis të zbardhë,",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 586,
      "verse_key": "81:19",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُۥ لَقَوۡلُ رَسُولٖ كَرِيمٖ",
      "translation": "se ky (Kuran) është Fjalë e (kumtuar prej) një të Dërguari fisnik,[396]",
      "footnotes": "[396] Prej Xhebrailit (a.s.)."
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 586,
      "verse_key": "81:20",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ذِي قُوَّةٍ عِندَ ذِي ٱلۡعَرۡشِ مَكِينٖ",
      "translation": "që zotëron fuqi dhe pozitë të lartë te Zoti i Fronit",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 586,
      "verse_key": "81:21",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مُّطَاعٖ ثَمَّ أَمِينٖ",
      "translation": "e që është i respektuar atje dhe besnik.",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 586,
      "verse_key": "81:22",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا صَاحِبُكُم بِمَجۡنُونٖ",
      "translation": "Shoku juaj[397] nuk është i marrë.",
      "footnotes": "[397] Muhamedi (a.s.)."
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 586,
      "verse_key": "81:23",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَقَدۡ رَءَاهُ بِٱلۡأُفُقِ ٱلۡمُبِينِ",
      "translation": "Ai e ka parë atë[398] në horizontin e qartë",
      "footnotes": "[398] Pra, Profeti (a.s.) e ka parë Xhebrailin (a.s.) në formën e tij fillestare në fillimet e Shpalljes Hyjnore."
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 586,
      "verse_key": "81:24",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا هُوَ عَلَى ٱلۡغَيۡبِ بِضَنِينٖ",
      "translation": "dhe nuk mbulon asnjë të fshehtë prej së Padukshmes.",
      "footnotes": ""
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 586,
      "verse_key": "81:25",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا هُوَ بِقَوۡلِ شَيۡطَٰنٖ رَّجِيمٖ",
      "translation": "As ky (Kuran) nuk është fjalë e djallit të mallkuar.",
      "footnotes": ""
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 586,
      "verse_key": "81:26",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَيۡنَ تَذۡهَبُونَ",
      "translation": "Prandaj, nga po ia mbani?",
      "footnotes": ""
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 586,
      "verse_key": "81:27",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنۡ هُوَ إِلَّا ذِكۡرٞ لِّلۡعَٰلَمِينَ",
      "translation": "Ai është veçse këshillë për botët:",
      "footnotes": ""
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 586,
      "verse_key": "81:28",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لِمَن شَآءَ مِنكُمۡ أَن يَسۡتَقِيمَ",
      "translation": "për secilin nga ju që dëshiron të ndjekë rrugën e drejtë.",
      "footnotes": ""
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 586,
      "verse_key": "81:29",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا تَشَآءُونَ إِلَّآ أَن يَشَآءَ ٱللَّهُ رَبُّ ٱلۡعَٰلَمِينَ",
      "translation": "Por ju nuk mund të dëshironi, përveçse me vullnetin e Allahut, Zotit të botëve!",
      "footnotes": ""
    }
  ]
}