{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 526,
      "verse_key": "53:1",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلنَّجۡمِ إِذَا هَوَىٰ",
      "translation": "Betohem në yllin kur perëndon,",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 526,
      "verse_key": "53:2",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَا ضَلَّ صَاحِبُكُمۡ وَمَا غَوَىٰ",
      "translation": "se shoku juaj (Muhamedi a.s.) nuk ka humbur e nuk ka devijuar!",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 526,
      "verse_key": "53:3",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا يَنطِقُ عَنِ ٱلۡهَوَىٰٓ",
      "translation": "Ai nuk flet sipas qejfit të vet,",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 526,
      "verse_key": "53:4",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنۡ هُوَ إِلَّا وَحۡيٞ يُوحَىٰ",
      "translation": "por ajo (që thotë) është vetëm shpallje hyjnore, që i vjen atij.",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 526,
      "verse_key": "53:5",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلَّمَهُۥ شَدِيدُ ٱلۡقُوَىٰ",
      "translation": "Atë ia ka mësuar atij një (engjëll) shumë i fuqishëm (Xhebraili),",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 526,
      "verse_key": "53:6",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ذُو مِرَّةٖ فَٱسۡتَوَىٰ",
      "translation": "mendjelartë, që iu paraqit qartë,",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 526,
      "verse_key": "53:7",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَهُوَ بِٱلۡأُفُقِ ٱلۡأَعۡلَىٰ",
      "translation": "në horizontin më të lartë.",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 526,
      "verse_key": "53:8",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ دَنَا فَتَدَلَّىٰ",
      "translation": "Pastaj iu afrua e iu lëshua,",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 526,
      "verse_key": "53:9",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَكَانَ قَابَ قَوۡسَيۡنِ أَوۡ أَدۡنَىٰ",
      "translation": "sa largësia midis dy harqeve ose edhe më afër,",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 526,
      "verse_key": "53:10",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَوۡحَىٰٓ إِلَىٰ عَبۡدِهِۦ مَآ أَوۡحَىٰ",
      "translation": "e i shpalli robit të Allahut, atë që i shpalli.",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 526,
      "verse_key": "53:11",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَا كَذَبَ ٱلۡفُؤَادُ مَا رَأَىٰٓ",
      "translation": "Zemra (e Muhamedit) nuk gënjen për atë që ka parë.",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 526,
      "verse_key": "53:12",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَتُمَٰرُونَهُۥ عَلَىٰ مَا يَرَىٰ",
      "translation": "Vallë, a do ta kundërshtonit atë për çfarë ka parë,",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 526,
      "verse_key": "53:13",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَقَدۡ رَءَاهُ نَزۡلَةً أُخۡرَىٰ",
      "translation": "ndërkohë që ai e ka parë atë edhe një herë tjetër,",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 526,
      "verse_key": "53:14",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عِندَ سِدۡرَةِ ٱلۡمُنتَهَىٰ",
      "translation": "te Sidretul Munteha-ja,",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 526,
      "verse_key": "53:15",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عِندَهَا جَنَّةُ ٱلۡمَأۡوَىٰٓ",
      "translation": "ku ndodhet Xheneti i Strehimit.",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 526,
      "verse_key": "53:16",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذۡ يَغۡشَى ٱلسِّدۡرَةَ مَا يَغۡشَىٰ",
      "translation": "Kur Sidrën çfarë nuk e mbulonte?![338]",
      "footnotes": "[338] Pra, Muhamedi (a.s.) e ka parë Xhebrailin në formën e tij të plotë te Sidretul Munteha, e cila është një pemë e madhe në të majtë të Fronit Hyjnor, mbi qiellin e shtatë. Atë e mbulojnë engjëj dhe mrekulli të ndryshme të Zotit. Aty ndodhet edhe Xheneti i Strehimit, ku strehohen shpirtrat e dëshmorëve dhe të besimtarëve të devotshëm."
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 526,
      "verse_key": "53:17",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَا زَاغَ ٱلۡبَصَرُ وَمَا طَغَىٰ",
      "translation": "Shikimi i tij nuk u shmang dhe as nuk e kaloi masën,",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 526,
      "verse_key": "53:18",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَقَدۡ رَأَىٰ مِنۡ ءَايَٰتِ رَبِّهِ ٱلۡكُبۡرَىٰٓ",
      "translation": "por ai pa disa nga mrekullitë më madhështore të Zotit të tij!",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 526,
      "verse_key": "53:19",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَرَءَيۡتُمُ ٱللَّٰتَ وَٱلۡعُزَّىٰ",
      "translation": "A e keni parë Latën dhe Uzzën?",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 526,
      "verse_key": "53:20",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَنَوٰةَ ٱلثَّالِثَةَ ٱلۡأُخۡرَىٰٓ",
      "translation": "Po Menatën,[339] të tretën, të fundit?",
      "footnotes": "[339] Këta janë emrat e disa idhujve, që arabët paganë i konsideronin si bijat e Allahut."
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 526,
      "verse_key": "53:21",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَكُمُ ٱلذَّكَرُ وَلَهُ ٱلۡأُنثَىٰ",
      "translation": "Vallë, për ju janë djemtë, ndërsa për Atë vajzat?!",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 526,
      "verse_key": "53:22",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تِلۡكَ إِذٗا قِسۡمَةٞ ضِيزَىٰٓ",
      "translation": "Kjo ndarje është e padrejtë.",
      "footnotes": ""
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 526,
      "verse_key": "53:23",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنۡ هِيَ إِلَّآ أَسۡمَآءٞ سَمَّيۡتُمُوهَآ أَنتُمۡ وَءَابَآؤُكُم مَّآ أَنزَلَ ٱللَّهُ بِهَا مِن سُلۡطَٰنٍۚ إِن يَتَّبِعُونَ إِلَّا ٱلظَّنَّ وَمَا تَهۡوَى ٱلۡأَنفُسُۖ وَلَقَدۡ جَآءَهُم مِّن رَّبِّهِمُ ٱلۡهُدَىٰٓ",
      "translation": "Këta janë vetëm emra që ua keni ngjitur (idhujve) ju dhe të parët tuaj, kurse Allahu nuk ju ka dërguar për ata asnjë provë. Ata ndjekin vetëm hamendjet dhe atë që ua ka qejfi, ndonëse u ka ardhur prej Zotit të tyre udhëzimi (për rrugën e drejtë).",
      "footnotes": ""
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 526,
      "verse_key": "53:24",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَمۡ لِلۡإِنسَٰنِ مَا تَمَنَّىٰ",
      "translation": "A do të ketë njeriu gjithçka që dëshiron?",
      "footnotes": ""
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 526,
      "verse_key": "53:25",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلِلَّهِ ٱلۡأٓخِرَةُ وَٱلۡأُولَىٰ",
      "translation": "Vetëm Allahut i përkasin bota tjetër dhe kjo botë!",
      "footnotes": ""
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 526,
      "verse_key": "53:26",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "۞ وَكَم مِّن مَّلَكٖ فِي ٱلسَّمَٰوَٰتِ لَا تُغۡنِي شَفَٰعَتُهُمۡ شَيۡـًٔا إِلَّا مِنۢ بَعۡدِ أَن يَأۡذَنَ ٱللَّهُ لِمَن يَشَآءُ وَيَرۡضَىٰٓ",
      "translation": "Sa engjëj ka në qiej, ndërmjetësimi i të cilëve nuk bën asnjë dobi, veçse pasi të japë leje Allahu, për kë të dojë e të pëlqejë Vetë!",
      "footnotes": ""
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 527,
      "verse_key": "53:27",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ ٱلَّذِينَ لَا يُؤۡمِنُونَ بِٱلۡأٓخِرَةِ لَيُسَمُّونَ ٱلۡمَلَٰٓئِكَةَ تَسۡمِيَةَ ٱلۡأُنثَىٰ",
      "translation": "Vërtet, ata që nuk besojnë në jetën tjetër, u japin engjëjve emra femërorë.",
      "footnotes": ""
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 527,
      "verse_key": "53:28",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا لَهُم بِهِۦ مِنۡ عِلۡمٍۖ إِن يَتَّبِعُونَ إِلَّا ٱلظَّنَّۖ وَإِنَّ ٱلظَّنَّ لَا يُغۡنِي مِنَ ٱلۡحَقِّ شَيۡـٔٗا",
      "translation": "Ata nuk kanë asnjë njohuri për këtë, por ndjekin vetëm hamendjen, ndonëse hamendja nuk mund ta zëvendësojë aspak të vërtetën.",
      "footnotes": ""
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 527,
      "verse_key": "53:29",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَعۡرِضۡ عَن مَّن تَوَلَّىٰ عَن ذِكۡرِنَا وَلَمۡ يُرِدۡ إِلَّا ٱلۡحَيَوٰةَ ٱلدُّنۡيَا",
      "translation": "Andaj, ti (o Muhamed) largohu prej atij, që i kthen kurrizin Kuranit dhe, që dëshiron vetëm jetën e kësaj bote!",
      "footnotes": ""
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 527,
      "verse_key": "53:30",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ذَٰلِكَ مَبۡلَغُهُم مِّنَ ٱلۡعِلۡمِۚ إِنَّ رَبَّكَ هُوَ أَعۡلَمُ بِمَن ضَلَّ عَن سَبِيلِهِۦ وَهُوَ أَعۡلَمُ بِمَنِ ٱهۡتَدَىٰ",
      "translation": "Kaq arrin gjithë dija e tyre! Në të vërtetë, Zoti yt i njeh më së miri ata që janë shmangur nga rruga e Tij dhe ata që ndjekin rrugën e drejtë.",
      "footnotes": ""
    },
    {
      "id": 31,
      "verse_number": 31,
      "page_number": 527,
      "verse_key": "53:31",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلِلَّهِ مَا فِي ٱلسَّمَٰوَٰتِ وَمَا فِي ٱلۡأَرۡضِ لِيَجۡزِيَ ٱلَّذِينَ أَسَٰٓـُٔواْ بِمَا عَمِلُواْ وَيَجۡزِيَ ٱلَّذِينَ أَحۡسَنُواْ بِٱلۡحُسۡنَى",
      "translation": "Allahut i përket gjithçka që gjendet në qiej dhe në Tokë, për t’i dënuar ata që bëjnë keq, sipas veprave të veta e, për t’i shpërblyer ata që bëjnë mirë, me shpërblimin më të mirë.",
      "footnotes": ""
    },
    {
      "id": 32,
      "verse_number": 32,
      "page_number": 527,
      "verse_key": "53:32",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِينَ يَجۡتَنِبُونَ كَبَٰٓئِرَ ٱلۡإِثۡمِ وَٱلۡفَوَٰحِشَ إِلَّا ٱللَّمَمَۚ إِنَّ رَبَّكَ وَٰسِعُ ٱلۡمَغۡفِرَةِۚ هُوَ أَعۡلَمُ بِكُمۡ إِذۡ أَنشَأَكُم مِّنَ ٱلۡأَرۡضِ وَإِذۡ أَنتُمۡ أَجِنَّةٞ فِي بُطُونِ أُمَّهَٰتِكُمۡۖ فَلَا تُزَكُّوٓاْ أَنفُسَكُمۡۖ هُوَ أَعۡلَمُ بِمَنِ ٱتَّقَىٰٓ",
      "translation": "Për ata që ruhen prej gjynaheve të mëdha dhe veprave imorale, përveç gjynaheve të vogla, me të vërtetë, Zoti yt është i Gjerë në falje. Ai ju njeh mjaft mirë, qysh se ju krijoi nga dheu dhe qysh se keni qenë embrion në barkun e nënave tuaja. Prandaj, mos u lavdëroni me pafajësinë tuaj! Ai i njeh më së miri ata që ruhen nga gjynahet.",
      "footnotes": ""
    },
    {
      "id": 33,
      "verse_number": 33,
      "page_number": 527,
      "verse_key": "53:33",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَرَءَيۡتَ ٱلَّذِي تَوَلَّىٰ",
      "translation": "A e ke vënë re atë që shmanget (nga e vërteta)",
      "footnotes": ""
    },
    {
      "id": 34,
      "verse_number": 34,
      "page_number": 527,
      "verse_key": "53:34",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَعۡطَىٰ قَلِيلٗا وَأَكۡدَىٰٓ",
      "translation": "dhe jep pak e pa qejf?",
      "footnotes": ""
    },
    {
      "id": 35,
      "verse_number": 35,
      "page_number": 527,
      "verse_key": "53:35",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَعِندَهُۥ عِلۡمُ ٱلۡغَيۡبِ فَهُوَ يَرَىٰٓ",
      "translation": "A i di ai vallë gjërat e padukshme, a i sheh ato?",
      "footnotes": ""
    },
    {
      "id": 36,
      "verse_number": 36,
      "page_number": 527,
      "verse_key": "53:36",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَمۡ لَمۡ يُنَبَّأۡ بِمَا فِي صُحُفِ مُوسَىٰ",
      "translation": "Vallë, a nuk ka dëgjuar ai për atë që thuhet në Shkrimet e Musait",
      "footnotes": ""
    },
    {
      "id": 37,
      "verse_number": 37,
      "page_number": 527,
      "verse_key": "53:37",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِبۡرَٰهِيمَ ٱلَّذِي وَفَّىٰٓ",
      "translation": "dhe të Ibrahimit - i cili i plotësoi detyrimet:",
      "footnotes": ""
    },
    {
      "id": 38,
      "verse_number": 38,
      "page_number": 527,
      "verse_key": "53:38",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَّا تَزِرُ وَازِرَةٞ وِزۡرَ أُخۡرَىٰ",
      "translation": "se askush nuk do t’i mbartë gjynahet e tjetrit",
      "footnotes": ""
    },
    {
      "id": 39,
      "verse_number": 39,
      "page_number": 527,
      "verse_key": "53:39",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَن لَّيۡسَ لِلۡإِنسَٰنِ إِلَّا مَا سَعَىٰ",
      "translation": "dhe se njeriu do të ketë vetëm atë për të cilën ka punuar;",
      "footnotes": ""
    },
    {
      "id": 40,
      "verse_number": 40,
      "page_number": 527,
      "verse_key": "53:40",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّ سَعۡيَهُۥ سَوۡفَ يُرَىٰ",
      "translation": "se përpjekjet e tij do të shihen",
      "footnotes": ""
    },
    {
      "id": 41,
      "verse_number": 41,
      "page_number": 527,
      "verse_key": "53:41",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ يُجۡزَىٰهُ ٱلۡجَزَآءَ ٱلۡأَوۡفَىٰ",
      "translation": "dhe se do të shpërblehet me shpërblim të plotë;",
      "footnotes": ""
    },
    {
      "id": 42,
      "verse_number": 42,
      "page_number": 527,
      "verse_key": "53:42",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّ إِلَىٰ رَبِّكَ ٱلۡمُنتَهَىٰ",
      "translation": "se te Zoti yt do të jetë kthimi;",
      "footnotes": ""
    },
    {
      "id": 43,
      "verse_number": 43,
      "page_number": 527,
      "verse_key": "53:43",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّهُۥ هُوَ أَضۡحَكَ وَأَبۡكَىٰ",
      "translation": "se Ai jep gazin dhe vajin",
      "footnotes": ""
    },
    {
      "id": 44,
      "verse_number": 44,
      "page_number": 527,
      "verse_key": "53:44",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّهُۥ هُوَ أَمَاتَ وَأَحۡيَا",
      "translation": "dhe se Ai jep vdekjen dhe jetën;",
      "footnotes": ""
    },
    {
      "id": 45,
      "verse_number": 45,
      "page_number": 528,
      "verse_key": "53:45",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّهُۥ خَلَقَ ٱلزَّوۡجَيۡنِ ٱلذَّكَرَ وَٱلۡأُنثَىٰ",
      "translation": "se Ai krijon çiftet, mashkull e femër,",
      "footnotes": ""
    },
    {
      "id": 46,
      "verse_number": 46,
      "page_number": 528,
      "verse_key": "53:46",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مِن نُّطۡفَةٍ إِذَا تُمۡنَىٰ",
      "translation": "prej një pike fare kur derdhet,",
      "footnotes": ""
    },
    {
      "id": 47,
      "verse_number": 47,
      "page_number": 528,
      "verse_key": "53:47",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّ عَلَيۡهِ ٱلنَّشۡأَةَ ٱلۡأُخۡرَىٰ",
      "translation": "dhe se Atij i takon krijimi tjetër;",
      "footnotes": ""
    },
    {
      "id": 48,
      "verse_number": 48,
      "page_number": 528,
      "verse_key": "53:48",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّهُۥ هُوَ أَغۡنَىٰ وَأَقۡنَىٰ",
      "translation": "se Ai jep pasuri dhe kënaqësi të mjaftueshme,",
      "footnotes": ""
    },
    {
      "id": 49,
      "verse_number": 49,
      "page_number": 528,
      "verse_key": "53:49",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّهُۥ هُوَ رَبُّ ٱلشِّعۡرَىٰ",
      "translation": "Ai, që është Zoti i Siriusit[340];",
      "footnotes": "[340] Siriusi është yll i cili adhurohej nga arabët paganë."
    },
    {
      "id": 50,
      "verse_number": 50,
      "page_number": 528,
      "verse_key": "53:50",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّهُۥٓ أَهۡلَكَ عَادًا ٱلۡأُولَىٰ",
      "translation": "se Ai i ka shkatërruar fiset e lashta Ad",
      "footnotes": ""
    },
    {
      "id": 51,
      "verse_number": 51,
      "page_number": 528,
      "verse_key": "53:51",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَثَمُودَاْ فَمَآ أَبۡقَىٰ",
      "translation": "e Themud, duke mos lënë askënd prej tyre,",
      "footnotes": ""
    },
    {
      "id": 52,
      "verse_number": 52,
      "page_number": 528,
      "verse_key": "53:52",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَقَوۡمَ نُوحٖ مِّن قَبۡلُۖ إِنَّهُمۡ كَانُواْ هُمۡ أَظۡلَمَ وَأَطۡغَىٰ",
      "translation": "si dhe popullin e Nuhut më parë, sepse qe më i padrejtë dhe rebel;",
      "footnotes": ""
    },
    {
      "id": 53,
      "verse_number": 53,
      "page_number": 528,
      "verse_key": "53:53",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡمُؤۡتَفِكَةَ أَهۡوَىٰ",
      "translation": "se Ai i asgjësoi edhe qytetet e përmbysura (të popullit të Lutit),",
      "footnotes": ""
    },
    {
      "id": 54,
      "verse_number": 54,
      "page_number": 528,
      "verse_key": "53:54",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَغَشَّىٰهَا مَا غَشَّىٰ",
      "translation": "të cilat i mbuloi ajo që i mbuloi?!",
      "footnotes": ""
    },
    {
      "id": 55,
      "verse_number": 55,
      "page_number": 528,
      "verse_key": "53:55",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكَ تَتَمَارَىٰ",
      "translation": "Atëherë, në çfarë dhuntie të Zotit tënd dyshon ti?",
      "footnotes": ""
    },
    {
      "id": 56,
      "verse_number": 56,
      "page_number": 528,
      "verse_key": "53:56",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "هَٰذَا نَذِيرٞ مِّنَ ٱلنُّذُرِ ٱلۡأُولَىٰٓ",
      "translation": "Ky (Profet) është paralajmërues si ata të mëparshmit.",
      "footnotes": ""
    },
    {
      "id": 57,
      "verse_number": 57,
      "page_number": 528,
      "verse_key": "53:57",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَزِفَتِ ٱلۡأٓزِفَةُ",
      "translation": "Ora e Kiametit po afrohet",
      "footnotes": ""
    },
    {
      "id": 58,
      "verse_number": 58,
      "page_number": 528,
      "verse_key": "53:58",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَيۡسَ لَهَا مِن دُونِ ٱللَّهِ كَاشِفَةٌ",
      "translation": "dhe askush, përveç Allahut, nuk mund ta zbulojë atë!",
      "footnotes": ""
    },
    {
      "id": 59,
      "verse_number": 59,
      "page_number": 528,
      "verse_key": "53:59",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَمِنۡ هَٰذَا ٱلۡحَدِيثِ تَعۡجَبُونَ",
      "translation": "Vallë, prej kësaj fjale po çuditeni",
      "footnotes": ""
    },
    {
      "id": 60,
      "verse_number": 60,
      "page_number": 528,
      "verse_key": "53:60",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَتَضۡحَكُونَ وَلَا تَبۡكُونَ",
      "translation": "dhe po qeshni e nuk po qani?",
      "footnotes": ""
    },
    {
      "id": 61,
      "verse_number": 61,
      "page_number": 528,
      "verse_key": "53:61",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنتُمۡ سَٰمِدُونَ",
      "translation": "Madje, jeni edhe të shkujdesur!",
      "footnotes": ""
    },
    {
      "id": 62,
      "verse_number": 62,
      "page_number": 528,
      "verse_key": "53:62",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": "obligatory",
      "sajdah_number": 12,
      "text": "فَٱسۡجُدُواْۤ لِلَّهِۤ وَٱعۡبُدُواْ۩",
      "translation": "Më mirë përuluni në sexhde për Allahun dhe adhurojeni Atë!",
      "footnotes": ""
    }
  ]
}