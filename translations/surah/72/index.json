{
  "verses": [
    {
      "id": 5448,
      "number": 1,
      "verse_key": "72:1",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 508,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 572,
      "juz_number": 29,
      "text": "قُلۡ أُوحِيَ إِلَيَّ أَنَّهُ ٱسۡتَمَعَ نَفَرٞ مِّنَ ٱلۡجِنِّ فَقَالُوٓاْ إِنَّا سَمِعۡنَا قُرۡءَانًا عَجَبٗا"
    },
    {
      "id": 5449,
      "number": 2,
      "verse_key": "72:2",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 508,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 572,
      "juz_number": 29,
      "text": "يَهۡدِيٓ إِلَى ٱلرُّشۡدِ فَـَٔامَنَّا بِهِۦۖ وَلَن نُّشۡرِكَ بِرَبِّنَآ أَحَدٗا"
    },
    {
      "id": 5450,
      "number": 3,
      "verse_key": "72:3",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 508,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 572,
      "juz_number": 29,
      "text": "وَأَنَّهُۥ تَعَٰلَىٰ جَدُّ رَبِّنَا مَا ٱتَّخَذَ صَٰحِبَةٗ وَلَا وَلَدٗا"
    },
    {
      "id": 5451,
      "number": 4,
      "verse_key": "72:4",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 508,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 572,
      "juz_number": 29,
      "text": "وَأَنَّهُۥ كَانَ يَقُولُ سَفِيهُنَا عَلَى ٱللَّهِ شَطَطٗا"
    },
    {
      "id": 5452,
      "number": 5,
      "verse_key": "72:5",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 508,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 572,
      "juz_number": 29,
      "text": "وَأَنَّا ظَنَنَّآ أَن لَّن تَقُولَ ٱلۡإِنسُ وَٱلۡجِنُّ عَلَى ٱللَّهِ كَذِبٗا"
    },
    {
      "id": 5453,
      "number": 6,
      "verse_key": "72:6",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 508,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 572,
      "juz_number": 29,
      "text": "وَأَنَّهُۥ كَانَ رِجَالٞ مِّنَ ٱلۡإِنسِ يَعُوذُونَ بِرِجَالٖ مِّنَ ٱلۡجِنِّ فَزَادُوهُمۡ رَهَقٗا"
    },
    {
      "id": 5454,
      "number": 7,
      "verse_key": "72:7",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 508,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 572,
      "juz_number": 29,
      "text": "وَأَنَّهُمۡ ظَنُّواْ كَمَا ظَنَنتُمۡ أَن لَّن يَبۡعَثَ ٱللَّهُ أَحَدٗا"
    },
    {
      "id": 5455,
      "number": 8,
      "verse_key": "72:8",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 508,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 572,
      "juz_number": 29,
      "text": "وَأَنَّا لَمَسۡنَا ٱلسَّمَآءَ فَوَجَدۡنَٰهَا مُلِئَتۡ حَرَسٗا شَدِيدٗا وَشُهُبٗا"
    },
    {
      "id": 5456,
      "number": 9,
      "verse_key": "72:9",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 508,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 572,
      "juz_number": 29,
      "text": "وَأَنَّا كُنَّا نَقۡعُدُ مِنۡهَا مَقَٰعِدَ لِلسَّمۡعِۖ فَمَن يَسۡتَمِعِ ٱلۡأٓنَ يَجِدۡ لَهُۥ شِهَابٗا رَّصَدٗا"
    },
    {
      "id": 5457,
      "number": 10,
      "verse_key": "72:10",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 508,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 572,
      "juz_number": 29,
      "text": "وَأَنَّا لَا نَدۡرِيٓ أَشَرٌّ أُرِيدَ بِمَن فِي ٱلۡأَرۡضِ أَمۡ أَرَادَ بِهِمۡ رَبُّهُمۡ رَشَدٗا"
    },
    {
      "id": 5458,
      "number": 11,
      "verse_key": "72:11",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 508,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 572,
      "juz_number": 29,
      "text": "وَأَنَّا مِنَّا ٱلصَّٰلِحُونَ وَمِنَّا دُونَ ذَٰلِكَۖ كُنَّا طَرَآئِقَ قِدَدٗا"
    },
    {
      "id": 5459,
      "number": 12,
      "verse_key": "72:12",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 508,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 572,
      "juz_number": 29,
      "text": "وَأَنَّا ظَنَنَّآ أَن لَّن نُّعۡجِزَ ٱللَّهَ فِي ٱلۡأَرۡضِ وَلَن نُّعۡجِزَهُۥ هَرَبٗا"
    },
    {
      "id": 5460,
      "number": 13,
      "verse_key": "72:13",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 508,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 572,
      "juz_number": 29,
      "text": "وَأَنَّا لَمَّا سَمِعۡنَا ٱلۡهُدَىٰٓ ءَامَنَّا بِهِۦۖ فَمَن يُؤۡمِنۢ بِرَبِّهِۦ فَلَا يَخَافُ بَخۡسٗا وَلَا رَهَقٗا"
    },
    {
      "id": 5461,
      "number": 14,
      "verse_key": "72:14",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 508,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 573,
      "juz_number": 29,
      "text": "وَأَنَّا مِنَّا ٱلۡمُسۡلِمُونَ وَمِنَّا ٱلۡقَٰسِطُونَۖ فَمَنۡ أَسۡلَمَ فَأُوْلَٰٓئِكَ تَحَرَّوۡاْ رَشَدٗا"
    },
    {
      "id": 5462,
      "number": 15,
      "verse_key": "72:15",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 508,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 573,
      "juz_number": 29,
      "text": "وَأَمَّا ٱلۡقَٰسِطُونَ فَكَانُواْ لِجَهَنَّمَ حَطَبٗا"
    },
    {
      "id": 5463,
      "number": 16,
      "verse_key": "72:16",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 508,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 573,
      "juz_number": 29,
      "text": "وَأَلَّوِ ٱسۡتَقَٰمُواْ عَلَى ٱلطَّرِيقَةِ لَأَسۡقَيۡنَٰهُم مَّآءً غَدَقٗا"
    },
    {
      "id": 5464,
      "number": 17,
      "verse_key": "72:17",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 508,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 573,
      "juz_number": 29,
      "text": "لِّنَفۡتِنَهُمۡ فِيهِۚ وَمَن يُعۡرِضۡ عَن ذِكۡرِ رَبِّهِۦ يَسۡلُكۡهُ عَذَابٗا صَعَدٗا"
    },
    {
      "id": 5465,
      "number": 18,
      "verse_key": "72:18",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 508,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 573,
      "juz_number": 29,
      "text": "وَأَنَّ ٱلۡمَسَٰجِدَ لِلَّهِ فَلَا تَدۡعُواْ مَعَ ٱللَّهِ أَحَدٗا"
    },
    {
      "id": 5466,
      "number": 19,
      "verse_key": "72:19",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 508,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 573,
      "juz_number": 29,
      "text": "وَأَنَّهُۥ لَمَّا قَامَ عَبۡدُ ٱللَّهِ يَدۡعُوهُ كَادُواْ يَكُونُونَ عَلَيۡهِ لِبَدٗا"
    },
    {
      "id": 5467,
      "number": 20,
      "verse_key": "72:20",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 509,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 573,
      "juz_number": 29,
      "text": "قُلۡ إِنَّمَآ أَدۡعُواْ رَبِّي وَلَآ أُشۡرِكُ بِهِۦٓ أَحَدٗا"
    },
    {
      "id": 5468,
      "number": 21,
      "verse_key": "72:21",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 509,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 573,
      "juz_number": 29,
      "text": "قُلۡ إِنِّي لَآ أَمۡلِكُ لَكُمۡ ضَرّٗا وَلَا رَشَدٗا"
    },
    {
      "id": 5469,
      "number": 22,
      "verse_key": "72:22",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 509,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 573,
      "juz_number": 29,
      "text": "قُلۡ إِنِّي لَن يُجِيرَنِي مِنَ ٱللَّهِ أَحَدٞ وَلَنۡ أَجِدَ مِن دُونِهِۦ مُلۡتَحَدًا"
    },
    {
      "id": 5470,
      "number": 23,
      "verse_key": "72:23",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 509,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 573,
      "juz_number": 29,
      "text": "إِلَّا بَلَٰغٗا مِّنَ ٱللَّهِ وَرِسَٰلَٰتِهِۦۚ وَمَن يَعۡصِ ٱللَّهَ وَرَسُولَهُۥ فَإِنَّ لَهُۥ نَارَ جَهَنَّمَ خَٰلِدِينَ فِيهَآ أَبَدًا"
    },
    {
      "id": 5471,
      "number": 24,
      "verse_key": "72:24",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 509,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 573,
      "juz_number": 29,
      "text": "حَتَّىٰٓ إِذَا رَأَوۡاْ مَا يُوعَدُونَ فَسَيَعۡلَمُونَ مَنۡ أَضۡعَفُ نَاصِرٗا وَأَقَلُّ عَدَدٗا"
    },
    {
      "id": 5472,
      "number": 25,
      "verse_key": "72:25",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 509,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 573,
      "juz_number": 29,
      "text": "قُلۡ إِنۡ أَدۡرِيٓ أَقَرِيبٞ مَّا تُوعَدُونَ أَمۡ يَجۡعَلُ لَهُۥ رَبِّيٓ أَمَدًا"
    },
    {
      "id": 5473,
      "number": 26,
      "verse_key": "72:26",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 509,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 573,
      "juz_number": 29,
      "text": "عَٰلِمُ ٱلۡغَيۡبِ فَلَا يُظۡهِرُ عَلَىٰ غَيۡبِهِۦٓ أَحَدًا"
    },
    {
      "id": 5474,
      "number": 27,
      "verse_key": "72:27",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 509,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 573,
      "juz_number": 29,
      "text": "إِلَّا مَنِ ٱرۡتَضَىٰ مِن رَّسُولٖ فَإِنَّهُۥ يَسۡلُكُ مِنۢ بَيۡنِ يَدَيۡهِ وَمِنۡ خَلۡفِهِۦ رَصَدٗا"
    },
    {
      "id": 5475,
      "number": 28,
      "verse_key": "72:28",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 509,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 573,
      "juz_number": 29,
      "text": "لِّيَعۡلَمَ أَن قَدۡ أَبۡلَغُواْ رِسَٰلَٰتِ رَبِّهِمۡ وَأَحَاطَ بِمَا لَدَيۡهِمۡ وَأَحۡصَىٰ كُلَّ شَيۡءٍ عَدَدَۢا"
    }
  ]
}