{
  "verses": [
    {
      "id": 6139,
      "number": 1,
      "verse_key": "99:1",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 543,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 599,
      "juz_number": 30,
      "text": "إِذَا زُلۡزِلَتِ ٱلۡأَرۡضُ زِلۡزَالَهَا"
    },
    {
      "id": 6140,
      "number": 2,
      "verse_key": "99:2",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 543,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 599,
      "juz_number": 30,
      "text": "وَأَخۡرَجَتِ ٱلۡأَرۡضُ أَثۡقَالَهَا"
    },
    {
      "id": 6141,
      "number": 3,
      "verse_key": "99:3",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 543,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 599,
      "juz_number": 30,
      "text": "وَقَالَ ٱلۡإِنسَٰنُ مَا لَهَا"
    },
    {
      "id": 6142,
      "number": 4,
      "verse_key": "99:4",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 543,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 599,
      "juz_number": 30,
      "text": "يَوۡمَئِذٖ تُحَدِّثُ أَخۡبَارَهَا"
    },
    {
      "id": 6143,
      "number": 5,
      "verse_key": "99:5",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 543,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 599,
      "juz_number": 30,
      "text": "بِأَنَّ رَبَّكَ أَوۡحَىٰ لَهَا"
    },
    {
      "id": 6144,
      "number": 6,
      "verse_key": "99:6",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 543,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 599,
      "juz_number": 30,
      "text": "يَوۡمَئِذٖ يَصۡدُرُ ٱلنَّاسُ أَشۡتَاتٗا لِّيُرَوۡاْ أَعۡمَٰلَهُمۡ"
    },
    {
      "id": 6145,
      "number": 7,
      "verse_key": "99:7",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 543,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 599,
      "juz_number": 30,
      "text": "فَمَن يَعۡمَلۡ مِثۡقَالَ ذَرَّةٍ خَيۡرٗا يَرَهُۥ"
    },
    {
      "id": 6146,
      "number": 8,
      "verse_key": "99:8",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 543,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 599,
      "juz_number": 30,
      "text": "وَمَن يَعۡمَلۡ مِثۡقَالَ ذَرَّةٖ شَرّٗا يَرَهُۥ"
    }
  ]
}