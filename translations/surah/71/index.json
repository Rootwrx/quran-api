{
  "verses": [
    {
      "id": 5420,
      "number": 1,
      "verse_key": "71:1",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 506,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 570,
      "juz_number": 29,
      "text": "إِنَّآ أَرۡسَلۡنَا نُوحًا إِلَىٰ قَوۡمِهِۦٓ أَنۡ أَنذِرۡ قَوۡمَكَ مِن قَبۡلِ أَن يَأۡتِيَهُمۡ عَذَابٌ أَلِيمٞ"
    },
    {
      "id": 5421,
      "number": 2,
      "verse_key": "71:2",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 506,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 570,
      "juz_number": 29,
      "text": "قَالَ يَٰقَوۡمِ إِنِّي لَكُمۡ نَذِيرٞ مُّبِينٌ"
    },
    {
      "id": 5422,
      "number": 3,
      "verse_key": "71:3",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 506,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 570,
      "juz_number": 29,
      "text": "أَنِ ٱعۡبُدُواْ ٱللَّهَ وَٱتَّقُوهُ وَأَطِيعُونِ"
    },
    {
      "id": 5423,
      "number": 4,
      "verse_key": "71:4",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 506,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 570,
      "juz_number": 29,
      "text": "يَغۡفِرۡ لَكُم مِّن ذُنُوبِكُمۡ وَيُؤَخِّرۡكُمۡ إِلَىٰٓ أَجَلٖ مُّسَمًّىۚ إِنَّ أَجَلَ ٱللَّهِ إِذَا جَآءَ لَا يُؤَخَّرُۚ لَوۡ كُنتُمۡ تَعۡلَمُونَ"
    },
    {
      "id": 5424,
      "number": 5,
      "verse_key": "71:5",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 506,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 570,
      "juz_number": 29,
      "text": "قَالَ رَبِّ إِنِّي دَعَوۡتُ قَوۡمِي لَيۡلٗا وَنَهَارٗا"
    },
    {
      "id": 5425,
      "number": 6,
      "verse_key": "71:6",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 506,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 570,
      "juz_number": 29,
      "text": "فَلَمۡ يَزِدۡهُمۡ دُعَآءِيٓ إِلَّا فِرَارٗا"
    },
    {
      "id": 5426,
      "number": 7,
      "verse_key": "71:7",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 506,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 570,
      "juz_number": 29,
      "text": "وَإِنِّي كُلَّمَا دَعَوۡتُهُمۡ لِتَغۡفِرَ لَهُمۡ جَعَلُوٓاْ أَصَٰبِعَهُمۡ فِيٓ ءَاذَانِهِمۡ وَٱسۡتَغۡشَوۡاْ ثِيَابَهُمۡ وَأَصَرُّواْ وَٱسۡتَكۡبَرُواْ ٱسۡتِكۡبَارٗا"
    },
    {
      "id": 5427,
      "number": 8,
      "verse_key": "71:8",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 506,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 570,
      "juz_number": 29,
      "text": "ثُمَّ إِنِّي دَعَوۡتُهُمۡ جِهَارٗا"
    },
    {
      "id": 5428,
      "number": 9,
      "verse_key": "71:9",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 506,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 570,
      "juz_number": 29,
      "text": "ثُمَّ إِنِّيٓ أَعۡلَنتُ لَهُمۡ وَأَسۡرَرۡتُ لَهُمۡ إِسۡرَارٗا"
    },
    {
      "id": 5429,
      "number": 10,
      "verse_key": "71:10",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 506,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 570,
      "juz_number": 29,
      "text": "فَقُلۡتُ ٱسۡتَغۡفِرُواْ رَبَّكُمۡ إِنَّهُۥ كَانَ غَفَّارٗا"
    },
    {
      "id": 5430,
      "number": 11,
      "verse_key": "71:11",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 506,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 571,
      "juz_number": 29,
      "text": "يُرۡسِلِ ٱلسَّمَآءَ عَلَيۡكُم مِّدۡرَارٗا"
    },
    {
      "id": 5431,
      "number": 12,
      "verse_key": "71:12",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 506,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 571,
      "juz_number": 29,
      "text": "وَيُمۡدِدۡكُم بِأَمۡوَٰلٖ وَبَنِينَ وَيَجۡعَل لَّكُمۡ جَنَّٰتٖ وَيَجۡعَل لَّكُمۡ أَنۡهَٰرٗا"
    },
    {
      "id": 5432,
      "number": 13,
      "verse_key": "71:13",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 506,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 571,
      "juz_number": 29,
      "text": "مَّا لَكُمۡ لَا تَرۡجُونَ لِلَّهِ وَقَارٗا"
    },
    {
      "id": 5433,
      "number": 14,
      "verse_key": "71:14",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 506,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 571,
      "juz_number": 29,
      "text": "وَقَدۡ خَلَقَكُمۡ أَطۡوَارًا"
    },
    {
      "id": 5434,
      "number": 15,
      "verse_key": "71:15",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 506,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 571,
      "juz_number": 29,
      "text": "أَلَمۡ تَرَوۡاْ كَيۡفَ خَلَقَ ٱللَّهُ سَبۡعَ سَمَٰوَٰتٖ طِبَاقٗا"
    },
    {
      "id": 5435,
      "number": 16,
      "verse_key": "71:16",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 506,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 571,
      "juz_number": 29,
      "text": "وَجَعَلَ ٱلۡقَمَرَ فِيهِنَّ نُورٗا وَجَعَلَ ٱلشَّمۡسَ سِرَاجٗا"
    },
    {
      "id": 5436,
      "number": 17,
      "verse_key": "71:17",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 506,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 571,
      "juz_number": 29,
      "text": "وَٱللَّهُ أَنۢبَتَكُم مِّنَ ٱلۡأَرۡضِ نَبَاتٗا"
    },
    {
      "id": 5437,
      "number": 18,
      "verse_key": "71:18",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 506,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 571,
      "juz_number": 29,
      "text": "ثُمَّ يُعِيدُكُمۡ فِيهَا وَيُخۡرِجُكُمۡ إِخۡرَاجٗا"
    },
    {
      "id": 5438,
      "number": 19,
      "verse_key": "71:19",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 506,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 571,
      "juz_number": 29,
      "text": "وَٱللَّهُ جَعَلَ لَكُمُ ٱلۡأَرۡضَ بِسَاطٗا"
    },
    {
      "id": 5439,
      "number": 20,
      "verse_key": "71:20",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 506,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 571,
      "juz_number": 29,
      "text": "لِّتَسۡلُكُواْ مِنۡهَا سُبُلٗا فِجَاجٗا"
    },
    {
      "id": 5440,
      "number": 21,
      "verse_key": "71:21",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 507,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 571,
      "juz_number": 29,
      "text": "قَالَ نُوحٞ رَّبِّ إِنَّهُمۡ عَصَوۡنِي وَٱتَّبَعُواْ مَن لَّمۡ يَزِدۡهُ مَالُهُۥ وَوَلَدُهُۥٓ إِلَّا خَسَارٗا"
    },
    {
      "id": 5441,
      "number": 22,
      "verse_key": "71:22",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 507,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 571,
      "juz_number": 29,
      "text": "وَمَكَرُواْ مَكۡرٗا كُبَّارٗا"
    },
    {
      "id": 5442,
      "number": 23,
      "verse_key": "71:23",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 507,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 571,
      "juz_number": 29,
      "text": "وَقَالُواْ لَا تَذَرُنَّ ءَالِهَتَكُمۡ وَلَا تَذَرُنَّ وَدّٗا وَلَا سُوَاعٗا وَلَا يَغُوثَ وَيَعُوقَ وَنَسۡرٗا"
    },
    {
      "id": 5443,
      "number": 24,
      "verse_key": "71:24",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 507,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 571,
      "juz_number": 29,
      "text": "وَقَدۡ أَضَلُّواْ كَثِيرٗاۖ وَلَا تَزِدِ ٱلظَّٰلِمِينَ إِلَّا ضَلَٰلٗا"
    },
    {
      "id": 5444,
      "number": 25,
      "verse_key": "71:25",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 507,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 571,
      "juz_number": 29,
      "text": "مِّمَّا خَطِيٓـَٰٔتِهِمۡ أُغۡرِقُواْ فَأُدۡخِلُواْ نَارٗا فَلَمۡ يَجِدُواْ لَهُم مِّن دُونِ ٱللَّهِ أَنصَارٗا"
    },
    {
      "id": 5445,
      "number": 26,
      "verse_key": "71:26",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 507,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 571,
      "juz_number": 29,
      "text": "وَقَالَ نُوحٞ رَّبِّ لَا تَذَرۡ عَلَى ٱلۡأَرۡضِ مِنَ ٱلۡكَٰفِرِينَ دَيَّارًا"
    },
    {
      "id": 5446,
      "number": 27,
      "verse_key": "71:27",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 507,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 571,
      "juz_number": 29,
      "text": "إِنَّكَ إِن تَذَرۡهُمۡ يُضِلُّواْ عِبَادَكَ وَلَا يَلِدُوٓاْ إِلَّا فَاجِرٗا كَفَّارٗا"
    },
    {
      "id": 5447,
      "number": 28,
      "verse_key": "71:28",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 507,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 571,
      "juz_number": 29,
      "text": "رَّبِّ ٱغۡفِرۡ لِي وَلِوَٰلِدَيَّ وَلِمَن دَخَلَ بَيۡتِيَ مُؤۡمِنٗا وَلِلۡمُؤۡمِنِينَ وَٱلۡمُؤۡمِنَٰتِۖ وَلَا تَزِدِ ٱلظَّٰلِمِينَ إِلَّا تَبَارَۢا"
    }
  ]
}