{
  "verses": [
    {
      "id": 5592,
      "number": 1,
      "verse_key": "76:1",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 516,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 578,
      "juz_number": 29,
      "text": "هَلۡ أَتَىٰ عَلَى ٱلۡإِنسَٰنِ حِينٞ مِّنَ ٱلدَّهۡرِ لَمۡ يَكُن شَيۡـٔٗا مَّذۡكُورًا"
    },
    {
      "id": 5593,
      "number": 2,
      "verse_key": "76:2",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 516,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 578,
      "juz_number": 29,
      "text": "إِنَّا خَلَقۡنَا ٱلۡإِنسَٰنَ مِن نُّطۡفَةٍ أَمۡشَاجٖ نَّبۡتَلِيهِ فَجَعَلۡنَٰهُ سَمِيعَۢا بَصِيرًا"
    },
    {
      "id": 5594,
      "number": 3,
      "verse_key": "76:3",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 516,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 578,
      "juz_number": 29,
      "text": "إِنَّا هَدَيۡنَٰهُ ٱلسَّبِيلَ إِمَّا شَاكِرٗا وَإِمَّا كَفُورًا"
    },
    {
      "id": 5595,
      "number": 4,
      "verse_key": "76:4",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 516,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 578,
      "juz_number": 29,
      "text": "إِنَّآ أَعۡتَدۡنَا لِلۡكَٰفِرِينَ سَلَٰسِلَاْ وَأَغۡلَٰلٗا وَسَعِيرًا"
    },
    {
      "id": 5596,
      "number": 5,
      "verse_key": "76:5",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 516,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 578,
      "juz_number": 29,
      "text": "إِنَّ ٱلۡأَبۡرَارَ يَشۡرَبُونَ مِن كَأۡسٖ كَانَ مِزَاجُهَا كَافُورًا"
    },
    {
      "id": 5597,
      "number": 6,
      "verse_key": "76:6",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 516,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 579,
      "juz_number": 29,
      "text": "عَيۡنٗا يَشۡرَبُ بِهَا عِبَادُ ٱللَّهِ يُفَجِّرُونَهَا تَفۡجِيرٗا"
    },
    {
      "id": 5598,
      "number": 7,
      "verse_key": "76:7",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 516,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 579,
      "juz_number": 29,
      "text": "يُوفُونَ بِٱلنَّذۡرِ وَيَخَافُونَ يَوۡمٗا كَانَ شَرُّهُۥ مُسۡتَطِيرٗا"
    },
    {
      "id": 5599,
      "number": 8,
      "verse_key": "76:8",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 516,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 579,
      "juz_number": 29,
      "text": "وَيُطۡعِمُونَ ٱلطَّعَامَ عَلَىٰ حُبِّهِۦ مِسۡكِينٗا وَيَتِيمٗا وَأَسِيرًا"
    },
    {
      "id": 5600,
      "number": 9,
      "verse_key": "76:9",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 516,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 579,
      "juz_number": 29,
      "text": "إِنَّمَا نُطۡعِمُكُمۡ لِوَجۡهِ ٱللَّهِ لَا نُرِيدُ مِنكُمۡ جَزَآءٗ وَلَا شُكُورًا"
    },
    {
      "id": 5601,
      "number": 10,
      "verse_key": "76:10",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 516,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 579,
      "juz_number": 29,
      "text": "إِنَّا نَخَافُ مِن رَّبِّنَا يَوۡمًا عَبُوسٗا قَمۡطَرِيرٗا"
    },
    {
      "id": 5602,
      "number": 11,
      "verse_key": "76:11",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 516,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 579,
      "juz_number": 29,
      "text": "فَوَقَىٰهُمُ ٱللَّهُ شَرَّ ذَٰلِكَ ٱلۡيَوۡمِ وَلَقَّىٰهُمۡ نَضۡرَةٗ وَسُرُورٗا"
    },
    {
      "id": 5603,
      "number": 12,
      "verse_key": "76:12",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 516,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 579,
      "juz_number": 29,
      "text": "وَجَزَىٰهُم بِمَا صَبَرُواْ جَنَّةٗ وَحَرِيرٗا"
    },
    {
      "id": 5604,
      "number": 13,
      "verse_key": "76:13",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 516,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 579,
      "juz_number": 29,
      "text": "مُّتَّكِـِٔينَ فِيهَا عَلَى ٱلۡأَرَآئِكِۖ لَا يَرَوۡنَ فِيهَا شَمۡسٗا وَلَا زَمۡهَرِيرٗا"
    },
    {
      "id": 5605,
      "number": 14,
      "verse_key": "76:14",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 516,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 579,
      "juz_number": 29,
      "text": "وَدَانِيَةً عَلَيۡهِمۡ ظِلَٰلُهَا وَذُلِّلَتۡ قُطُوفُهَا تَذۡلِيلٗا"
    },
    {
      "id": 5606,
      "number": 15,
      "verse_key": "76:15",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 516,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 579,
      "juz_number": 29,
      "text": "وَيُطَافُ عَلَيۡهِم بِـَٔانِيَةٖ مِّن فِضَّةٖ وَأَكۡوَابٖ كَانَتۡ قَوَارِيرَا۠"
    },
    {
      "id": 5607,
      "number": 16,
      "verse_key": "76:16",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 516,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 579,
      "juz_number": 29,
      "text": "قَوَارِيرَاْ مِن فِضَّةٖ قَدَّرُوهَا تَقۡدِيرٗا"
    },
    {
      "id": 5608,
      "number": 17,
      "verse_key": "76:17",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 516,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 579,
      "juz_number": 29,
      "text": "وَيُسۡقَوۡنَ فِيهَا كَأۡسٗا كَانَ مِزَاجُهَا زَنجَبِيلًا"
    },
    {
      "id": 5609,
      "number": 18,
      "verse_key": "76:18",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 516,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 579,
      "juz_number": 29,
      "text": "عَيۡنٗا فِيهَا تُسَمَّىٰ سَلۡسَبِيلٗا"
    },
    {
      "id": 5610,
      "number": 19,
      "verse_key": "76:19",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 516,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 579,
      "juz_number": 29,
      "text": "۞ وَيَطُوفُ عَلَيۡهِمۡ وِلۡدَٰنٞ مُّخَلَّدُونَ إِذَا رَأَيۡتَهُمۡ حَسِبۡتَهُمۡ لُؤۡلُؤٗا مَّنثُورٗا"
    },
    {
      "id": 5611,
      "number": 20,
      "verse_key": "76:20",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 516,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 579,
      "juz_number": 29,
      "text": "وَإِذَا رَأَيۡتَ ثَمَّ رَأَيۡتَ نَعِيمٗا وَمُلۡكٗا كَبِيرًا"
    },
    {
      "id": 5612,
      "number": 21,
      "verse_key": "76:21",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 516,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 579,
      "juz_number": 29,
      "text": "عَٰلِيَهُمۡ ثِيَابُ سُندُسٍ خُضۡرٞ وَإِسۡتَبۡرَقٞۖ وَحُلُّوٓاْ أَسَاوِرَ مِن فِضَّةٖ وَسَقَىٰهُمۡ رَبُّهُمۡ شَرَابٗا طَهُورًا"
    },
    {
      "id": 5613,
      "number": 22,
      "verse_key": "76:22",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 516,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 579,
      "juz_number": 29,
      "text": "إِنَّ هَٰذَا كَانَ لَكُمۡ جَزَآءٗ وَكَانَ سَعۡيُكُم مَّشۡكُورًا"
    },
    {
      "id": 5614,
      "number": 23,
      "verse_key": "76:23",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 517,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 579,
      "juz_number": 29,
      "text": "إِنَّا نَحۡنُ نَزَّلۡنَا عَلَيۡكَ ٱلۡقُرۡءَانَ تَنزِيلٗا"
    },
    {
      "id": 5615,
      "number": 24,
      "verse_key": "76:24",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 517,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 579,
      "juz_number": 29,
      "text": "فَٱصۡبِرۡ لِحُكۡمِ رَبِّكَ وَلَا تُطِعۡ مِنۡهُمۡ ءَاثِمًا أَوۡ كَفُورٗا"
    },
    {
      "id": 5616,
      "number": 25,
      "verse_key": "76:25",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 517,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 579,
      "juz_number": 29,
      "text": "وَٱذۡكُرِ ٱسۡمَ رَبِّكَ بُكۡرَةٗ وَأَصِيلٗا"
    },
    {
      "id": 5617,
      "number": 26,
      "verse_key": "76:26",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 517,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 580,
      "juz_number": 29,
      "text": "وَمِنَ ٱلَّيۡلِ فَٱسۡجُدۡ لَهُۥ وَسَبِّحۡهُ لَيۡلٗا طَوِيلًا"
    },
    {
      "id": 5618,
      "number": 27,
      "verse_key": "76:27",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 517,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 580,
      "juz_number": 29,
      "text": "إِنَّ هَٰٓؤُلَآءِ يُحِبُّونَ ٱلۡعَاجِلَةَ وَيَذَرُونَ وَرَآءَهُمۡ يَوۡمٗا ثَقِيلٗا"
    },
    {
      "id": 5619,
      "number": 28,
      "verse_key": "76:28",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 517,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 580,
      "juz_number": 29,
      "text": "نَّحۡنُ خَلَقۡنَٰهُمۡ وَشَدَدۡنَآ أَسۡرَهُمۡۖ وَإِذَا شِئۡنَا بَدَّلۡنَآ أَمۡثَٰلَهُمۡ تَبۡدِيلًا"
    },
    {
      "id": 5620,
      "number": 29,
      "verse_key": "76:29",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 517,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 580,
      "juz_number": 29,
      "text": "إِنَّ هَٰذِهِۦ تَذۡكِرَةٞۖ فَمَن شَآءَ ٱتَّخَذَ إِلَىٰ رَبِّهِۦ سَبِيلٗا"
    },
    {
      "id": 5621,
      "number": 30,
      "verse_key": "76:30",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 517,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 580,
      "juz_number": 29,
      "text": "وَمَا تَشَآءُونَ إِلَّآ أَن يَشَآءَ ٱللَّهُۚ إِنَّ ٱللَّهَ كَانَ عَلِيمًا حَكِيمٗا"
    },
    {
      "id": 5622,
      "number": 31,
      "verse_key": "76:31",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 517,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 580,
      "juz_number": 29,
      "text": "يُدۡخِلُ مَن يَشَآءُ فِي رَحۡمَتِهِۦۚ وَٱلظَّٰلِمِينَ أَعَدَّ لَهُمۡ عَذَابًا أَلِيمَۢا"
    }
  ]
}