{
  "verses": [
    {
      "id": 5242,
      "number": 1,
      "verse_key": "67:1",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 498,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 562,
      "juz_number": 29,
      "text": "تَبَٰرَكَ ٱلَّذِي بِيَدِهِ ٱلۡمُلۡكُ وَهُوَ عَلَىٰ كُلِّ شَيۡءٖ قَدِيرٌ"
    },
    {
      "id": 5243,
      "number": 2,
      "verse_key": "67:2",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 498,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 562,
      "juz_number": 29,
      "text": "ٱلَّذِي خَلَقَ ٱلۡمَوۡتَ وَٱلۡحَيَوٰةَ لِيَبۡلُوَكُمۡ أَيُّكُمۡ أَحۡسَنُ عَمَلٗاۚ وَهُوَ ٱلۡعَزِيزُ ٱلۡغَفُورُ"
    },
    {
      "id": 5244,
      "number": 3,
      "verse_key": "67:3",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 498,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 562,
      "juz_number": 29,
      "text": "ٱلَّذِي خَلَقَ سَبۡعَ سَمَٰوَٰتٖ طِبَاقٗاۖ مَّا تَرَىٰ فِي خَلۡقِ ٱلرَّحۡمَٰنِ مِن تَفَٰوُتٖۖ فَٱرۡجِعِ ٱلۡبَصَرَ هَلۡ تَرَىٰ مِن فُطُورٖ"
    },
    {
      "id": 5245,
      "number": 4,
      "verse_key": "67:4",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 498,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 562,
      "juz_number": 29,
      "text": "ثُمَّ ٱرۡجِعِ ٱلۡبَصَرَ كَرَّتَيۡنِ يَنقَلِبۡ إِلَيۡكَ ٱلۡبَصَرُ خَاسِئٗا وَهُوَ حَسِيرٞ"
    },
    {
      "id": 5246,
      "number": 5,
      "verse_key": "67:5",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 498,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 562,
      "juz_number": 29,
      "text": "وَلَقَدۡ زَيَّنَّا ٱلسَّمَآءَ ٱلدُّنۡيَا بِمَصَٰبِيحَ وَجَعَلۡنَٰهَا رُجُومٗا لِّلشَّيَٰطِينِۖ وَأَعۡتَدۡنَا لَهُمۡ عَذَابَ ٱلسَّعِيرِ"
    },
    {
      "id": 5247,
      "number": 6,
      "verse_key": "67:6",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 498,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 562,
      "juz_number": 29,
      "text": "وَلِلَّذِينَ كَفَرُواْ بِرَبِّهِمۡ عَذَابُ جَهَنَّمَۖ وَبِئۡسَ ٱلۡمَصِيرُ"
    },
    {
      "id": 5248,
      "number": 7,
      "verse_key": "67:7",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 498,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 562,
      "juz_number": 29,
      "text": "إِذَآ أُلۡقُواْ فِيهَا سَمِعُواْ لَهَا شَهِيقٗا وَهِيَ تَفُورُ"
    },
    {
      "id": 5249,
      "number": 8,
      "verse_key": "67:8",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 498,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 562,
      "juz_number": 29,
      "text": "تَكَادُ تَمَيَّزُ مِنَ ٱلۡغَيۡظِۖ كُلَّمَآ أُلۡقِيَ فِيهَا فَوۡجٞ سَأَلَهُمۡ خَزَنَتُهَآ أَلَمۡ يَأۡتِكُمۡ نَذِيرٞ"
    },
    {
      "id": 5250,
      "number": 9,
      "verse_key": "67:9",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 498,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 562,
      "juz_number": 29,
      "text": "قَالُواْ بَلَىٰ قَدۡ جَآءَنَا نَذِيرٞ فَكَذَّبۡنَا وَقُلۡنَا مَا نَزَّلَ ٱللَّهُ مِن شَيۡءٍ إِنۡ أَنتُمۡ إِلَّا فِي ضَلَٰلٖ كَبِيرٖ"
    },
    {
      "id": 5251,
      "number": 10,
      "verse_key": "67:10",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 498,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 562,
      "juz_number": 29,
      "text": "وَقَالُواْ لَوۡ كُنَّا نَسۡمَعُ أَوۡ نَعۡقِلُ مَا كُنَّا فِيٓ أَصۡحَٰبِ ٱلسَّعِيرِ"
    },
    {
      "id": 5252,
      "number": 11,
      "verse_key": "67:11",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 498,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 562,
      "juz_number": 29,
      "text": "فَٱعۡتَرَفُواْ بِذَنۢبِهِمۡ فَسُحۡقٗا لِّأَصۡحَٰبِ ٱلسَّعِيرِ"
    },
    {
      "id": 5253,
      "number": 12,
      "verse_key": "67:12",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 498,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 562,
      "juz_number": 29,
      "text": "إِنَّ ٱلَّذِينَ يَخۡشَوۡنَ رَبَّهُم بِٱلۡغَيۡبِ لَهُم مَّغۡفِرَةٞ وَأَجۡرٞ كَبِيرٞ"
    },
    {
      "id": 5254,
      "number": 13,
      "verse_key": "67:13",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 498,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 563,
      "juz_number": 29,
      "text": "وَأَسِرُّواْ قَوۡلَكُمۡ أَوِ ٱجۡهَرُواْ بِهِۦٓۖ إِنَّهُۥ عَلِيمُۢ بِذَاتِ ٱلصُّدُورِ"
    },
    {
      "id": 5255,
      "number": 14,
      "verse_key": "67:14",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 498,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 563,
      "juz_number": 29,
      "text": "أَلَا يَعۡلَمُ مَنۡ خَلَقَ وَهُوَ ٱللَّطِيفُ ٱلۡخَبِيرُ"
    },
    {
      "id": 5256,
      "number": 15,
      "verse_key": "67:15",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 499,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 563,
      "juz_number": 29,
      "text": "هُوَ ٱلَّذِي جَعَلَ لَكُمُ ٱلۡأَرۡضَ ذَلُولٗا فَٱمۡشُواْ فِي مَنَاكِبِهَا وَكُلُواْ مِن رِّزۡقِهِۦۖ وَإِلَيۡهِ ٱلنُّشُورُ"
    },
    {
      "id": 5257,
      "number": 16,
      "verse_key": "67:16",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 499,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 563,
      "juz_number": 29,
      "text": "ءَأَمِنتُم مَّن فِي ٱلسَّمَآءِ أَن يَخۡسِفَ بِكُمُ ٱلۡأَرۡضَ فَإِذَا هِيَ تَمُورُ"
    },
    {
      "id": 5258,
      "number": 17,
      "verse_key": "67:17",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 499,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 563,
      "juz_number": 29,
      "text": "أَمۡ أَمِنتُم مَّن فِي ٱلسَّمَآءِ أَن يُرۡسِلَ عَلَيۡكُمۡ حَاصِبٗاۖ فَسَتَعۡلَمُونَ كَيۡفَ نَذِيرِ"
    },
    {
      "id": 5259,
      "number": 18,
      "verse_key": "67:18",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 499,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 563,
      "juz_number": 29,
      "text": "وَلَقَدۡ كَذَّبَ ٱلَّذِينَ مِن قَبۡلِهِمۡ فَكَيۡفَ كَانَ نَكِيرِ"
    },
    {
      "id": 5260,
      "number": 19,
      "verse_key": "67:19",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 499,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 563,
      "juz_number": 29,
      "text": "أَوَلَمۡ يَرَوۡاْ إِلَى ٱلطَّيۡرِ فَوۡقَهُمۡ صَٰٓفَّٰتٖ وَيَقۡبِضۡنَۚ مَا يُمۡسِكُهُنَّ إِلَّا ٱلرَّحۡمَٰنُۚ إِنَّهُۥ بِكُلِّ شَيۡءِۭ بَصِيرٌ"
    },
    {
      "id": 5261,
      "number": 20,
      "verse_key": "67:20",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 499,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 563,
      "juz_number": 29,
      "text": "أَمَّنۡ هَٰذَا ٱلَّذِي هُوَ جُندٞ لَّكُمۡ يَنصُرُكُم مِّن دُونِ ٱلرَّحۡمَٰنِۚ إِنِ ٱلۡكَٰفِرُونَ إِلَّا فِي غُرُورٍ"
    },
    {
      "id": 5262,
      "number": 21,
      "verse_key": "67:21",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 499,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 563,
      "juz_number": 29,
      "text": "أَمَّنۡ هَٰذَا ٱلَّذِي يَرۡزُقُكُمۡ إِنۡ أَمۡسَكَ رِزۡقَهُۥۚ بَل لَّجُّواْ فِي عُتُوّٖ وَنُفُورٍ"
    },
    {
      "id": 5263,
      "number": 22,
      "verse_key": "67:22",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 499,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 563,
      "juz_number": 29,
      "text": "أَفَمَن يَمۡشِي مُكِبًّا عَلَىٰ وَجۡهِهِۦٓ أَهۡدَىٰٓ أَمَّن يَمۡشِي سَوِيًّا عَلَىٰ صِرَٰطٖ مُّسۡتَقِيمٖ"
    },
    {
      "id": 5264,
      "number": 23,
      "verse_key": "67:23",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 499,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 563,
      "juz_number": 29,
      "text": "قُلۡ هُوَ ٱلَّذِيٓ أَنشَأَكُمۡ وَجَعَلَ لَكُمُ ٱلسَّمۡعَ وَٱلۡأَبۡصَٰرَ وَٱلۡأَفۡـِٔدَةَۚ قَلِيلٗا مَّا تَشۡكُرُونَ"
    },
    {
      "id": 5265,
      "number": 24,
      "verse_key": "67:24",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 499,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 563,
      "juz_number": 29,
      "text": "قُلۡ هُوَ ٱلَّذِي ذَرَأَكُمۡ فِي ٱلۡأَرۡضِ وَإِلَيۡهِ تُحۡشَرُونَ"
    },
    {
      "id": 5266,
      "number": 25,
      "verse_key": "67:25",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 499,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 563,
      "juz_number": 29,
      "text": "وَيَقُولُونَ مَتَىٰ هَٰذَا ٱلۡوَعۡدُ إِن كُنتُمۡ صَٰدِقِينَ"
    },
    {
      "id": 5267,
      "number": 26,
      "verse_key": "67:26",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 499,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 563,
      "juz_number": 29,
      "text": "قُلۡ إِنَّمَا ٱلۡعِلۡمُ عِندَ ٱللَّهِ وَإِنَّمَآ أَنَا۠ نَذِيرٞ مُّبِينٞ"
    },
    {
      "id": 5268,
      "number": 27,
      "verse_key": "67:27",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 499,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 564,
      "juz_number": 29,
      "text": "فَلَمَّا رَأَوۡهُ زُلۡفَةٗ سِيٓـَٔتۡ وُجُوهُ ٱلَّذِينَ كَفَرُواْ وَقِيلَ هَٰذَا ٱلَّذِي كُنتُم بِهِۦ تَدَّعُونَ"
    },
    {
      "id": 5269,
      "number": 28,
      "verse_key": "67:28",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 499,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 564,
      "juz_number": 29,
      "text": "قُلۡ أَرَءَيۡتُمۡ إِنۡ أَهۡلَكَنِيَ ٱللَّهُ وَمَن مَّعِيَ أَوۡ رَحِمَنَا فَمَن يُجِيرُ ٱلۡكَٰفِرِينَ مِنۡ عَذَابٍ أَلِيمٖ"
    },
    {
      "id": 5270,
      "number": 29,
      "verse_key": "67:29",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 499,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 564,
      "juz_number": 29,
      "text": "قُلۡ هُوَ ٱلرَّحۡمَٰنُ ءَامَنَّا بِهِۦ وَعَلَيۡهِ تَوَكَّلۡنَاۖ فَسَتَعۡلَمُونَ مَنۡ هُوَ فِي ضَلَٰلٖ مُّبِينٖ"
    },
    {
      "id": 5271,
      "number": 30,
      "verse_key": "67:30",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 499,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 564,
      "juz_number": 29,
      "text": "قُلۡ أَرَءَيۡتُمۡ إِنۡ أَصۡبَحَ مَآؤُكُمۡ غَوۡرٗا فَمَن يَأۡتِيكُم بِمَآءٖ مَّعِينِۭ"
    }
  ]
}