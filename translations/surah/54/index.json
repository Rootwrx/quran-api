{
  "verses": [
    {
      "id": 4847,
      "number": 1,
      "verse_key": "54:1",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 465,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 528,
      "juz_number": 27,
      "text": "ٱقۡتَرَبَتِ ٱلسَّاعَةُ وَٱنشَقَّ ٱلۡقَمَرُ"
    },
    {
      "id": 4848,
      "number": 2,
      "verse_key": "54:2",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 465,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 528,
      "juz_number": 27,
      "text": "وَإِن يَرَوۡاْ ءَايَةٗ يُعۡرِضُواْ وَيَقُولُواْ سِحۡرٞ مُّسۡتَمِرّٞ"
    },
    {
      "id": 4849,
      "number": 3,
      "verse_key": "54:3",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 465,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 528,
      "juz_number": 27,
      "text": "وَكَذَّبُواْ وَٱتَّبَعُوٓاْ أَهۡوَآءَهُمۡۚ وَكُلُّ أَمۡرٖ مُّسۡتَقِرّٞ"
    },
    {
      "id": 4850,
      "number": 4,
      "verse_key": "54:4",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 465,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 528,
      "juz_number": 27,
      "text": "وَلَقَدۡ جَآءَهُم مِّنَ ٱلۡأَنۢبَآءِ مَا فِيهِ مُزۡدَجَرٌ"
    },
    {
      "id": 4851,
      "number": 5,
      "verse_key": "54:5",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 465,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 528,
      "juz_number": 27,
      "text": "حِكۡمَةُۢ بَٰلِغَةٞۖ فَمَا تُغۡنِ ٱلنُّذُرُ"
    },
    {
      "id": 4852,
      "number": 6,
      "verse_key": "54:6",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 465,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 528,
      "juz_number": 27,
      "text": "فَتَوَلَّ عَنۡهُمۡۘ يَوۡمَ يَدۡعُ ٱلدَّاعِ إِلَىٰ شَيۡءٖ نُّكُرٍ"
    },
    {
      "id": 4853,
      "number": 7,
      "verse_key": "54:7",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 465,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 529,
      "juz_number": 27,
      "text": "خُشَّعًا أَبۡصَٰرُهُمۡ يَخۡرُجُونَ مِنَ ٱلۡأَجۡدَاثِ كَأَنَّهُمۡ جَرَادٞ مُّنتَشِرٞ"
    },
    {
      "id": 4854,
      "number": 8,
      "verse_key": "54:8",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 465,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 529,
      "juz_number": 27,
      "text": "مُّهۡطِعِينَ إِلَى ٱلدَّاعِۖ يَقُولُ ٱلۡكَٰفِرُونَ هَٰذَا يَوۡمٌ عَسِرٞ"
    },
    {
      "id": 4855,
      "number": 9,
      "verse_key": "54:9",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 465,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 529,
      "juz_number": 27,
      "text": "۞ كَذَّبَتۡ قَبۡلَهُمۡ قَوۡمُ نُوحٖ فَكَذَّبُواْ عَبۡدَنَا وَقَالُواْ مَجۡنُونٞ وَٱزۡدُجِرَ"
    },
    {
      "id": 4856,
      "number": 10,
      "verse_key": "54:10",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 465,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 529,
      "juz_number": 27,
      "text": "فَدَعَا رَبَّهُۥٓ أَنِّي مَغۡلُوبٞ فَٱنتَصِرۡ"
    },
    {
      "id": 4857,
      "number": 11,
      "verse_key": "54:11",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 465,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 529,
      "juz_number": 27,
      "text": "فَفَتَحۡنَآ أَبۡوَٰبَ ٱلسَّمَآءِ بِمَآءٖ مُّنۡهَمِرٖ"
    },
    {
      "id": 4858,
      "number": 12,
      "verse_key": "54:12",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 465,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 529,
      "juz_number": 27,
      "text": "وَفَجَّرۡنَا ٱلۡأَرۡضَ عُيُونٗا فَٱلۡتَقَى ٱلۡمَآءُ عَلَىٰٓ أَمۡرٖ قَدۡ قُدِرَ"
    },
    {
      "id": 4859,
      "number": 13,
      "verse_key": "54:13",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 465,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 529,
      "juz_number": 27,
      "text": "وَحَمَلۡنَٰهُ عَلَىٰ ذَاتِ أَلۡوَٰحٖ وَدُسُرٖ"
    },
    {
      "id": 4860,
      "number": 14,
      "verse_key": "54:14",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 465,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 529,
      "juz_number": 27,
      "text": "تَجۡرِي بِأَعۡيُنِنَا جَزَآءٗ لِّمَن كَانَ كُفِرَ"
    },
    {
      "id": 4861,
      "number": 15,
      "verse_key": "54:15",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 465,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 529,
      "juz_number": 27,
      "text": "وَلَقَد تَّرَكۡنَٰهَآ ءَايَةٗ فَهَلۡ مِن مُّدَّكِرٖ"
    },
    {
      "id": 4862,
      "number": 16,
      "verse_key": "54:16",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 465,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 529,
      "juz_number": 27,
      "text": "فَكَيۡفَ كَانَ عَذَابِي وَنُذُرِ"
    },
    {
      "id": 4863,
      "number": 17,
      "verse_key": "54:17",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 465,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 529,
      "juz_number": 27,
      "text": "وَلَقَدۡ يَسَّرۡنَا ٱلۡقُرۡءَانَ لِلذِّكۡرِ فَهَلۡ مِن مُّدَّكِرٖ"
    },
    {
      "id": 4864,
      "number": 18,
      "verse_key": "54:18",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 465,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 529,
      "juz_number": 27,
      "text": "كَذَّبَتۡ عَادٞ فَكَيۡفَ كَانَ عَذَابِي وَنُذُرِ"
    },
    {
      "id": 4865,
      "number": 19,
      "verse_key": "54:19",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 465,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 529,
      "juz_number": 27,
      "text": "إِنَّآ أَرۡسَلۡنَا عَلَيۡهِمۡ رِيحٗا صَرۡصَرٗا فِي يَوۡمِ نَحۡسٖ مُّسۡتَمِرّٖ"
    },
    {
      "id": 4866,
      "number": 20,
      "verse_key": "54:20",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 465,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 529,
      "juz_number": 27,
      "text": "تَنزِعُ ٱلنَّاسَ كَأَنَّهُمۡ أَعۡجَازُ نَخۡلٖ مُّنقَعِرٖ"
    },
    {
      "id": 4867,
      "number": 21,
      "verse_key": "54:21",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 465,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 529,
      "juz_number": 27,
      "text": "فَكَيۡفَ كَانَ عَذَابِي وَنُذُرِ"
    },
    {
      "id": 4868,
      "number": 22,
      "verse_key": "54:22",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 465,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 529,
      "juz_number": 27,
      "text": "وَلَقَدۡ يَسَّرۡنَا ٱلۡقُرۡءَانَ لِلذِّكۡرِ فَهَلۡ مِن مُّدَّكِرٖ"
    },
    {
      "id": 4869,
      "number": 23,
      "verse_key": "54:23",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 466,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 529,
      "juz_number": 27,
      "text": "كَذَّبَتۡ ثَمُودُ بِٱلنُّذُرِ"
    },
    {
      "id": 4870,
      "number": 24,
      "verse_key": "54:24",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 466,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 529,
      "juz_number": 27,
      "text": "فَقَالُوٓاْ أَبَشَرٗا مِّنَّا وَٰحِدٗا نَّتَّبِعُهُۥٓ إِنَّآ إِذٗا لَّفِي ضَلَٰلٖ وَسُعُرٍ"
    },
    {
      "id": 4871,
      "number": 25,
      "verse_key": "54:25",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 466,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 529,
      "juz_number": 27,
      "text": "أَءُلۡقِيَ ٱلذِّكۡرُ عَلَيۡهِ مِنۢ بَيۡنِنَا بَلۡ هُوَ كَذَّابٌ أَشِرٞ"
    },
    {
      "id": 4872,
      "number": 26,
      "verse_key": "54:26",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 466,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 529,
      "juz_number": 27,
      "text": "سَيَعۡلَمُونَ غَدٗا مَّنِ ٱلۡكَذَّابُ ٱلۡأَشِرُ"
    },
    {
      "id": 4873,
      "number": 27,
      "verse_key": "54:27",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 466,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 529,
      "juz_number": 27,
      "text": "إِنَّا مُرۡسِلُواْ ٱلنَّاقَةِ فِتۡنَةٗ لَّهُمۡ فَٱرۡتَقِبۡهُمۡ وَٱصۡطَبِرۡ"
    },
    {
      "id": 4874,
      "number": 28,
      "verse_key": "54:28",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 466,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 530,
      "juz_number": 27,
      "text": "وَنَبِّئۡهُمۡ أَنَّ ٱلۡمَآءَ قِسۡمَةُۢ بَيۡنَهُمۡۖ كُلُّ شِرۡبٖ مُّحۡتَضَرٞ"
    },
    {
      "id": 4875,
      "number": 29,
      "verse_key": "54:29",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 466,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 530,
      "juz_number": 27,
      "text": "فَنَادَوۡاْ صَاحِبَهُمۡ فَتَعَاطَىٰ فَعَقَرَ"
    },
    {
      "id": 4876,
      "number": 30,
      "verse_key": "54:30",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 466,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 530,
      "juz_number": 27,
      "text": "فَكَيۡفَ كَانَ عَذَابِي وَنُذُرِ"
    },
    {
      "id": 4877,
      "number": 31,
      "verse_key": "54:31",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 466,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 530,
      "juz_number": 27,
      "text": "إِنَّآ أَرۡسَلۡنَا عَلَيۡهِمۡ صَيۡحَةٗ وَٰحِدَةٗ فَكَانُواْ كَهَشِيمِ ٱلۡمُحۡتَظِرِ"
    },
    {
      "id": 4878,
      "number": 32,
      "verse_key": "54:32",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 466,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 530,
      "juz_number": 27,
      "text": "وَلَقَدۡ يَسَّرۡنَا ٱلۡقُرۡءَانَ لِلذِّكۡرِ فَهَلۡ مِن مُّدَّكِرٖ"
    },
    {
      "id": 4879,
      "number": 33,
      "verse_key": "54:33",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 466,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 530,
      "juz_number": 27,
      "text": "كَذَّبَتۡ قَوۡمُ لُوطِۭ بِٱلنُّذُرِ"
    },
    {
      "id": 4880,
      "number": 34,
      "verse_key": "54:34",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 466,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 530,
      "juz_number": 27,
      "text": "إِنَّآ أَرۡسَلۡنَا عَلَيۡهِمۡ حَاصِبًا إِلَّآ ءَالَ لُوطٖۖ نَّجَّيۡنَٰهُم بِسَحَرٖ"
    },
    {
      "id": 4881,
      "number": 35,
      "verse_key": "54:35",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 466,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 530,
      "juz_number": 27,
      "text": "نِّعۡمَةٗ مِّنۡ عِندِنَاۚ كَذَٰلِكَ نَجۡزِي مَن شَكَرَ"
    },
    {
      "id": 4882,
      "number": 36,
      "verse_key": "54:36",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 466,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 530,
      "juz_number": 27,
      "text": "وَلَقَدۡ أَنذَرَهُم بَطۡشَتَنَا فَتَمَارَوۡاْ بِٱلنُّذُرِ"
    },
    {
      "id": 4883,
      "number": 37,
      "verse_key": "54:37",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 466,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 530,
      "juz_number": 27,
      "text": "وَلَقَدۡ رَٰوَدُوهُ عَن ضَيۡفِهِۦ فَطَمَسۡنَآ أَعۡيُنَهُمۡ فَذُوقُواْ عَذَابِي وَنُذُرِ"
    },
    {
      "id": 4884,
      "number": 38,
      "verse_key": "54:38",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 466,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 530,
      "juz_number": 27,
      "text": "وَلَقَدۡ صَبَّحَهُم بُكۡرَةً عَذَابٞ مُّسۡتَقِرّٞ"
    },
    {
      "id": 4885,
      "number": 39,
      "verse_key": "54:39",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 466,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 530,
      "juz_number": 27,
      "text": "فَذُوقُواْ عَذَابِي وَنُذُرِ"
    },
    {
      "id": 4886,
      "number": 40,
      "verse_key": "54:40",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 466,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 530,
      "juz_number": 27,
      "text": "وَلَقَدۡ يَسَّرۡنَا ٱلۡقُرۡءَانَ لِلذِّكۡرِ فَهَلۡ مِن مُّدَّكِرٖ"
    },
    {
      "id": 4887,
      "number": 41,
      "verse_key": "54:41",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 467,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 530,
      "juz_number": 27,
      "text": "وَلَقَدۡ جَآءَ ءَالَ فِرۡعَوۡنَ ٱلنُّذُرُ"
    },
    {
      "id": 4888,
      "number": 42,
      "verse_key": "54:42",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 467,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 530,
      "juz_number": 27,
      "text": "كَذَّبُواْ بِـَٔايَٰتِنَا كُلِّهَا فَأَخَذۡنَٰهُمۡ أَخۡذَ عَزِيزٖ مُّقۡتَدِرٍ"
    },
    {
      "id": 4889,
      "number": 43,
      "verse_key": "54:43",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 467,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 530,
      "juz_number": 27,
      "text": "أَكُفَّارُكُمۡ خَيۡرٞ مِّنۡ أُوْلَٰٓئِكُمۡ أَمۡ لَكُم بَرَآءَةٞ فِي ٱلزُّبُرِ"
    },
    {
      "id": 4890,
      "number": 44,
      "verse_key": "54:44",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 467,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 530,
      "juz_number": 27,
      "text": "أَمۡ يَقُولُونَ نَحۡنُ جَمِيعٞ مُّنتَصِرٞ"
    },
    {
      "id": 4891,
      "number": 45,
      "verse_key": "54:45",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 467,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 530,
      "juz_number": 27,
      "text": "سَيُهۡزَمُ ٱلۡجَمۡعُ وَيُوَلُّونَ ٱلدُّبُرَ"
    },
    {
      "id": 4892,
      "number": 46,
      "verse_key": "54:46",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 467,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 530,
      "juz_number": 27,
      "text": "بَلِ ٱلسَّاعَةُ مَوۡعِدُهُمۡ وَٱلسَّاعَةُ أَدۡهَىٰ وَأَمَرُّ"
    },
    {
      "id": 4893,
      "number": 47,
      "verse_key": "54:47",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 467,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 530,
      "juz_number": 27,
      "text": "إِنَّ ٱلۡمُجۡرِمِينَ فِي ضَلَٰلٖ وَسُعُرٖ"
    },
    {
      "id": 4894,
      "number": 48,
      "verse_key": "54:48",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 467,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 530,
      "juz_number": 27,
      "text": "يَوۡمَ يُسۡحَبُونَ فِي ٱلنَّارِ عَلَىٰ وُجُوهِهِمۡ ذُوقُواْ مَسَّ سَقَرَ"
    },
    {
      "id": 4895,
      "number": 49,
      "verse_key": "54:49",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 467,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 530,
      "juz_number": 27,
      "text": "إِنَّا كُلَّ شَيۡءٍ خَلَقۡنَٰهُ بِقَدَرٖ"
    },
    {
      "id": 4896,
      "number": 50,
      "verse_key": "54:50",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 467,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 531,
      "juz_number": 27,
      "text": "وَمَآ أَمۡرُنَآ إِلَّا وَٰحِدَةٞ كَلَمۡحِۭ بِٱلۡبَصَرِ"
    },
    {
      "id": 4897,
      "number": 51,
      "verse_key": "54:51",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 467,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 531,
      "juz_number": 27,
      "text": "وَلَقَدۡ أَهۡلَكۡنَآ أَشۡيَاعَكُمۡ فَهَلۡ مِن مُّدَّكِرٖ"
    },
    {
      "id": 4898,
      "number": 52,
      "verse_key": "54:52",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 467,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 531,
      "juz_number": 27,
      "text": "وَكُلُّ شَيۡءٖ فَعَلُوهُ فِي ٱلزُّبُرِ"
    },
    {
      "id": 4899,
      "number": 53,
      "verse_key": "54:53",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 467,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 531,
      "juz_number": 27,
      "text": "وَكُلُّ صَغِيرٖ وَكَبِيرٖ مُّسۡتَطَرٌ"
    },
    {
      "id": 4900,
      "number": 54,
      "verse_key": "54:54",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 467,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 531,
      "juz_number": 27,
      "text": "إِنَّ ٱلۡمُتَّقِينَ فِي جَنَّٰتٖ وَنَهَرٖ"
    },
    {
      "id": 4901,
      "number": 55,
      "verse_key": "54:55",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 467,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 531,
      "juz_number": 27,
      "text": "فِي مَقۡعَدِ صِدۡقٍ عِندَ مَلِيكٖ مُّقۡتَدِرِۭ"
    }
  ]
}