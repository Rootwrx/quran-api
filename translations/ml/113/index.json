{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 604,
      "verse_key": "113:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قُلۡ أَعُوذُ بِرَبِّ ٱلۡفَلَقِ",
      "translation": "പറയുക: പുലരിയുടെ രക്ഷിതാവിനോട് ഞാന്‍ ശരണം തേടുന്നു.",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 604,
      "verse_key": "113:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مِن شَرِّ مَا خَلَقَ",
      "translation": "അവന്‍ സൃഷ്ടിച്ചുട്ടുള്ളവയുടെ കെടുതിയില്‍ നിന്ന്‌.",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 604,
      "verse_key": "113:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمِن شَرِّ غَاسِقٍ إِذَا وَقَبَ",
      "translation": "ഇരുളടയുമ്പോഴുള്ള രാത്രിയുടെ കെടുതിയില്‍നിന്നും.",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 604,
      "verse_key": "113:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمِن شَرِّ ٱلنَّفَّٰثَٰتِ فِي ٱلۡعُقَدِ",
      "translation": "കെട്ടുകളില്‍ ഊതുന്ന സ്ത്രീകളുടെ കെടുതിയില്‍ നിന്നും(1)",
      "footnotes": "1) 'കെട്ടുകളില്‍ ഊതുന്നവര്‍' എന്നതുകൊണ്ടുള്ള വിവക്ഷ മന്ത്രിച്ചൂതുന്ന മാരണക്കാരികളായ സ്ത്രീകളാണ്."
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 604,
      "verse_key": "113:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمِن شَرِّ حَاسِدٍ إِذَا حَسَدَ",
      "translation": "അസൂയാലു അസൂയപ്പെടുമ്പോള്‍ അവന്‍റെ കെടുതിയില്‍നിന്നും.",
      "footnotes": ""
    }
  ]
}