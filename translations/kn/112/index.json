{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 604,
      "verse_key": "112:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قُلۡ هُوَ ٱللَّهُ أَحَدٌ",
      "translation": "ಹೇಳಿರಿ: “ಅವನು ಅಲ್ಲಾಹು, ಏಕೈಕನು.",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 604,
      "verse_key": "112:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱللَّهُ ٱلصَّمَدُ",
      "translation": "ಅಲ್ಲಾಹು ನಿರಪೇಕ್ಷನು.",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 604,
      "verse_key": "112:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَمۡ يَلِدۡ وَلَمۡ يُولَدۡ",
      "translation": "ಅವನು ಯಾರಿಗೂ ಜನ್ಮವನ್ನು ನೀಡಿಲ್ಲ. ಅವನು ಯಾರಿಂದಲೂ ಜನ್ಮವನ್ನು ಪಡೆದಿಲ್ಲ.",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 604,
      "verse_key": "112:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَمۡ يَكُن لَّهُۥ كُفُوًا أَحَدُۢ",
      "translation": "ಅವನಿಗೆ ಸರಿಸಾಟಿಯಾಗಿ ಯಾರೂ ಇಲ್ಲ.”",
      "footnotes": ""
    }
  ]
}