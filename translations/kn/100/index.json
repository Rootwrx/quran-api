{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 599,
      "verse_key": "100:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡعَٰدِيَٰتِ ضَبۡحٗا",
      "translation": "ಏದುಸಿರು ಬಿಡುತ್ತಾ ದೌಡಾಯಿಸುವ ಕುದುರೆಗಳ ಮೇಲಾಣೆ!",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 599,
      "verse_key": "100:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَٱلۡمُورِيَٰتِ قَدۡحٗا",
      "translation": "ಗೊರಸನ್ನು ಕಲ್ಲಿಗೆ ಉಜ್ಜಿ ಬೆಂಕಿ ಕಿಡಿ ಹಾರಿಸುವವುಗಳ ಮೇಲಾಣೆ!",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 599,
      "verse_key": "100:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَٱلۡمُغِيرَٰتِ صُبۡحٗا",
      "translation": "ಮುಂಜಾನೆಯ ಸಮಯದಲ್ಲಿ ಆಕ್ರಮಣ ಮಾಡುವವುಗಳ ಮೇಲಾಣೆ!",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 599,
      "verse_key": "100:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَثَرۡنَ بِهِۦ نَقۡعٗا",
      "translation": "ಆಗ ಅವು ಧೂಳನ್ನು ಹರಡುತ್ತವೆ.",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 599,
      "verse_key": "100:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَوَسَطۡنَ بِهِۦ جَمۡعًا",
      "translation": "ನಂತರ ಅದರ ಮೂಲಕ ಸೈನ್ಯಗಳ ನಡುಭಾಗಕ್ಕೆ ನುಗ್ಗುತ್ತವೆ.",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 599,
      "verse_key": "100:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ ٱلۡإِنسَٰنَ لِرَبِّهِۦ لَكَنُودٞ",
      "translation": "ನಿಶ್ಚಯವಾಗಿಯೂ ಮನುಷ್ಯನು ತನ್ನ ಪರಿಪಾಲಕನಿಗೆ (ಅಲ್ಲಾಹನಿಗೆ) ಕೃತಜ್ಞತೆಯಿಲ್ಲದವನಾಗಿದ್ದಾನೆ.",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 599,
      "verse_key": "100:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِنَّهُۥ عَلَىٰ ذَٰلِكَ لَشَهِيدٞ",
      "translation": "ನಿಶ್ಚಯವಾಗಿಯೂ ಅವನೇ ಅದಕ್ಕೆ ಸಾಕ್ಷಿಯಾಗಿದ್ದಾನೆ.",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 599,
      "verse_key": "100:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِنَّهُۥ لِحُبِّ ٱلۡخَيۡرِ لَشَدِيدٌ",
      "translation": "ಅವನಿಗೆ ಸಂಪತ್ತಿನ ಮೇಲೆ ಅತಿಯಾದ ಮೋಹವಿದೆ.",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 599,
      "verse_key": "100:9",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "۞ أَفَلَا يَعۡلَمُ إِذَا بُعۡثِرَ مَا فِي ٱلۡقُبُورِ",
      "translation": "ಸಮಾಧಿಗಳಲ್ಲಿರುವುದನ್ನು ಹೊರತೆಗೆಯಲಾಗುವ ಆ ಸಮಯದ ಬಗ್ಗೆ ಅವನಿಗೆ ತಿಳಿದಿಲ್ಲವೇ?",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 600,
      "verse_key": "100:10",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَحُصِّلَ مَا فِي ٱلصُّدُورِ",
      "translation": "ಹೃದಯಗಳಲ್ಲಿರುವ ರಹಸ್ಯ ಮಾತುಗಳನ್ನು ಬಹಿರಂಗಪಡಿಸಲಾಗುವ (ಆ ಸಮಯದ ಬಗ್ಗೆ).",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 600,
      "verse_key": "100:11",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ رَبَّهُم بِهِمۡ يَوۡمَئِذٖ لَّخَبِيرُۢ",
      "translation": "ನಿಶ್ಚಯವಾಗಿಯೂ ಅಂದು ಅವರ ಪರಿಪಾಲಕನು (ಅಲ್ಲಾಹು) ಅವರ ಬಗ್ಗೆ ಸೂಕ್ಷ್ಮಜ್ಞಾನವುಳ್ಳವನಾಗಿದ್ದಾನೆ.",
      "footnotes": ""
    }
  ]
}