      "page_number": 533,
      "verse_key": "55:50",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِيهِمَا عَيۡنَانِ تَجۡرِيَانِ",
      "translation": "Ils y trouveront deux sources courantes.",
      "footnotes": ""
    },
