      "id": 48,
      "verse_number": 48,
      "page_number": 533,
      "verse_key": "55:48",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ذَوَاتَآ أَفۡنَانٖ",
      "translation": "Aux branches touffues.",
      "footnotes": ""
    },
