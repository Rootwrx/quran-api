{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 604,
      "verse_key": "114:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قُلۡ أَعُوذُ بِرَبِّ ٱلنَّاسِ",
      "translation": "1 Dis : « J’implore la protection du Seigneur des hommes,",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 604,
      "verse_key": "114:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَلِكِ ٱلنَّاسِ",
      "translation": "2 Maître des hommes,",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 604,
      "verse_key": "114:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَٰهِ ٱلنَّاسِ",
      "translation": "3 Dieu des hommes,",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 604,
      "verse_key": "114:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مِن شَرِّ ٱلۡوَسۡوَاسِ ٱلۡخَنَّاسِ",
      "translation": "4 contre le mal du démon qui se dérobe[1618] ou s’insinue chez l’homme[1619],",
      "footnotes": "[1618] Lorsque l’homme se souvient de son Seigneur et invoque Son nom.\n[1619] Lorsque l’homme se montre insouciant et oublie son Seigneur."
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 604,
      "verse_key": "114:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِي يُوَسۡوِسُ فِي صُدُورِ ٱلنَّاسِ",
      "translation": "5 soufflant d’insidieuses pensées dans le cœur des hommes,",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 604,
      "verse_key": "114:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مِنَ ٱلۡجِنَّةِ وَٱلنَّاسِ",
      "translation": "6 qu’il soit du nombre des djinns ou du nombre des hommes. »",
      "footnotes": ""
    }
  ]
}