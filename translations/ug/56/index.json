{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 534,
      "verse_key": "56:1",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذَا وَقَعَتِ ٱلۡوَاقِعَةُ",
      "translation": " قىيامەت قايىم بولغان چاغدا[1]،",
      "footnotes": null
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 534,
      "verse_key": "56:2",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَيۡسَ لِوَقۡعَتِهَا كَاذِبَةٌ",
      "translation": "ھېچ ئادەم ئۇنى ئىنكار قىلالمايدۇ[2].",
      "footnotes": null
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 534,
      "verse_key": "56:3",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "خَافِضَةٞ رَّافِعَةٌ",
      "translation": "زېمىن قاتتىق تەۋرىتىلگەن، تاغلار پارچىلىنىپ توزاندەك توزۇپ كەتكەن چاغدا، قىيامەت (بەزىلەرنى دوزاخقا كىرگۈزۈش بىلەن دەرىجىسىنى) چۈشۈرىدۇ، (بەزىلەرنى جەننەتكە كىرگۈزۈش بىلەن دەرىجىسىنى) كۆتۈرىدۇ[3ـ6].",
      "footnotes": null
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 534,
      "verse_key": "56:4",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذَا رُجَّتِ ٱلۡأَرۡضُ رَجّٗا",
      "translation": "زېمىن قاتتىق تەۋرىتىلگەن، تاغلار پارچىلىنىپ توزاندەك توزۇپ كەتكەن چاغدا، قىيامەت (بەزىلەرنى دوزاخقا كىرگۈزۈش بىلەن دەرىجىسىنى) چۈشۈرىدۇ، (بەزىلەرنى جەننەتكە كىرگۈزۈش بىلەن دەرىجىسىنى) كۆتۈرىدۇ[3ـ6].",
      "footnotes": null
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 534,
      "verse_key": "56:5",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَبُسَّتِ ٱلۡجِبَالُ بَسّٗا",
      "translation": "زېمىن قاتتىق تەۋرىتىلگەن، تاغلار پارچىلىنىپ توزاندەك توزۇپ كەتكەن چاغدا، قىيامەت (بەزىلەرنى دوزاخقا كىرگۈزۈش بىلەن دەرىجىسىنى) چۈشۈرىدۇ، (بەزىلەرنى جەننەتكە كىرگۈزۈش بىلەن دەرىجىسىنى) كۆتۈرىدۇ[3ـ6].",
      "footnotes": null
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 534,
      "verse_key": "56:6",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَكَانَتۡ هَبَآءٗ مُّنۢبَثّٗا",
      "translation": "زېمىن قاتتىق تەۋرىتىلگەن، تاغلار پارچىلىنىپ توزاندەك توزۇپ كەتكەن چاغدا، قىيامەت (بەزىلەرنى دوزاخقا كىرگۈزۈش بىلەن دەرىجىسىنى) چۈشۈرىدۇ، (بەزىلەرنى جەننەتكە كىرگۈزۈش بىلەن دەرىجىسىنى) كۆتۈرىدۇ[3ـ6].",
      "footnotes": null
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 534,
      "verse_key": "56:7",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَكُنتُمۡ أَزۡوَٰجٗا ثَلَٰثَةٗ",
      "translation": " سىلەر (قىيامەتتە) ئۈچ پىرقىغە بۆلۈنىسىلەر[7].",
      "footnotes": null
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 534,
      "verse_key": "56:8",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَصۡحَٰبُ ٱلۡمَيۡمَنَةِ مَآ أَصۡحَٰبُ ٱلۡمَيۡمَنَةِ",
      "translation": " (بىرىنچى پىرقە) سائادەتمەنلەردۇر، سائادەتمەنلەر قانداق ئادەملەر؟ (ئۇلار نامە ـ ئەمالى ئوڭ تەرىپىدىن بېرىلىدىغان) ئادەملەردۇر[8].",
      "footnotes": null
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 534,
      "verse_key": "56:9",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَصۡحَٰبُ ٱلۡمَشۡـَٔمَةِ مَآ أَصۡحَٰبُ ٱلۡمَشۡـَٔمَةِ",
      "translation": " (ئىككىنچى پىرقە) بەختسىز ئادەملەردۇر، بەختسىز ئادەملەر قانداق ئادەملەر؟ (ئۇلار نامە ـ ئەمالى سول تەرىپىدىن بېرىلىدىغان ئادەملەردۇر)[9].",
      "footnotes": null
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 534,
      "verse_key": "56:10",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلسَّٰبِقُونَ ٱلسَّٰبِقُونَ",
      "translation": " (ئۈچىنچى پىرقە ياخشى ئىشلارنى) ئەڭ ئالدىدا قىلغۇچىلار بولۇپ، (ئۇلار جەننەتكە) ئەڭ ئالدىدا كىرگۈچىلەردۇر[10].",
      "footnotes": null
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 534,
      "verse_key": "56:11",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أُوْلَٰٓئِكَ ٱلۡمُقَرَّبُونَ",
      "translation": " ئۇلار نازۇنېمەتلىك جەننەتلەردە ئاللاھقا يېقىن بولغۇچىلاردۇر[11ـ12].",
      "footnotes": null
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 534,
      "verse_key": "56:12",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِي جَنَّٰتِ ٱلنَّعِيمِ",
      "translation": " ئۇلار نازۇنېمەتلىك جەننەتلەردە ئاللاھقا يېقىن بولغۇچىلاردۇر[11ـ12].",
      "footnotes": null
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 534,
      "verse_key": "56:13",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُلَّةٞ مِّنَ ٱلۡأَوَّلِينَ",
      "translation": "بۇلار ئۆتكەنكىلەرنىڭ ئىچىدە كۆپتۇر، [13]",
      "footnotes": null
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 534,
      "verse_key": "56:14",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَقَلِيلٞ مِّنَ ٱلۡأٓخِرِينَ",
      "translation": " كېيىنكىلەرنىڭ ئىچىدە ئازدۇر[ـ14].",
      "footnotes": null
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 534,
      "verse_key": "56:15",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلَىٰ سُرُرٖ مَّوۡضُونَةٖ",
      "translation": "ئۇلار (ئالتۇندىن) توقۇلغان تەختلەر ئۈستىگە يۆلەنگەن ھالدا بىر ـ بىرىگە قارىشىپ ئولتۇرۇشىدۇ[15ـ16]. ",
      "footnotes": null
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 534,
      "verse_key": "56:16",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مُّتَّكِـِٔينَ عَلَيۡهَا مُتَقَٰبِلِينَ",
      "translation": "ئۇلار (ئالتۇندىن) توقۇلغان تەختلەر ئۈستىگە يۆلەنگەن ھالدا بىر ـ بىرىگە قارىشىپ ئولتۇرۇشىدۇ[15ـ16]. ",
      "footnotes": null
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 535,
      "verse_key": "56:17",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَطُوفُ عَلَيۡهِمۡ وِلۡدَٰنٞ مُّخَلَّدُونَ",
      "translation": "(قېرىماي) ھەمىشە بىر خىل تۇرىدىغان غىلمانلار ئېقىپ تۇرغان شارابتىن تولدۇرۇلغان پىيالە، چەينەك، جاملارنى ئۇلارغا ئايلىنىپ سۇنۇپ تۇرىدۇ[17ـ18].",
      "footnotes": null
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 535,
      "verse_key": "56:18",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بِأَكۡوَابٖ وَأَبَارِيقَ وَكَأۡسٖ مِّن مَّعِينٖ",
      "translation": "(قېرىماي) ھەمىشە بىر خىل تۇرىدىغان غىلمانلار ئېقىپ تۇرغان شارابتىن تولدۇرۇلغان پىيالە، چەينەك، جاملارنى ئۇلارغا ئايلىنىپ سۇنۇپ تۇرىدۇ[17ـ18].",
      "footnotes": null
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 535,
      "verse_key": "56:19",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَّا يُصَدَّعُونَ عَنۡهَا وَلَا يُنزِفُونَ",
      "translation": " ئۇ شارابنى ئىچىش بىلەن ئۇلارنىڭ بېشى ئاغرىمايدۇ، مەست بولمايدۇ[19].",
      "footnotes": null
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 535,
      "verse_key": "56:20",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَفَٰكِهَةٖ مِّمَّا يَتَخَيَّرُونَ",
      "translation": "ئۇلارغا ئىختىيار قىلغان مېۋىلەر ۋە كۆڭۈللىرى تارتقان قۇش گۆشلىرى بېرىلىدۇ[20ـ21].",
      "footnotes": null
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 535,
      "verse_key": "56:21",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَحۡمِ طَيۡرٖ مِّمَّا يَشۡتَهُونَ",
      "translation": "ئۇلارغا ئىختىيار قىلغان مېۋىلەر ۋە كۆڭۈللىرى تارتقان قۇش گۆشلىرى بېرىلىدۇ[20ـ21].",
      "footnotes": null
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 535,
      "verse_key": "56:22",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَحُورٌ عِينٞ",
      "translation": "ئۇلارغا سەدەپنىڭ ئىچىدىكى گۆھەرگە ئوخشايدىغان شەھلا كۆزلۈك ھۆرلەر بېرىلىدۇ[22].",
      "footnotes": null
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 535,
      "verse_key": "56:23",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَأَمۡثَٰلِ ٱللُّؤۡلُوِٕ ٱلۡمَكۡنُونِ",
      "translation": " بۇ ئۇلارنىڭ قىلغان ياخشى ئەمەللىرىنى مۇكاپاتلاش ئۈچۈندۇر[23].",
      "footnotes": null
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 535,
      "verse_key": "56:24",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "جَزَآءَۢ بِمَا كَانُواْ يَعۡمَلُونَ",
      "translation": "ئۇلار جەننەتتە بىھۇدە ۋە يالغان سۆزلەرنى ئاڭلىمايدۇ، پەقەت «سالام! سالام!» سۆزىنىلا ئاڭلايدۇ[24ـ25].",
      "footnotes": null
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 535,
      "verse_key": "56:25",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَا يَسۡمَعُونَ فِيهَا لَغۡوٗا وَلَا تَأۡثِيمًا",
      "translation": "ئۇلار جەننەتتە بىھۇدە ۋە يالغان سۆزلەرنى ئاڭلىمايدۇ، پەقەت «سالام! سالام!» سۆزىنىلا ئاڭلايدۇ[24ـ25].",
      "footnotes": null
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 535,
      "verse_key": "56:26",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَّا قِيلٗا سَلَٰمٗا سَلَٰمٗا",
      "translation": " سائادەتمەنلەرنىڭ (يەنى نامە ـ ئەمالى ئوڭ تەرەپتىن بېرىلگەنلەرنىڭ) ئەھۋالى ناھايىتى ياخشى بولىدۇ[26].",
      "footnotes": null
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 535,
      "verse_key": "56:27",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَصۡحَٰبُ ٱلۡيَمِينِ مَآ أَصۡحَٰبُ ٱلۡيَمِينِ",
      "translation": " سائادەتمەنلەر قانداق ئادەملەر؟[27]",
      "footnotes": null
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 535,
      "verse_key": "56:28",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِي سِدۡرٖ مَّخۡضُودٖ",
      "translation": "ئۇلار سىدرى دەرەخلىرىدىن، سانجاق ـ سانجاق بولۇپ كەتكەن مەۋز دەرەخلىرىدىن، ھەمىشە تۇرىدىغان سايىدىن، ئېقىپ تۇرغان سۇدىن، تۈگىمەيدىغان ۋە چەكلەنمەيدىغان مېۋىلەردىن، ئېگىز (يۇمشاق) تۆشەكلەردىن بەھرىمەن بولىدۇ[28ـ34].",
      "footnotes": null
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 535,
      "verse_key": "56:29",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَطَلۡحٖ مَّنضُودٖ",
      "translation": "ئۇلار سىدرى دەرەخلىرىدىن، سانجاق ـ سانجاق بولۇپ كەتكەن مەۋز دەرەخلىرىدىن، ھەمىشە تۇرىدىغان سايىدىن، ئېقىپ تۇرغان سۇدىن، تۈگىمەيدىغان ۋە چەكلەنمەيدىغان مېۋىلەردىن، ئېگىز (يۇمشاق) تۆشەكلەردىن بەھرىمەن بولىدۇ[28ـ34].",
      "footnotes": null
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 535,
      "verse_key": "56:30",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَظِلّٖ مَّمۡدُودٖ",
      "translation": "ئۇلار سىدرى دەرەخلىرىدىن، سانجاق ـ سانجاق بولۇپ كەتكەن مەۋز دەرەخلىرىدىن، ھەمىشە تۇرىدىغان سايىدىن، ئېقىپ تۇرغان سۇدىن، تۈگىمەيدىغان ۋە چەكلەنمەيدىغان مېۋىلەردىن، ئېگىز (يۇمشاق) تۆشەكلەردىن بەھرىمەن بولىدۇ[28ـ34].",
      "footnotes": null
    },
    {
      "id": 31,
      "verse_number": 31,
      "page_number": 535,
      "verse_key": "56:31",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَآءٖ مَّسۡكُوبٖ",
      "translation": "ئۇلار سىدرى دەرەخلىرىدىن، سانجاق ـ سانجاق بولۇپ كەتكەن مەۋز دەرەخلىرىدىن، ھەمىشە تۇرىدىغان سايىدىن، ئېقىپ تۇرغان سۇدىن، تۈگىمەيدىغان ۋە چەكلەنمەيدىغان مېۋىلەردىن، ئېگىز (يۇمشاق) تۆشەكلەردىن بەھرىمەن بولىدۇ[28ـ34].",
      "footnotes": null
    },
    {
      "id": 32,
      "verse_number": 32,
      "page_number": 535,
      "verse_key": "56:32",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَفَٰكِهَةٖ كَثِيرَةٖ",
      "translation": "ئۇلار سىدرى دەرەخلىرىدىن، سانجاق ـ سانجاق بولۇپ كەتكەن مەۋز دەرەخلىرىدىن، ھەمىشە تۇرىدىغان سايىدىن، ئېقىپ تۇرغان سۇدىن، تۈگىمەيدىغان ۋە چەكلەنمەيدىغان مېۋىلەردىن، ئېگىز (يۇمشاق) تۆشەكلەردىن بەھرىمەن بولىدۇ[28ـ34].",
      "footnotes": null
    },
    {
      "id": 33,
      "verse_number": 33,
      "page_number": 535,
      "verse_key": "56:33",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَّا مَقۡطُوعَةٖ وَلَا مَمۡنُوعَةٖ",
      "translation": "ئۇلار سىدرى دەرەخلىرىدىن، سانجاق ـ سانجاق بولۇپ كەتكەن مەۋز دەرەخلىرىدىن، ھەمىشە تۇرىدىغان سايىدىن، ئېقىپ تۇرغان سۇدىن، تۈگىمەيدىغان ۋە چەكلەنمەيدىغان مېۋىلەردىن، ئېگىز (يۇمشاق) تۆشەكلەردىن بەھرىمەن بولىدۇ[28ـ34].",
      "footnotes": null
    },
    {
      "id": 34,
      "verse_number": 34,
      "page_number": 535,
      "verse_key": "56:34",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَفُرُشٖ مَّرۡفُوعَةٍ",
      "translation": "ئۇلار سىدرى دەرەخلىرىدىن، سانجاق ـ سانجاق بولۇپ كەتكەن مەۋز دەرەخلىرىدىن، ھەمىشە تۇرىدىغان سايىدىن، ئېقىپ تۇرغان سۇدىن، تۈگىمەيدىغان ۋە چەكلەنمەيدىغان مېۋىلەردىن، ئېگىز (يۇمشاق) تۆشەكلەردىن بەھرىمەن بولىدۇ[28ـ34].",
      "footnotes": null
    },
    {
      "id": 35,
      "verse_number": 35,
      "page_number": 535,
      "verse_key": "56:35",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّآ أَنشَأۡنَٰهُنَّ إِنشَآءٗ",
      "translation": " شۈبھىسىزكى، بىز ھۆرلەرنى يېڭىدىن ياراتتۇق، ئۇلارنى پاكىز، ئەرلىرىگە ئامراق، تەڭتۇش قىلدۇق[35ـ37].",
      "footnotes": null
    },
    {
      "id": 36,
      "verse_number": 36,
      "page_number": 535,
      "verse_key": "56:36",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَجَعَلۡنَٰهُنَّ أَبۡكَارًا",
      "translation": " شۈبھىسىزكى، بىز ھۆرلەرنى يېڭىدىن ياراتتۇق، ئۇلارنى پاكىز، ئەرلىرىگە ئامراق، تەڭتۇش قىلدۇق[35ـ37].",
      "footnotes": null
    },
    {
      "id": 37,
      "verse_number": 37,
      "page_number": 535,
      "verse_key": "56:37",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عُرُبًا أَتۡرَابٗا",
      "translation": " شۈبھىسىزكى، بىز ھۆرلەرنى يېڭىدىن ياراتتۇق، ئۇلارنى پاكىز، ئەرلىرىگە ئامراق، تەڭتۇش قىلدۇق[35ـ37].",
      "footnotes": null
    },
    {
      "id": 38,
      "verse_number": 38,
      "page_number": 535,
      "verse_key": "56:38",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لِّأَصۡحَٰبِ ٱلۡيَمِينِ",
      "translation": " بۇ ھۆرلەر سائادەتمەنلەر ئۈچۈندۇر[38].",
      "footnotes": null
    },
    {
      "id": 39,
      "verse_number": 39,
      "page_number": 535,
      "verse_key": "56:39",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُلَّةٞ مِّنَ ٱلۡأَوَّلِينَ",
      "translation": "بۇلار بولسا ئىلگىرىكىلەرنىڭ ئىچىدىن بىر جامائەدۇر[39].",
      "footnotes": null
    },
    {
      "id": 40,
      "verse_number": 40,
      "page_number": 535,
      "verse_key": "56:40",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَثُلَّةٞ مِّنَ ٱلۡأٓخِرِينَ",
      "translation": " كېيىنكىلەرنىڭ ئىچىدىن بىر جامائەدۇر[40].",
      "footnotes": null
    },
    {
      "id": 41,
      "verse_number": 41,
      "page_number": 535,
      "verse_key": "56:41",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَصۡحَٰبُ ٱلشِّمَالِ مَآ أَصۡحَٰبُ ٱلشِّمَالِ",
      "translation": "بەختسىزلەر (يەنى نامە ـ ئەمالى سول تەرىپىدىن بېرىلگەنلەر) (دوزىخىلاردۇر). بەختسىزلەر قانداق ئادەملەر؟[41]",
      "footnotes": null
    },
    {
      "id": 42,
      "verse_number": 42,
      "page_number": 535,
      "verse_key": "56:42",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِي سَمُومٖ وَحَمِيمٖ",
      "translation": " ئۇلار (بەدەننىڭ تۆشۈكلىرىگە كىرىپ كېتىدىغان ئاتەشلىك شامالنىڭ، زىيادە ھارارەتلىك قايناقسۇنىڭ ۋە قارا تۈتۈندىن بولغان سالقىنمۇ ئەمەس، كۆركەممۇ ئەمەس سايىنىڭ ئىچىدە بولىدۇ[42ـ44].",
      "footnotes": null
    },
    {
      "id": 43,
      "verse_number": 43,
      "page_number": 535,
      "verse_key": "56:43",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَظِلّٖ مِّن يَحۡمُومٖ",
      "translation": " ئۇلار (بەدەننىڭ تۆشۈكلىرىگە كىرىپ كېتىدىغان ئاتەشلىك شامالنىڭ، زىيادە ھارارەتلىك قايناقسۇنىڭ ۋە قارا تۈتۈندىن بولغان سالقىنمۇ ئەمەس، كۆركەممۇ ئەمەس سايىنىڭ ئىچىدە بولىدۇ[42ـ44].",
      "footnotes": null
    },
    {
      "id": 44,
      "verse_number": 44,
      "page_number": 535,
      "verse_key": "56:44",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَّا بَارِدٖ وَلَا كَرِيمٍ",
      "translation": " ئۇلار (بەدەننىڭ تۆشۈكلىرىگە كىرىپ كېتىدىغان ئاتەشلىك شامالنىڭ، زىيادە ھارارەتلىك قايناقسۇنىڭ ۋە قارا تۈتۈندىن بولغان سالقىنمۇ ئەمەس، كۆركەممۇ ئەمەس سايىنىڭ ئىچىدە بولىدۇ[42ـ44].",
      "footnotes": null
    },
    {
      "id": 45,
      "verse_number": 45,
      "page_number": 535,
      "verse_key": "56:45",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُمۡ كَانُواْ قَبۡلَ ذَٰلِكَ مُتۡرَفِينَ",
      "translation": "چۈنكى ئۇلار بۇنىڭدىن ئىلگىرى (يەنى دۇنيادا) شەھۋەتكە چۆمگەن ئىدى[45].",
      "footnotes": null
    },
    {
      "id": 46,
      "verse_number": 46,
      "page_number": 535,
      "verse_key": "56:46",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَكَانُواْ يُصِرُّونَ عَلَى ٱلۡحِنثِ ٱلۡعَظِيمِ",
      "translation": " ئۇلار چوڭ گۇناھتا (يەنى كۇفرىدا) چىڭ تۇرغان ئىدى[46].",
      "footnotes": null
    },
    {
      "id": 47,
      "verse_number": 47,
      "page_number": 535,
      "verse_key": "56:47",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَكَانُواْ يَقُولُونَ أَئِذَا مِتۡنَا وَكُنَّا تُرَابٗا وَعِظَٰمًا أَءِنَّا لَمَبۡعُوثُونَ",
      "translation": "ئۇلار: «بىز ئۆلۈپ توپىغا ۋە قۇرۇق سۆڭەككە ئايلانغان چاغدا چوقۇم تىرىلدۈرۈلەمدۇق؟ ئاتا ـ بوۋىلىرىمىزمۇ تىرىلدۈرۈلەمدۇ؟» دەيتتى[47ـ48].",
      "footnotes": null
    },
    {
      "id": 48,
      "verse_number": 48,
      "page_number": 535,
      "verse_key": "56:48",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَوَءَابَآؤُنَا ٱلۡأَوَّلُونَ",
      "translation": "ئۇلار: «بىز ئۆلۈپ توپىغا ۋە قۇرۇق سۆڭەككە ئايلانغان چاغدا چوقۇم تىرىلدۈرۈلەمدۇق؟ ئاتا ـ بوۋىلىرىمىزمۇ تىرىلدۈرۈلەمدۇ؟» دەيتتى[47ـ48].",
      "footnotes": null
    },
    {
      "id": 49,
      "verse_number": 49,
      "page_number": 535,
      "verse_key": "56:49",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قُلۡ إِنَّ ٱلۡأَوَّلِينَ وَٱلۡأٓخِرِينَ",
      "translation": " ئېيتقىنكى، «ئىلگىرىكىلەر ۋە كېيىنكىلەر مەلۇم كۈننىڭ مۇئەييەن ۋاقتىدا (يەنى قىيامەتتە) توپلىنىدۇ[49ـ50].",
      "footnotes": null
    },
    {
      "id": 50,
      "verse_number": 50,
      "page_number": 535,
      "verse_key": "56:50",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَمَجۡمُوعُونَ إِلَىٰ مِيقَٰتِ يَوۡمٖ مَّعۡلُومٖ",
      "translation": " ئېيتقىنكى، «ئىلگىرىكىلەر ۋە كېيىنكىلەر مەلۇم كۈننىڭ مۇئەييەن ۋاقتىدا (يەنى قىيامەتتە) توپلىنىدۇ[49ـ50].",
      "footnotes": null
    },
    {
      "id": 51,
      "verse_number": 51,
      "page_number": 536,
      "verse_key": "56:51",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ إِنَّكُمۡ أَيُّهَا ٱلضَّآلُّونَ ٱلۡمُكَذِّبُونَ",
      "translation": "ئاندىن سىلەر، ئى قايتا تىرىلىشنى ئىنكار قىلغۇچى گۇمراھلار![51]",
      "footnotes": null
    },
    {
      "id": 52,
      "verse_number": 52,
      "page_number": 536,
      "verse_key": "56:52",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَأٓكِلُونَ مِن شَجَرٖ مِّن زَقُّومٖ",
      "translation": "چوقۇم زەققۇم دەرىخىدىن يەيسىلەر[52].",
      "footnotes": null
    },
    {
      "id": 53,
      "verse_number": 53,
      "page_number": 536,
      "verse_key": "56:53",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَمَالِـُٔونَ مِنۡهَا ٱلۡبُطُونَ",
      "translation": " ئۇنىڭدىن قورساقلىرىڭلارنى تولدۇرىسىلەر[53].",
      "footnotes": null
    },
    {
      "id": 54,
      "verse_number": 54,
      "page_number": 536,
      "verse_key": "56:54",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَشَٰرِبُونَ عَلَيۡهِ مِنَ ٱلۡحَمِيمِ",
      "translation": "ئۇنىڭ ئۈستىگە ھارارىتى يۇقىرى بولغان قايناقسۇنى تەشنا بولغان تۆگىلەردەك ئىچىسىلەر»[54ـ55].",
      "footnotes": null
    },
    {
      "id": 55,
      "verse_number": 55,
      "page_number": 536,
      "verse_key": "56:55",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَشَٰرِبُونَ شُرۡبَ ٱلۡهِيمِ",
      "translation": "ئۇنىڭ ئۈستىگە ھارارىتى يۇقىرى بولغان قايناقسۇنى تەشنا بولغان تۆگىلەردەك ئىچىسىلەر»[54ـ55].",
      "footnotes": null
    },
    {
      "id": 56,
      "verse_number": 56,
      "page_number": 536,
      "verse_key": "56:56",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "هَٰذَا نُزُلُهُمۡ يَوۡمَ ٱلدِّينِ",
      "translation": " مانا بۇ، ئۇلارنىڭ قىيامەت كۈنىدىكى زىياپىتىدۇر[56].",
      "footnotes": null
    },
    {
      "id": 57,
      "verse_number": 57,
      "page_number": 536,
      "verse_key": "56:57",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "نَحۡنُ خَلَقۡنَٰكُمۡ فَلَوۡلَا تُصَدِّقُونَ",
      "translation": " (ئىنسانلار!) سىلەرنى بىز ياراتتۇق، سىلەر قايتا تىرىلىشكە ئىشەنمەمسىلەر؟[57]",
      "footnotes": null
    },
    {
      "id": 58,
      "verse_number": 58,
      "page_number": 536,
      "verse_key": "56:58",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَرَءَيۡتُم مَّا تُمۡنُونَ",
      "translation": "سىلەر (ئاياللارنىڭ بەچچىدانىغا) تۆكۈلگەن مەنىنى دەپ بېقىڭلارچۇ![58] ",
      "footnotes": null
    },
    {
      "id": 59,
      "verse_number": 59,
      "page_number": 536,
      "verse_key": "56:59",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ءَأَنتُمۡ تَخۡلُقُونَهُۥٓ أَمۡ نَحۡنُ ٱلۡخَٰلِقُونَ",
      "translation": "ئۇنى سىلەر (ئىنسان قىلىپ) يارىتامسىلەر؟ ياكى بىز (ئىنسان قىلىپ) يارىتامدۇق؟[59]",
      "footnotes": null
    },
    {
      "id": 60,
      "verse_number": 60,
      "page_number": 536,
      "verse_key": "56:60",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "نَحۡنُ قَدَّرۡنَا بَيۡنَكُمُ ٱلۡمَوۡتَ وَمَا نَحۡنُ بِمَسۡبُوقِينَ",
      "translation": " بىز سىلەرنىڭ ئاراڭلاردا ئۆلۈمنى بەلگىلىدۇق (يەنى ھەر بىرىڭلارنىڭ ئۆلۈش ۋاقتىنى بېكىتتۇق)، سىلەرنى باشقا بىر قەۋمگە ئالماشتۇرۇشتىن ۋە سىلەرنى سىلەر بىلمەيدىغان بىر شەكىلدە يارىتىشتىن ئاجىز ئەمەسمىز[60ـ61].",
      "footnotes": null
    },
    {
      "id": 61,
      "verse_number": 61,
      "page_number": 536,
      "verse_key": "56:61",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلَىٰٓ أَن نُّبَدِّلَ أَمۡثَٰلَكُمۡ وَنُنشِئَكُمۡ فِي مَا لَا تَعۡلَمُونَ",
      "translation": " بىز سىلەرنىڭ ئاراڭلاردا ئۆلۈمنى بەلگىلىدۇق (يەنى ھەر بىرىڭلارنىڭ ئۆلۈش ۋاقتىنى بېكىتتۇق)، سىلەرنى باشقا بىر قەۋمگە ئالماشتۇرۇشتىن ۋە سىلەرنى سىلەر بىلمەيدىغان بىر شەكىلدە يارىتىشتىن ئاجىز ئەمەسمىز[60ـ61].",
      "footnotes": null
    },
    {
      "id": 62,
      "verse_number": 62,
      "page_number": 536,
      "verse_key": "56:62",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَقَدۡ عَلِمۡتُمُ ٱلنَّشۡأَةَ ٱلۡأُولَىٰ فَلَوۡلَا تَذَكَّرُونَ",
      "translation": "شۈبھىسىزكى، سىلەر دەسلەپكى يارىتىشنى بىلدىڭلار (يەنى دەسلەپتە ئاللاھنىڭ سىلەرنى يوقتىن بار قىلغانلىقىنى تونۇدۇڭلار). سىلەر (ئاللاھنىڭ سىلەرنى دەسلەپ ياراتقاندەك قايتا يارىتىشىنى) ئويلىمامسىلەر؟[62] ",
      "footnotes": null
    },
    {
      "id": 63,
      "verse_number": 63,
      "page_number": 536,
      "verse_key": "56:63",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَرَءَيۡتُم مَّا تَحۡرُثُونَ",
      "translation": " سىلەر تېرىغان زىرائەتنى دەپ بېقىڭلارچۇ![63]",
      "footnotes": null
    },
    {
      "id": 64,
      "verse_number": 64,
      "page_number": 536,
      "verse_key": "56:64",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ءَأَنتُمۡ تَزۡرَعُونَهُۥٓ أَمۡ نَحۡنُ ٱلزَّٰرِعُونَ",
      "translation": "ئۇنى سىلەر ئۈندۈردۈڭلارمۇ ياكى بىز ئۈندۈرۈپ (دان چىقىدىغان زىرائەت قىلىپ ئۆستۈردۇقمۇ؟)[64].",
      "footnotes": null
    },
    {
      "id": 65,
      "verse_number": 65,
      "page_number": 536,
      "verse_key": "56:65",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَوۡ نَشَآءُ لَجَعَلۡنَٰهُ حُطَٰمٗا فَظَلۡتُمۡ تَفَكَّهُونَ",
      "translation": "ئەگەر بىز خالىساق ئەلۋەتتە ئۇنى دانسىز قۇرۇق چۆپكە ئايلاندۇراتتۇق ـ دە، سىلەر ئەجەبلىنىپ قايغۇراتتىڭلار[65].",
      "footnotes": null
    },
    {
      "id": 66,
      "verse_number": 66,
      "page_number": 536,
      "verse_key": "56:66",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّا لَمُغۡرَمُونَ",
      "translation": "سىلەر: «بىز ھەقىقەتەن (ئەمگەك بىلەن ئۇرۇقنى) زىيان تارتتۇق، بەلكى بىز (رىزىقتىن) مەھرۇم قالدۇق» (دەيتتىڭلار)[66ـ67].",
      "footnotes": null
    },
    {
      "id": 67,
      "verse_number": 67,
      "page_number": 536,
      "verse_key": "56:67",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بَلۡ نَحۡنُ مَحۡرُومُونَ",
      "translation": "سىلەر: «بىز ھەقىقەتەن (ئەمگەك بىلەن ئۇرۇقنى) زىيان تارتتۇق، بەلكى بىز (رىزىقتىن) مەھرۇم قالدۇق» (دەيتتىڭلار)[66ـ67].",
      "footnotes": null
    },
    {
      "id": 68,
      "verse_number": 68,
      "page_number": 536,
      "verse_key": "56:68",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَرَءَيۡتُمُ ٱلۡمَآءَ ٱلَّذِي تَشۡرَبُونَ",
      "translation": "سىلەر ئىچىۋاتقان سۇنى دەپ بېقىڭلارچۇ![68]",
      "footnotes": null
    },
    {
      "id": 69,
      "verse_number": 69,
      "page_number": 536,
      "verse_key": "56:69",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ءَأَنتُمۡ أَنزَلۡتُمُوهُ مِنَ ٱلۡمُزۡنِ أَمۡ نَحۡنُ ٱلۡمُنزِلُونَ",
      "translation": "ئۇنى بۇلۇتتىن سىلەر چۈشۈردۈڭلارمۇ ياكى بىز چۈشۈردۇقمۇ؟[69]",
      "footnotes": null
    },
    {
      "id": 70,
      "verse_number": 70,
      "page_number": 536,
      "verse_key": "56:70",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَوۡ نَشَآءُ جَعَلۡنَٰهُ أُجَاجٗا فَلَوۡلَا تَشۡكُرُونَ",
      "translation": "ئەگەر بىز خالىساق ئۇنى تۇزلۇق قىلىپ قوياتتۇق (پەرۋەردىگارىڭلارنىڭ زور نېمەتلىرىگە) نېمىشقا شۈكۈر قىلمايسىلەر؟[70] ",
      "footnotes": null
    },
    {
      "id": 71,
      "verse_number": 71,
      "page_number": 536,
      "verse_key": "56:71",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَرَءَيۡتُمُ ٱلنَّارَ ٱلَّتِي تُورُونَ",
      "translation": " سىلەر ياندۇرۇۋاتقان ئوتنى دەپ بېقىڭلارچۇ![71]",
      "footnotes": null
    },
    {
      "id": 72,
      "verse_number": 72,
      "page_number": 536,
      "verse_key": "56:72",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ءَأَنتُمۡ أَنشَأۡتُمۡ شَجَرَتَهَآ أَمۡ نَحۡنُ ٱلۡمُنشِـُٔونَ",
      "translation": "ئۇنىڭ دەرىخىنى سىلەر ئۆستۈردۈڭلارمۇ ياكى بىز ئۆستۈردۇقمۇ؟[72]",
      "footnotes": null
    },
    {
      "id": 73,
      "verse_number": 73,
      "page_number": 536,
      "verse_key": "56:73",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "نَحۡنُ جَعَلۡنَٰهَا تَذۡكِرَةٗ وَمَتَٰعٗا لِّلۡمُقۡوِينَ",
      "translation": "ئۇنى بىز ئىبرەت ۋە يولۇچىلار پايدىلىنىدىغان نەرسە قىلدۇق[73].",
      "footnotes": null
    },
    {
      "id": 74,
      "verse_number": 74,
      "page_number": 536,
      "verse_key": "56:74",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَسَبِّحۡ بِٱسۡمِ رَبِّكَ ٱلۡعَظِيمِ",
      "translation": "ئۇلۇغ پەرۋەردىگارىڭنى (مۇشرىكلارنىڭ ئىپتىرالىرىدىن) پاك ئېتىقاد قىلغىن[74].",
      "footnotes": null
    },
    {
      "id": 75,
      "verse_number": 75,
      "page_number": 536,
      "verse_key": "56:75",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "۞ فَلَآ أُقۡسِمُ بِمَوَٰقِعِ ٱلنُّجُومِ",
      "translation": "يۇلتۇزلارنىڭ جايلىرى بىلەن قەسەم قىلىمەن[75].",
      "footnotes": null
    },
    {
      "id": 76,
      "verse_number": 76,
      "page_number": 536,
      "verse_key": "56:76",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِنَّهُۥ لَقَسَمٞ لَّوۡ تَعۡلَمُونَ عَظِيمٌ",
      "translation": "شۈبھىسىزكى، ئەگەر بىلسەڭلار، ئۇ (يۇلتۇزلار ئاللاھنىڭ ئۇلۇغ قۇدرىتىنى كۆرسىتىدىغانلىقى ئۈچۈن) ئەلۋەتتە كاتتا قەسەمدۇر[76].",
      "footnotes": null
    },
    {
      "id": 77,
      "verse_number": 77,
      "page_number": 537,
      "verse_key": "56:77",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُۥ لَقُرۡءَانٞ كَرِيمٞ",
      "translation": "شەك ـ شۈبھىسىزكى، ئۇ ئۇلۇغ قۇرئاندۇر[77].",
      "footnotes": null
    },
    {
      "id": 78,
      "verse_number": 78,
      "page_number": 537,
      "verse_key": "56:78",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِي كِتَٰبٖ مَّكۡنُونٖ",
      "translation": "كىتابتا (يەنى لەۋھۇلمەھپۇزدا) ساقلانغاندۇر[78].",
      "footnotes": null
    },
    {
      "id": 79,
      "verse_number": 79,
      "page_number": 537,
      "verse_key": "56:79",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَّا يَمَسُّهُۥٓ إِلَّا ٱلۡمُطَهَّرُونَ",
      "translation": "ئۇنى پەقەت پاك بولغانلارلا تۇتىدۇ[79].",
      "footnotes": null
    },
    {
      "id": 80,
      "verse_number": 80,
      "page_number": 537,
      "verse_key": "56:80",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تَنزِيلٞ مِّن رَّبِّ ٱلۡعَٰلَمِينَ",
      "translation": " ئۇ ئالەملەرنىڭ پەرۋەردىگارى تەرىپىدىن نازىل قىلىنغاندۇر[80]. ",
      "footnotes": null
    },
    {
      "id": 81,
      "verse_number": 81,
      "page_number": 537,
      "verse_key": "56:81",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَبِهَٰذَا ٱلۡحَدِيثِ أَنتُم مُّدۡهِنُونَ",
      "translation": "(ئى كۇففارلار جامائەسى!) بۇ قۇرئاننى ئىنكار قىلامسىلەر؟[81] ",
      "footnotes": null
    },
    {
      "id": 82,
      "verse_number": 82,
      "page_number": 537,
      "verse_key": "56:82",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَتَجۡعَلُونَ رِزۡقَكُمۡ أَنَّكُمۡ تُكَذِّبُونَ",
      "translation": "(سىلەر ئاللاھنىڭ رىزىق بەرگەنلىكىگە) شۈكۈر قىلىشنىڭ ئورنىغا (رىزىق بەرگۈچىنى) ئىنكار قىلامسىلەر؟[82]",
      "footnotes": null
    },
    {
      "id": 83,
      "verse_number": 83,
      "page_number": 537,
      "verse_key": "56:83",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلَوۡلَآ إِذَا بَلَغَتِ ٱلۡحُلۡقُومَ",
      "translation": "جان ھەلقۇمغا يەتكەن چاغدا (سەكراتتىكى كىشىگە) قاراپ تۇرىسىلەر[83ـ84].",
      "footnotes": null
    },
    {
      "id": 84,
      "verse_number": 84,
      "page_number": 537,
      "verse_key": "56:84",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنتُمۡ حِينَئِذٖ تَنظُرُونَ",
      "translation": "جان ھەلقۇمغا يەتكەن چاغدا (سەكراتتىكى كىشىگە) قاراپ تۇرىسىلەر[83ـ84].",
      "footnotes": null
    },
    {
      "id": 85,
      "verse_number": 85,
      "page_number": 537,
      "verse_key": "56:85",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَنَحۡنُ أَقۡرَبُ إِلَيۡهِ مِنكُمۡ وَلَٰكِن لَّا تُبۡصِرُونَ",
      "translation": "بىز (ئىلمىمىز ۋە قۇدرىتىمىز بىلەن) ئۇنىڭغا سىلەردىن يېقىنمىز، لېكىن سىلەر (ئۇنى) كۆرمەيسىلەر (يەنى بىلمەيسىلەر)[85].",
      "footnotes": null
    },
    {
      "id": 86,
      "verse_number": 86,
      "page_number": 537,
      "verse_key": "56:86",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلَوۡلَآ إِن كُنتُمۡ غَيۡرَ مَدِينِينَ",
      "translation": " ئەگەر (سىلەر گۇمان قىلغاندەك ئەمەلىڭلارغا قاراپ) جازاغا تارتىلمايدىغان بولساڭلار نېمىشقا ئۇنى (يەنى ئۇ مېيىتنىڭ جېنىنى بەدىنىگە) قايتۇرمايسىلەر؟ ئەگەر (سۆزۈڭلاردا) راستچىل بولساڭلار[86ـ87].",
      "footnotes": null
    },
    {
      "id": 87,
      "verse_number": 87,
      "page_number": 537,
      "verse_key": "56:87",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تَرۡجِعُونَهَآ إِن كُنتُمۡ صَٰدِقِينَ",
