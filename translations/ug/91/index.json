{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 595,
      "verse_key": "91:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلشَّمۡسِ وَضُحَىٰهَا",
      "translation": "قۇياش ۋە ئۇنىڭ نۇرى بىلەن قەسەمكى[1]،",
      "footnotes": null
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 595,
      "verse_key": "91:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡقَمَرِ إِذَا تَلَىٰهَا",
      "translation": " قۇياشقا ئەگەشكەن چاغدىكى ئاي بىلەن قەسەمكى[2]،",
      "footnotes": null
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 595,
      "verse_key": "91:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلنَّهَارِ إِذَا جَلَّىٰهَا",
      "translation": "قۇياشنى ئاشكارا قىلغان كۈندۈز بىلەن قەسەمكى[3]، ",
      "footnotes": null
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 595,
      "verse_key": "91:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّيۡلِ إِذَا يَغۡشَىٰهَا",
      "translation": "قۇياشنى يوشۇرغان چاغدىكى كېچە بىلەن قەسەمكى[4]،",
      "footnotes": null
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 595,
      "verse_key": "91:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلسَّمَآءِ وَمَا بَنَىٰهَا",
      "translation": " ئاسمان بىلەن ۋە ئۇنى بىنا قىلغان زات بىلەن قەسەمكى[5]،",
      "footnotes": null
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 595,
      "verse_key": "91:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡأَرۡضِ وَمَا طَحَىٰهَا",
      "translation": " زېمىن بىلەن ۋە ئۇنى بىنا قىلغان زات بىلەن قەسەمكى[6]،",
      "footnotes": null
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 595,
      "verse_key": "91:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَنَفۡسٖ وَمَا سَوَّىٰهَا",
      "translation": "نەپس بىلەن ۋە ئۇنى چىرايلىق قىلىپ ياراتقان، ئۇنىڭغا ياخشى ـ يامانلىقنى بىلدۈرگەن زات بىلەن قەسەمكى[7ـ8]،",
      "footnotes": null
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 595,
      "verse_key": "91:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَلۡهَمَهَا فُجُورَهَا وَتَقۡوَىٰهَا",
      "translation": "نەپس بىلەن ۋە ئۇنى چىرايلىق قىلىپ ياراتقان، ئۇنىڭغا ياخشى ـ يامانلىقنى بىلدۈرگەن زات بىلەن قەسەمكى[7ـ8]،",
      "footnotes": null
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 595,
      "verse_key": "91:9",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قَدۡ أَفۡلَحَ مَن زَكَّىٰهَا",
      "translation": "نەپسىنى پاك قىلغان ئادەم چوقۇم مۇرادىغا يېتىدۇ[9].",
      "footnotes": null
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 595,
      "verse_key": "91:10",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَقَدۡ خَابَ مَن دَسَّىٰهَا",
      "translation": "نەپسىنى (كۇفرى ۋە پىسقى ـ فۇجۇر بىلەن) كەمسىتكەن ئادەم چوقۇم نائۈمىد بولىدۇ[10].",
      "footnotes": null
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 595,
      "verse_key": "91:11",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَذَّبَتۡ ثَمُودُ بِطَغۡوَىٰهَآ",
      "translation": "سەمۇد ھەددىدىن ئاشقانلىقى ئۈچۈن (پەيغەمبەرنى) ئىنكار قىلدى[11].",
      "footnotes": null
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 595,
      "verse_key": "91:12",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذِ ٱنۢبَعَثَ أَشۡقَىٰهَا",
      "translation": " ئەينى زاماندا سەمۇدنىڭ ئەڭ بەتبەخت ئادىمى (مۆجىزە بولۇپ كەلگەن تۆگىنى) ئۆلتۈرۈشكە ئالدىرىدى[12].",
      "footnotes": null
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 595,
      "verse_key": "91:13",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَقَالَ لَهُمۡ رَسُولُ ٱللَّهِ نَاقَةَ ٱللَّهِ وَسُقۡيَٰهَا",
      "translation": "ئۇلارغا ئاللاھنىڭ پەيغەمبىرى (يەنى سالىھ ئەلەيھىسسالام): «ئاللاھنىڭ تۆگىسىگە چېقىلماڭلار، ئۇنى سۇ ئىچىشتىن توسماڭلار» دېدى[13].",
      "footnotes": null
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 595,
      "verse_key": "91:14",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَكَذَّبُوهُ فَعَقَرُوهَا فَدَمۡدَمَ عَلَيۡهِمۡ رَبُّهُم بِذَنۢبِهِمۡ فَسَوَّىٰهَا",
      "translation": "ئۇلار پەيغەمبەرنى (يەنى سالىھ ئەلەيھىسسالامنى) يالغانچى قىلدى، تۆگىنى ئۆلتۈردى، گۇناھى تۈپەيلىدىن ئاللاھ ئۇلارنى تەلتۆكۈس ھالاك قىلدى، ئومۇميۈزلۈك ھالاك قىلدى[14].",
      "footnotes": null
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 595,
      "verse_key": "91:15",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَا يَخَافُ عُقۡبَٰهَا",
      "translation": "ئاللاھ ئۇنىڭ (يەنى ئۇلارنى ھالاك قىلغانلىقنىڭ) ئاقىۋىتىدىن قورقمايدۇ[15].",
      "footnotes": null
    }
  ]
}