{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 600,
      "verse_key": "101:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلۡقَارِعَةُ",
      "translation": "قىيامەت[1]، ",
      "footnotes": null
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 600,
      "verse_key": "101:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَا ٱلۡقَارِعَةُ",
      "translation": "قىيامەت نېمىدۇر[2].",
      "footnotes": null
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 600,
      "verse_key": "101:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَآ أَدۡرَىٰكَ مَا ٱلۡقَارِعَةُ",
      "translation": " قىيامەتنىڭ نېمە ئىكەنلىكىنى قانداق بىلەلەيسەن؟[3]",
      "footnotes": null
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 600,
      "verse_key": "101:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَوۡمَ يَكُونُ ٱلنَّاسُ كَٱلۡفَرَاشِ ٱلۡمَبۡثُوثِ",
      "translation": "شۇ كۈندە ئىنسانلار تارىلىپ كەتكەن پەرۋانىدەك بولۇپ كېتىدۇ[4].",
      "footnotes": null
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 600,
      "verse_key": "101:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَتَكُونُ ٱلۡجِبَالُ كَٱلۡعِهۡنِ ٱلۡمَنفُوشِ",
      "translation": "تاغلار ئېتىلغان يۇڭدەك بولۇپ كېتىدۇ[5].",
      "footnotes": null
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 600,
      "verse_key": "101:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَمَّا مَن ثَقُلَتۡ مَوَٰزِينُهُۥ",
      "translation": "تارازىسى ئېغىر كەلگەن (يەنى ياخشىلىقلىرى يامانلىقلىرىنى بېسىپ چۈشكەن) ئادەمگە كەلسەك، ئۇ كۆڭۈللۈك تۇرمۇشتا (يەنى نازۇنېمەتلىك جەننەتتە) بولىدۇ[6ـ7]. ",
      "footnotes": null
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 600,
      "verse_key": "101:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَهُوَ فِي عِيشَةٖ رَّاضِيَةٖ",
      "translation": "تارازىسى ئېغىر كەلگەن (يەنى ياخشىلىقلىرى يامانلىقلىرىنى بېسىپ چۈشكەن) ئادەمگە كەلسەك، ئۇ كۆڭۈللۈك تۇرمۇشتا (يەنى نازۇنېمەتلىك جەننەتتە) بولىدۇ[6ـ7]. ",
      "footnotes": null
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 600,
      "verse_key": "101:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَمَّا مَنۡ خَفَّتۡ مَوَٰزِينُهُۥ",
      "translation": " تارازىسى يېنىك كەلگەن (يەنى يامانلىقلىرى ياخشىلىقلىرىنى بېسىپ كەتكەن، يا ياخشىلىقى بولمىغان) ئادەمگە كەلسەك، ئۇنىڭ جايى ھاۋىيە بولىدۇ[8ـ9].",
      "footnotes": null
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 600,
      "verse_key": "101:9",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأُمُّهُۥ هَاوِيَةٞ",
      "translation": " تارازىسى يېنىك كەلگەن (يەنى يامانلىقلىرى ياخشىلىقلىرىنى بېسىپ كەتكەن، يا ياخشىلىقى بولمىغان) ئادەمگە كەلسەك، ئۇنىڭ جايى ھاۋىيە بولىدۇ[8ـ9].",
      "footnotes": null
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 600,
      "verse_key": "101:10",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَآ أَدۡرَىٰكَ مَا هِيَهۡ",
      "translation": "ھاۋىيەنىڭ نېمە ئىكەنلىكىنى سەن قانداق بىلەلەيسەن؟[10]",
      "footnotes": null
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 600,
      "verse_key": "101:11",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "نَارٌ حَامِيَةُۢ",
      "translation": "ھاۋىيە قىزىق ئوتتۇر[11]. ",
      "footnotes": null
    }
  ]
}