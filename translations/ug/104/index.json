{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 601,
      "verse_key": "104:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيۡلٞ لِّكُلِّ هُمَزَةٖ لُّمَزَةٍ",
      "translation": "(كىشىلەرنى) ئەيىبلىگۈچى سۇخەنچىگە ۋاي![1].",
      "footnotes": null
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 601,
      "verse_key": "104:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِي جَمَعَ مَالٗا وَعَدَّدَهُۥ",
      "translation": "ئۇ مال توپلىدى ۋە ئۇنى (ياخشىلىق يوللىرىغا سەرپ قىلماي ساقلاش ئۈچۈن) سانىدى[2]. ",
      "footnotes": null
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 601,
      "verse_key": "104:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَحۡسَبُ أَنَّ مَالَهُۥٓ أَخۡلَدَهُۥ",
      "translation": "ئۇ مېلى ئۆزىنى (دۇنيادا مەڭگۈ قالدۇرىدۇ دەپ ئويلايدۇ)[3].",
      "footnotes": null
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 601,
      "verse_key": "104:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّاۖ لَيُنۢبَذَنَّ فِي ٱلۡحُطَمَةِ",
      "translation": "ھەرگىز ئۇنداق ئەمەس، ئۇ چوقۇم ھۇتەمەگە تاشلىنىدۇ[4].",
      "footnotes": null
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 601,
      "verse_key": "104:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَآ أَدۡرَىٰكَ مَا ٱلۡحُطَمَةُ",
      "translation": "ھۇتەمەنىڭ نېمىلىكىنى قانداق بىلەلەيسەن؟[5] ",
      "footnotes": null
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 601,
      "verse_key": "104:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "نَارُ ٱللَّهِ ٱلۡمُوقَدَةُ",
      "translation": " ئۇ، ئاللاھنىڭ ياندۇرغان ئوتىدۇركى، (ئۇنىڭ ئەلىمى) يۈرەكلەرگە يېتىپ بارىدۇ[6ـ7].",
      "footnotes": null
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 601,
      "verse_key": "104:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّتِي تَطَّلِعُ عَلَى ٱلۡأَفۡـِٔدَةِ",
      "translation": " ئۇ، ئاللاھنىڭ ياندۇرغان ئوتىدۇركى، (ئۇنىڭ ئەلىمى) يۈرەكلەرگە يېتىپ بارىدۇ[6ـ7].",
      "footnotes": null
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 601,
      "verse_key": "104:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهَا عَلَيۡهِم مُّؤۡصَدَةٞ",
      "translation": "ئۇلار چوقۇم دوزاخقا سولىنىدۇ[8].",
      "footnotes": null
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 601,
      "verse_key": "104:9",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِي عَمَدٖ مُّمَدَّدَةِۭ",
      "translation": " ئېگىز تۈۋرۈكلەرگە باغلىنىدۇ[9].",
      "footnotes": null
    }
  ]
}