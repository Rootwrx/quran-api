{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 526,
      "verse_key": "53:1",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلنَّجۡمِ إِذَا هَوَىٰ",
      "translation": "ساقىغان يۇلتۇزلار بىلەن قەسەمكى[1]، ",
      "footnotes": null
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 526,
      "verse_key": "53:2",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَا ضَلَّ صَاحِبُكُمۡ وَمَا غَوَىٰ",
      "translation": "سىلەرنىڭ ھەمراھىڭلار (يەنى مۇھەممەد ئەلەيھىسسالام) گۇمراھ بولمىدى ۋە يولدىن ئاداشمىدى[2].",
      "footnotes": null
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 526,
      "verse_key": "53:3",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا يَنطِقُ عَنِ ٱلۡهَوَىٰٓ",
      "translation": "(ئۇ) ئۆز نەپسى خاھىشى بويىچە سۆزلىمەيدۇ[3].",
      "footnotes": null
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 526,
      "verse_key": "53:4",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنۡ هُوَ إِلَّا وَحۡيٞ يُوحَىٰ",
      "translation": "پەقەت ئۇنىڭغا نازىل قىلىنغان ۋەھىينىلا سۆزلەيدۇ[4].",
      "footnotes": null
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 526,
      "verse_key": "53:5",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلَّمَهُۥ شَدِيدُ ٱلۡقُوَىٰ",
      "translation": "ۋەھىينى ئۇنىڭغا كۈچلۈك پەرىشتە (جىبرىئىل) تەلىم بەردى[5].",
      "footnotes": null
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 526,
      "verse_key": "53:6",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ذُو مِرَّةٖ فَٱسۡتَوَىٰ",
      "translation": " ئۇنىڭ (يەنى جىبرىئىلنىڭ) كۆرۈنۈشى چىرايلىق بولۇپ، يۇقىرى ئۇپۇقتا (ئۆز شەكلىدە) تۇردى[6ـ7].",
      "footnotes": null
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 526,
      "verse_key": "53:7",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَهُوَ بِٱلۡأُفُقِ ٱلۡأَعۡلَىٰ",
      "translation": " ئۇنىڭ (يەنى جىبرىئىلنىڭ) كۆرۈنۈشى چىرايلىق بولۇپ، يۇقىرى ئۇپۇقتا (ئۆز شەكلىدە) تۇردى[6ـ7].",
      "footnotes": null
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 526,
      "verse_key": "53:8",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ دَنَا فَتَدَلَّىٰ",
      "translation": " ئاندىن ئۇ ئاستا ـ ئاستا (مۇھەممەد ئەلەيھىسسالامغا) يېقىنلىشىپ تۆۋەنگە ساڭگىلىدى[8].",
      "footnotes": null
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 526,
      "verse_key": "53:9",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَكَانَ قَابَ قَوۡسَيۡنِ أَوۡ أَدۡنَىٰ",
      "translation": "ئۇ (پەيغەمبەر ئەلەيھىسسالامغا) ئىككى ياچاق مىقدارى ياكى ئۇنىڭدىنمۇ يېقىنراق يېقىنلاشتى[9].",
      "footnotes": null
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 526,
      "verse_key": "53:10",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَوۡحَىٰٓ إِلَىٰ عَبۡدِهِۦ مَآ أَوۡحَىٰ",
      "translation": " ئاللاھ بەندىسىگە (جىبرىئىل ئارقىلىق تېگىشلىك) ۋەھىيلەرنى ۋەھىي قىلدى[10].",
      "footnotes": null
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 526,
      "verse_key": "53:11",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَا كَذَبَ ٱلۡفُؤَادُ مَا رَأَىٰٓ",
      "translation": "ئۇنىڭ كۆرگىنىنى دىلى ئىنكار قىلمىدى (يەنى راست كۆردى)[11].",
      "footnotes": null
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 526,
      "verse_key": "53:12",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَتُمَٰرُونَهُۥ عَلَىٰ مَا يَرَىٰ",
      "translation": "(ئى مۇشرىكلار جامائەسى!) ئۇنىڭ كۆرگەنلىرى ئۈستىدە ئۇنىڭ بىلەن مۇنازىرە قىلىشامسىلەر؟ [12]",
      "footnotes": null
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 526,
      "verse_key": "53:13",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَقَدۡ رَءَاهُ نَزۡلَةً أُخۡرَىٰ",
      "translation": " ئۇ جىبرىئىلنى ھەقىقەتەن ئىككىنچى قېتىم سىدرەتۇلمۇنتەھا (دەرىخىنىڭ) يېنىدا كۆردى[13ـ14].",
      "footnotes": null
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 526,
      "verse_key": "53:14",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عِندَ سِدۡرَةِ ٱلۡمُنتَهَىٰ",
      "translation": "ئىككىنچى قېتىم سىدرەتۇلمۇنتەھا (دەرىخىنىڭ) يېنىدا كۆردى[13ـ14].",
      "footnotes": null
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 526,
      "verse_key": "53:15",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عِندَهَا جَنَّةُ ٱلۡمَأۡوَىٰٓ",
      "translation": "جەننەتۇلمەئۋا بولسا سىدرەتۇلمۇنتەھانىڭ يېنىدىدۇر[15].",
      "footnotes": null
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 526,
      "verse_key": "53:16",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذۡ يَغۡشَى ٱلسِّدۡرَةَ مَا يَغۡشَىٰ",
      "translation": " (ئۇ) سىدرەتۇلمۇنتەھانى (ئالەملەرنىڭ پەرۋەردىگارىنىڭ نۇرى) قاپلىغان چاغدا (كۆردى)[16].",
      "footnotes": null
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 526,
      "verse_key": "53:17",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَا زَاغَ ٱلۡبَصَرُ وَمَا طَغَىٰ",
      "translation": "(پەيغەمبەر) نىڭ كۆزى (ئوڭ ـ سولغا) بۇرالمىدى، (كۆزلەنگەن نەرسىدىن) ئۆتۈپ كەتمىدى[17].",
      "footnotes": null
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 526,
      "verse_key": "53:18",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَقَدۡ رَأَىٰ مِنۡ ءَايَٰتِ رَبِّهِ ٱلۡكُبۡرَىٰٓ",
      "translation": " شۈبھىسىزكى، ئۇ پەرۋەردىگارىڭنىڭ (قۇدرىتىنى كۆرسىتىدىغان بۈيۈك ئالامەتلەرنى) كۆردى[18].",
      "footnotes": null
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 526,
      "verse_key": "53:19",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَرَءَيۡتُمُ ٱللَّٰتَ وَٱلۡعُزَّىٰ",
      "translation": " ئېيتىپ بېقىڭلارچۇ! لات، ئۇززا ۋە ئۈچىنچىسى بولغان ماناتلار (ئاللاھتائالادەك كۈچ ـ قۇۋۋەتكە ئىگىمۇ؟)[19ـ20].",
      "footnotes": null
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 526,
      "verse_key": "53:20",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَنَوٰةَ ٱلثَّالِثَةَ ٱلۡأُخۡرَىٰٓ",
      "translation": " ئېيتىپ بېقىڭلارچۇ! لات، ئۇززا ۋە ئۈچىنچىسى بولغان ماناتلار (ئاللاھتائالادەك كۈچ ـ قۇۋۋەتكە ئىگىمۇ؟)[19ـ20].",
      "footnotes": null
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 526,
      "verse_key": "53:21",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَكُمُ ٱلذَّكَرُ وَلَهُ ٱلۡأُنثَىٰ",
      "translation": " (ئى مۇشرىكلار جامائەسى!) (سىلەرچە ياخشى ھېسابلانغان) ئوغۇل بالا سىلەرگە خاس بولۇپ، (سىلەرچە يامان ھېسابلانغان) قىز بالا ئاللاھقا خاسمۇ؟[21]",
      "footnotes": null
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 526,
      "verse_key": "53:22",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تِلۡكَ إِذٗا قِسۡمَةٞ ضِيزَىٰٓ",
      "translation": " ئۇنداقتا بۇ ئادالەتسىز تەقسىماتتۇر[22]. ",
      "footnotes": null
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 526,
      "verse_key": "53:23",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنۡ هِيَ إِلَّآ أَسۡمَآءٞ سَمَّيۡتُمُوهَآ أَنتُمۡ وَءَابَآؤُكُم مَّآ أَنزَلَ ٱللَّهُ بِهَا مِن سُلۡطَٰنٍۚ إِن يَتَّبِعُونَ إِلَّا ٱلظَّنَّ وَمَا تَهۡوَى ٱلۡأَنفُسُۖ وَلَقَدۡ جَآءَهُم مِّن رَّبِّهِمُ ٱلۡهُدَىٰٓ",
      "translation": "بۇ بۇتلار پەقەت سىلەر ۋە ئاتا ـ بوۋاڭلار ئاتىۋالغان قۇرۇق ناملاردىنلا ئىبارەت، ئاللاھ بۇنى ئىسپاتلايدىغان ھېچبىر دەلىل ـ پاكىت چۈشۈرگىنى يوق، ئۇلارغا پەرۋەردىگارى تەرىپىدىن شەك ـ شۈبھىسىز ھىدايەت (يەنى پەيغەمبەر ۋە قۇرئان) كەلگەن تۇرۇقلۇق، ئۇلار پەقەت گۇمانغىلا ۋە نەپسىلىرىنىڭ خاھىشىغىلا ئەگىشىدۇ[23].",
      "footnotes": null
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 526,
      "verse_key": "53:24",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَمۡ لِلۡإِنسَٰنِ مَا تَمَنَّىٰ",
      "translation": " ئىنسان نېمىنى ئارزۇ قىلسا شۇ بولامدۇ؟[24]",
      "footnotes": null
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 526,
      "verse_key": "53:25",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلِلَّهِ ٱلۡأٓخِرَةُ وَٱلۡأُولَىٰ",
      "translation": " ئاخىرەت ۋە دۇنيا ئاللاھنىڭ ئىلكىدىدۇر (يەنى دۇنيا ۋە ئاخىرەتتىكى ئىشلارنىڭ ھەممىسى ئاللاھنىڭ خاھىشى بويىچە بولىدۇ)[25].",
      "footnotes": null
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 526,
      "verse_key": "53:26",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "۞ وَكَم مِّن مَّلَكٖ فِي ٱلسَّمَٰوَٰتِ لَا تُغۡنِي شَفَٰعَتُهُمۡ شَيۡـًٔا إِلَّا مِنۢ بَعۡدِ أَن يَأۡذَنَ ٱللَّهُ لِمَن يَشَآءُ وَيَرۡضَىٰٓ",
      "translation": " ئاسمانلاردا نۇرغۇن پەرىشتىلەر بولۇپ، ئۇلارنىڭ شاپائىتى ھېچ نەرسىگە ئەسقاتمايدۇ، پەقەت ئاللاھ ئۆزى خالىغان ۋە رازى بولغان ئادەمگە شاپائەت قىلىشقا رۇخسەت قىلسىلا (ئاندىن) ئۇلارنىڭ شاپائىتى ئەسقاتىدۇ[26]. ",
      "footnotes": null
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 527,
      "verse_key": "53:27",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ ٱلَّذِينَ لَا يُؤۡمِنُونَ بِٱلۡأٓخِرَةِ لَيُسَمُّونَ ٱلۡمَلَٰٓئِكَةَ تَسۡمِيَةَ ٱلۡأُنثَىٰ",
      "translation": "شۈبھىسىزكى، ئاخىرەتكە ئىمان ئېيتمايدىغانلار پەرىشتىلەرنى (ئاللاھنىڭ قىزلىرى دەپ) چىشى جىنس نامى بىلەن ئاتايدۇ[27].",
      "footnotes": null
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 527,
      "verse_key": "53:28",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا لَهُم بِهِۦ مِنۡ عِلۡمٍۖ إِن يَتَّبِعُونَ إِلَّا ٱلظَّنَّۖ وَإِنَّ ٱلظَّنَّ لَا يُغۡنِي مِنَ ٱلۡحَقِّ شَيۡـٔٗا",
      "translation": " ئۇلار مۇنداق ئاتاشتا ھېچقانداق ئىلىمگە ئىگە ئەمەس، ئۇلار پەقەت گۇمانغىلا ئاساسلىنىدۇ، گۇمان دېگەن ھەقنى ئىسپاتلاشتا ھېچ نەرسىگە يارىمايدۇ[28].",
      "footnotes": null
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 527,
      "verse_key": "53:29",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَعۡرِضۡ عَن مَّن تَوَلَّىٰ عَن ذِكۡرِنَا وَلَمۡ يُرِدۡ إِلَّا ٱلۡحَيَوٰةَ ٱلدُّنۡيَا",
      "translation": "بىزنىڭ زىكرىمىز (يەنى ئىمان بىلەن قۇرئان) دىن باش تارتقان، پەقەت دۇنيا تىرىكچىلىكنىلا كۆزلىگەنلەردىن يۈز ئۆرۈگىن[29].",
      "footnotes": null
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 527,
      "verse_key": "53:30",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ذَٰلِكَ مَبۡلَغُهُم مِّنَ ٱلۡعِلۡمِۚ إِنَّ رَبَّكَ هُوَ أَعۡلَمُ بِمَن ضَلَّ عَن سَبِيلِهِۦ وَهُوَ أَعۡلَمُ بِمَنِ ٱهۡتَدَىٰ",
      "translation": " ئۇلارنىڭ ئىلىمدىن يەتكەن يېرى ئەنە شۇ، شۈبھىسىزكى، پەرۋەردىگارىڭ ئۆزىنىڭ يولىدىن ئازغان ئادەمنى ئوبدان بىلىدۇ، ھىدايەت تاپقان ئادەمنىمۇ ئوبدان بىلىدۇ[30].",
      "footnotes": null
    },
    {
      "id": 31,
      "verse_number": 31,
      "page_number": 527,
      "verse_key": "53:31",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلِلَّهِ مَا فِي ٱلسَّمَٰوَٰتِ وَمَا فِي ٱلۡأَرۡضِ لِيَجۡزِيَ ٱلَّذِينَ أَسَٰٓـُٔواْ بِمَا عَمِلُواْ وَيَجۡزِيَ ٱلَّذِينَ أَحۡسَنُواْ بِٱلۡحُسۡنَى",
      "translation": "ئاسمانلاردىكى ۋە زېمىندىكى نەرسىلەرنىڭ ھەممىسى ئاللاھنىڭ مۈلكىدۇر، (ئاللاھ) يامانلىق قىلغانلارنى قىلمىشلىرىغا قاراپ جازالايدۇ، ياخشىلىق قىلغانلارغا چىرايلىق مۇكاپات بېرىدۇ[31].",
      "footnotes": null
    },
    {
      "id": 32,
      "verse_number": 32,
      "page_number": 527,
      "verse_key": "53:32",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِينَ يَجۡتَنِبُونَ كَبَٰٓئِرَ ٱلۡإِثۡمِ وَٱلۡفَوَٰحِشَ إِلَّا ٱللَّمَمَۚ إِنَّ رَبَّكَ وَٰسِعُ ٱلۡمَغۡفِرَةِۚ هُوَ أَعۡلَمُ بِكُمۡ إِذۡ أَنشَأَكُم مِّنَ ٱلۡأَرۡضِ وَإِذۡ أَنتُمۡ أَجِنَّةٞ فِي بُطُونِ أُمَّهَٰتِكُمۡۖ فَلَا تُزَكُّوٓاْ أَنفُسَكُمۡۖ هُوَ أَعۡلَمُ بِمَنِ ٱتَّقَىٰٓ",
      "translation": "ئۇلار چوڭ گۇناھلاردىن ۋە قەبىھ ئىشلاردىن يىراق بولىدۇ. كىچىك گۇناھلار بۇنىڭدىن مۇستەسنا، ھەقىقەتەن پەرۋەردىگارىڭنىڭ مەغپىرىتى كەڭدۇر، ئاللاھ سىلەرنى زېمىندىن ياراتقان چاغدىكى (ۋاقتىڭلاردىن تارتىپ، ئاناڭلارنىڭ قورساقلىرىدىكى بالا) ۋاقتىڭلاردىن تارتىپ ئوبدان بىلىدۇ، شۇنىڭ ئۈچۈن سىلەر ئۆزۈڭلارنى پاك ھېسابلىماڭلار، ئاللاھ تەقۋادار بولغان ئادەمنى ئوبدان بىلىدۇ[32].",
      "footnotes": null
    },
    {
      "id": 33,
      "verse_number": 33,
      "page_number": 527,
      "verse_key": "53:33",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَرَءَيۡتَ ٱلَّذِي تَوَلَّىٰ",
      "translation": " (ئى مۇھەممەد!) ئىماندىن يۈز ئۆرۈگەن ئادەمدىن خەۋەر بەرگىن[33].",
      "footnotes": null
    },
    {
      "id": 34,
      "verse_number": 34,
      "page_number": 527,
      "verse_key": "53:34",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَعۡطَىٰ قَلِيلٗا وَأَكۡدَىٰٓ",
      "translation": " ئۇ (شەرت قىلىنغان مالدىن) ئازغىنا بەردى، قالغىنىنى بەرمىدى[34].",
      "footnotes": null
    },
    {
      "id": 35,
      "verse_number": 35,
      "page_number": 527,
      "verse_key": "53:35",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَعِندَهُۥ عِلۡمُ ٱلۡغَيۡبِ فَهُوَ يَرَىٰٓ",
      "translation": " ئۇنىڭ دەرگاھىدا ئىلمىي غەيب بولۇپ، ئۇ غەيبنى بىلەمدۇ؟[35]",
      "footnotes": null
    },
    {
      "id": 36,
      "verse_number": 36,
      "page_number": 527,
      "verse_key": "53:36",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَمۡ لَمۡ يُنَبَّأۡ بِمَا فِي صُحُفِ مُوسَىٰ",
      "translation": " ئۇ مۇسانىڭ ۋە ئىبراھىمنىڭ سەھىپىلىرىدىكى سۆزدىن خەۋەردار بولمىدىمۇ؟[36]",
      "footnotes": null
    },
    {
      "id": 37,
      "verse_number": 37,
      "page_number": 527,
      "verse_key": "53:37",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِبۡرَٰهِيمَ ٱلَّذِي وَفَّىٰٓ",
      "translation": "ئىبراھىم ۋاپادار كىشى ئىدى، [37]",
      "footnotes": null
    },
    {
      "id": 38,
      "verse_number": 38,
      "page_number": 527,
      "verse_key": "53:38",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَّا تَزِرُ وَازِرَةٞ وِزۡرَ أُخۡرَىٰ",
      "translation": "بىر گۇناھكار ئادەم يەنە بىراۋنىڭ گۇناھىنى كۆتەرمەيدۇ (يەنى بىراۋ باشقا بىراۋنىڭ گۇناھى تۈپەيلىدىن جازاغا تارتىلمايدۇ)[ 38].",
      "footnotes": null
    },
    {
      "id": 39,
      "verse_number": 39,
      "page_number": 527,
      "verse_key": "53:39",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَن لَّيۡسَ لِلۡإِنسَٰنِ إِلَّا مَا سَعَىٰ",
      "translation": " ئىنسان پەقەت ئۆزىنىڭ ئىشلىگەن ئىشىنىڭ نەتىجىسىنى كۆرىدۇ[39].",
      "footnotes": null
    },
    {
      "id": 40,
      "verse_number": 40,
      "page_number": 527,
      "verse_key": "53:40",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّ سَعۡيَهُۥ سَوۡفَ يُرَىٰ",
      "translation": " ئۇنىڭ قىلغان ئىشى كەلگۈسىدە كۆرۈلىدۇ (يەنى ئۇنىڭ ئەمەلى قىيامەت كۈنى ئۇنىڭغا توغرىلىنىدۇ)[40].",
      "footnotes": null
    },
    {
      "id": 41,
      "verse_number": 41,
      "page_number": 527,
      "verse_key": "53:41",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ يُجۡزَىٰهُ ٱلۡجَزَآءَ ٱلۡأَوۡفَىٰ",
      "translation": "ئاندىن ئۇنىڭغا تولۇق مۇكاپات (ياكى) تولۇق جازا بېرىلىدۇ[41].",
      "footnotes": null
    },
    {
      "id": 42,
      "verse_number": 42,
      "page_number": 527,
      "verse_key": "53:42",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّ إِلَىٰ رَبِّكَ ٱلۡمُنتَهَىٰ",
      "translation": "ھەممىنىڭ ئاخىر بارىدىغان جايى پەرۋەردىگارىڭنىڭ دەرگاھىدۇر[42].",
      "footnotes": null
    },
    {
      "id": 43,
      "verse_number": 43,
      "page_number": 527,
      "verse_key": "53:43",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّهُۥ هُوَ أَضۡحَكَ وَأَبۡكَىٰ",
      "translation": "ئاللاھ ئىنساننى كۈلدۈرەلەيدۇ ۋە يىغلىتالايدۇ[43].",
      "footnotes": null
    },
    {
      "id": 44,
      "verse_number": 44,
      "page_number": 527,
      "verse_key": "53:44",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّهُۥ هُوَ أَمَاتَ وَأَحۡيَا",
      "translation": " ئاللاھ ئۆلتۈرۈشكە ۋە تىرىلدۈرۈشكە قادىر[44].",
      "footnotes": null
    },
    {
      "id": 45,
      "verse_number": 45,
      "page_number": 528,
      "verse_key": "53:45",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّهُۥ خَلَقَ ٱلزَّوۡجَيۡنِ ٱلذَّكَرَ وَٱلۡأُنثَىٰ",
      "translation": " ئاللاھ بىر جۈپنى ـ ئەركەك بىلەن چىشىنى ياراتتى[45].",
      "footnotes": null
    },
    {
      "id": 46,
      "verse_number": 46,
      "page_number": 528,
      "verse_key": "53:46",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مِن نُّطۡفَةٍ إِذَا تُمۡنَىٰ",
      "translation": " ئېتىلىپ چىققان مەنىدىن (ياراتتى)[46].",
      "footnotes": null
    },
    {
      "id": 47,
      "verse_number": 47,
      "page_number": 528,
      "verse_key": "53:47",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّ عَلَيۡهِ ٱلنَّشۡأَةَ ٱلۡأُخۡرَىٰ",
      "translation": " (ھېساب ئۈچۈن ئىنسانلارنى) ئىككىنچى قېتىم يارىتىشنى ئۈستىگە ئالدى[47].",
      "footnotes": null
    },
    {
      "id": 48,
      "verse_number": 48,
      "page_number": 528,
      "verse_key": "53:48",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّهُۥ هُوَ أَغۡنَىٰ وَأَقۡنَىٰ",
      "translation": " ئاللاھ ئىنساننى باي قىلالايدۇ ۋە (ئاز بىلەنمۇ) مەمنۇن قىلالايدۇ[48].",
      "footnotes": null
    },
    {
      "id": 49,
      "verse_number": 49,
      "page_number": 528,
      "verse_key": "53:49",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّهُۥ هُوَ رَبُّ ٱلشِّعۡرَىٰ",
      "translation": " ئاللاھ شىئرا يۇلتۇزىنىڭ پەرۋەردىگارىدۇر[49].",
      "footnotes": null
    },
    {
      "id": 50,
      "verse_number": 50,
      "page_number": 528,
      "verse_key": "53:50",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّهُۥٓ أَهۡلَكَ عَادًا ٱلۡأُولَىٰ",
      "translation": " ئۇ (يەنى ئاللاھ) قەدىمكى ئادنى ھالاك قىلدى[50].",
      "footnotes": null
    },
    {
      "id": 51,
      "verse_number": 51,
      "page_number": 528,
      "verse_key": "53:51",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَثَمُودَاْ فَمَآ أَبۡقَىٰ",
      "translation": " سەمۇدنىمۇ (ھالاك قىلدى، ئۇلاردىن بىرىنىمۇ) قويمىدى[51].",
      "footnotes": null
    },
    {
      "id": 52,
      "verse_number": 52,
      "page_number": 528,
      "verse_key": "53:52",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَقَوۡمَ نُوحٖ مِّن قَبۡلُۖ إِنَّهُمۡ كَانُواْ هُمۡ أَظۡلَمَ وَأَطۡغَىٰ",
      "translation": "ئىلگىرى نۇھنىڭ قەۋمىنى (ھالاك قىلغان ئىدى)، شۈبھىسىزكى، ئۇلار تېخىمۇ زالىم، تېخىمۇ سەركەش ئىدى[52].",
      "footnotes": null
    },
    {
      "id": 53,
      "verse_number": 53,
      "page_number": 528,
      "verse_key": "53:53",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡمُؤۡتَفِكَةَ أَهۡوَىٰ",
      "translation": ". لۇت قەۋمىنىڭ شەھەرلىرىنى دۈم كۆمتۈرىۋەتتى، ئۇنى (دەھشەتلىك تاشلار) قاپلىۋالدى[53 ـ 54].",
      "footnotes": null
    },
    {
      "id": 54,
      "verse_number": 54,
      "page_number": 528,
      "verse_key": "53:54",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَغَشَّىٰهَا مَا غَشَّىٰ",
      "translation": ". لۇت قەۋمىنىڭ شەھەرلىرىنى دۈم كۆمتۈرىۋەتتى، ئۇنى (دەھشەتلىك تاشلار) قاپلىۋالدى[53 ـ 54].",
      "footnotes": null
    },
    {
      "id": 55,
      "verse_number": 55,
      "page_number": 528,
      "verse_key": "53:55",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكَ تَتَمَارَىٰ",
      "translation": "ئىنسان!) پەرۋەردىگارىڭنىڭ قايسىبىر نېمىتىدىن گۇمانلىنىسەن؟[55]",
      "footnotes": null
    },
    {
      "id": 56,
      "verse_number": 56,
      "page_number": 528,
      "verse_key": "53:56",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "هَٰذَا نَذِيرٞ مِّنَ ٱلنُّذُرِ ٱلۡأُولَىٰٓ",
      "translation": " بۇ (يەنى مۇھەممەد ئەلەيھىسسالام) ئىلگىرىكى ئاگاھلاندۇرغۇچىلارنىڭ جۈملىسىدىن بولغان بىر ئاگاھلاندۇرغۇچىدۇر[56].",
      "footnotes": null
    },
    {
      "id": 57,
      "verse_number": 57,
      "page_number": 528,
      "verse_key": "53:57",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَزِفَتِ ٱلۡأٓزِفَةُ",
      "translation": "قىيامەت يېقىنلاشتى[57].",
      "footnotes": null
    },
    {
      "id": 58,
      "verse_number": 58,
      "page_number": 528,
      "verse_key": "53:58",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَيۡسَ لَهَا مِن دُونِ ٱللَّهِ كَاشِفَةٌ",
