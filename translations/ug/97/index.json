{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 598,
      "verse_key": "97:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّآ أَنزَلۡنَٰهُ فِي لَيۡلَةِ ٱلۡقَدۡرِ",
      "translation": "بىز قۇرئاننى ھەقىقەتەن شەبى قەدرىدە نازىل قىلدۇق[1].",
      "footnotes": null
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 598,
      "verse_key": "97:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَآ أَدۡرَىٰكَ مَا لَيۡلَةُ ٱلۡقَدۡرِ",
      "translation": " شەبى قەدرىنىڭ نېمە ئىكەنلىكىنى بىلەمسەن؟[2] ",
      "footnotes": null
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 598,
      "verse_key": "97:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَيۡلَةُ ٱلۡقَدۡرِ خَيۡرٞ مِّنۡ أَلۡفِ شَهۡرٖ",
      "translation": "شەبى قەدرى (شەرەپ ۋە پەزىلەتتە) مىڭ ئايدىن ئارتۇقتۇر[3].",
      "footnotes": null
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 598,
      "verse_key": "97:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تَنَزَّلُ ٱلۡمَلَٰٓئِكَةُ وَٱلرُّوحُ فِيهَا بِإِذۡنِ رَبِّهِم مِّن كُلِّ أَمۡرٖ",
      "translation": "پەرىشتىلەر ۋە جىبرىئىل شۇ كېچىدە پەرۋەردىگارىنىڭ ئەمرى بىلەن (ئاللاھ تەقدىر قىلغان) بارلىق ئىش ئۈچۈن (زېمىنغا) چۈشىدۇ[4].",
      "footnotes": null
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 598,
      "verse_key": "97:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "سَلَٰمٌ هِيَ حَتَّىٰ مَطۡلَعِ ٱلۡفَجۡرِ",
      "translation": "شۇ كېچە تاڭ يورۇغانغا قەدەر پۈتۈنلەي تىنچ ـ ئامانلىقتىن ئىبارەتتۇر[5].",
      "footnotes": null
    }
  ]
}