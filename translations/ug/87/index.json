{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 591,
      "verse_key": "87:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "سَبِّحِ ٱسۡمَ رَبِّكَ ٱلۡأَعۡلَى",
      "translation": "ھەممىدىن ئۈستۈن پەرۋەردىگارىڭنىڭ نامىنى پاك ئېتىقاد قىلغىن[1].",
      "footnotes": null
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 591,
      "verse_key": "87:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِي خَلَقَ فَسَوَّىٰ",
      "translation": " ئۇ (پۈتۈن مەخلۇقاتنى) ياراتتى، (ئۇلارنى) بېجىرىم (يەنى چىرايلىق ۋە مۇناسىپ شەكىلدە) قىلدى[2]. ",
      "footnotes": null
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 591,
      "verse_key": "87:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّذِي قَدَّرَ فَهَدَىٰ",
      "translation": "مەخلۇقاتقا پايدىلىق ئىشلارنى (تەقدىر قىلدى، ئۇلارغا پايدىلىنىش يوللىرىنى) كۆرسەتتى[3]. ",
      "footnotes": null
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 591,
      "verse_key": "87:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّذِيٓ أَخۡرَجَ ٱلۡمَرۡعَىٰ",
      "translation": "ئوت ـ چۆپلەرنى ئۆستۈرۈپ بەردى[4].",
      "footnotes": null
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 591,
      "verse_key": "87:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَجَعَلَهُۥ غُثَآءً أَحۡوَىٰ",
      "translation": "ئاندىن ئۇلارنى قارامتۇل قۇرۇق ئوت ـ خەسكە ئايلاندۇردى[5].",
      "footnotes": null
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 591,
      "verse_key": "87:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "سَنُقۡرِئُكَ فَلَا تَنسَىٰٓ",
      "translation": " (ئى مۇھەممەد!) ساڭا (قۇرئاننى) ئوقۇتىمىز، ئاللاھ (نەسخى) قىلماقچى بولغان نەرسىدىن باشقىنى ئۇنتۇمايسەن. ئاللاھ (بەندىلەرنىڭ) ئاشكارا ۋە يوشۇرۇن (سۆز ـ ھەرىكەتلىرىنى) بىلىپ تۇرىدۇ[6ـ7].",
      "footnotes": null
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 591,
      "verse_key": "87:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَّا مَا شَآءَ ٱللَّهُۚ إِنَّهُۥ يَعۡلَمُ ٱلۡجَهۡرَ وَمَا يَخۡفَىٰ",
      "translation": " (ئى مۇھەممەد!) ساڭا (قۇرئاننى) ئوقۇتىمىز، ئاللاھ (نەسخى) قىلماقچى بولغان نەرسىدىن باشقىنى ئۇنتۇمايسەن. ئاللاھ (بەندىلەرنىڭ) ئاشكارا ۋە يوشۇرۇن (سۆز ـ ھەرىكەتلىرىنى) بىلىپ تۇرىدۇ[6ـ7].",
      "footnotes": null
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 591,
      "verse_key": "87:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَنُيَسِّرُكَ لِلۡيُسۡرَىٰ",
      "translation": " سېنى بىز ئاسان يولغا (يەنى ئىسلام شەرىئىتىگە) مۇۋەپپەق قىلىمىز[8].",
      "footnotes": null
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 591,
      "verse_key": "87:9",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَذَكِّرۡ إِن نَّفَعَتِ ٱلذِّكۡرَىٰ",
      "translation": "ۋەز ـ نەسىھەتنىڭ پايدىسى بولىدىغان بولسا، (كىشىلەرگە قۇرئان بىلەن) ۋەز ـ نەسىھەت قىلغىن[9].",
      "footnotes": null
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 591,
      "verse_key": "87:10",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "سَيَذَّكَّرُ مَن يَخۡشَىٰ",
      "translation": " ئاللاھتىن (قورققان) ئادەم ۋەز ـ نەسىھەتنى قوبۇل قىلىدۇ[10].",
      "footnotes": null
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 591,
      "verse_key": "87:11",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيَتَجَنَّبُهَا ٱلۡأَشۡقَى",
      "translation": "ئەڭ شەقى ئادەم ۋەز ـ نەسىھەتتىن قاچىدۇ[11].",
      "footnotes": null
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 591,
      "verse_key": "87:12",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِي يَصۡلَى ٱلنَّارَ ٱلۡكُبۡرَىٰ",
      "translation": "ئۇ كاتتا ئوتقا (يەنى دوزاخقا) كىرىدۇ[12].",
      "footnotes": null
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 591,
      "verse_key": "87:13",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ لَا يَمُوتُ فِيهَا وَلَا يَحۡيَىٰ",
      "translation": " ئاندىن ئۇ ئوتتا ئۆلمەيدۇ ۋە (ئوبدان) ياشىمايدۇ[13].",
      "footnotes": null
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 591,
      "verse_key": "87:14",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قَدۡ أَفۡلَحَ مَن تَزَكَّىٰ",
      "translation": " (كۇفرىدىن ۋە گۇناھتىن) پاك بولغان ئادەم مەقسىتىگە يەتتى[14].",
      "footnotes": null
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 591,
      "verse_key": "87:15",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَذَكَرَ ٱسۡمَ رَبِّهِۦ فَصَلَّىٰ",
      "translation": " ئۇ پەرۋەردىگارىنىڭ نامىنى ياد ئەتتى، ئاندىن ناماز ئوقۇدى[15]. ",
      "footnotes": null
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 592,
      "verse_key": "87:16",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بَلۡ تُؤۡثِرُونَ ٱلۡحَيَوٰةَ ٱلدُّنۡيَا",
      "translation": "سىلەر دۇنيا تىرىكچىلىكىنى ئارتۇق كۆرىسىلەر[16].",
      "footnotes": null
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 592,
      "verse_key": "87:17",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡأٓخِرَةُ خَيۡرٞ وَأَبۡقَىٰٓ",
      "translation": " ئاخىرەت ياخشىدۇر ۋە باقىيدۇر[17].",
      "footnotes": null
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 592,
      "verse_key": "87:18",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ هَٰذَا لَفِي ٱلصُّحُفِ ٱلۡأُولَىٰ",
      "translation": " بۇ (يەنى بۇ سۈرىدىكى ۋەزلەر) شەك ـ شۈبھىسىز بۇرۇنقى كىتابلاردا ـ ئىبراھىم ۋە مۇسالارنىڭ كىتابلىرىدا باردۇر[18ـ19]. ",
      "footnotes": null
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 592,
      "verse_key": "87:19",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "صُحُفِ إِبۡرَٰهِيمَ وَمُوسَىٰ",
      "translation": " بۇ (يەنى بۇ سۈرىدىكى ۋەزلەر) شەك ـ شۈبھىسىز بۇرۇنقى كىتابلاردا ـ ئىبراھىم ۋە مۇسالارنىڭ كىتابلىرىدا باردۇر[18ـ19]. ",
      "footnotes": null
    }
  ]
}