{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 595,
      "verse_key": "92:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّيۡلِ إِذَا يَغۡشَىٰ",
      "translation": "قاراڭغۇلۇقى ئالەمنى قاپلىغان كېچە بىلەن قەسەمكى[1]،",
      "footnotes": null
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 595,
      "verse_key": "92:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلنَّهَارِ إِذَا تَجَلَّىٰ",
      "translation": "يورۇغان چاغدىكى كۈندۈز بىلەن قەسەمكى[2]،",
      "footnotes": null
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 595,
      "verse_key": "92:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا خَلَقَ ٱلذَّكَرَ وَٱلۡأُنثَىٰٓ",
      "translation": "ئەر ـ ئايالنى ياراتقان زات ئاللاھ بىلەن قەسەمكى[3]،",
      "footnotes": null
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 595,
      "verse_key": "92:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ سَعۡيَكُمۡ لَشَتَّىٰ",
      "translation": " سىلەرنىڭ ئەمەلىڭلار شەك ـ شۈبھىسىز خىلمۇخىلدۇر[4].",
      "footnotes": null
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 595,
      "verse_key": "92:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَمَّا مَنۡ أَعۡطَىٰ وَٱتَّقَىٰ",
      "translation": " (پۇل ـ مېلىنى ئاللاھنىڭ رازىلىقى ئۈچۈن) سەرپ قىلغان، تەقۋادارلىق قىلغان، (كەلىمە تەۋھىدنى) تەستىق قىلغان ئادەمگە كەلسەك[5ـ6]، ",
      "footnotes": null
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 595,
      "verse_key": "92:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَصَدَّقَ بِٱلۡحُسۡنَىٰ",
      "translation": " (پۇل ـ مېلىنى ئاللاھنىڭ رازىلىقى ئۈچۈن) سەرپ قىلغان، تەقۋادارلىق قىلغان، (كەلىمە تەۋھىدنى) تەستىق قىلغان ئادەمگە كەلسەك[5ـ6]، ",
      "footnotes": null
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 595,
      "verse_key": "92:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَسَنُيَسِّرُهُۥ لِلۡيُسۡرَىٰ",
      "translation": "ئۇنىڭغا بىز ياخشى يولنى مۇيەسسەر قىلىپ بېرىمىز[7].",
      "footnotes": null
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 595,
      "verse_key": "92:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَمَّا مَنۢ بَخِلَ وَٱسۡتَغۡنَىٰ",
      "translation": "بېخىللىق قىلىپ (ئاللاھنىڭ ساۋابىدىن) ئۆزىنى بىھاجەت ھېسابلىغان، كەلىمە تەۋھىدنى ئىنكار قىلغان ئادەمگە كەلسەك[8ـ9]،",
      "footnotes": null
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 595,
      "verse_key": "92:9",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَكَذَّبَ بِٱلۡحُسۡنَىٰ",
      "translation": "بېخىللىق قىلىپ (ئاللاھنىڭ ساۋابىدىن) ئۆزىنى بىھاجەت ھېسابلىغان، كەلىمە تەۋھىدنى ئىنكار قىلغان ئادەمگە كەلسەك[8ـ9]،",
      "footnotes": null
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 595,
      "verse_key": "92:10",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَسَنُيَسِّرُهُۥ لِلۡعُسۡرَىٰ",
      "translation": " بىز ئۇنىڭغا يامان يولنى مۇيەسسەر قىلىپ بېرىمىز[10].",
      "footnotes": null
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 595,
      "verse_key": "92:11",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا يُغۡنِي عَنۡهُ مَالُهُۥٓ إِذَا تَرَدَّىٰٓ",
      "translation": " ئۇ (دوزاخقا) تاشلانغان چاغدا، ئۇنىڭ مېلى ھېچ نەرسىگە ئەسقاتمايدۇ[11].",
      "footnotes": null
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 595,
      "verse_key": "92:12",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ عَلَيۡنَا لَلۡهُدَىٰ",
      "translation": " توغرا يولنى كۆرسىتىشكە شەك ـ شۈبھىسىز بىز مەسئۇلمىز[12].",
      "footnotes": null
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 595,
      "verse_key": "92:13",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِنَّ لَنَا لَلۡأٓخِرَةَ وَٱلۡأُولَىٰ",
      "translation": " دۇنيا ۋە ئاخىرەت شەك ـ شۈبھىسىز بىزنىڭ ئىلكىمىزدىدۇر[13].",
      "footnotes": null
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 595,
      "verse_key": "92:14",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَنذَرۡتُكُمۡ نَارٗا تَلَظَّىٰ",
      "translation": "سىلەرنى لاۋۇلداپ تۇرغان ئوتتىن ئاگاھلاندۇردۇم[14].",
      "footnotes": null
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 596,
      "verse_key": "92:15",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَا يَصۡلَىٰهَآ إِلَّا ٱلۡأَشۡقَى",
      "translation": "ئۇنىڭغا پەقەت (ھەقنى) ئىنكار قىلغان، (ئىماندىن) يۈز ئۆرۈگەن ئادەملا كىرىدۇ[15ـ16].",
      "footnotes": null
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 596,
      "verse_key": "92:16",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِي كَذَّبَ وَتَوَلَّىٰ",
      "translation": "ئۇنىڭغا پەقەت (ھەقنى) ئىنكار قىلغان، (ئىماندىن) يۈز ئۆرۈگەن ئادەملا كىرىدۇ[15ـ16].",
      "footnotes": null
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 596,
      "verse_key": "92:17",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَسَيُجَنَّبُهَا ٱلۡأَتۡقَى",
      "translation": " پاكلىنىش ئۈچۈن مال ـ مۈلكىنى (ياخشىلىق يوللىرىغا) سەرپ قىلىدىغان ئادەم ئۇنىڭدىن (يەنى ئوتتىن) يىراق قىلىنىدۇ[17ـ18].",
      "footnotes": null
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 596,
      "verse_key": "92:18",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِي يُؤۡتِي مَالَهُۥ يَتَزَكَّىٰ",
      "translation": " پاكلىنىش ئۈچۈن مال ـ مۈلكىنى (ياخشىلىق يوللىرىغا) سەرپ قىلىدىغان ئادەم ئۇنىڭدىن (يەنى ئوتتىن) يىراق قىلىنىدۇ[17ـ18].",
      "footnotes": null
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 596,
      "verse_key": "92:19",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا لِأَحَدٍ عِندَهُۥ مِن نِّعۡمَةٖ تُجۡزَىٰٓ",
      "translation": " ئۇنىڭ قىلغان ياخشىلىقى بىراۋنىڭ ياخشىلىقىنى ياندۇرۇش ئۈچۈن ئەمەس، پەقەت ئۇلۇغ پەرۋەردىگارىنىڭ رازىلىقى ئۈچۈندۇر[19ـ20].",
      "footnotes": null
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 596,
      "verse_key": "92:20",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَّا ٱبۡتِغَآءَ وَجۡهِ رَبِّهِ ٱلۡأَعۡلَىٰ",
      "translation": " ئۇنىڭ قىلغان ياخشىلىقى بىراۋنىڭ ياخشىلىقىنى ياندۇرۇش ئۈچۈن ئەمەس، پەقەت ئۇلۇغ پەرۋەردىگارىنىڭ رازىلىقى ئۈچۈندۇر[19ـ20].",
      "footnotes": null
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 596,
      "verse_key": "92:21",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَسَوۡفَ يَرۡضَىٰ",
      "translation": "ئۇ (يەنى ئېھسان قىلغۇچى ئادەم) (پەرۋەردىگارىنىڭ بەرگەن مۇكاپاتىدىن) چوقۇم رازى بولىدۇ[21].",
      "footnotes": null
    }
  ]
}