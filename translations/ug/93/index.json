{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 596,
      "verse_key": "93:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلضُّحَىٰ",
      "translation": "چاشگاھ ۋاقتى بىلەن قەسەمكى[1]،",
      "footnotes": null
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 596,
      "verse_key": "93:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّيۡلِ إِذَا سَجَىٰ",
      "translation": " قاراڭغۇلۇقى (ئەتراپنى قاپلىغان) چاغدىكى كېچە بىلەن قەسەمكى[2]، ",
      "footnotes": null
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 596,
      "verse_key": "93:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَا وَدَّعَكَ رَبُّكَ وَمَا قَلَىٰ",
      "translation": " (ئى مۇھەممەد!) پەرۋەردىگارىڭ سېنى تاشلىۋەتكىنىمۇ يوق، يامان كۆرۈپ قالغىنىمۇ يوق[3].",
      "footnotes": null
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 596,
      "verse_key": "93:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَلۡأٓخِرَةُ خَيۡرٞ لَّكَ مِنَ ٱلۡأُولَىٰ",
      "translation": "ساڭا ئاخىرەت ئەلۋەتتە دۇنيادىن ياخشىدۇر[4]،",
      "footnotes": null
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 596,
      "verse_key": "93:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَسَوۡفَ يُعۡطِيكَ رَبُّكَ فَتَرۡضَىٰٓ",
      "translation": " پەرۋەردىگارىڭ ساڭا (ساۋاب، ھۆرمەت ۋە شاپائەت قاتارلىقلارنى) ئاتا قىلىدۇ، سەن مەمنۇن بولىسەن[5].",
      "footnotes": null
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 596,
      "verse_key": "93:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَمۡ يَجِدۡكَ يَتِيمٗا فَـَٔاوَىٰ",
      "translation": "ئۇ سېنى يېتىم بىلىپ پاناھ بەرمىدىمۇ؟[6]",
      "footnotes": null
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 596,
      "verse_key": "93:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَوَجَدَكَ ضَآلّٗا فَهَدَىٰ",
      "translation": "قايمۇققان بىلىپ ھىدايەت قىلمىدىمۇ؟[7]",
      "footnotes": null
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 596,
      "verse_key": "93:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَوَجَدَكَ عَآئِلٗا فَأَغۡنَىٰ",
      "translation": "سېنى يوقسۇز بىلىپ (كىشىلەردىن) بىھاجەت قىلمىدىمۇ؟[8]",
      "footnotes": null
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 596,
      "verse_key": "93:9",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَمَّا ٱلۡيَتِيمَ فَلَا تَقۡهَرۡ",
      "translation": "يېتىمنى خارلىما[9].",
      "footnotes": null
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 596,
      "verse_key": "93:10",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَمَّا ٱلسَّآئِلَ فَلَا تَنۡهَرۡ",
      "translation": "سائىلغا قوپاللىق قىلما[10]. ",
      "footnotes": null
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 596,
      "verse_key": "93:11",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَمَّا بِنِعۡمَةِ رَبِّكَ فَحَدِّثۡ",
      "translation": "پەرۋەردىگارىڭنىڭ (ساڭا بەرگەن) نېمىتىنى (كىشىلەرگە) سۆزلەپ بەرگىن (چۈنكى نېمەتنى سۆزلەپ بەرگەنلىك نېمەتكە شۈكۈر قىلغانلىق بولىدۇ)[11].",
      "footnotes": null
    }
  ]
}