{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 583,
      "verse_key": "79:1",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلنَّٰزِعَٰتِ غَرۡقٗا",
      "translation": "(كۇففارلارنىڭ جانلىرىنى) قاتتىق تارتىپ ئالغۇچى پەرىشتىلەر بىلەن قەسەمكى[1]، ",
      "footnotes": null
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 583,
      "verse_key": "79:2",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلنَّٰشِطَٰتِ نَشۡطٗا",
      "translation": "(مۆمىنلەرنىڭ جانلىرىنى) سىلىق ئالغۇچى پەرىشتىلەر بىلەن قەسەمكى[2]، ",
      "footnotes": null
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 583,
      "verse_key": "79:3",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلسَّٰبِحَٰتِ سَبۡحٗا",
      "translation": "(ئاللاھنىڭ ئەمرىنى ئاسماندىن) تېز ئېلىپ چۈشكۈچى پەرىشتىلەر بىلەن قەسەمكى[3]،",
      "footnotes": null
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 583,
      "verse_key": "79:4",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَٱلسَّٰبِقَٰتِ سَبۡقٗا",
      "translation": "(مۆمىنلەرنىڭ جانلىرىنى ئېلىپ جەننەتكە) ئىلگىرلىگۈچى پەرىشتىلەر بىلەن قەسەمكى[4]،",
      "footnotes": null
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 583,
      "verse_key": "79:5",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَٱلۡمُدَبِّرَٰتِ أَمۡرٗا",
      "translation": " (ئاللاھنىڭ ئەمرى بىلەن) كائىناتنىڭ ئىشلىرىنى باشقۇرغۇچى پەرىشتىلەر بىلەن قەسەمكى، (سىلەر چوقۇم تىرىلدۈرۈلىسىلەر، سىلەردىن چوقۇم ھېساب ئېلىنىدۇ)[5].",
      "footnotes": null
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 583,
      "verse_key": "79:6",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَوۡمَ تَرۡجُفُ ٱلرَّاجِفَةُ",
      "translation": "بۇ كۈن (يەنى قىيامەت كۈنى) دە زېمىن (ۋە ئۇنىڭدىكى شەيئىلەر قوشۇلۇپ) تەۋرەيدۇ[6].",
      "footnotes": null
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 583,
      "verse_key": "79:7",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تَتۡبَعُهَا ٱلرَّادِفَةُ",
      "translation": "ئۇنىڭ ئارقىسىدىنلا ئاسمان (ۋە ئۇنىڭدىكى شەيئىلەر قوشۇلۇپ) تەۋرەيدۇ[7].",
      "footnotes": null
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 583,
      "verse_key": "79:8",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قُلُوبٞ يَوۡمَئِذٖ وَاجِفَةٌ",
      "translation": " بۇ كۈندە دىللارنى (يەنى كۇففارلارنىڭ دىللىرىنى) قورقۇنچ باسىدۇ[8].",
      "footnotes": null
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 583,
      "verse_key": "79:9",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَبۡصَٰرُهَا خَٰشِعَةٞ",
      "translation": " شۇ (دىللارنىڭ ئىگىلىرىنىڭ) كۆزلىرى (قورقۇنچتىن) تىكىلىپ قارىيالمايدۇ[9].",
      "footnotes": null
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 583,
      "verse_key": "79:10",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَقُولُونَ أَءِنَّا لَمَرۡدُودُونَ فِي ٱلۡحَافِرَةِ",
      "translation": "ئۇلار ئېيتىدۇ: «راستلا بىز ئاۋۋالقى ھالىمىزغا قايتۇرۇلامدۇق؟ (يەنى ئۆلگەندىن كېيىن تىرىلەمدۇق؟)[10]",
      "footnotes": null
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 583,
      "verse_key": "79:11",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَءِذَا كُنَّا عِظَٰمٗا نَّخِرَةٗ",
      "translation": " ئۇ بىزنىڭ چىرىگەن سۆڭەك بولۇپ قالغان ۋاقتىمىز ئەمەسمۇ؟»[11] ",
      "footnotes": null
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 583,
      "verse_key": "79:12",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قَالُواْ تِلۡكَ إِذٗا كَرَّةٌ خَاسِرَةٞ",
      "translation": "ئۇلار ئېيتتى: «ئۇنداق بولىدىغان بولسا، بۇ (يەنى ئۆلگەندىن كېيىن تىرىلىش) زىيانلىق قايتىشتۇر (يەنى بىز ئۆلگەندىن كېيىن تىرىلىدىغان بولساق، دوزاخ ئەھلى بولغانلىقىمىز ئۈچۈن زىيان تارتقۇچىلارمىز)»[12].",
      "footnotes": null
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 583,
      "verse_key": "79:13",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِنَّمَا هِيَ زَجۡرَةٞ وَٰحِدَةٞ",
      "translation": " بىر سەيھە (يەنى ئىسراپىلنىڭ سۇر چېلىشى) بىلەنلا ئۇلار (يەنى جىمى خالايىق) (يەر ئاستىدىن) زېمىننىڭ ئۈستىگە چىقىپ قالىدۇ[13ـ14].",
      "footnotes": null
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 583,
      "verse_key": "79:14",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِذَا هُم بِٱلسَّاهِرَةِ",
      "translation": " بىر سەيھە (يەنى ئىسراپىلنىڭ سۇر چېلىشى) بىلەنلا ئۇلار (يەنى جىمى خالايىق) (يەر ئاستىدىن) زېمىننىڭ ئۈستىگە چىقىپ قالىدۇ[13ـ14].",
      "footnotes": null
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 583,
      "verse_key": "79:15",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "هَلۡ أَتَىٰكَ حَدِيثُ مُوسَىٰٓ",
      "translation": "ساڭا مۇسانىڭ خەۋىرى كەلدىمۇ؟[15]",
      "footnotes": null
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 584,
      "verse_key": "79:16",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذۡ نَادَىٰهُ رَبُّهُۥ بِٱلۡوَادِ ٱلۡمُقَدَّسِ طُوًى",
      "translation": " ئۆز ۋاقتىدا ئۇنى پەرۋەردىگارى تۇۋا ناملىق مۇقەددەس ۋادىدا (مۇنداق دەپ) چاقىردى:[16] ",
      "footnotes": null
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 584,
      "verse_key": "79:17",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱذۡهَبۡ إِلَىٰ فِرۡعَوۡنَ إِنَّهُۥ طَغَىٰ",
      "translation": "«سەن پىرئەۋننىڭ قېشىغا بارغىن، ئۇ ھەقىقەتەن ھەددىدىن ئاشتى[17].",
      "footnotes": null
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 584,
      "verse_key": "79:18",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَقُلۡ هَل لَّكَ إِلَىٰٓ أَن تَزَكَّىٰ",
      "translation": "ئۇنىڭغا :<كۇفرىدىن ۋە گۇناھلاردىن>پاك بولۇشنى خالامسەن.18",
      "footnotes": null
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 584,
      "verse_key": "79:19",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَهۡدِيَكَ إِلَىٰ رَبِّكَ فَتَخۡشَىٰ",
      "translation": " سېنى پەرۋەردىگارىمنى تونۇشقا يېتەكلىشىمنى خالامسەن؟ (ئۇنى تونۇساڭ پەرۋەردىگارىڭدىن) قورقىسەن ـ دە، (ياخشى ئەمەللەرنى قىلىسەن)، دېگىن»[19].",
      "footnotes": null
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 584,
      "verse_key": "79:20",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَرَىٰهُ ٱلۡأٓيَةَ ٱلۡكُبۡرَىٰ",
      "translation": " مۇسا پىرئەۋنگە چوڭ مۆجىزىنى كۆرسەتتى[20].",
      "footnotes": null
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 584,
      "verse_key": "79:21",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَكَذَّبَ وَعَصَىٰ",
      "translation": " پىرئەۋن (ئاللاھنىڭ پەيغەمبىرى مۇسانى) ئىنكار قىلدى ۋە (ئاللاھنىڭ ئەمرىگە) ئاسىيلىق قىلدى[21].",
      "footnotes": null
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 584,
      "verse_key": "79:22",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ أَدۡبَرَ يَسۡعَىٰ",
      "translation": "ئاندىن پىرئەۋن (مۇسادىن) يۈز ئۆرۈپ (ھىيلە ـ مىكىر ئىشلىتىشكە) كىرىشتى[22].",
      "footnotes": null
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 584,
      "verse_key": "79:23",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَحَشَرَ فَنَادَىٰ",
      "translation": " (ئۇ سېھرىگەرلەرنى، ئەسكەرلىرىنى، تەۋەلىرىنى) توپلىدى ھەم نىدا قىلىپ[23]",
      "footnotes": null
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 584,
      "verse_key": "79:24",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَقَالَ أَنَا۠ رَبُّكُمُ ٱلۡأَعۡلَىٰ",
      "translation": "دېدى: «مەن سىلەرنىڭ بۈيۈك پەرۋەردىگارىڭلار بولىمەن»[24].",
      "footnotes": null
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 584,
      "verse_key": "79:25",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَخَذَهُ ٱللَّهُ نَكَالَ ٱلۡأٓخِرَةِ وَٱلۡأُولَىٰٓ",
      "translation": "ئاللاھ ئۇنى دۇنيا ۋە ئاخىرەتنىڭ ئازابى بىلەن جازالىدى[25].",
      "footnotes": null
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 584,
      "verse_key": "79:26",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ فِي ذَٰلِكَ لَعِبۡرَةٗ لِّمَن يَخۡشَىٰٓ",
      "translation": " شەك ـ شۈبھىسىزكى، ئاللاھتىن قورقىدىغانلار بۇنىڭدىن ئىبرەت ئالىدۇ[26].",
      "footnotes": null
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 584,
      "verse_key": "79:27",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ءَأَنتُمۡ أَشَدُّ خَلۡقًا أَمِ ٱلسَّمَآءُۚ بَنَىٰهَا",
      "translation": " سىلەرنى يارىتىش مۈشكۈلمۇ ياكى ئاسماننى يارىتىش مۈشكۈلمۇ؟ ئاللاھ ئاسماننى ياراتتى[27].",
      "footnotes": null
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 584,
      "verse_key": "79:28",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "رَفَعَ سَمۡكَهَا فَسَوَّىٰهَا",
      "translation": "ئۇنىڭ ئېگىزلىكىنى يۈكسەك قىلدى ۋە كەم ـ كۈتىسىز قىلدى[28].",
      "footnotes": null
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 584,
      "verse_key": "79:29",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَغۡطَشَ لَيۡلَهَا وَأَخۡرَجَ ضُحَىٰهَا",
      "translation": "ئۇنىڭ كېچىسىنى قاپقاراڭغۇ، كۈندۈزىنى يوپيورۇق قىلدى[29].",
      "footnotes": null
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 584,
      "verse_key": "79:30",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡأَرۡضَ بَعۡدَ ذَٰلِكَ دَحَىٰهَآ",
      "translation": " شۇنىڭدىن كېيىن زېمىننى (ئەھلى زېمىننىڭ تۇرۇشىغا لايىق قىلىپ) يايدى[30].",
      "footnotes": null
    },
    {
      "id": 31,
      "verse_number": 31,
      "page_number": 584,
      "verse_key": "79:31",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَخۡرَجَ مِنۡهَا مَآءَهَا وَمَرۡعَىٰهَا",
      "translation": "زېمىندىن بۇلاقلارنى ئاقتۇردى، ئۆسۈملۈكلەرنى ئۆستۈردى[31]. ",
      "footnotes": null
    },
    {
      "id": 32,
      "verse_number": 32,
      "page_number": 584,
      "verse_key": "79:32",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡجِبَالَ أَرۡسَىٰهَا",
      "translation": " تاغلارنى (زېمىندا) مۇقىم قىلدى[32].",
      "footnotes": null
    },
    {
      "id": 33,
      "verse_number": 33,
      "page_number": 584,
      "verse_key": "79:33",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَتَٰعٗا لَّكُمۡ وَلِأَنۡعَٰمِكُمۡ",
      "translation": " ئاللاھتائالا ئۇلارنىڭ ھەممىسىنى سىلەرنىڭ ۋە ھايۋانلىرىڭلارنىڭ پايدىلىنىشى ئۈچۈن (ياراتتى)[33].",
      "footnotes": null
    },
    {
      "id": 34,
      "verse_number": 34,
      "page_number": 584,
      "verse_key": "79:34",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِذَا جَآءَتِ ٱلطَّآمَّةُ ٱلۡكُبۡرَىٰ",
      "translation": " قىيامەت كەلگەندە[34]،",
      "footnotes": null
    },
    {
      "id": 35,
      "verse_number": 35,
      "page_number": 584,
      "verse_key": "79:35",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَوۡمَ يَتَذَكَّرُ ٱلۡإِنسَٰنُ مَا سَعَىٰ",
      "translation": "(شۇ) كۈندە ئىنسان (ياخشى ـ يامان) قىلمىشلىرىنى ئەسلەيدۇ[35].",
      "footnotes": null
    },
    {
      "id": 36,
      "verse_number": 36,
      "page_number": 584,
      "verse_key": "79:36",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَبُرِّزَتِ ٱلۡجَحِيمُ لِمَن يَرَىٰ",
      "translation": "قارىغۇچىلارغا جەھەننەم ئوچۇق كۆرسىتىلىدۇ[36].",
      "footnotes": null
    },
    {
      "id": 37,
      "verse_number": 37,
      "page_number": 584,
      "verse_key": "79:37",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَمَّا مَن طَغَىٰ",
      "translation": "كىمكى (كۇفرىدا، گۇناھتا) چېكىدىن ئاشىدىكەن[37]،",
      "footnotes": null
    },
    {
      "id": 38,
      "verse_number": 38,
      "page_number": 584,
      "verse_key": "79:38",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَءَاثَرَ ٱلۡحَيَوٰةَ ٱلدُّنۡيَا",
      "translation": " دۇنيا تىرىكچىلىكىنى (ئاخىرەتتىن) ئارتۇق كۆرىدىكەن[38]،",
      "footnotes": null
    },
    {
      "id": 39,
      "verse_number": 39,
      "page_number": 584,
      "verse_key": "79:39",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِنَّ ٱلۡجَحِيمَ هِيَ ٱلۡمَأۡوَىٰ",
      "translation": " ئۇنىڭ جايى ھەقىقەتەن جەھەننەم بولىدۇ[39].",
      "footnotes": null
    },
    {
      "id": 40,
      "verse_number": 40,
      "page_number": 584,
      "verse_key": "79:40",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَمَّا مَنۡ خَافَ مَقَامَ رَبِّهِۦ وَنَهَى ٱلنَّفۡسَ عَنِ ٱلۡهَوَىٰ",
      "translation": "پەرۋەردىگارىنىڭ ئالدىدا (سوراققا تارتىلىشتىن) قورقىدىغان، ئۆزىنى نەپسى خاھىشىغا بېرىلىشتىن (يەنى ھارام قىلىنغان نەرسىلەردىن) چەكلىگەن ئادەمگە كەلسەك[40]، ",
      "footnotes": null
    },
    {
      "id": 41,
      "verse_number": 41,
      "page_number": 584,
      "verse_key": "79:41",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِنَّ ٱلۡجَنَّةَ هِيَ ٱلۡمَأۡوَىٰ",
      "translation": " ھەقىقەتەن ئۇنىڭ جايى جەننەت بولىدۇ[41].",
      "footnotes": null
    },
    {
      "id": 42,
      "verse_number": 42,
      "page_number": 584,
      "verse_key": "79:42",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَسۡـَٔلُونَكَ عَنِ ٱلسَّاعَةِ أَيَّانَ مُرۡسَىٰهَا",
      "translation": " ئۇلار سەندىن قىيامەتنىڭ قاچان بولىدىغانلىقىنى سورايدۇ[42].",
      "footnotes": null
    },
    {
      "id": 43,
      "verse_number": 43,
      "page_number": 584,
      "verse_key": "79:43",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِيمَ أَنتَ مِن ذِكۡرَىٰهَآ",
      "translation": " سەن ئۇنى قانداقمۇ بايان قىلىپ بېرەلەيسەن؟[43]",
      "footnotes": null
    },
    {
      "id": 44,
      "verse_number": 44,
      "page_number": 584,
      "verse_key": "79:44",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَىٰ رَبِّكَ مُنتَهَىٰهَآ",
      "translation": " ئۇنى بىلىش پەرۋەردىگارىڭغا خاستۇر[44].",
      "footnotes": null
    },
    {
      "id": 45,
      "verse_number": 45,
      "page_number": 584,
      "verse_key": "79:45",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّمَآ أَنتَ مُنذِرُ مَن يَخۡشَىٰهَا",
      "translation": " سېنىڭ ۋەزىپەڭ پەقەت قىيامەتتىن قورقىدىغان ئادەمنى ئاگاھلاندۇرۇشتۇر[45].",
      "footnotes": null
    },
    {
      "id": 46,
      "verse_number": 46,
      "page_number": 584,
      "verse_key": "79:46",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَأَنَّهُمۡ يَوۡمَ يَرَوۡنَهَا لَمۡ يَلۡبَثُوٓاْ إِلَّا عَشِيَّةً أَوۡ ضُحَىٰهَا",
      "translation": " ئۇلار قىيامەتنى كۆرگەن كۈندە، گويا ئۆزلىرىنى (دۇنيادا) پەقەت بىر كەچقۇرۇن ياكى بىر ئەتىگەن تۇرغاندەك (گۇمان قىلىدۇ)[46].",
      "footnotes": null
    }
  ]
}