{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 585,
      "verse_key": "80:1",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَبَسَ وَتَوَلَّىٰٓ",
      "translation": "يېنىغا ئەما كەلگەنلىكى ئۈچۈن (پەيغەمبەر ئەلەيھىسسالام) تەرىنى تۈردى ۋە يۈزىنى ئۆرىۋالدى[1ـ2].",
      "footnotes": null
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 585,
      "verse_key": "80:2",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَن جَآءَهُ ٱلۡأَعۡمَىٰ",
      "translation": "يېنىغا ئەما كەلگەنلىكى ئۈچۈن (پەيغەمبەر ئەلەيھىسسالام) تەرىنى تۈردى ۋە يۈزىنى ئۆرىۋالدى[1ـ2].",
      "footnotes": null
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 585,
      "verse_key": "80:3",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا يُدۡرِيكَ لَعَلَّهُۥ يَزَّكَّىٰٓ",
      "translation": " (ئى مۇھەممەد!) نېمە بىلىسەن؟ (بەلكى) ئۇ (سەندىن ئالغان مەرىپەت بىلەن گۇناھلىرىدىن) پاكلىنىشى مۇمكىن[3].",
      "footnotes": null
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 585,
      "verse_key": "80:4",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَوۡ يَذَّكَّرُ فَتَنفَعَهُ ٱلذِّكۡرَىٰٓ",
      "translation": "ياكى ۋەز ـ نەسىھەت ئاڭلاپ ئۇنىڭدىن پايدىلىنىشى مۇمكىن[4]. ",
      "footnotes": null
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 585,
      "verse_key": "80:5",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَمَّا مَنِ ٱسۡتَغۡنَىٰ",
      "translation": "ئۆزىنى (ئىماندىن) بىھاجەت ھېسابلايدىغان ئادەمگە كەلسەك، سەن ئۇنىڭغا يۈزلىنىسەن (يەنى ئۇنىڭغا قۇلاق سېلىپ، تەبلىغ قىلىشقا كۆڭۈل بۆلىسەن)[5ـ6].",
      "footnotes": null
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 585,
      "verse_key": "80:6",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَنتَ لَهُۥ تَصَدَّىٰ",
      "translation": "ئۆزىنى (ئىماندىن) بىھاجەت ھېسابلايدىغان ئادەمگە كەلسەك، سەن ئۇنىڭغا يۈزلىنىسەن (يەنى ئۇنىڭغا قۇلاق سېلىپ، تەبلىغ قىلىشقا كۆڭۈل بۆلىسەن)[5ـ6].",
      "footnotes": null
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 585,
      "verse_key": "80:7",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا عَلَيۡكَ أَلَّا يَزَّكَّىٰ",
      "translation": "ئۇ (كۇفرىدىن، گۇناھلاردىن) پاكلانمىسا، ساڭا نېمە زىيىنىدى[7].",
      "footnotes": null
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 585,
      "verse_key": "80:8",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَمَّا مَن جَآءَكَ يَسۡعَىٰ",
      "translation": "سەندىن مەرىپەت تىلەپ، ئاللاھتىن قورققان ھالدا يۈگۈرۈپ كەلگەن كىشىگە كەلسەك[8ـ9]،",
      "footnotes": null
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 585,
      "verse_key": "80:9",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَهُوَ يَخۡشَىٰ",
      "translation": "سەندىن مەرىپەت تىلەپ، ئاللاھتىن قورققان ھالدا يۈگۈرۈپ كەلگەن كىشىگە كەلسەك[8ـ9]،",
      "footnotes": null
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 585,
      "verse_key": "80:10",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَنتَ عَنۡهُ تَلَهَّىٰ",
      "translation": " سەن ئۇنىڭغا قارىماي قويىسەن[10].",
      "footnotes": null
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 585,
      "verse_key": "80:11",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّآ إِنَّهَا تَذۡكِرَةٞ",
      "translation": "(بۇنىڭدىن كېيىن) ئۇنداق قىلمىغىن. بۇ ئايەتلەر ۋەزدۇر[11].",
      "footnotes": null
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 585,
      "verse_key": "80:12",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَمَن شَآءَ ذَكَرَهُۥ",
      "translation": "خالىغان ئادەم ئۇنىڭدىن ۋەز ـ نەسىھەت ئالىدۇ[12].",
      "footnotes": null
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 585,
      "verse_key": "80:13",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِي صُحُفٖ مُّكَرَّمَةٖ",
      "translation": "ئۇ (يەنى قۇرئان ئايەتلىرى) قىممەتلىك، (قەدرى) يۇقىرى پاك سەھىپىلەرگە يېزىلغاندۇر[13ـ14].",
      "footnotes": null
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 585,
      "verse_key": "80:14",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَّرۡفُوعَةٖ مُّطَهَّرَةِۭ",
      "translation": "ئۇ (يەنى قۇرئان ئايەتلىرى) قىممەتلىك، (قەدرى) يۇقىرى پاك سەھىپىلەرگە يېزىلغاندۇر[13ـ14].",
      "footnotes": null
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 585,
      "verse_key": "80:15",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بِأَيۡدِي سَفَرَةٖ",
      "translation": " (ئۇ) ھۆرمەتلىك ياخشى (پەرىشتىلەردىن بولغان) پۈتۈكچىلەرنىڭ قولىدىدۇر[15ـ16].",
      "footnotes": null
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 585,
      "verse_key": "80:16",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كِرَامِۭ بَرَرَةٖ",
      "translation": " (ئۇ) ھۆرمەتلىك ياخشى (پەرىشتىلەردىن بولغان) پۈتۈكچىلەرنىڭ قولىدىدۇر[15ـ16].",
      "footnotes": null
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 585,
      "verse_key": "80:17",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قُتِلَ ٱلۡإِنسَٰنُ مَآ أَكۡفَرَهُۥ",
      "translation": "ئىنسانغا لەنەت بولسۇنكى، ئۇ ئەجەب كاپىر بولدى![17]",
      "footnotes": null
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 585,
      "verse_key": "80:18",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مِنۡ أَيِّ شَيۡءٍ خَلَقَهُۥ",
      "translation": "ئاللاھ ئۇنى نېمىدىن ياراتتى؟[18]",
      "footnotes": null
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 585,
      "verse_key": "80:19",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مِن نُّطۡفَةٍ خَلَقَهُۥ فَقَدَّرَهُۥ",
      "translation": "ئاللاھ ئۇنى ئابىمەنىيدىن ياراتتى، (ئۇ تولۇق يارىتىلىپ بولغىچە) ئاللاھ ئۇنى بىر قانچە باسقۇچلارغا بۆلۈپ پەيدىن ـ پەي ياراتتى[19].",
      "footnotes": null
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 585,
      "verse_key": "80:20",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ ٱلسَّبِيلَ يَسَّرَهُۥ",
      "translation": "ئاندىن ئۇنىڭغا يولنى ئاسانلاشتۇرۇپ بەردى[20].",
      "footnotes": null
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 585,
      "verse_key": "80:21",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ أَمَاتَهُۥ فَأَقۡبَرَهُۥ",
      "translation": " ئاندىن ئۇنى ۋاپات قىلدۇردى، ئۇنىڭغا (كۆمۈلىدىغان) قەبرىنى بەرپا قىلدى[21].",
      "footnotes": null
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 585,
      "verse_key": "80:22",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ إِذَا شَآءَ أَنشَرَهُۥ",
      "translation": "ئاندىن ئاللاھ ئۇنى خالىغان ۋاقىتتا تىرىلدۈرىدۇ[22]. ",
      "footnotes": null
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 585,
      "verse_key": "80:23",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّا لَمَّا يَقۡضِ مَآ أَمَرَهُۥ",
      "translation": "ھەرگىز ئۇنداق ئەمەس (يەنى بۇ كاپىر ئىنسان تەكەببۇرلۇقىدىن يانسۇن)، ئۇ ئاللاھنىڭ بۇيرۇغانلىرىنى بەجا كەلتۈرمىدى[23].",
      "footnotes": null
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 585,
      "verse_key": "80:24",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلۡيَنظُرِ ٱلۡإِنسَٰنُ إِلَىٰ طَعَامِهِۦٓ",
      "translation": " ئىنسان ئۆزىنىڭ يېمەكلىكلىرىگە (ئىبرەت نەزىرى بىلەن) قارىسۇن[24].",
      "footnotes": null
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 585,
      "verse_key": "80:25",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَنَّا صَبَبۡنَا ٱلۡمَآءَ صَبّٗا",
      "translation": "(بىز بۇلۇتتىن) زور مىقداردا يامغۇر ياغدۇردۇق[25].",
      "footnotes": null
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 585,
      "verse_key": "80:26",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ شَقَقۡنَا ٱلۡأَرۡضَ شَقّٗا",
      "translation": " ئاندىن زېمىننى (ئۇنىڭدىن ئۆسۈملۈكلەرنى چىقىرىش بىلەن) ياردۇق[26].",
      "footnotes": null
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 585,
      "verse_key": "80:27",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَنۢبَتۡنَا فِيهَا حَبّٗا",
      "translation": "سىلەرنىڭ ۋە ھايۋانلىرىڭلارنىڭ مەنپەئەتلىنىشى ئۈچۈن، زېمىندا ئاشلىقلارنى، ئۈزۈمنى، ئوتياشلارنى، زەيتۇننى، خورمىنى، دەرەخلىرى قويۇق باغچىلارنى، تۈرلۈك مېۋىلەرنى، ئوت ـ چۆپلەرنى ئۆستۈردۇق[27ـ32].",
      "footnotes": null
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 585,
      "verse_key": "80:28",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَعِنَبٗا وَقَضۡبٗا",
      "translation": "سىلەرنىڭ ۋە ھايۋانلىرىڭلارنىڭ مەنپەئەتلىنىشى ئۈچۈن، زېمىندا ئاشلىقلارنى، ئۈزۈمنى، ئوتياشلارنى، زەيتۇننى، خورمىنى، دەرەخلىرى قويۇق باغچىلارنى، تۈرلۈك مېۋىلەرنى، ئوت ـ چۆپلەرنى ئۆستۈردۇق[27ـ32].",
      "footnotes": null
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 585,
      "verse_key": "80:29",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَزَيۡتُونٗا وَنَخۡلٗا",
      "translation": "سىلەرنىڭ ۋە ھايۋانلىرىڭلارنىڭ مەنپەئەتلىنىشى ئۈچۈن، زېمىندا ئاشلىقلارنى، ئۈزۈمنى، ئوتياشلارنى، زەيتۇننى، خورمىنى، دەرەخلىرى قويۇق باغچىلارنى، تۈرلۈك مېۋىلەرنى، ئوت ـ چۆپلەرنى ئۆستۈردۇق[27ـ32].",
      "footnotes": null
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 585,
      "verse_key": "80:30",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَحَدَآئِقَ غُلۡبٗا",
      "translation": "سىلەرنىڭ ۋە ھايۋانلىرىڭلارنىڭ مەنپەئەتلىنىشى ئۈچۈن، زېمىندا ئاشلىقلارنى، ئۈزۈمنى، ئوتياشلارنى، زەيتۇننى، خورمىنى، دەرەخلىرى قويۇق باغچىلارنى، تۈرلۈك مېۋىلەرنى، ئوت ـ چۆپلەرنى ئۆستۈردۇق[27ـ32].",
      "footnotes": null
    },
    {
      "id": 31,
      "verse_number": 31,
      "page_number": 585,
      "verse_key": "80:31",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَفَٰكِهَةٗ وَأَبّٗا",
      "translation": "سىلەرنىڭ ۋە ھايۋانلىرىڭلارنىڭ مەنپەئەتلىنىشى ئۈچۈن، زېمىندا ئاشلىقلارنى، ئۈزۈمنى، ئوتياشلارنى، زەيتۇننى، خورمىنى، دەرەخلىرى قويۇق باغچىلارنى، تۈرلۈك مېۋىلەرنى، ئوت ـ چۆپلەرنى ئۆستۈردۇق[27ـ32].",
      "footnotes": null
    },
    {
      "id": 32,
      "verse_number": 32,
      "page_number": 585,
      "verse_key": "80:32",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَّتَٰعٗا لَّكُمۡ وَلِأَنۡعَٰمِكُمۡ",
      "translation": "سىلەرنىڭ ۋە ھايۋانلىرىڭلارنىڭ مەنپەئەتلىنىشى ئۈچۈن، زېمىندا ئاشلىقلارنى، ئۈزۈمنى، ئوتياشلارنى، زەيتۇننى، خورمىنى، دەرەخلىرى قويۇق باغچىلارنى، تۈرلۈك مېۋىلەرنى، ئوت ـ چۆپلەرنى ئۆستۈردۇق[27ـ32].",
      "footnotes": null
    },
    {
      "id": 33,
      "verse_number": 33,
      "page_number": 585,
      "verse_key": "80:33",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِذَا جَآءَتِ ٱلصَّآخَّةُ",
      "translation": "(قىيامەتنىڭ) قۇلاقنى گاس قىلغۇدەك قاتتىق ئاۋازى كەلگەن چاغدا (ھەممە ئادەم ئۆز ھالى بىلەن بولۇپ قالىدۇ)[33]. ",
      "footnotes": null
    },
    {
      "id": 34,
      "verse_number": 34,
      "page_number": 585,
      "verse_key": "80:34",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَوۡمَ يَفِرُّ ٱلۡمَرۡءُ مِنۡ أَخِيهِ",
      "translation": "ئۇ كۈندە كىشى ئۆزىنىڭ قېرىندىشىدىن، ئانىسىدىن، ئاتىسىدىن، خوتۇنىدىن ۋە بالىلىرىدىن قاچىدۇ[34ـ36].",
      "footnotes": null
    },
    {
      "id": 35,
      "verse_number": 35,
      "page_number": 585,
      "verse_key": "80:35",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأُمِّهِۦ وَأَبِيهِ",
      "translation": "ئۇ كۈندە كىشى ئۆزىنىڭ قېرىندىشىدىن، ئانىسىدىن، ئاتىسىدىن، خوتۇنىدىن ۋە بالىلىرىدىن قاچىدۇ[34ـ36].",
      "footnotes": null
    },
    {
      "id": 36,
      "verse_number": 36,
      "page_number": 585,
      "verse_key": "80:36",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَصَٰحِبَتِهِۦ وَبَنِيهِ",
      "translation": "ئۇ كۈندە كىشى ئۆزىنىڭ قېرىندىشىدىن، ئانىسىدىن، ئاتىسىدىن، خوتۇنىدىن ۋە بالىلىرىدىن قاچىدۇ[34ـ36].",
      "footnotes": null
    },
    {
      "id": 37,
      "verse_number": 37,
      "page_number": 585,
      "verse_key": "80:37",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لِكُلِّ ٱمۡرِيٕٖ مِّنۡهُمۡ يَوۡمَئِذٖ شَأۡنٞ يُغۡنِيهِ",
      "translation": " ئۇ كۈندە ھەر ئادەمنىڭ بىر ھالى بولىدۇكى، ئۇنىڭ باشقىلارغا قارىشىغا (شۇ ھالى) يار بەرمەيدۇ[37].",
      "footnotes": null
    },
    {
      "id": 38,
      "verse_number": 38,
      "page_number": 585,
      "verse_key": "80:38",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وُجُوهٞ يَوۡمَئِذٖ مُّسۡفِرَةٞ",
      "translation": "ئۇ كۈندە نۇرغۇن يۈزلەردىن نۇر، كۈلكە ۋە خۇشال ـ خۇراملىق يېغىپ تۇرىدۇ[38ـ39].",
      "footnotes": null
    },
    {
      "id": 39,
      "verse_number": 39,
      "page_number": 585,
      "verse_key": "80:39",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ضَاحِكَةٞ مُّسۡتَبۡشِرَةٞ",
      "translation": "ئۇ كۈندە نۇرغۇن يۈزلەردىن نۇر، كۈلكە ۋە خۇشال ـ خۇراملىق يېغىپ تۇرىدۇ[38ـ39].",
      "footnotes": null
    },
    {
      "id": 40,
      "verse_number": 40,
      "page_number": 585,
      "verse_key": "80:40",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَوُجُوهٞ يَوۡمَئِذٍ عَلَيۡهَا غَبَرَةٞ",
      "translation": " يەنە بۇ كۈندە نۇرغۇن يۈزلەرنى چاڭ ـ توزان بېسىپ كەتكەن، قارىداپ كەتكەن بولىدۇ[40ـ41]،",
      "footnotes": null
    },
    {
      "id": 41,
      "verse_number": 41,
      "page_number": 585,
      "verse_key": "80:41",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تَرۡهَقُهَا قَتَرَةٌ",
      "translation": " يەنە بۇ كۈندە نۇرغۇن يۈزلەرنى چاڭ ـ توزان بېسىپ كەتكەن، قارىداپ كەتكەن بولىدۇ[40ـ41]،",
      "footnotes": null
    },
    {
      "id": 42,
      "verse_number": 42,
      "page_number": 585,
      "verse_key": "80:42",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أُوْلَٰٓئِكَ هُمُ ٱلۡكَفَرَةُ ٱلۡفَجَرَةُ",
      "translation": "بۇلار بولسا كاپىرلاردۇر، فاجىرلاردۇر[42].",
      "footnotes": null
    }
  ]
