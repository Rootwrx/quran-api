{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 582,
      "verse_key": "78:1",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَمَّ يَتَسَآءَلُونَ",
      "translation": "(مۇشرىكلار ئۆزئارا نېمە توغرۇلۇق) سورىشىدۇ؟[1]",
      "footnotes": null
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 582,
      "verse_key": "78:2",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَنِ ٱلنَّبَإِ ٱلۡعَظِيمِ",
      "translation": "ئۇلار چوڭ بىر خەۋەر (يەنى قىيامەت توغرۇلۇق) سورىشىدۇ[2].",
      "footnotes": null
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 582,
      "verse_key": "78:3",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِي هُمۡ فِيهِ مُخۡتَلِفُونَ",
      "translation": "ئۇلار شۇ (يەنى قىيامەت) توغرىسىدا ئىختىلاپ قىلىشقۇچىلاردۇر[3]. ",
      "footnotes": null
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 582,
      "verse_key": "78:4",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّا سَيَعۡلَمُونَ",
      "translation": " ھەرگىز ئۇنداق ئەمەس (قىيامەت ئۇلارنىڭ ئويلىغىنىدەك يالغان ئەمەس)، ئۇلار (ھەقىقىي ئەھۋالنى) كەلگۈسىدە بىلىدۇ[4].",
      "footnotes": null
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 582,
      "verse_key": "78:5",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ كَلَّا سَيَعۡلَمُونَ",
      "translation": "ھەرگىز ئۇنداق ئەمەس، ئۇلار (بېشىغا كەلگەن ئازابنى) كەلگۈسىدە بىلىدۇ[5].",
      "footnotes": null
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 582,
      "verse_key": "78:6",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَمۡ نَجۡعَلِ ٱلۡأَرۡضَ مِهَٰدٗا",
      "translation": "(زېمىننىڭ ئۈستىدە مۇقىملىشىشىڭلار ۋە كەڭ تۈزلەڭلىكلەرگە زىرائەت تېرىپ پايدىلىنىشىڭلار ئۈچۈن) زېمىننى بىسات قىلمىدۇقمۇ؟[6]",
      "footnotes": null
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 582,
      "verse_key": "78:7",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡجِبَالَ أَوۡتَادٗا",
      "translation": " (زېمىننىڭ تەۋرەپ كەتمەسلىكى ئۈچۈن) تاغلارنى قوزۇق قىلمىدۇقمۇ؟[7]",
      "footnotes": null
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 582,
      "verse_key": "78:8",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَخَلَقۡنَٰكُمۡ أَزۡوَٰجٗا",
      "translation": "سىلەرنى ئەر ـ ئايال قىلىپ ياراتتۇق[8].",
      "footnotes": null
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 582,
      "verse_key": "78:9",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَجَعَلۡنَا نَوۡمَكُمۡ سُبَاتٗا",
      "translation": "ئۇيقۇنى سىلەرنىڭ (بەدىنىڭلار ئۈچۈن) راھەت قىلدۇق[9].",
      "footnotes": null
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 582,
      "verse_key": "78:10",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَجَعَلۡنَا ٱلَّيۡلَ لِبَاسٗا",
      "translation": "كېچىنى لىباس قىلدۇق[10].",
      "footnotes": null
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 582,
      "verse_key": "78:11",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَجَعَلۡنَا ٱلنَّهَارَ مَعَاشٗا",
      "translation": "كۈندۈزنى (سىلەر) تىرىكچىلىك قىلىدىغان (ۋاقىت) قىلدۇق[11].",
      "footnotes": null
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 582,
      "verse_key": "78:12",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَبَنَيۡنَا فَوۡقَكُمۡ سَبۡعٗا شِدَادٗا",
      "translation": " ئۈستۈڭلاردا مۇستەھكەم يەتتە ئاسماننى بىنا قىلدۇق[12].",
      "footnotes": null
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 582,
      "verse_key": "78:13",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَجَعَلۡنَا سِرَاجٗا وَهَّاجٗا",
      "translation": " (سىلەر ئۈچۈن) يېنىپ تۇرغان چىراغنى (قۇياشنى) ياراتتۇق[13].",
      "footnotes": null
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 582,
      "verse_key": "78:14",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنزَلۡنَا مِنَ ٱلۡمُعۡصِرَٰتِ مَآءٗ ثَجَّاجٗا",
      "translation": " ئاشلىقلارنى، ئوت ـ چۆپلەرنى، دەرەخلىرى قويۇق باغچىلارنى ئۆستۈرۈش ئۈچۈن بۇلۇتلاردىن مول يامغۇر ياغدۇرۇپ بەردۇق[14ـ16].",
      "footnotes": null
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 582,
      "verse_key": "78:15",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لِّنُخۡرِجَ بِهِۦ حَبّٗا وَنَبَاتٗا",
      "translation": " ئاشلىقلارنى، ئوت ـ چۆپلەرنى، دەرەخلىرى قويۇق باغچىلارنى ئۆستۈرۈش ئۈچۈن بۇلۇتلاردىن مول يامغۇر ياغدۇرۇپ بەردۇق[14ـ16].",
      "footnotes": null
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 582,
      "verse_key": "78:16",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَجَنَّٰتٍ أَلۡفَافًا",
      "translation": " ئاشلىقلارنى، ئوت ـ چۆپلەرنى، دەرەخلىرى قويۇق باغچىلارنى ئۆستۈرۈش ئۈچۈن بۇلۇتلاردىن مول يامغۇر ياغدۇرۇپ بەردۇق[14ـ16].",
      "footnotes": null
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 582,
      "verse_key": "78:17",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ يَوۡمَ ٱلۡفَصۡلِ كَانَ مِيقَٰتٗا",
      "translation": " قىيامەت كۈنى ھەقىقەتەن (مۇكاپاتلاش ۋە جازالاش) ۋاقتىدۇر[17]. ",
      "footnotes": null
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 582,
      "verse_key": "78:18",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَوۡمَ يُنفَخُ فِي ٱلصُّورِ فَتَأۡتُونَ أَفۡوَاجٗا",
      "translation": " ئۇ كۈنى سۇر چېلىنىدۇ، سىلەر توپ ـ توپ بولۇپ كېلىسىلەر[18].",
      "footnotes": null
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 582,
      "verse_key": "78:19",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَفُتِحَتِ ٱلسَّمَآءُ فَكَانَتۡ أَبۡوَٰبٗا",
      "translation": "ئاسمان ئېچىلىپ نۇرغۇن ئىشىكلەر پەيدا بولىدۇ[19].",
      "footnotes": null
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 582,
      "verse_key": "78:20",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَسُيِّرَتِ ٱلۡجِبَالُ فَكَانَتۡ سَرَابًا",
      "translation": " تاغلار ھەرىكەتلەندۈرۈلۈپ سەرابتەك بولۇپ قالىدۇ (يەنى تاغلار توزۇپ يىراقتىن قارىغۇچىغا سۇدەك تۇيۇلىدۇ)[20].",
      "footnotes": null
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 582,
      "verse_key": "78:21",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ جَهَنَّمَ كَانَتۡ مِرۡصَادٗا",
      "translation": " جەھەننەم ھەقىقەتەن (كۇففارلارنى) كۈتۈپ تۇرىدۇ[21].",
      "footnotes": null
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 582,
      "verse_key": "78:22",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لِّلطَّٰغِينَ مَـَٔابٗا",
      "translation": "جەھەننەم، شۈبھىسىزكى، گۇناھكارلارنىڭ جايىدۇر[22].",
      "footnotes": null
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 582,
      "verse_key": "78:23",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَّٰبِثِينَ فِيهَآ أَحۡقَابٗا",
      "translation": "ئۇلار جەھەننەمدە ئۇزاق مۇددەت قالىدۇ[23].",
      "footnotes": null
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 582,
      "verse_key": "78:24",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَّا يَذُوقُونَ فِيهَا بَرۡدٗا وَلَا شَرَابًا",
      "translation": "ئۇلار جەھەننەمدە (جەھەننەمنىڭ ھارارىتىنى يىنىكلىتىلگەن) بىرەر سۆرۈنلۈككە ۋە (تەشنالىقنى قاندۇرىدىغان) بىرەر ئىچىملىككە ئېرىشەلمەيدۇ[24].",
      "footnotes": null
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 582,
      "verse_key": "78:25",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَّا حَمِيمٗا وَغَسَّاقٗا",
      "translation": " ئۇلارنىڭ ئېرىشىدىغىنى پەقەت يۇقىرى ھارارەتلىك قايناقسۇ ۋە يىرىڭدىنلا ئىبارەت بولىدۇ[25]. ",
      "footnotes": null
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 582,
      "verse_key": "78:26",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "جَزَآءٗ وِفَاقًا",
      "translation": "بۇ ئۇلارغا بېرىلىدىغان مۇۋاپىق جازادۇر. [26]",
      "footnotes": null
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 582,
      "verse_key": "78:27",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُمۡ كَانُواْ لَا يَرۡجُونَ حِسَابٗا",
      "translation": " چۈنكى ئۇلار (قىلمىشلىرىدىن) ھېساب ئېلىنىشتىن قورقمايتتى[27]",
      "footnotes": null
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 582,
      "verse_key": "78:28",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَكَذَّبُواْ بِـَٔايَٰتِنَا كِذَّابٗا",
      "translation": "ھەمدە بىزنىڭ ئايەتلىرىمىزنى قاتتىق ئىنكار قىلاتتى[28].",
      "footnotes": null
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 582,
      "verse_key": "78:29",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَكُلَّ شَيۡءٍ أَحۡصَيۡنَٰهُ كِتَٰبٗا",
      "translation": " ھەر نەرسىنى تولۇق خاتىرىلەپ قويغان ئىدۇق[29].",
      "footnotes": null
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 582,
      "verse_key": "78:30",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَذُوقُواْ فَلَن نَّزِيدَكُمۡ إِلَّا عَذَابًا",
      "translation": "(ئۇلارغا ئېيتىمىزكى) «تېتىڭلار! سىلەرگە پەقەت ئازابنىلا زىيادە قىلىمىز»[30].",
      "footnotes": null
    },
    {
      "id": 31,
      "verse_number": 31,
      "page_number": 583,
      "verse_key": "78:31",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ لِلۡمُتَّقِينَ مَفَازًا",
      "translation": "شۈبھىسىزكى، تەقۋادارلار نېمەتكە ئېرىشىدۇ[31].",
      "footnotes": null
    },
    {
      "id": 32,
      "verse_number": 32,
      "page_number": 583,
      "verse_key": "78:32",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "حَدَآئِقَ وَأَعۡنَٰبٗا",
      "translation": "(ئۇ نېمەت) باغلار، ئۈزۈملەردۇر[32].",
      "footnotes": null
    },
    {
      "id": 33,
      "verse_number": 33,
      "page_number": 583,
      "verse_key": "78:33",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَكَوَاعِبَ أَتۡرَابٗا",
      "translation": "تەڭتۇش قىزلاردۇر[33].",
      "footnotes": null
    },
    {
      "id": 34,
      "verse_number": 34,
      "page_number": 583,
      "verse_key": "78:34",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَكَأۡسٗا دِهَاقٗا",
      "translation": "(شاراب بىلەن) تولدۇرۇلغان جاملاردۇر[34].",
      "footnotes": null
    },
    {
      "id": 35,
      "verse_number": 35,
      "page_number": 583,
      "verse_key": "78:35",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَّا يَسۡمَعُونَ فِيهَا لَغۡوٗا وَلَا كِذَّٰبٗا",
      "translation": "ئۇلار جەننەتتە يالغان سۆز، بىھۇدە سۆز ئاڭلىمايدۇ[35].",
      "footnotes": null
    },
    {
      "id": 36,
      "verse_number": 36,
      "page_number": 583,
      "verse_key": "78:36",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "جَزَآءٗ مِّن رَّبِّكَ عَطَآءً حِسَابٗا",
      "translation": " (ئەنە شۇلار) پەرۋەردىگارىڭنىڭ بەرگەن مۇكاپاتى، يېتەرلىك ئاتاسىدۇر[36].",
      "footnotes": null
    },
    {
      "id": 37,
      "verse_number": 37,
      "page_number": 583,
      "verse_key": "78:37",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "رَّبِّ ٱلسَّمَٰوَٰتِ وَٱلۡأَرۡضِ وَمَا بَيۡنَهُمَا ٱلرَّحۡمَٰنِۖ لَا يَمۡلِكُونَ مِنۡهُ خِطَابٗا",
      "translation": " ئۇ ئاسمانلارنىڭ، زېمىننىڭ ۋە ئۇلارنىڭ ئارىسىدىكى شەيئىلەرنىڭ پەرۋەردىگارىدۇر. ناھايىتى شەپقەتلىكتۇر. (ئۇ كۈندە ھېچ كىشى) ئۇنىڭغا سۆز قىلىشقا قادىر بولالمايدۇ[37].",
      "footnotes": null
    },
    {
      "id": 38,
      "verse_number": 38,
      "page_number": 583,
      "verse_key": "78:38",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَوۡمَ يَقُومُ ٱلرُّوحُ وَٱلۡمَلَٰٓئِكَةُ صَفّٗاۖ لَّا يَتَكَلَّمُونَ إِلَّا مَنۡ أَذِنَ لَهُ ٱلرَّحۡمَٰنُ وَقَالَ صَوَابٗا",
      "translation": "جىبرىئىل ۋە پەرىشتىلەر سەپ بولۇپ تۇرۇپ كېتىدىغان كۈندە (يەنى قىيامەتتە)، ئۇلار(نىڭ ئارىسىدا) مېھرىبان ئاللاھنىڭ ئىزنىگە ئېرىشكەن ۋە توغرا سۆزنى قىلغانلاردىن باشقا ھېچ ئەھەدىنىڭ سۆز قىلىشىغا بولمايدۇ[38].",
      "footnotes": null
    },
    {
      "id": 39,
      "verse_number": 39,
      "page_number": 583,
      "verse_key": "78:39",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ذَٰلِكَ ٱلۡيَوۡمُ ٱلۡحَقُّۖ فَمَن شَآءَ ٱتَّخَذَ إِلَىٰ رَبِّهِۦ مَـَٔابًا",
      "translation": " ئەنە شۇ چوقۇم بولىدىغان كۈندۇر. كىمكى (ئىمان بىلەن، ياخشى ئەمەل بىلەن) پەرۋەردىگارىغا قايتىدىغان يولنى تۇتۇشنى خالىسا (تۇتسۇن)[39].",
      "footnotes": null
    },
    {
      "id": 40,
      "verse_number": 40,
      "page_number": 583,
      "verse_key": "78:40",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّآ أَنذَرۡنَٰكُمۡ عَذَابٗا قَرِيبٗا يَوۡمَ يَنظُرُ ٱلۡمَرۡءُ مَا قَدَّمَتۡ يَدَاهُ وَيَقُولُ ٱلۡكَافِرُ يَٰلَيۡتَنِي كُنتُ تُرَٰبَۢا",
      "translation": " بىز سىلەرنى ھەقىقەتەن (يۈز بېرىشى) يېقىن ئازابتىن (يەنى ئاخىرەت ئازابىدىن) ئاگاھلاندۇردۇق، ئۇ كۈندە كىشى ئىلگىرى قىلغان ئەمەللىرىنى كۆرىدۇ. كاپىر: «كاشكى مەن تۇپراق بولۇپ كەتكەن بولسامچۇ!» دەيدۇ[40].",
      "footnotes": null
    }
  ]
