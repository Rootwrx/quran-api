{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 596,
      "verse_key": "94:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَمۡ نَشۡرَحۡ لَكَ صَدۡرَكَ",
      "translation": "(ئى مۇھەممەد!) سېنىڭ كۆكسىڭنى (ھىدايەت، ھېكمەت ۋە قۇرئاننىڭ نۇرى بىلەن) كەڭ قىلمىدۇقمۇ؟[1] ",
      "footnotes": null
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 596,
      "verse_key": "94:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَوَضَعۡنَا عَنكَ وِزۡرَكَ",
      "translation": "سېنىڭ بېلىڭنى ئېگىۋەتكەن ئېغىر يۈكنى ئۈستۈڭدىن ئېلىپ تاشلىدۇق[2ـ3].",
      "footnotes": null
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 596,
      "verse_key": "94:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِيٓ أَنقَضَ ظَهۡرَكَ",
      "translation": "سېنىڭ بېلىڭنى ئېگىۋەتكەن ئېغىر يۈكنى ئۈستۈڭدىن ئېلىپ تاشلىدۇق[2ـ3].",
      "footnotes": null
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 596,
      "verse_key": "94:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَرَفَعۡنَا لَكَ ذِكۡرَكَ",
      "translation": "سېنىڭ شۆھرىتىڭنى ئۈستۈن قىلدۇق[4].",
      "footnotes": null
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 596,
      "verse_key": "94:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِنَّ مَعَ ٱلۡعُسۡرِ يُسۡرًا",
      "translation": "ئەلۋەتتە ھەر بىر مۈشكۈللۈك بىلەن بىر ئاسانلىق بار[5].",
      "footnotes": null
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 596,
      "verse_key": "94:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ مَعَ ٱلۡعُسۡرِ يُسۡرٗا",
      "translation": " ئەلۋەتتە شۇ مۈشكۈللۈك بىلەن يەنە بىر ئاسانلىق بار[6].",
      "footnotes": null
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 596,
      "verse_key": "94:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِذَا فَرَغۡتَ فَٱنصَبۡ",
      "translation": "(ئى مۇھەممەد! دەۋەتتىن) بوشىغان چېغىڭدا (ئاللاھقا) ئىبادەت قىلىشقا تىرىشقىن[7].",
      "footnotes": null
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 596,
      "verse_key": "94:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِلَىٰ رَبِّكَ فَٱرۡغَب",
      "translation": "پەرۋەردىگارىڭغا يۈزلەنگىن[8].",
      "footnotes": null
    }
  ]
}