{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 580,
      "verse_key": "77:1",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡمُرۡسَلَٰتِ عُرۡفٗا",
      "translation": "ئارقىمۇئارقا چىققۇچى (لەرزان) شاماللار بىلەن قەسەمكى[1]،",
      "footnotes": null
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 580,
      "verse_key": "77:2",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَٱلۡعَٰصِفَٰتِ عَصۡفٗا",
      "translation": "قاتتىق چىققۇچى بورانلار بىلەن قەسەمكى[2]،",
      "footnotes": null
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 580,
      "verse_key": "77:3",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلنَّٰشِرَٰتِ نَشۡرٗا",
      "translation": " (بۇلۇتلارنى ئاللاھ خالىغان تەرەپلەرگە) تارقاتقۇچى شاماللار بىلەن قەسەمكى[3]،",
      "footnotes": null
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 580,
      "verse_key": "77:4",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَٱلۡفَٰرِقَٰتِ فَرۡقٗا",
      "translation": " (ھەق بىلەن باتىلنىڭ، ھارام بىلەن ھالالنىڭ) ئارىسىنى ئايرىغۇچى پەرىشتىلەر بىلەن قەسەمكى[4]، ",
      "footnotes": null
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 580,
      "verse_key": "77:5",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَٱلۡمُلۡقِيَٰتِ ذِكۡرًا",
      "translation": " (بەندىلەرگە) ئۆزرە (قالدۇرماسلىق) ياكى (ئاللاھنىڭ ئازابىدىن) ئاگاھلاندۇرۇش ئۈچۈن ۋەھىينى (پەيغەمبەرلەرگە) ئېلىپ چۈشكۈچى پەرىشتىلەر بىلەن قەسەمكى[5ـ6]، ",
      "footnotes": null
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 580,
      "verse_key": "77:6",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عُذۡرًا أَوۡ نُذۡرًا",
      "translation": " (بەندىلەرگە) ئۆزرە (قالدۇرماسلىق) ياكى (ئاللاھنىڭ ئازابىدىن) ئاگاھلاندۇرۇش ئۈچۈن ۋەھىينى (پەيغەمبەرلەرگە) ئېلىپ چۈشكۈچى پەرىشتىلەر بىلەن قەسەمكى[5ـ6]، ",
      "footnotes": null
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 580,
      "verse_key": "77:7",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّمَا تُوعَدُونَ لَوَٰقِعٞ",
      "translation": "سىلەر ئاگاھلاندۇرۇلغان ئىش (يەنى قىيامەت، ھېساب ۋە جازا) چوقۇم مەيدانغا كەلگۈچىدۇر[7].",
      "footnotes": null
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 580,
      "verse_key": "77:8",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِذَا ٱلنُّجُومُ طُمِسَتۡ",
      "translation": "يۇلتۇزلارنىڭ (نۇرى) ئۆچۈرۈلگەن چاغدا[8].",
      "footnotes": null
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 580,
      "verse_key": "77:9",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلسَّمَآءُ فُرِجَتۡ",
      "translation": " ئاسمان يېرىلغان چاغدا[9]،",
      "footnotes": null
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 580,
      "verse_key": "77:10",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡجِبَالُ نُسِفَتۡ",
      "translation": " تاغلار تىتىلىپ (شامالدا ئۇچۇپ يۈرگەن زەررىلەردەك) ئۇچۇپ يۈرگەن چاغدا[10]،",
      "footnotes": null
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 580,
      "verse_key": "77:11",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلرُّسُلُ أُقِّتَتۡ",
      "translation": "پەيغەمبەرلەرگە (ئۇلار بىلەن ئۈممەتلىرى ئارىسىدا ھۆكۈم چىقىرىلىشقا) ۋاقىت بەلگىلەنگەن چاغدا (ۋەدە قىلىنغان قىيامەت مەيدانغا كېلىدۇ)[11].",
      "footnotes": null
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 580,
      "verse_key": "77:12",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لِأَيِّ يَوۡمٍ أُجِّلَتۡ",
      "translation": "(پەيغەمبەرلەرنىڭ قەۋملىرىگە ئائىت ئىشلار) قايسى بۈيۈك كۈنگە كېچىكتۈرۈلدى؟[12]",
      "footnotes": null
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 580,
      "verse_key": "77:13",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لِيَوۡمِ ٱلۡفَصۡلِ",
      "translation": "خالايىقنىڭ ئارىسىدا ھۆكۈم چىقىرىلىدىغان كۈن ئۈچۈن (تەخىر قىلىندى)[13]. ",
      "footnotes": null
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 580,
      "verse_key": "77:14",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَآ أَدۡرَىٰكَ مَا يَوۡمُ ٱلۡفَصۡلِ",
      "translation": "ھۆكۈم چىقىرىلىدىغان كۈننىڭ نېمىلىكىنى (يەنى ئۇنىڭ دەھشىتىنى) قانداق بىلەلەيسەن؟[14]",
      "footnotes": null
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 580,
      "verse_key": "77:15",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ",
      "translation": " بۇ كۈندە (ئۇ كۈننى) ئىنكار قىلغۇچىلارغا ۋاي![15] ",
      "footnotes": null
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 580,
      "verse_key": "77:16",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَمۡ نُهۡلِكِ ٱلۡأَوَّلِينَ",
      "translation": "(پەيغەمبەرلىرىنى ئىنكار قىلغانلىقلىرى ئۈچۈن) ئىلگىرىكىلەرنى ھالاك قىلمىدۇقمۇ؟[16]",
      "footnotes": null
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 580,
      "verse_key": "77:17",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ نُتۡبِعُهُمُ ٱلۡأٓخِرِينَ",
      "translation": " ئاندىن ئۇلارغا ئەگەشتۈرۈپ كېيىنكىلەرنى ھالاك قىلدۇق[17].",
      "footnotes": null
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 580,
      "verse_key": "77:18",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَذَٰلِكَ نَفۡعَلُ بِٱلۡمُجۡرِمِينَ",
      "translation": " گۇناھكارلارنى مۇشۇنداق قىلىمىز (يەنى جازالايمىز)[18].",
      "footnotes": null
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 580,
      "verse_key": "77:19",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ",
      "translation": " بۇ كۈندە (ئۇ كۈننى) ئىنكار قىلغۇچىلارغا ۋاي![19] ",
      "footnotes": null
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 581,
      "verse_key": "77:20",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَمۡ نَخۡلُقكُّم مِّن مَّآءٖ مَّهِينٖ",
      "translation": "سىلەرنى بىز ئاجىز مەنىدىن ياراتمىدۇقمۇ؟[20]",
      "footnotes": null
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 581,
      "verse_key": "77:21",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَجَعَلۡنَٰهُ فِي قَرَارٖ مَّكِينٍ",
      "translation": " ئۇنى بىز مەلۇم ۋاقىتقىچە (يەنى تۇغۇلغىچە) پۇختا ئارامگاھ (يەنى بەچچىدان) دا قىلدۇق[21ـ22].",
      "footnotes": null
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 581,
      "verse_key": "77:22",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَىٰ قَدَرٖ مَّعۡلُومٖ",
      "translation": " ئۇنى بىز مەلۇم ۋاقىتقىچە (يەنى تۇغۇلغىچە) پۇختا ئارامگاھ (يەنى بەچچىدان) دا قىلدۇق[21ـ22].",
      "footnotes": null
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 581,
      "verse_key": "77:23",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَقَدَرۡنَا فَنِعۡمَ ٱلۡقَٰدِرُونَ",
      "translation": "بىز (ئۇنى ئابىمەنىيدىن يارىتىشقا) قادىر بولدۇق، بىز (ئۇنى ئەڭ چىرايلىق شەكىلدە يارىتىشقا) نېمىدېگەن ياخشى قادىر بولغۇچىمىز![23]",
      "footnotes": null
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 581,
      "verse_key": "77:24",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ",
      "translation": "بۇ كۈندە (ئۇ كۈننى) ئىنكار قىلغۇچىلارغا ۋاي![24]",
      "footnotes": null
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 581,
      "verse_key": "77:25",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَمۡ نَجۡعَلِ ٱلۡأَرۡضَ كِفَاتًا",
      "translation": " زېمىننى بىز ئۆز ئىچىگە ئالىدىغان قىلمىدۇقمۇ؟[25] ",
      "footnotes": null
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 581,
      "verse_key": "77:26",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَحۡيَآءٗ وَأَمۡوَٰتٗا",
      "translation": "(يەنى) تىرىكلەرنى ۋە ئۆلۈكلەرنى (ئۆز ئىچىگە ئالىدىغان قىلمىدۇقمۇ؟)[26] ",
      "footnotes": null
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 581,
      "verse_key": "77:27",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَجَعَلۡنَا فِيهَا رَوَٰسِيَ شَٰمِخَٰتٖ وَأَسۡقَيۡنَٰكُم مَّآءٗ فُرَاتٗا",
      "translation": " بىز زېمىنغا ئېگىز تاغلارنى ئورناتتۇق، سىلەرنى تاتلىق سۇ بىلەن سۇغاردۇق[27].",
      "footnotes": null
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 581,
      "verse_key": "77:28",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ",
      "translation": " بۇ كۈندە (ئۇ كۈننى) ئىنكار قىلغۇچىلارغا ۋاي![28]",
      "footnotes": null
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 581,
      "verse_key": "77:29",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱنطَلِقُوٓاْ إِلَىٰ مَا كُنتُم بِهِۦ تُكَذِّبُونَ",
      "translation": "(ئۇلارغا دوزاخ مۇئەككەللىرى): «سىلەر (دۇنيادىكى چېغىڭلاردا) ئىنكار قىلغان (دوزاخ) ئازابىغا بېرىڭلار، [29]",
      "footnotes": null
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 581,
      "verse_key": "77:30",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱنطَلِقُوٓاْ إِلَىٰ ظِلّٖ ذِي ثَلَٰثِ شُعَبٖ",
      "translation": "(ئۇلارغا دوزاخ مۇئەككەلىرى ئۈچ شاخلىق سايىگە بېرىڭلار» (دەيدۇ)[30].",
      "footnotes": null
    },
    {
      "id": 31,
      "verse_number": 31,
      "page_number": 581,
      "verse_key": "77:31",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَّا ظَلِيلٖ وَلَا يُغۡنِي مِنَ ٱللَّهَبِ",
      "translation": "ئۇ سايە (جەھەننەمنىڭ تۈتۈنى بولۇپ كىشىنى) سەگىدەتمەيدۇ، (جەھەننەمنىڭ) يالقۇنىنىمۇ توسمايدۇ[31].",
      "footnotes": null
    },
    {
      "id": 32,
      "verse_number": 32,
      "page_number": 581,
      "verse_key": "77:32",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهَا تَرۡمِي بِشَرَرٖ كَٱلۡقَصۡرِ",
      "translation": "ئۇ يالقۇن قەسىردەك (چوڭ) ئۇچقۇنلارنى چىقىرىدۇ[32].",
      "footnotes": null
    },
    {
      "id": 33,
      "verse_number": 33,
      "page_number": 581,
      "verse_key": "77:33",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَأَنَّهُۥ جِمَٰلَتٞ صُفۡرٞ",
      "translation": " ئۇ ئۇچقۇنلار قارا ـ سېرىق تۆگىلەرگە ئوخشايدۇ[33].",
      "footnotes": null
    },
    {
      "id": 34,
      "verse_number": 34,
      "page_number": 581,
      "verse_key": "77:34",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ",
      "translation": " بۇ كۈندە (ئۇ كۈننى) ئىنكار قىلغۇچىلارغا ۋاي![34]",
      "footnotes": null
    },
    {
      "id": 35,
      "verse_number": 35,
      "page_number": 581,
      "verse_key": "77:35",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "هَٰذَا يَوۡمُ لَا يَنطِقُونَ",
      "translation": " بۇ كۈندە ئۇلار سۆز قىلالمايدۇ[35].",
      "footnotes": null
    },
    {
      "id": 36,
      "verse_number": 36,
      "page_number": 581,
      "verse_key": "77:36",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَا يُؤۡذَنُ لَهُمۡ فَيَعۡتَذِرُونَ",
      "translation": " ئۇلارنىڭ ئۆزرە ئېيتىشىغا ئىزنى بېرىلمەيدۇ[36].",
      "footnotes": null
    },
    {
      "id": 37,
      "verse_number": 37,
      "page_number": 581,
      "verse_key": "77:37",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ",
      "translation": "بۇ كۈندە (ئۇ كۈننى) ئىنكار قىلغۇچىلارغا ۋاي![37]",
      "footnotes": null
    },
    {
      "id": 38,
      "verse_number": 38,
      "page_number": 581,
      "verse_key": "77:38",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "هَٰذَا يَوۡمُ ٱلۡفَصۡلِۖ جَمَعۡنَٰكُمۡ وَٱلۡأَوَّلِينَ",
      "translation": "بۇ (خالايىقنىڭ ئارىسىدا) ھۆكۈم چىقىرىلىدىغان كۈندۇر، (ئۇ كۈندە) سىلەرنى ۋە ئاۋۋالقىلارنى (يەنى ئۆتكەنكى ئۈممەتلەرنى) (ئاراڭلاردا ھۆكۈم چىقىرىش ئۈچۈن) يىغىمىز[38].",
      "footnotes": null
    },
    {
      "id": 39,
      "verse_number": 39,
      "page_number": 581,
      "verse_key": "77:39",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِن كَانَ لَكُمۡ كَيۡدٞ فَكِيدُونِ",
      "translation": " ئەگەر (ئازابتىن قۇتۇلۇشقا) بىرەر چارەڭلار بولسا، چارە قىلىپ بېقىڭلار[39].",
      "footnotes": null
    },
    {
      "id": 40,
      "verse_number": 40,
      "page_number": 581,
      "verse_key": "77:40",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ",
      "translation": " بۇ كۈندە (ئۇ كۈننى) ئىنكار قىلغۇچىلارغا ۋاي![40]",
      "footnotes": null
    },
    {
      "id": 41,
      "verse_number": 41,
      "page_number": 581,
      "verse_key": "77:41",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ ٱلۡمُتَّقِينَ فِي ظِلَٰلٖ وَعُيُونٖ",
      "translation": "شۈبھىسىزكى، تەقۋادارلار سايىلەردىن، بۇلاقلاردىن ۋە كۆڭلى تارتقان مېۋىلەردىن بەھرىمەن بولىدۇ[41ـ42].",
      "footnotes": null
    },
    {
      "id": 42,
      "verse_number": 42,
      "page_number": 581,
      "verse_key": "77:42",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَفَوَٰكِهَ مِمَّا يَشۡتَهُونَ",
      "translation": "شۈبھىسىزكى، تەقۋادارلار سايىلەردىن، بۇلاقلاردىن ۋە كۆڭلى تارتقان مېۋىلەردىن بەھرىمەن بولىدۇ[41ـ42].",
      "footnotes": null
    },
    {
      "id": 43,
      "verse_number": 43,
      "page_number": 581,
      "verse_key": "77:43",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كُلُواْ وَٱشۡرَبُواْ هَنِيٓـَٔۢا بِمَا كُنتُمۡ تَعۡمَلُونَ",
      "translation": "(ئۇلارغا) «دۇنيادا قىلغان (ياخشى) ئەمەللىرىڭلار ئۈچۈن خۇشال ـ خۇرام يەڭلار ۋە ئىچىڭلار» (دېيىلىدۇ)[43].",
      "footnotes": null
    },
    {
      "id": 44,
      "verse_number": 44,
      "page_number": 581,
      "verse_key": "77:44",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّا كَذَٰلِكَ نَجۡزِي ٱلۡمُحۡسِنِينَ",
      "translation": " شۈبھىسىزكى، بىز ياخشى ئىش قىلغۇچىلارنى مۇشۇنداق مۇكاپاتلايمىز[44].",
      "footnotes": null
    },
    {
      "id": 45,
      "verse_number": 45,
      "page_number": 581,
      "verse_key": "77:45",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ",
      "translation": " بۇ كۈندە (ئۇ كۈننى) ئىنكار قىلغۇچىلارغا ۋاي![45] ",
      "footnotes": null
    },
    {
      "id": 46,
      "verse_number": 46,
      "page_number": 581,
      "verse_key": "77:46",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كُلُواْ وَتَمَتَّعُواْ قَلِيلًا إِنَّكُم مُّجۡرِمُونَ",
      "translation": "(كۇففارلارغا) «(دۇنيانىڭ لەززەتلىرىدىن) ۋاقىتلىق يەڭلار ۋە بەھرىمەن بولۇڭلار، شۈبھىسىزكى، سىلەر گۇناھكارسىلەر» (دېيىلىدۇ)[46].",
      "footnotes": null
    },
    {
      "id": 47,
      "verse_number": 47,
      "page_number": 581,
      "verse_key": "77:47",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ",
      "translation": " بۇ كۈندە (ئۇ كۈننى) ئىنكار قىلغۇچىلارغا ۋاي![47]",
      "footnotes": null
    },
    {
      "id": 48,
      "verse_number": 48,
      "page_number": 581,
      "verse_key": "77:48",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا قِيلَ لَهُمُ ٱرۡكَعُواْ لَا يَرۡكَعُونَ",
      "translation": " ئۇلارغا: «رۇكۇ قىلىڭلار» دېيىلسە، ئۇلار رۇكۇ قىلمايدۇ[48].",
      "footnotes": null
    },
    {
      "id": 49,
      "verse_number": 49,
      "page_number": 581,
      "verse_key": "77:49",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ",
      "translation": " بۇ كۈندە (ئۇ كۈننى) ئىنكار قىلغۇچىلارغا ۋاي![49] ",
      "footnotes": null
    },
    {
      "id": 50,
      "verse_number": 50,
      "page_number": 581,
      "verse_key": "77:50",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ حَدِيثِۭ بَعۡدَهُۥ يُؤۡمِنُونَ",
      "translation": " ئۇلار (شانلىق دەلىللەرنى ئۆز ئىچىگە ئالغان، پاساھەت ـ بالاغەتتە ئەڭ يۇقىرى بولغان بۇ قۇرئانغا ئىشەنمىسە) باشقا قايسى سۆزگە ئىشىنىدۇ؟[50] ",
      "footnotes": null
    }
  ]
}