{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 566,
      "verse_key": "69:1",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلۡحَآقَّةُ",
      "translation": "(بولۇشى ئېنىق) قىيامەت[1]. ",
      "footnotes": null
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 566,
      "verse_key": "69:2",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَا ٱلۡحَآقَّةُ",
      "translation": " قىيامەت نېمىدۇر؟[2] ",
      "footnotes": null
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 566,
      "verse_key": "69:3",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَآ أَدۡرَىٰكَ مَا ٱلۡحَآقَّةُ",
      "translation": "قىيامەتنىڭ نېمە ئىكەنلىكىنى قانداق بىلەلەيسەن؟[3]",
      "footnotes": null
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 566,
      "verse_key": "69:4",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَذَّبَتۡ ثَمُودُ وَعَادُۢ بِٱلۡقَارِعَةِ",
      "translation": " سەمۇد ۋە ئاد (قەبىلىلىرى) قىيامەتنى ئىنكار قىلدى[4].",
      "footnotes": null
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 566,
      "verse_key": "69:5",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَمَّا ثَمُودُ فَأُهۡلِكُواْ بِٱلطَّاغِيَةِ",
      "translation": "سەمۇد بولسا ئادەتتىن تاشقىرى قاتتىق ئاۋاز بىلەن ھالاك قىلىندى[5].",
      "footnotes": null
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 566,
      "verse_key": "69:6",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَمَّا عَادٞ فَأُهۡلِكُواْ بِرِيحٖ صَرۡصَرٍ عَاتِيَةٖ",
      "translation": " ئاد بولسا ھەددىدىن زىيادە قاتتىق ۋە سوغۇق بوران بىلەن ھالاك قىلىندى[6].",
      "footnotes": null
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 566,
      "verse_key": "69:7",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "سَخَّرَهَا عَلَيۡهِمۡ سَبۡعَ لَيَالٖ وَثَمَٰنِيَةَ أَيَّامٍ حُسُومٗاۖ فَتَرَى ٱلۡقَوۡمَ فِيهَا صَرۡعَىٰ كَأَنَّهُمۡ أَعۡجَازُ نَخۡلٍ خَاوِيَةٖ",
      "translation": "ئاللاھ ئۇلارغا بوراننى يەتتە كېچە، سەككىز كۈندۈز ئۈزۈلدۈرمەي ئاپىرىدە قىلدى. (ئاد) قەۋمىنىڭ (ئۆز ماكانلىرىدا ھالاك بولۇپ) خورما دەرىخىنىڭ پور كۆتەكلىرىدەك ياتقانلىقىنى كۆرۈسەن[7].",
      "footnotes": null
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 566,
      "verse_key": "69:8",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَهَلۡ تَرَىٰ لَهُم مِّنۢ بَاقِيَةٖ",
      "translation": " ئۇلاردىن بىرەر ئادەمنىڭ قالغانلىقىنى كۆرەلەمسەن؟[8]",
      "footnotes": null
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 567,
      "verse_key": "69:9",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَجَآءَ فِرۡعَوۡنُ وَمَن قَبۡلَهُۥ وَٱلۡمُؤۡتَفِكَٰتُ بِٱلۡخَاطِئَةِ",
      "translation": "پىرئەۋن ھەم ئۇنىڭدىن ئىلگىرىكىلەر ۋە شەھەرلىرى دۈم كۆمتۈرۈۋېتىلگەنلەر (يەنى لۇت قەۋمى) خاتا ئىشلارنى قىلدى[9].",
      "footnotes": null
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 567,
      "verse_key": "69:10",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَعَصَوۡاْ رَسُولَ رَبِّهِمۡ فَأَخَذَهُمۡ أَخۡذَةٗ رَّابِيَةً",
      "translation": "ئۇلار پەرۋەردىگارىنىڭ پەيغەمبىرىگە ئاسىيلىق قىلدى. شۇنىڭ بىلەن ئۇلارنى ئاللاھ قاتتىق جازالىدى[10].",
      "footnotes": null
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 567,
      "verse_key": "69:11",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّا لَمَّا طَغَا ٱلۡمَآءُ حَمَلۡنَٰكُمۡ فِي ٱلۡجَارِيَةِ",
      "translation": " شۈبھىسىزكى، (نۇھنىڭ زامانىدا) چوڭ سۇ يامراپ كەتكەن چاغدا، سىلەرنى (يەنى ئەجدادىڭلارنى) بىز كېمىگە سالدۇق[11]",
      "footnotes": null
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 567,
      "verse_key": "69:12",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لِنَجۡعَلَهَا لَكُمۡ تَذۡكِرَةٗ وَتَعِيَهَآ أُذُنٞ وَٰعِيَةٞ",
      "translation": "  سىلەرگە ئۇنى ئىبرەت قىلىش ئۈچۈن، ئېسىدە تۇتۇۋالىدىغان قۇلاقلارنىڭ ئۇنى تۇتۇۋېلىشى ئۈچۈن (بىز شۇنداق قىلدۇق)[ـ12].",
      "footnotes": null
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 567,
      "verse_key": "69:13",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِذَا نُفِخَ فِي ٱلصُّورِ نَفۡخَةٞ وَٰحِدَةٞ",
      "translation": "بىرىنچى قېتىملىق سۇر چېلىنغان چاغدا[13]،",
      "footnotes": null
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 567,
      "verse_key": "69:14",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَحُمِلَتِ ٱلۡأَرۡضُ وَٱلۡجِبَالُ فَدُكَّتَا دَكَّةٗ وَٰحِدَةٗ",
      "translation": "زېمىن ۋە تاغلار (ئورۇنلىرىدىن) كۆتىرىلىپ (بىر ـ بىرىگە) ئۇرۇلۇپ (ئۇۋىلىپ كەتكەن) چاغدا[14]، ",
      "footnotes": null
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 567,
      "verse_key": "69:15",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَيَوۡمَئِذٖ وَقَعَتِ ٱلۡوَاقِعَةُ",
      "translation": "بۇ كۈندە چوڭ ئىش (يەنى قىيامەت) يۈز بېرىدۇ[15].",
      "footnotes": null
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 567,
      "verse_key": "69:16",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱنشَقَّتِ ٱلسَّمَآءُ فَهِيَ يَوۡمَئِذٖ وَاهِيَةٞ",
      "translation": " ئاسمان يېرىلىدۇ، بۇ كۈندە ئاسمان (قالايمىقانلىشىپ) ئاجىزلىشىپ كېتىدۇ[16].",
      "footnotes": null
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 567,
      "verse_key": "69:17",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡمَلَكُ عَلَىٰٓ أَرۡجَآئِهَاۚ وَيَحۡمِلُ عَرۡشَ رَبِّكَ فَوۡقَهُمۡ يَوۡمَئِذٖ ثَمَٰنِيَةٞ",
      "translation": " پەرىشتىلەر ئاسماننىڭ ئەتراپىدا تۇرىدۇ. بۇ كۈندە پەرۋەردىگارىڭنىڭ ئەرشىنى سەككىز پەرىشتە (باشلىرى) ئۈستىدە كۆتۈرۈپ تۇرىدۇ[17].",
      "footnotes": null
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 567,
      "verse_key": "69:18",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَوۡمَئِذٖ تُعۡرَضُونَ لَا تَخۡفَىٰ مِنكُمۡ خَافِيَةٞ",
      "translation": "بۇ كۈندە سىلەر (ئاللاھقا) (ھېساب بېرىش ئۈچۈن) توغرىلىنىسىلەر، سىلەر ھېچقانداق بىر سىرنى يوشۇرۇپ قالالمايسىلەر[18].",
      "footnotes": null
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 567,
      "verse_key": "69:19",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَمَّا مَنۡ أُوتِيَ كِتَٰبَهُۥ بِيَمِينِهِۦ فَيَقُولُ هَآؤُمُ ٱقۡرَءُواْ كِتَٰبِيَهۡ",
      "translation": "نامە ـ ئەمالى ئوڭ قولىغا بېرىلگەن ئادەم (خۇشاللىقتىن): «بۇ مېنىڭ نامە ـ ئەمالىمنى ئېلىپ ئوقۇپ بېقىڭلار، مەن ھېساباتىمغا مۇلاقات بولىدىغانلىقىمغا مۇقەررەر ئىشەنگەن ئىدىم» دەيدۇ[19ـ20].",
      "footnotes": null
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 567,
      "verse_key": "69:20",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنِّي ظَنَنتُ أَنِّي مُلَٰقٍ حِسَابِيَهۡ",
      "translation": "نامە ـ ئەمالى ئوڭ قولىغا بېرىلگەن ئادەم (خۇشاللىقتىن): «بۇ مېنىڭ نامە ـ ئەمالىمنى ئېلىپ ئوقۇپ بېقىڭلار، مەن ھېساباتىمغا مۇلاقات بولىدىغانلىقىمغا مۇقەررەر ئىشەنگەن ئىدىم» دەيدۇ[19ـ20].",
      "footnotes": null
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 567,
      "verse_key": "69:21",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَهُوَ فِي عِيشَةٖ رَّاضِيَةٖ",
      "translation": "ئۇ كۆڭۈللۈك تۇرمۇشتا بولىدۇ[21].",
      "footnotes": null
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 567,
      "verse_key": "69:22",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِي جَنَّةٍ عَالِيَةٖ",
      "translation": " ئېسىل جەننەتتە بولىدۇ[22].",
      "footnotes": null
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 567,
      "verse_key": "69:23",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قُطُوفُهَا دَانِيَةٞ",
      "translation": "ئۇنىڭ مېۋىلىرى (ئۆرە تۇرغاندىمۇ، ئولتۇرغاندىمۇ، ياتقاندىمۇ قولىنى سوزۇپلا ئالالايدىغان دەرىجىدە) يېقىن بولىدۇ[23].",
      "footnotes": null
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 567,
      "verse_key": "69:24",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كُلُواْ وَٱشۡرَبُواْ هَنِيٓـَٔۢا بِمَآ أَسۡلَفۡتُمۡ فِي ٱلۡأَيَّامِ ٱلۡخَالِيَةِ",
      "translation": " (ئۇلارغا) «ئۆتكەنكى كۈنلەردە (يەنى دۇنيادىكى چاغلاردا) ياخشى ئەمەللەرنى قىلغانلىقىڭلار ئۈچۈن، خۇشال ـ خۇرام يەڭلار، ئىچىڭلار» دېيىلىدۇ[24].",
      "footnotes": null
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 567,
      "verse_key": "69:25",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَمَّا مَنۡ أُوتِيَ كِتَٰبَهُۥ بِشِمَالِهِۦ فَيَقُولُ يَٰلَيۡتَنِي لَمۡ أُوتَ كِتَٰبِيَهۡ",
      "translation": " نامە ـ ئەمالى سول قولىغا بېرىلگەن ئادەم ئېيتىدۇكى، «ماڭا نامە ـ ئەمالىم بېرىلمىگەن بولسىچۇ![25] ",
      "footnotes": null
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 567,
      "verse_key": "69:26",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَمۡ أَدۡرِ مَا حِسَابِيَهۡ",
      "translation": " ھېسابىمنىڭ نېمە بولغانلىقىنى بىلمىسەم ئىدىم![26] ",
      "footnotes": null
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 567,
      "verse_key": "69:27",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَٰلَيۡتَهَا كَانَتِ ٱلۡقَاضِيَةَ",
      "translation": "دۇنيادا ئۆلۈمىم ھاياتىمنى (ئاخىرەتتە قايتا تىرىلمەس قىلىپ) ئاخىرلاشتۇرسىچۇ![27]",
      "footnotes": null
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 567,
      "verse_key": "69:28",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَآ أَغۡنَىٰ عَنِّي مَالِيَهۡۜ",
      "translation": "مېنىڭ مال ـ مۈلكۈم ماڭا ھېچ نەرسىگە ئەسقاتمىدى[28].",
      "footnotes": null
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 567,
      "verse_key": "69:29",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "هَلَكَ عَنِّي سُلۡطَٰنِيَهۡ",
      "translation": "مېنىڭ ھوقۇقۇم قولۇمدىن كەتتى»[29].",
      "footnotes": null
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 567,
      "verse_key": "69:30",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "خُذُوهُ فَغُلُّوهُ",
      "translation": " (ئاللاھ دوزاخقا مۇئەككەل پەرىشتىلەرگە) ئېيتىدۇ: «ئۇنى تۇتۇڭلار، ئۇنىڭغا تاقاق سېلىڭلار[30]،",
      "footnotes": null
    },
    {
      "id": 31,
      "verse_number": 31,
      "page_number": 567,
      "verse_key": "69:31",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ ٱلۡجَحِيمَ صَلُّوهُ",
      "translation": " ئاندىن ئۇنى دوزاخقا تاشلاڭلار[31]، ئ",
      "footnotes": null
    },
    {
      "id": 32,
      "verse_number": 32,
      "page_number": 567,
      "verse_key": "69:32",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ فِي سِلۡسِلَةٖ ذَرۡعُهَا سَبۡعُونَ ذِرَاعٗا فَٱسۡلُكُوهُ",
      "translation": "اندىن ئۇنى ئۇزۇنلۇقى 70 گەز كېلىدىغان زەنجىر بىلەن باغلاڭلار»[32].",
      "footnotes": null
    },
    {
      "id": 33,
      "verse_number": 33,
      "page_number": 567,
      "verse_key": "69:33",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُۥ كَانَ لَا يُؤۡمِنُ بِٱللَّهِ ٱلۡعَظِيمِ",
      "translation": "(ئۇنىڭ بۇنداق ئازابلىنىشى شۇنىڭ ئۈچۈندۇركى) ئۇ ھەقىقەتەن ئۇلۇغ ئاللاھقا ئىمان ئېيتمىغان ئىدى[33].",
      "footnotes": null
    },
    {
      "id": 34,
      "verse_number": 34,
      "page_number": 567,
      "verse_key": "69:34",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَا يَحُضُّ عَلَىٰ طَعَامِ ٱلۡمِسۡكِينِ",
      "translation": "مىسكىنگە تاماق بېرىشنى تەرغىب قىلمايتتى[34].",
      "footnotes": null
    },
    {
      "id": 35,
      "verse_number": 35,
      "page_number": 568,
      "verse_key": "69:35",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلَيۡسَ لَهُ ٱلۡيَوۡمَ هَٰهُنَا حَمِيمٞ",
      "translation": " بۈگۈن ئۇنىڭغا بۇ يەردە (ئۇنىڭدىن ئازابنى كۆتۈرۈۋېتىدىغان) دوست يوقتۇر[35].",
      "footnotes": null
    },
    {
      "id": 36,
      "verse_number": 36,
      "page_number": 568,
      "verse_key": "69:36",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَا طَعَامٌ إِلَّا مِنۡ غِسۡلِينٖ",
      "translation": "ئۇنىڭغا يىرىڭدىن باشقا تاماق يوقتۇر[36].",
      "footnotes": null
    },
    {
      "id": 37,
      "verse_number": 37,
      "page_number": 568,
      "verse_key": "69:37",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَّا يَأۡكُلُهُۥٓ إِلَّا ٱلۡخَٰطِـُٔونَ",
      "translation": " ئۇنى پەقەت ئازغان ئادەملەرلا (يەنى كاپىرلارلا) يەيدۇ[37].",
      "footnotes": null
    },
    {
      "id": 38,
      "verse_number": 38,
      "page_number": 568,
      "verse_key": "69:38",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلَآ أُقۡسِمُ بِمَا تُبۡصِرُونَ",
      "translation": " مەن سىلەر كۆرۈپ تۇرىدىغان نەرسىلەر بىلەن قەسەم قىلىمەن،[38]",
      "footnotes": null
    },
    {
      "id": 39,
      "verse_number": 39,
      "page_number": 568,
      "verse_key": "69:39",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا لَا تُبۡصِرُونَ",
      "translation": "سىلەر كۆرمەيدىغان نەرسىلەر بىلەنمۇ قەسەم قىلىمەن (يەنى بارلىق مەخلۇقات بىلەن قەسەم قىلىمەن)[ـ39].",
      "footnotes": null
    },
    {
      "id": 40,
      "verse_number": 40,
      "page_number": 568,
      "verse_key": "69:40",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُۥ لَقَوۡلُ رَسُولٖ كَرِيمٖ",
      "translation": "بۇ قۇرئان ئەلۋەتتە ھۈرمەتلىك پەيغەمبەرنىڭ (ئاللاھ تەرىپىدىن يەتكۈزگەن) سۆزىدۇر[40].",
      "footnotes": null
    },
    {
      "id": 41,
      "verse_number": 41,
      "page_number": 568,
      "verse_key": "69:41",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا هُوَ بِقَوۡلِ شَاعِرٖۚ قَلِيلٗا مَّا تُؤۡمِنُونَ",
      "translation": "ئۇ شائىرنىڭ سۆزى ئەمەستۇر. سىلەر ناھايىتى ئاز ئىشىنىسىلەر[41].",
      "footnotes": null
    },
    {
      "id": 42,
      "verse_number": 42,
      "page_number": 568,
      "verse_key": "69:42",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَا بِقَوۡلِ كَاهِنٖۚ قَلِيلٗا مَّا تَذَكَّرُونَ",
      "translation": "ئۇ كاھىننىڭ سۆزىمۇ ئەمەس، سىلەر ئاز ۋەز ـ نەسىھەت ئالىسىلەر[42].",
      "footnotes": null
    },
    {
      "id": 43,
      "verse_number": 43,
      "page_number": 568,
      "verse_key": "69:43",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تَنزِيلٞ مِّن رَّبِّ ٱلۡعَٰلَمِينَ",
      "translation": "(ئۇ) ئالەملەرنىڭ پەرۋەردىگارى تەرىپىدىن نازىل قىلىنغاندۇر[43].",
      "footnotes": null
    },
    {
      "id": 44,
      "verse_number": 44,
      "page_number": 568,
      "verse_key": "69:44",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَوۡ تَقَوَّلَ عَلَيۡنَا بَعۡضَ ٱلۡأَقَاوِيلِ",
      "translation": "ئەگەر ئۇ بىزنىڭ نامىمىزدىن يالغاننى توقۇيدىغان بولسا[44]،",
      "footnotes": null
    },
    {
      "id": 45,
      "verse_number": 45,
      "page_number": 568,
      "verse_key": "69:45",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَأَخَذۡنَا مِنۡهُ بِٱلۡيَمِينِ",
      "translation": "ئەلۋەتتە ئۇنى قۇدرىتىمىز بىلەن جازالايتتۇق[45].",
      "footnotes": null
    },
    {
      "id": 46,
      "verse_number": 46,
      "page_number": 568,
      "verse_key": "69:46",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ لَقَطَعۡنَا مِنۡهُ ٱلۡوَتِينَ",
      "translation": " ئاندىن ئۇنىڭ ئاساسى قىزىل تومۇرىنى كېسىپ تاشلايتتۇق[46].",
      "footnotes": null
    },
    {
      "id": 47,
      "verse_number": 47,
      "page_number": 568,
      "verse_key": "69:47",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَمَا مِنكُم مِّنۡ أَحَدٍ عَنۡهُ حَٰجِزِينَ",
      "translation": "سىلەردىن ھېچبىر ئادەم ئۇنى قوغداپ قالالمايتتى[47].",
      "footnotes": null
    },
    {
      "id": 48,
      "verse_number": 48,
      "page_number": 568,
      "verse_key": "69:48",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِنَّهُۥ لَتَذۡكِرَةٞ لِّلۡمُتَّقِينَ",
      "translation": " شەك ـ شۈبھىسىزكى، بۇ قۇرئان تەقۋادارلار ئۈچۈن ۋەز ـ نەسىھەتتۇر[48].",
      "footnotes": null
    },
    {
      "id": 49,
      "verse_number": 49,
      "page_number": 568,
      "verse_key": "69:49",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِنَّا لَنَعۡلَمُ أَنَّ مِنكُم مُّكَذِّبِينَ",
      "translation": " بىز شەك ـ شۈبھىسىز بىلىمىزكى، سىلەرنىڭ ئاراڭلاردا (قۇرئان ئايەتلىرى شۇنچە روشەن تۇرۇقلۇق) ئۇنى ئىنكار قىلغۇچىلار بار[49].",
      "footnotes": null
    },
    {
      "id": 50,
      "verse_number": 50,
      "page_number": 568,
      "verse_key": "69:50",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِنَّهُۥ لَحَسۡرَةٌ عَلَى ٱلۡكَٰفِرِينَ",
      "translation": " قۇرئان كاپىرلارغا (ئۇلار قۇرئاننى تەستىق قىلغۇچىلارنىڭ ساۋابىنى ۋە ئىنكار قىلغۇچىلارنىڭ ئازابىنى كۆرگەن چاغدا) ئەلۋەتتە ھەسرەت ئېلىپ كېلىدۇ[50].",
      "footnotes": null
    },
    {
      "id": 51,
      "verse_number": 51,
      "page_number": 568,
      "verse_key": "69:51",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِنَّهُۥ لَحَقُّ ٱلۡيَقِينِ",
      "translation": "قۇرئان، مۇقەررەركى شەكسىز ھەقىقەتتۇر[51]. ",
      "footnotes": null
    },
    {
      "id": 52,
      "verse_number": 52,
      "page_number": 568,
      "verse_key": "69:52",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَسَبِّحۡ بِٱسۡمِ رَبِّكَ ٱلۡعَظِيمِ",
      "translation": "ئۇلۇغ پەرۋەردىگارىڭنى (مۇشرىكلارنىڭ ئىپتىرالىرىدىن) پاك ئېتىقاد قىلغىن[52].",
      "footnotes": null
    }
  ]
}