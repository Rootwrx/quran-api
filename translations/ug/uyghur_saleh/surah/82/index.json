{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 587,
      "verse_key": "82:1",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذَا ٱلسَّمَآءُ ٱنفَطَرَتۡ",
      "translation": "ئاسمان يېرىلغان چاغدا[1]، ",
      "footnotes": null
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 587,
      "verse_key": "82:2",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡكَوَاكِبُ ٱنتَثَرَتۡ",
      "translation": "يۇلتۇزلار تۆكۈلگەن چاغدا[2]،",
      "footnotes": null
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 587,
      "verse_key": "82:3",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡبِحَارُ فُجِّرَتۡ",
      "translation": "دېڭىزلار بىر ـ بىرىگە قوشۇلغان چاغدا[3]،",
      "footnotes": null
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 587,
      "verse_key": "82:4",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡقُبُورُ بُعۡثِرَتۡ",
      "translation": " قەبرىلەر ئاستى ـ ئۈستى قىلىنغان (يەنى ئۆلۈكلەر چىقىرىلغان) چاغدا[4]،",
      "footnotes": null
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 587,
      "verse_key": "82:5",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلِمَتۡ نَفۡسٞ مَّا قَدَّمَتۡ وَأَخَّرَتۡ",
      "translation": "ھەر ئادەم ئىلگىرى ـ كېيىن قىلغان ئىشلىرىنىڭ ھەممىسىنى بىلىدۇ[5].",
      "footnotes": null
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 587,
      "verse_key": "82:6",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَٰٓأَيُّهَا ٱلۡإِنسَٰنُ مَا غَرَّكَ بِرَبِّكَ ٱلۡكَرِيمِ",
      "translation": " ئى ئىنسان! سېنى مەرھەمەتلىك پەرۋەردىگارىڭغا ئاسىيلىق قىلىشقا نېمە ئالدىدى؟[6]",
      "footnotes": null
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 587,
      "verse_key": "82:7",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِي خَلَقَكَ فَسَوَّىٰكَ فَعَدَلَكَ",
      "translation": "ئۇ سېنى ياراتتى، (ئەزالىرىڭنى) بېجىرىم، قامىتىڭنى تۈز قىلدى[7].",
      "footnotes": null
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 587,
      "verse_key": "82:8",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِيٓ أَيِّ صُورَةٖ مَّا شَآءَ رَكَّبَكَ",
      "translation": " سېنى ئۇ خالىغان شەكىلدە قۇراشتۇردى[8].",
      "footnotes": null
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 587,
      "verse_key": "82:9",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّا بَلۡ تُكَذِّبُونَ بِٱلدِّينِ",
      "translation": "ھەرگىز ئۇنداق قىلماڭلار (يەنى ئاللاھنىڭ كەرەمىگە مەغرۇر بولماڭلار). (ئى مەككە كۇففارلىرى!) بەلكى سىلەر دىننى ئىنكار قىلىسىلەر[9].",
      "footnotes": null
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 587,
      "verse_key": "82:10",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِنَّ عَلَيۡكُمۡ لَحَٰفِظِينَ",
      "translation": "ھالبۇكى سىلەرنىڭ ئۈستۈڭلاردا سىلەرنى كۈزىتىپ تۇرغۇچى پەرىشتىلەر بار[10].",
      "footnotes": null
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 587,
      "verse_key": "82:11",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كِرَامٗا كَٰتِبِينَ",
      "translation": "ئۇلار (ئاللاھنىڭ دەرگاھىدا) ھۆرمەتلىك پەرىشتىلەر بولۇپ، (سىلەرنىڭ سۆزلىرىڭلارنى ۋە ئەمەللىرىڭلار) نى يېزىپ تۇرىدۇ[11].",
      "footnotes": null
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 587,
      "verse_key": "82:12",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَعۡلَمُونَ مَا تَفۡعَلُونَ",
      "translation": "ئۇلار قىلمىشىڭلارنى بىلىپ تۇرىدۇ[12]. ",
      "footnotes": null
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 587,
      "verse_key": "82:13",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ ٱلۡأَبۡرَارَ لَفِي نَعِيمٖ",
      "translation": "مۆمىنلەر شەك ـ شۈبھىسىز جەننەتتە بولىدۇ[13]،",
      "footnotes": null
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 587,
      "verse_key": "82:14",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِنَّ ٱلۡفُجَّارَ لَفِي جَحِيمٖ",
      "translation": " كاپىرلار شەك ـ شۈبھىسىز دوزاختا بولىدۇ[14].",
      "footnotes": null
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 587,
      "verse_key": "82:15",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَصۡلَوۡنَهَا يَوۡمَ ٱلدِّينِ",
      "translation": "ئۇلار قىيامەت كۈنى دوزاخقا كىرىدۇ[15].",
      "footnotes": null
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 587,
      "verse_key": "82:16",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا هُمۡ عَنۡهَا بِغَآئِبِينَ",
      "translation": "ئۇلار ھەرگىز دوزاختىن ئايرىلمايدۇ[16]. ",
      "footnotes": null
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 587,
      "verse_key": "82:17",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَآ أَدۡرَىٰكَ مَا يَوۡمُ ٱلدِّينِ",
      "translation": "قىيامەت كۈنىنىڭ (دەھشەتلىكلىكىنى) قانداقلىقىنى قانداق بىلەلەيسەن؟[17] ",
      "footnotes": null
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 587,
      "verse_key": "82:18",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ مَآ أَدۡرَىٰكَ مَا يَوۡمُ ٱلدِّينِ",
      "translation": "قىيامەت كۈنىنىڭ (دەھشەتلىكلىكىنى) قانداقلىقىنى قانداق بىلەلەيسەن؟[18] ",
      "footnotes": null
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 587,
      "verse_key": "82:19",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَوۡمَ لَا تَمۡلِكُ نَفۡسٞ لِّنَفۡسٖ شَيۡـٔٗاۖ وَٱلۡأَمۡرُ يَوۡمَئِذٖ لِّلَّهِ",
      "translation": " ئۇ كۈندە بىر ئادەمنىڭ يەنە بىر ئادەمگە ياردەم بېرىش قولىدىن كەلمەيدۇ، بۇ كۈندە ھۆكۈم ئاللاھقا خاستۇر[19].",
      "footnotes": null
    }
  ]
}