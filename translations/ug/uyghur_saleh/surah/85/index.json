{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 590,
      "verse_key": "85:1",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلسَّمَآءِ ذَاتِ ٱلۡبُرُوجِ",
      "translation": "بۇرۇجلارنىڭ ئىگىسى بولغان ئاسمان بىلەن، قىيامەت كۈنى بىلەن، جۈمە كۈنى بىلەن، ھارپا كۈنى بىلەن قەسەمكى[1ـ3]،",
      "footnotes": null
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 590,
      "verse_key": "85:2",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡيَوۡمِ ٱلۡمَوۡعُودِ",
      "translation": "بۇرۇجلارنىڭ ئىگىسى بولغان ئاسمان بىلەن، قىيامەت كۈنى بىلەن، جۈمە كۈنى بىلەن، ھارپا كۈنى بىلەن قەسەمكى[1ـ3]،",
      "footnotes": null
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 590,
      "verse_key": "85:3",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَشَاهِدٖ وَمَشۡهُودٖ",
      "translation": "بۇرۇجلارنىڭ ئىگىسى بولغان ئاسمان بىلەن، قىيامەت كۈنى بىلەن، جۈمە كۈنى بىلەن، ھارپا كۈنى بىلەن قەسەمكى[1ـ3]،",
      "footnotes": null
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 590,
      "verse_key": "85:4",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قُتِلَ أَصۡحَٰبُ ٱلۡأُخۡدُودِ",
      "translation": "ئورەكلەرنىڭ ئىگىللىرىگە لەنەت بولسۇن.4",
      "footnotes": null
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 590,
      "verse_key": "85:5",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلنَّارِ ذَاتِ ٱلۡوَقُودِ",
      "translation": " ئۇ ئورەكلەر لاۋۇلداپ تۇرغان ئوتلار بىلەن تولدۇرۇلغان ئىدى[5]. ",
      "footnotes": null
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 590,
      "verse_key": "85:6",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذۡ هُمۡ عَلَيۡهَا قُعُودٞ",
      "translation": " ئەينى ۋاقىتتا ئۇلار ئورەكلەر ئۈستىدە ئولتۇرۇپ ئۆزلىرىنىڭ مۆمىنلەرگە قىلىۋاتقان ئىشلىرىنى كۆرۈپ تۇراتتى[6ـ7]،",
      "footnotes": null
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 590,
      "verse_key": "85:7",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَهُمۡ عَلَىٰ مَا يَفۡعَلُونَ بِٱلۡمُؤۡمِنِينَ شُهُودٞ",
      "translation": " ئەينى ۋاقىتتا ئۇلار ئورەكلەر ئۈستىدە ئولتۇرۇپ ئۆزلىرىنىڭ مۆمىنلەرگە قىلىۋاتقان ئىشلىرىنى كۆرۈپ تۇراتتى[6ـ7]،",
      "footnotes": null
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 590,
      "verse_key": "85:8",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا نَقَمُواْ مِنۡهُمۡ إِلَّآ أَن يُؤۡمِنُواْ بِٱللَّهِ ٱلۡعَزِيزِ ٱلۡحَمِيدِ",
      "translation": " ئۇلار مۆمىنلەرنى پەقەت غالىب، مەدھىيەگە لايىق ئاللاھقا ئىمان ئېيتقانلىقلىرى ئۈچۈنلا يامان كۆردى. ئاسمانلارنىڭ ۋە زېمىننىڭ پادىشاھلىقى ئاللاھقا خاستۇر. ئاللاھ ھەممە نەرسىنى كۆرۈپ تۇرغۇچىدۇر[8ـ9].",
      "footnotes": null
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 590,
      "verse_key": "85:9",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِي لَهُۥ مُلۡكُ ٱلسَّمَٰوَٰتِ وَٱلۡأَرۡضِۚ وَٱللَّهُ عَلَىٰ كُلِّ شَيۡءٖ شَهِيدٌ",
      "translation": " ئۇلار مۆمىنلەرنى پەقەت غالىب، مەدھىيەگە لايىق ئاللاھقا ئىمان ئېيتقانلىقلىرى ئۈچۈنلا يامان كۆردى. ئاسمانلارنىڭ ۋە زېمىننىڭ پادىشاھلىقى ئاللاھقا خاستۇر. ئاللاھ ھەممە نەرسىنى كۆرۈپ تۇرغۇچىدۇر[8ـ9].",
      "footnotes": null
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 590,
      "verse_key": "85:10",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ ٱلَّذِينَ فَتَنُواْ ٱلۡمُؤۡمِنِينَ وَٱلۡمُؤۡمِنَٰتِ ثُمَّ لَمۡ يَتُوبُواْ فَلَهُمۡ عَذَابُ جَهَنَّمَ وَلَهُمۡ عَذَابُ ٱلۡحَرِيقِ",
      "translation": " مۆمىن ئەر ۋە مۆمىن ئاياللارغا زىيانكەشلىك قىلغاندىن كېيىن كۇفرىدىن قايتمىغانلار، شۈبھىسىزكى، جەھەننەمنىڭ ئازابىغا دۇچار بولىدۇ، ئۇلارغا ئوت بىلەن ئازاب قىلىنىدۇ[10].",
      "footnotes": null
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 590,
      "verse_key": "85:11",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ ٱلَّذِينَ ءَامَنُواْ وَعَمِلُواْ ٱلصَّٰلِحَٰتِ لَهُمۡ جَنَّٰتٞ تَجۡرِي مِن تَحۡتِهَا ٱلۡأَنۡهَٰرُۚ ذَٰلِكَ ٱلۡفَوۡزُ ٱلۡكَبِيرُ",
      "translation": " ئىمان ئېيتقان ۋە ياخشى ئەمەللەرنى قىلغانلار، شۈبھىسىزكى، ئاستىدىن ئۆستەڭلار ئېقىپ تۇرىدىغان جەننەتلەرگە سازاۋەر بولىدۇ. بۇ چوڭ بەختتۇر[11].",
      "footnotes": null
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 590,
      "verse_key": "85:12",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ بَطۡشَ رَبِّكَ لَشَدِيدٌ",
      "translation": "پەرۋەردىگارىڭنىڭ جازاسى شەك ـ شۈبھىسىز قاتتىقتۇر[12].",
      "footnotes": null
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 590,
      "verse_key": "85:13",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُۥ هُوَ يُبۡدِئُ وَيُعِيدُ",
      "translation": " ئاللاھ (خالايىقنى) ھەقىقەتەن يوقتىن بار قىلىدۇ (يەنى ئۆلگەندىن كېيىن ئۇلارنى) تىرىلدۈرىدۇ[13].",
      "footnotes": null
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 590,
      "verse_key": "85:14",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَهُوَ ٱلۡغَفُورُ ٱلۡوَدُودُ",
      "translation": "ئاللاھ (مۆمىن بەندىلەرنىڭ گۇناھىنى) مەغپىرەت قىلغۇچىدۇر. (ئۇلارنى) دوست تۇتقۇچىدۇر[14]، ",
      "footnotes": null
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 590,
      "verse_key": "85:15",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ذُو ٱلۡعَرۡشِ ٱلۡمَجِيدُ",
      "translation": "ئەرشنىڭ ئىگىسىدۇر، ناھايىتى ئۇلۇغدۇر[15]،",
      "footnotes": null
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 590,
      "verse_key": "85:16",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَعَّالٞ لِّمَا يُرِيدُ",
      "translation": " خالىغىنىنى قىلغۇچىدۇر[16].",
      "footnotes": null
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 590,
      "verse_key": "85:17",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "هَلۡ أَتَىٰكَ حَدِيثُ ٱلۡجُنُودِ",
      "translation": "(ئى مۇھەممەد!) سەن لەشكەرلەرنىڭ ـ پىرئەۋن ۋە سەمۇدنىڭ قىسسەسىدىن خەۋەر تاپتىڭمۇ؟[17ـ18]",
      "footnotes": null
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 590,
      "verse_key": "85:18",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِرۡعَوۡنَ وَثَمُودَ",
      "translation": "(ئى مۇھەممەد!) سەن لەشكەرلەرنىڭ ـ پىرئەۋن ۋە سەمۇدنىڭ قىسسەسىدىن خەۋەر تاپتىڭمۇ؟[17ـ18]",
      "footnotes": null
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 590,
      "verse_key": "85:19",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بَلِ ٱلَّذِينَ كَفَرُواْ فِي تَكۡذِيبٖ",
      "translation": " (قۇرەيش مۇشرىكلىرى ئاشۇ كاپىرلارغا نازىل بولغان ئازابتىن ئىبرەت ئالماي) داۋاملىق ئىنكار قىلىشىۋاتىدۇ[19].",
      "footnotes": null
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 590,
      "verse_key": "85:20",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱللَّهُ مِن وَرَآئِهِم مُّحِيطُۢ",
      "translation": " ئاللاھ كاپىرلارنىڭ ئەتراپىنى قورشاپ تۇرغۇچىدۇر[20].",
      "footnotes": null
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 590,
      "verse_key": "85:21",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بَلۡ هُوَ قُرۡءَانٞ مَّجِيدٞ",
      "translation": "بەلكى (ئۇلار ئىنكار قىلغان نەرسە) لەۋھۇلمەھپۇزدا ساقلانغان ئۇلۇغ قۇرئاندۇر[21ـ22].",
      "footnotes": null
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 590,
      "verse_key": "85:22",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِي لَوۡحٖ مَّحۡفُوظِۭ",
      "translation": "بەلكى (ئۇلار ئىنكار قىلغان نەرسە) لەۋھۇلمەھپۇزدا ساقلانغان ئۇلۇغ قۇرئاندۇر[21ـ22].",
      "footnotes": null
    }
  ]
}