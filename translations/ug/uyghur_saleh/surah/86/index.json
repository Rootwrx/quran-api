{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 591,
      "verse_key": "86:1",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلسَّمَآءِ وَٱلطَّارِقِ",
      "translation": "ئاسمان بىلەن، تارىق (يەنى كېچىدە ئاشكارا بولغۇچى يۇلتۇز) بىلەن قەسەم قىلىمەن[1].",
      "footnotes": null
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 591,
      "verse_key": "86:2",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَآ أَدۡرَىٰكَ مَا ٱلطَّارِقُ",
      "translation": " تارىقنىڭ نېمىلىكىنى بىلەمسەن؟[2]",
      "footnotes": null
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 591,
      "verse_key": "86:3",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلنَّجۡمُ ٱلثَّاقِبُ",
      "translation": " ئۇ يورۇق يۇلتۇزدۇر[3].",
      "footnotes": null
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 591,
      "verse_key": "86:4",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِن كُلُّ نَفۡسٖ لَّمَّا عَلَيۡهَا حَافِظٞ",
      "translation": " ھەر بىر ئادەم بىلەن (شۇ ئادەمنى) ساقلىغۇچى (پەرىشتە) بار[4].",
      "footnotes": null
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 591,
      "verse_key": "86:5",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلۡيَنظُرِ ٱلۡإِنسَٰنُ مِمَّ خُلِقَ",
      "translation": " ئىنسان نېمىدىن يارىتىلغانلىقىغا قارىسۇن[5].",
      "footnotes": null
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 591,
      "verse_key": "86:6",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "خُلِقَ مِن مَّآءٖ دَافِقٖ",
      "translation": " ئۇ ئېتىلىپ چىققۇچى مەنىدىن يارىتىلدى[6].",
      "footnotes": null
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 591,
      "verse_key": "86:7",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَخۡرُجُ مِنۢ بَيۡنِ ٱلصُّلۡبِ وَٱلتَّرَآئِبِ",
      "translation": " ئۇ مەنى ئۇمۇرتقا بىلەن قوۋۇرغا سۆڭىكىنىڭ ئارىسىدىن چىقىدۇ[7]. ",
      "footnotes": null
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 591,
      "verse_key": "86:8",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُۥ عَلَىٰ رَجۡعِهِۦ لَقَادِرٞ",
      "translation": "شەك ـ شۈبھىسىزكى، ئاللاھ ئۇنى ئەسلىگە كەلتۈرۈشكە (يەنى تىرىلدۈرۈشكە) قادىردۇر[8].",
      "footnotes": null
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 591,
      "verse_key": "86:9",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَوۡمَ تُبۡلَى ٱلسَّرَآئِرُ",
      "translation": "سىرلار ئاشكارا قىلىنىدىغان كۈندە (يەنى قىيامەتتە)[9]،",
      "footnotes": null
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 591,
      "verse_key": "86:10",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَمَا لَهُۥ مِن قُوَّةٖ وَلَا نَاصِرٖ",
      "translation": "ئۇنىڭدىن (يەنى ئىنساندىن) (ئازابنى دەپئى قىلىدىغان) ھېچ كۈچ ۋە ياردەمچى يوقتۇر[10].",
      "footnotes": null
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 591,
      "verse_key": "86:11",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلسَّمَآءِ ذَاتِ ٱلرَّجۡعِ",
      "translation": " يامغۇرلۇق بۇلۇت بىلەن قەسەمكى[11]،",
      "footnotes": null
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 591,
      "verse_key": "86:12",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡأَرۡضِ ذَاتِ ٱلصَّدۡعِ",
      "translation": "يېرىلىپ (گىياھلارنى ئۈندۈرۈپ بېرىدىغان) زېمىن بىلەن قەسەمكى[12]،",
      "footnotes": null
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 591,
      "verse_key": "86:13",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُۥ لَقَوۡلٞ فَصۡلٞ",
      "translation": "قۇرئان شەك ـ شۈبھىسىز (ھەق بىلەن باتىلنى ئايرىغۇچى) سۆزدۇر[13]، ",
      "footnotes": null
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 591,
      "verse_key": "86:14",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا هُوَ بِٱلۡهَزۡلِ",
      "translation": "ئۇ چاقچاق ئەمەستۇر[14].",
      "footnotes": null
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 591,
      "verse_key": "86:15",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُمۡ يَكِيدُونَ كَيۡدٗا",
      "translation": "ئۇلار (يەنى مۇشرىكلار) (قۇرئاننى يوققا چىقىرىش ئۈچۈن) ھەقىقەتەن ھىيلە ـ مىكىر ئىشلىتىدۇ[15].",
      "footnotes": null
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 591,
      "verse_key": "86:16",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَكِيدُ كَيۡدٗا",
      "translation": "مەن ئۇلارنىڭ ھىيلە ـ مىكرىگە تاقابىل تۇرىمەن[16].",
      "footnotes": null
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 591,
      "verse_key": "86:17",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَمَهِّلِ ٱلۡكَٰفِرِينَ أَمۡهِلۡهُمۡ رُوَيۡدَۢا",
      "translation": " (ئى مۇھەممەد!) كاپىرلارنىڭ ھالاك بولۇشىغا ئالدىرىمىغىن، ئۇلارغا ئازراق مۆھلەت بەرگىن (ئۇزاققا قالماي ئۇلارغا قانداق قىلغانلىقىمنى كۆرىسەن)[17].",
      "footnotes": null
    }
  ]
}