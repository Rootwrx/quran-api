{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 597,
      "verse_key": "96:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱقۡرَأۡ بِٱسۡمِ رَبِّكَ ٱلَّذِي خَلَقَ",
      "translation": "ياراتقان پەرۋەردىگارىڭنىڭ ئىسمى بىلەن ئوقۇغىن[1].",
      "footnotes": null
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 597,
      "verse_key": "96:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "خَلَقَ ٱلۡإِنسَٰنَ مِنۡ عَلَقٍ",
      "translation": " ئۇ ئىنساننى لەختە قاندىن ياراتتى[2].",
      "footnotes": null
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 597,
      "verse_key": "96:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱقۡرَأۡ وَرَبُّكَ ٱلۡأَكۡرَمُ",
      "translation": "ئوقۇغىن، پەرۋەردىگارىڭ ئەڭ كەرەملىكتۇر[3].",
      "footnotes": null
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 597,
      "verse_key": "96:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِي عَلَّمَ بِٱلۡقَلَمِ",
      "translation": " ئۇ قەلەم بىلەن (خەت يېزىشنى) ئۆگەتتى[4].",
      "footnotes": null
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 597,
      "verse_key": "96:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلَّمَ ٱلۡإِنسَٰنَ مَا لَمۡ يَعۡلَمۡ",
      "translation": "ئىنسانغا بىلمىگەن نەرسىلەرنى بىلدۈردى[5].",
      "footnotes": null
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 597,
      "verse_key": "96:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّآ إِنَّ ٱلۡإِنسَٰنَ لَيَطۡغَىٰٓ",
      "translation": "شەك ـ شۈبھىسىز ئىنسان ئۆزىنى باي ساناپ (ئاللاھقا بويسۇنۇشتىن باش تارتىپ)، راستىنلا ھەددىدىن ئاشىدۇ[6ـ7].",
      "footnotes": null
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 597,
      "verse_key": "96:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَن رَّءَاهُ ٱسۡتَغۡنَىٰٓ",
      "translation": "شەك ـ شۈبھىسىز ئىنسان ئۆزىنى باي ساناپ (ئاللاھقا بويسۇنۇشتىن باش تارتىپ)، راستىنلا ھەددىدىن ئاشىدۇ[6ـ7].",
      "footnotes": null
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 597,
      "verse_key": "96:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ إِلَىٰ رَبِّكَ ٱلرُّجۡعَىٰٓ",
      "translation": " (ئى ئىنسان!) بارىدىغان يېرىڭ پەرۋەردىگارىڭنىڭ دەرگاھىدۇر (قىلمىشىڭغا قاراپ جازاغا ياكى مۇكاپاتقا ئېرىشىسەن)[8].",
      "footnotes": null
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 597,
      "verse_key": "96:9",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَرَءَيۡتَ ٱلَّذِي يَنۡهَىٰ",
      "translation": " ناماز ئوقۇۋاتقان بەندىنى (يەنى مۇھەممەد ئەلەيھىسسالامنى) توسقان ئادەمنىڭ (ھالىنى) ماڭا ئېيتىپ بەرسەڭچۇ؟[9ـ10]",
      "footnotes": null
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 597,
      "verse_key": "96:10",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَبۡدًا إِذَا صَلَّىٰٓ",
      "translation": " ناماز ئوقۇۋاتقان بەندىنى (يەنى مۇھەممەد ئەلەيھىسسالامنى) توسقان ئادەمنىڭ (ھالىنى) ماڭا ئېيتىپ بەرسەڭچۇ؟[9ـ10]",
      "footnotes": null
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 597,
      "verse_key": "96:11",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَرَءَيۡتَ إِن كَانَ عَلَى ٱلۡهُدَىٰٓ",
      "translation": "ماڭا (ئۇنىڭ ھالىنى) ئېيتىپ بەرسەڭچۇ؟ ئەگەر ئۇ (ھەددىدىن ئاشقۇچى) (نامازنى توسۇشنىڭ ئورنىغا) توغرا يولدا بولغان بولسا[11]،",
      "footnotes": null
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 597,
      "verse_key": "96:12",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَوۡ أَمَرَ بِٱلتَّقۡوَىٰٓ",
      "translation": " ياكى تەقۋادارلىققا ئەمر قىلغان بولسا (بۇ ئۇنىڭ ئۆزى ئۈچۈن ياخشى بولماسمىدى؟)[12]",
      "footnotes": null
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 597,
      "verse_key": "96:13",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَرَءَيۡتَ إِن كَذَّبَ وَتَوَلَّىٰٓ",
      "translation": " ماڭا ئېيتىپ بەرسەڭچۇ؟ ئەگەر ئۇ (قۇرئاننى) ئىنكار قىلسا، ئىماندىن (يۈز ئۆرۈسە)[13]،",
      "footnotes": null
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 597,
      "verse_key": "96:14",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَمۡ يَعۡلَم بِأَنَّ ٱللَّهَ يَرَىٰ",
      "translation": "ئاللاھنىڭ (ئۇنى) كۆرۈپ تۇرىدىغانلىقىنى بىلمىدىمۇ؟[14] ",
      "footnotes": null
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 597,
      "verse_key": "96:15",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّا لَئِن لَّمۡ يَنتَهِ لَنَسۡفَعَۢا بِٱلنَّاصِيَةِ",
      "translation": "ئۇ گۇمراھلىقتىن يانسۇن، ئەگەر ئۇ بۇنىڭدىن يانمىسا، چوقۇم ئۇنىڭ كوكۇلىسىدىن ـ يالغانچى گۇناھكارنىڭ كوكۇلىسىدىن تۇتۇپ (جەھەننەمگە) سۆرەيمىز[15 ـ 16].",
      "footnotes": null
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 597,
      "verse_key": "96:16",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "نَاصِيَةٖ كَٰذِبَةٍ خَاطِئَةٖ",
      "translation": "ئۇ گۇمراھلىقتىن يانسۇن، ئەگەر ئۇ بۇنىڭدىن يانمىسا، چوقۇم ئۇنىڭ كوكۇلىسىدىن ـ يالغانچى گۇناھكارنىڭ كوكۇلىسىدىن تۇتۇپ (جەھەننەمگە) سۆرەيمىز[15 ـ 16].",
      "footnotes": null
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 597,
      "verse_key": "96:17",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلۡيَدۡعُ نَادِيَهُۥ",
      "translation": "ئۇ ئۆزىنىڭ سورۇنداشلىرىنى ياردەمگە چاقىرسۇن![17]",
      "footnotes": null
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 597,
      "verse_key": "96:18",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "سَنَدۡعُ ٱلزَّبَانِيَةَ",
      "translation": " بىز زابانىيىلەر (يەنى جەھەننەمنىڭ كۈچلۈك ـ قاتتىق قول پەرىشتىلىرى) نى چاقىرىمىز[18].",
      "footnotes": null
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 597,
      "verse_key": "96:19",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": "obligatory",
      "sajdah_number": 14,
      "text": "كَلَّا لَا تُطِعۡهُ وَٱسۡجُدۡۤ وَٱقۡتَرِب۩",
      "translation": "(بۇ فاجىر گۇمراھلىقتىن) يانسۇن! ئۇنىڭغا ئىتائەت قىلمىغىن، (ئاللاھقا) سەجدە قىلغىن (شۇنىڭ بىلەن ئاللاھقا) يېقىنلىق ھاسىل قىلغىن[19].",
      "footnotes": null
    }
  ]
}