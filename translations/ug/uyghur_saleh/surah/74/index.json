{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 575,
      "verse_key": "74:1",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَٰٓأَيُّهَا ٱلۡمُدَّثِّرُ",
      "translation": "ئى (ۋەھىي نازىل بولغان چاغدا) كىيىمگە چۈمكىلىۋالغۇچى (پەيغەمبەر!)[1]",
      "footnotes": null
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 575,
      "verse_key": "74:2",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قُمۡ فَأَنذِرۡ",
      "translation": "تۇر! (ئىنسانلارنى) ئاللاھنىڭ ئازابىدىن ئاگاھلاندۇر[2].",
      "footnotes": null
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 575,
      "verse_key": "74:3",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَرَبَّكَ فَكَبِّرۡ",
      "translation": "پەرۋەردىگارىڭنى ئۇلۇغلا[3].",
      "footnotes": null
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 575,
      "verse_key": "74:4",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَثِيَابَكَ فَطَهِّرۡ",
      "translation": " كىيىمىڭنى پاك تۇت[4].",
      "footnotes": null
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 575,
      "verse_key": "74:5",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلرُّجۡزَ فَٱهۡجُرۡ",
      "translation": " ئازابتىن (يەنى ئازابقا قالدۇرىدىغان ئىشلاردىن) يىراق بول[5].",
      "footnotes": null
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 575,
      "verse_key": "74:6",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَا تَمۡنُن تَسۡتَكۡثِرُ",
      "translation": "كۆپ ئېلىش نىيىتىدە بەرمە (يەنى بىر كىشىگە بىرەر سوۋغا بەرسەڭ، ئۇنىڭدىن بەرگەن نەرسەڭدىن كۆپرەك نەرسە ئېلىشنى تەمە قىلما)[6]،",
      "footnotes": null
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 575,
      "verse_key": "74:7",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلِرَبِّكَ فَٱصۡبِرۡ",
      "translation": "پەرۋەردىگارىڭنىڭ (رازىلىقى) ئۈچۈن (قەۋمىڭدىن يەتكەن ئەزىيەتكە) سەۋر قىل[7].",
      "footnotes": null
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 575,
      "verse_key": "74:8",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِذَا نُقِرَ فِي ٱلنَّاقُورِ",
      "translation": " سۇر چېلىنغان ۋاقىت ـ ئۇ كۈن قىيىن كۈندۇر[8ـ9].",
      "footnotes": null
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 575,
      "verse_key": "74:9",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَذَٰلِكَ يَوۡمَئِذٖ يَوۡمٌ عَسِيرٌ",
      "translation": " سۇر چېلىنغان ۋاقىت ـ ئۇ كۈن قىيىن كۈندۇر[8ـ9].",
      "footnotes": null
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 575,
      "verse_key": "74:10",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلَى ٱلۡكَٰفِرِينَ غَيۡرُ يَسِيرٖ",
      "translation": " كاپىرلارغا ئاسان ئەمەستۇر[10]. ",
      "footnotes": null
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 575,
      "verse_key": "74:11",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ذَرۡنِي وَمَنۡ خَلَقۡتُ وَحِيدٗا",
      "translation": "(ئى مۇھەممەد!) مەن تەنھا ياراتقان كىشىنى (يەنى ۋەلىد ئىبن مۇغىرەنى) ماڭا قويۇۋەت[11].",
      "footnotes": null
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 575,
      "verse_key": "74:12",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَجَعَلۡتُ لَهُۥ مَالٗا مَّمۡدُودٗا",
      "translation": " ئۇنىڭغا نۇرغۇن مال بەردىم[12].",
      "footnotes": null
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 575,
      "verse_key": "74:13",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَبَنِينَ شُهُودٗا",
      "translation": "ئۇنىڭغا كۆز ئالدىدا تۇرىدىغان ئوغۇللارنى بەردىم[13].",
      "footnotes": null
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 575,
      "verse_key": "74:14",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَهَّدتُّ لَهُۥ تَمۡهِيدٗا",
      "translation": " ئۇنىڭ ئابرويىنى ئۆستۈردۈم[14].",
      "footnotes": null
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 575,
      "verse_key": "74:15",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ يَطۡمَعُ أَنۡ أَزِيدَ",
      "translation": " ئاندىن ئۇ يەنە كۆپ بېرىشىمنى تەمە قىلىدۇ[15].",
      "footnotes": null
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 575,
      "verse_key": "74:16",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّآۖ إِنَّهُۥ كَانَ لِأٓيَٰتِنَا عَنِيدٗا",
      "translation": "ياق، (يەنى ئۇنىڭ خالىغىنى بولمايدۇ)، چۈنكى ئۇ بىزنىڭ ئايەتلىرىمىزگە قارشى چىقتى[16].",
      "footnotes": null
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 575,
      "verse_key": "74:17",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "سَأُرۡهِقُهُۥ صَعُودًا",
      "translation": "ئۇنىڭغا قاتتىق ئازابىمنى تېتىتىمەن[17].",
      "footnotes": null
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 576,
      "verse_key": "74:18",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُۥ فَكَّرَ وَقَدَّرَ",
      "translation": " ئۇ (پەيغەمبەر ۋە قۇرئان ھەققىدە) ئويلاندى، (قۇرئان توغرۇلۇق نېمە دېيىشنى ۋە قانداق بوھتان قىلىشنى) پىلانلىدى[18].",
      "footnotes": null
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 576,
      "verse_key": "74:19",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَقُتِلَ كَيۡفَ قَدَّرَ",
      "translation": " لەنەت بولسۇنكى، ئۇ قانداق پىلانلىدى؟[19] ",
      "footnotes": null
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 576,
      "verse_key": "74:20",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ قُتِلَ كَيۡفَ قَدَّرَ",
      "translation": "يەنە لەنەت بولسۇنكى، ئۇ قانداق پىلانلىدى[20].",
      "footnotes": null
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 576,
      "verse_key": "74:21",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ نَظَرَ",
      "translation": "ئاندىن ئۇ قارىدى[21]،",
      "footnotes": null
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 576,
      "verse_key": "74:22",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ عَبَسَ وَبَسَرَ",
      "translation": " ئاندىن قوشۇمىسىنى تۈردى ۋە چىرايىنى پۈرۈشتۈردى[22]،",
      "footnotes": null
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 576,
      "verse_key": "74:23",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ أَدۡبَرَ وَٱسۡتَكۡبَرَ",
      "translation": " ئاندىن (ئىماندىن) يۈز ئۆرۈدى (پەيغەمبەرگە ئەگىشىشتىن) تەكەببۇرلۇق بىلەن باش تارتتى[23]، ",
      "footnotes": null
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 576,
      "verse_key": "74:24",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَقَالَ إِنۡ هَٰذَآ إِلَّا سِحۡرٞ يُؤۡثَرُ",
      "translation": "«بۇ (كالامۇللاھ ئەمەس) پەقەت ئۆگىنىلگەن سېھىردۇر، بۇ پەقەت ئىنسان سۆزىدۇر» دېدى[24ـ25].",
      "footnotes": null
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 576,
      "verse_key": "74:25",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنۡ هَٰذَآ إِلَّا قَوۡلُ ٱلۡبَشَرِ",
      "translation": "«بۇ (كالامۇللاھ ئەمەس) پەقەت ئۆگىنىلگەن سېھىردۇر، بۇ پەقەت ئىنسان سۆزىدۇر» دېدى[24ـ25].",
      "footnotes": null
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 576,
      "verse_key": "74:26",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "سَأُصۡلِيهِ سَقَرَ",
      "translation": "ئۇنى مەن سەقەرگە (يەنى دوزاخقا) سالىمەن[26].",
      "footnotes": null
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 576,
      "verse_key": "74:27",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَآ أَدۡرَىٰكَ مَا سَقَرُ",
      "translation": "سەقەرنىڭ نېمىلىكىنى قانداق بىلەلەيسەن؟[27]",
      "footnotes": null
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 576,
      "verse_key": "74:28",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَا تُبۡقِي وَلَا تَذَرُ",
      "translation": "ئۇ ھېچ نەرسىنى قالدۇرمايدۇ، قويمايدۇ (بەلكى كۆيدۈرۈپ تاشلايدۇ)[28].",
      "footnotes": null
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 576,
      "verse_key": "74:29",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَوَّاحَةٞ لِّلۡبَشَرِ",
      "translation": "ئۇ (چوڭلۇقىدىن) ئىنسانلارنىڭ (كۆزلىرىگە يىراق مۇساپىلەردىن) كۆرۈنۈپ تۇرىدۇ[29]. ",
      "footnotes": null
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 576,
      "verse_key": "74:30",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلَيۡهَا تِسۡعَةَ عَشَرَ",
      "translation": "ئۇنىڭغا 19 پەرىشتە مۇئەككەلدۇر[30].",
      "footnotes": null
    },
    {
      "id": 31,
      "verse_number": 31,
      "page_number": 576,
      "verse_key": "74:31",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا جَعَلۡنَآ أَصۡحَٰبَ ٱلنَّارِ إِلَّا مَلَٰٓئِكَةٗۖ وَمَا جَعَلۡنَا عِدَّتَهُمۡ إِلَّا فِتۡنَةٗ لِّلَّذِينَ كَفَرُواْ لِيَسۡتَيۡقِنَ ٱلَّذِينَ أُوتُواْ ٱلۡكِتَٰبَ وَيَزۡدَادَ ٱلَّذِينَ ءَامَنُوٓاْ إِيمَٰنٗا وَلَا يَرۡتَابَ ٱلَّذِينَ أُوتُواْ ٱلۡكِتَٰبَ وَٱلۡمُؤۡمِنُونَ وَلِيَقُولَ ٱلَّذِينَ فِي قُلُوبِهِم مَّرَضٞ وَٱلۡكَٰفِرُونَ مَاذَآ أَرَادَ ٱللَّهُ بِهَٰذَا مَثَلٗاۚ كَذَٰلِكَ يُضِلُّ ٱللَّهُ مَن يَشَآءُ وَيَهۡدِي مَن يَشَآءُۚ وَمَا يَعۡلَمُ جُنُودَ رَبِّكَ إِلَّا هُوَۚ وَمَا هِيَ إِلَّا ذِكۡرَىٰ لِلۡبَشَرِ",
      "translation": " بىز دوزاخ مۇئەككەللىرىنى پەقەت پەرىشتىلەردىن قىلدۇق، بىز ئۇلارنىڭ سانى بىلەن پەقەت كاپىرلارنى سىنىدۇق، تاكى ئەھلى كىتاب (ئۆزلىرىنىڭ ساماۋى كىتابلىرىدىكى بۇ ساننى كۆرۈپ قۇرئاننىڭ ھەق ئىكەنلىكىگە) ئىمان ئېيتماي، مۆمىنلەرنىڭ (پەيغەمبەر ئەلەيھىسسالامنىڭ بەرگەن خەۋىرىگە ئىشەنگەنلىكلىرىدىن ۋە ئەھلى كىتابنىڭ قۇرئان خەۋەرلىرىنىڭ تەۋرات، ئىنجىلدىكى خەۋەرلەرگە ئۇيغۇن ئىكەنلىكىنى ئېتىراپ قىلغانلىقىدىن) ئىمانى كۈچەيگەي! ئەھلى كىتاب ۋە مۆمىنلەر (بۇنىڭدىن) شەكلەنمەيدۇ، دىللىرىدا كېسەل بارلار (يەنى مۇناپىقلار)، كاپىرلار: «بۇ مىسالدىن ئاللاھ نېمىنى ئىرادە قىلىدۇ» دەيدۇ، ئاللاھ مۇشۇنداق خالىغان ئادەمنى گۇمراھ قىلىدۇ، خالىغان ئادەمنى ھىدايەت قىلىدۇ، پەرۋەردىگارىڭنىڭ قوشۇنىنى (يەنى پەرىشتىلەرنىڭ سانىنىڭ كۆپلۈكىنى، ئۇلارنىڭ كۈچ ـ قۇۋۋىتىنى ۋە ئۇلارنىڭ يارىتىلىشتىكى چوڭلۇقىنى) پەقەت ئۇ ئۆزى بىلىدۇ، بۇ، (يەنى ئاللاھ بايان قىلغان دوزاخ) پەقەت ئىنسانلار ئۈچۈن ۋەز ـ نەسىھەتتۇر[31].",
      "footnotes": null
    },
    {
      "id": 32,
      "verse_number": 32,
      "page_number": 576,
      "verse_key": "74:32",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّا وَٱلۡقَمَرِ",
      "translation": "ياق (يەنى ئۇ مەسخىرە قىلغۇچىلار قەبىھ قىلمىشىدىن يانسۇن)، ئاي بىلەن قەسەمكى[32]،",
      "footnotes": null
    },
    {
      "id": 33,
      "verse_number": 33,
      "page_number": 576,
      "verse_key": "74:33",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّيۡلِ إِذۡ أَدۡبَرَ",
      "translation": "ئۆتۈپ كەتكەن كېچە بىلەن قەسەمكى[33]،",
      "footnotes": null
    },
    {
      "id": 34,
      "verse_number": 34,
      "page_number": 576,
      "verse_key": "74:34",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلصُّبۡحِ إِذَآ أَسۡفَرَ",
      "translation": " يورۇغان سۈبھى بىلەن قەسەمكى[34]،",
      "footnotes": null
    },
    {
      "id": 35,
      "verse_number": 35,
      "page_number": 576,
      "verse_key": "74:35",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهَا لَإِحۡدَى ٱلۡكُبَرِ",
      "translation": "شۈبھىسىزكى، دوزاخ بالالاردىن بىرىدۇر (ئۇنى قانداقمۇ مەسخىرە قىلىسىلەر ۋە ئۇنىڭغا چىنپۈتمەيسىلەر؟)[35].",
      "footnotes": null
    },
    {
      "id": 36,
      "verse_number": 36,
      "page_number": 576,
      "verse_key": "74:36",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "نَذِيرٗا لِّلۡبَشَرِ",
      "translation": " ئۇ ئىنسانلارنى ئاگاھلاندۇرغۇچىدۇر[36].",
      "footnotes": null
    },
    {
      "id": 37,
      "verse_number": 37,
      "page_number": 576,
      "verse_key": "74:37",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لِمَن شَآءَ مِنكُمۡ أَن يَتَقَدَّمَ أَوۡ يَتَأَخَّرَ",
      "translation": " سىلەردىن (جەننەتكە) ئىلگىرىلەشنى خالىغان ياكى (دوزاختىن) چېكىنىشنى خالىغان ئادەمنى ئاگاھلاندۇرغۇچىدۇر[37].",
      "footnotes": null
    },
    {
      "id": 38,
      "verse_number": 38,
      "page_number": 576,
      "verse_key": "74:38",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كُلُّ نَفۡسِۭ بِمَا كَسَبَتۡ رَهِينَةٌ",
      "translation": " ھەر ئىنسان قىلمىشلىرى تۈپەيلىدىن (دوزاختا) مەھبۇستۇر[38].",
      "footnotes": null
    },
    {
      "id": 39,
      "verse_number": 39,
      "page_number": 576,
      "verse_key": "74:39",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَّآ أَصۡحَٰبَ ٱلۡيَمِينِ",
      "translation": "پەقەت ئەسھابى يەمىن (يەنى سائادەتمەن مۆمىنلەر) بۇنىڭدىن مۇستەسنا[39].",
      "footnotes": null
    },
    {
      "id": 40,
      "verse_number": 40,
      "page_number": 576,
      "verse_key": "74:40",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِي جَنَّٰتٖ يَتَسَآءَلُونَ",
      "translation": " ئۇلار جەننەتلەردە بولۇپ، گۇناھكارلاردىن: «سىلەرنى دوزاخقا كىرگۈزگەن نېمە؟» دەپ سورايدۇ[40ـ42].",
      "footnotes": null
    },
    {
      "id": 41,
      "verse_number": 41,
      "page_number": 576,
      "verse_key": "74:41",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَنِ ٱلۡمُجۡرِمِينَ",
      "translation": " ئۇلار جەننەتلەردە بولۇپ، گۇناھكارلاردىن: «سىلەرنى دوزاخقا كىرگۈزگەن نېمە؟» دەپ سورايدۇ[40ـ42].",
      "footnotes": null
    },
    {
      "id": 42,
      "verse_number": 42,
      "page_number": 576,
      "verse_key": "74:42",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَا سَلَكَكُمۡ فِي سَقَرَ",
      "translation": " ئۇلار جەننەتلەردە بولۇپ، گۇناھكارلاردىن: «سىلەرنى دوزاخقا كىرگۈزگەن نېمە؟» دەپ سورايدۇ[40ـ42].",
      "footnotes": null
    },
    {
      "id": 43,
      "verse_number": 43,
      "page_number": 576,
      "verse_key": "74:43",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قَالُواْ لَمۡ نَكُ مِنَ ٱلۡمُصَلِّينَ",
      "translation": " ئۇلار (جاۋابەن) ئېيتىدۇ: «بىز ناماز ئوقۇمىدۇق[43].",
      "footnotes": null
    },
    {
      "id": 44,
      "verse_number": 44,
      "page_number": 576,
      "verse_key": "74:44",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَمۡ نَكُ نُطۡعِمُ ٱلۡمِسۡكِينَ",
      "translation": "مىسكىنلەرگە تائام بەرمىدۇق[44].",
      "footnotes": null
    },
    {
      "id": 45,
      "verse_number": 45,
      "page_number": 576,
      "verse_key": "74:45",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَكُنَّا نَخُوضُ مَعَ ٱلۡخَآئِضِينَ",
      "translation": "بىز بىھۇدە سۆز قىلغۇچىلار بىلەن بىللە بىھۇدە سۆز قىلاتتۇق[45].",
      "footnotes": null
    },
    {
      "id": 46,
      "verse_number": 46,
      "page_number": 576,
      "verse_key": "74:46",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَكُنَّا نُكَذِّبُ بِيَوۡمِ ٱلدِّينِ",
      "translation": " قىيامەت كۈنىنى ئىنكار قىلاتتۇق[46]،",
      "footnotes": null
    },
    {
      "id": 47,
      "verse_number": 47,
      "page_number": 576,
      "verse_key": "74:47",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "حَتَّىٰٓ أَتَىٰنَا ٱلۡيَقِينُ",
      "translation": " تاكى بىز ئۆلگۈچە (شۇنداق قىلدۇق)»[47].",
      "footnotes": null
    },
    {
      "id": 48,
      "verse_number": 48,
      "page_number": 577,
      "verse_key": "74:48",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَمَا تَنفَعُهُمۡ شَفَٰعَةُ ٱلشَّٰفِعِينَ",
      "translation": "شاپائەت قىلغۇچىلارنىڭ شاپائىتى ئۇلارغا پايدا بەرمەيدۇ (يەنى ئۇلارنى ئاللاھنىڭ ئازابىدىن قۇتقۇزىدىغان شاپائەتچى بولمايدۇ، ئۇلارغا پۈتۈن ئەھلى زېمىن شاپائەت قىلغان تەقدىردىمۇ، ئۇلارنىڭ شاپائىتى قوبۇل قىلىنمايدۇ)[48].",
      "footnotes": null
    },
    {
      "id": 49,
      "verse_number": 49,
      "page_number": 577,
      "verse_key": "74:49",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَمَا لَهُمۡ عَنِ ٱلتَّذۡكِرَةِ مُعۡرِضِينَ",
      "translation": "ئۇلار نېمىشقا ۋەز (يەنى قۇرئان) دىن يۈز ئۆرۈيدۇ؟[49] ",
      "footnotes": null
    },
    {
      "id": 50,
      "verse_number": 50,
      "page_number": 577,
      "verse_key": "74:50",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَأَنَّهُمۡ حُمُرٞ مُّسۡتَنفِرَةٞ",
      "translation": "گويا ئۇلار يولۋاستىن قاچقان ياۋايى ئېشەكلەردۇر (يەنى بۇ مۇشرىكلار مۇھەممەد ئەلەيھىسسالامنى كۆرگەندە، ئۇنىڭدىن خۇددى يولۋاسنى كۆرۈپ قاچقان ياۋا ئېشەكلەردەك قاچىدۇ)[50ـ51].",
      "footnotes": null
    },
    {
      "id": 51,
      "verse_number": 51,
      "page_number": 577,
      "verse_key": "74:51",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَرَّتۡ مِن قَسۡوَرَةِۭ",
      "translation": "گويا ئۇلار يولۋاستىن قاچقان ياۋايى ئېشەكلەردۇر (يەنى بۇ مۇشرىكلار مۇھەممەد ئەلەيھىسسالامنى كۆرگەندە، ئۇنىڭدىن خۇددى يولۋاسنى كۆرۈپ قاچقان ياۋا ئېشەكلەردەك قاچىدۇ)[50ـ51].",
      "footnotes": null
    },
    {
      "id": 52,
      "verse_number": 52,
      "page_number": 577,
      "verse_key": "74:52",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بَلۡ يُرِيدُ كُلُّ ٱمۡرِيٕٖ مِّنۡهُمۡ أَن يُؤۡتَىٰ صُحُفٗا مُّنَشَّرَةٗ",
      "translation": " بەلكى ئۇلاردىن ھەر كىشى ئۆزىگە ئوچۇق سەھىپىلەر بېرىلىشىنى خالايدۇ (يەنى ئۇلار ئاللاھنىڭ مۇھەممەد ئەلەيھىسسالامغا نازىل قىلىنغان كىتابىدەك كىتاب نازىل قىلىنىشىنى تەمە قىلىدۇ)[52].",
      "footnotes": null
    },
    {
      "id": 53,
      "verse_number": 53,
      "page_number": 577,
      "verse_key": "74:53",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّاۖ بَل لَّا يَخَافُونَ ٱلۡأٓخِرَةَ",
      "translation": "ياق (يەنى ئۇلار مۇنداق تەمەدىن يانسۇن). بەلكى ئۇلار (ئاخىرەتتىن) قورقمايدۇ[53].",
      "footnotes": null
    },
    {
      "id": 54,
      "verse_number": 54,
      "page_number": 577,
      "verse_key": "74:54",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّآ إِنَّهُۥ تَذۡكِرَةٞ",
      "translation": "ياق، مۇقەررەركى، ئۇ (يەنى قۇرئان) ۋەز ـ نەسىھەتتۇر (ئۇلار ئۆزلىرىنىڭ بەختىيار بولۇشىنى كۆزلەيدىغان بولسا، قۇرئان ئۇلارنىڭ ۋەز ـ نەسىھەت ئېلىشىغا يېتەرلىكتۇر)[54].",
      "footnotes": null
    },
    {
      "id": 55,
      "verse_number": 55,
      "page_number": 577,
      "verse_key": "74:55",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَمَن شَآءَ ذَكَرَهُۥ",
      "translation": "خالىغان ئادەم ئۇنىڭدىن ۋەز ـ نەسىھەت ئالىدۇ[55].",
      "footnotes": null
    },
    {
      "id": 56,
      "verse_number": 56,
      "page_number": 577,
      "verse_key": "74:56",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا يَذۡكُرُونَ إِلَّآ أَن يَشَآءَ ٱللَّهُۚ هُوَ أَهۡلُ ٱلتَّقۡوَىٰ وَأَهۡلُ ٱلۡمَغۡفِرَةِ",
      "translation": " ئۇلار پەقەت خالىسا، ئاندىن ۋەز ـ نەسىھەت ئالىدۇ، ئاللاھ (بەندىلىرى) قورقۇشقا تېگىشلىك زاتتۇر، (بەندىلىرىنى) مەغپىرەت قىلىشقا لايىق زاتتۇر[56].",
      "footnotes": null
    }
  ]
}