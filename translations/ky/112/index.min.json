{
  "verses": [
    {
      "number": "1",
      "arabic_text": "قُلۡ هُوَ ٱللَّهُ أَحَدٌ",
      "translation": "Айт (оо, Мухаммад:) Ал – Аллах жалгыз!"
    },
    {
      "number": "2",
      "arabic_text": "ٱللَّهُ ٱلصَّمَدُ",
      "translation": "Аллах – Сомад (Беймуктаж)"
    },
    {
      "number": "3",
      "arabic_text": "لَمۡ يَلِدۡ وَلَمۡ يُولَدۡ",
      "translation": "Ал туубаган жана да туулбаган!"
    },
    {
      "number": "4",
      "arabic_text": "وَلَمۡ يَكُن لَّهُۥ كُفُوًا أَحَدُۢ",
      "translation": "Жана эч ким Ага тең эмес!"
    }
  ]
}