{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 585,
      "verse_key": "80:1",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَبَسَ وَتَوَلَّىٰٓ",
      "translation": "Fuula guuree gara gale.",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 585,
      "verse_key": "80:2",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَن جَآءَهُ ٱلۡأَعۡمَىٰ",
      "translation": "Sababa jaamaan isatti dhufeef.",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 585,
      "verse_key": "80:3",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا يُدۡرِيكَ لَعَلَّهُۥ يَزَّكَّىٰٓ",
      "translation": "Maaltu si beeksise? Qulqullaa’uun isaa ni kajeelama.",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 585,
      "verse_key": "80:4",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَوۡ يَذَّكَّرُ فَتَنفَعَهُ ٱلذِّكۡرَىٰٓ",
      "translation": "Yookiin gorfameeti gorsi isa fayyaddi.",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 585,
      "verse_key": "80:5",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَمَّا مَنِ ٱسۡتَغۡنَىٰ",
      "translation": "Nama duroome immoo,",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 585,
      "verse_key": "80:6",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَنتَ لَهُۥ تَصَدَّىٰ",
      "translation": "Ati [akka amananuuf jecha] gara isaa gara galta.",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 585,
      "verse_key": "80:7",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا عَلَيۡكَ أَلَّا يَزَّكَّىٰ",
      "translation": "Haala qulqullaa'uu dhiisuun isaa homaa sirra hin jirreen.",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 585,
      "verse_key": "80:8",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَمَّا مَن جَآءَكَ يَسۡعَىٰ",
      "translation": "Nama fiigaa sitti dhufe immoo,",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 585,
      "verse_key": "80:9",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَهُوَ يَخۡشَىٰ",
      "translation": "Kan Rabbiin sodaatu ta’ee,",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 585,
      "verse_key": "80:10",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَنتَ عَنۡهُ تَلَهَّىٰ",
      "translation": "Ati isa irraa dagatta.",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 585,
      "verse_key": "80:11",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّآ إِنَّهَا تَذۡكِرَةٞ",
      "translation": "Dhorgami! Isheen gorsa.",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 585,
      "verse_key": "80:12",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَمَن شَآءَ ذَكَرَهُۥ",
      "translation": "Nama fedhetu isa (Qur'aana) yaadata.",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 585,
      "verse_key": "80:13",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِي صُحُفٖ مُّكَرَّمَةٖ",
      "translation": "Galmeewwan kabajamtuu keessa jira.",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 585,
      "verse_key": "80:14",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَّرۡفُوعَةٖ مُّطَهَّرَةِۭ",
      "translation": "Ol fuudhamtuu, qulqulleeffamtuu (keessa jira).",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 585,
      "verse_key": "80:15",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بِأَيۡدِي سَفَرَةٖ",
      "translation": "Harka barressitootaa (maleykotaa) jira.",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 585,
      "verse_key": "80:16",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كِرَامِۭ بَرَرَةٖ",
      "translation": "Kabajamoo, ajajamtoota (harka jira).",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 585,
      "verse_key": "80:17",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قُتِلَ ٱلۡإِنسَٰنُ مَآ أَكۡفَرَهُۥ",
      "translation": "Namni abaarame! Sila maaltu isa kafarsiise?",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 585,
      "verse_key": "80:18",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مِنۡ أَيِّ شَيۡءٍ خَلَقَهُۥ",
      "translation": "(Rabbiin) waan akkamii irraa isa uume? (hin xiinxaluu?)",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 585,
      "verse_key": "80:19",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مِن نُّطۡفَةٍ خَلَقَهُۥ فَقَدَّرَهُۥ",
      "translation": "Bishaan saalaa irraa isa uumeeti, ergasii isa sirreesse.",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 585,
      "verse_key": "80:20",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ ٱلسَّبِيلَ يَسَّرَهُۥ",
      "translation": "Ergasii karaa (inni ittiin dhalatu) isaaf laaffise.",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 585,
      "verse_key": "80:21",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ أَمَاتَهُۥ فَأَقۡبَرَهُۥ",
      "translation": "Ergasii isa ajjeeseti isa awwaalchise.",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 585,
      "verse_key": "80:22",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ إِذَا شَآءَ أَنشَرَهُۥ",
      "translation": "Ergasii yeroo fedhe isa kaasa.",
      "footnotes": ""
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 585,
      "verse_key": "80:23",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّا لَمَّا يَقۡضِ مَآ أَمَرَهُۥ",
      "translation": "Dhugumatti, (namni) waan Rabbiin isa ajaje ammallee hin raawwanne.",
      "footnotes": ""
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 585,
      "verse_key": "80:24",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلۡيَنظُرِ ٱلۡإِنسَٰنُ إِلَىٰ طَعَامِهِۦٓ",
      "translation": "Namni gara nyaata isaa haa ilaalu.",
      "footnotes": ""
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 585,
      "verse_key": "80:25",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَنَّا صَبَبۡنَا ٱلۡمَآءَ صَبّٗا",
      "translation": "Nuti bishaan buusiinsa baay’inaan buufne.",
      "footnotes": ""
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 585,
      "verse_key": "80:26",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ شَقَقۡنَا ٱلۡأَرۡضَ شَقّٗا",
      "translation": "Ergasii dachii dhodhoosiinsa dhodhoofne.",
      "footnotes": ""
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 585,
      "verse_key": "80:27",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَنۢبَتۡنَا فِيهَا حَبّٗا",
      "translation": " Achumaan Ishee keessatti firii magarsine.",
      "footnotes": ""
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 585,
      "verse_key": "80:28",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَعِنَبٗا وَقَضۡبٗا",
      "translation": "Inabaafi margas,",
      "footnotes": ""
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 585,
      "verse_key": "80:29",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَزَيۡتُونٗا وَنَخۡلٗا",
      "translation": "Zeytunaafi muka timiraas,",
      "footnotes": ""
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 585,
      "verse_key": "80:30",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَحَدَآئِقَ غُلۡبٗا",
      "translation": "Ashaakiltiilee muka baay’atanis,",
      "footnotes": ""
    },
    {
      "id": 31,
      "verse_number": 31,
      "page_number": 585,
      "verse_key": "80:31",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَفَٰكِهَةٗ وَأَبّٗا",
      "translation": "Fuduraafi kaloo [akka nuti uumne haa ilaalu].",
      "footnotes": ""
    },
    {
      "id": 32,
      "verse_number": 32,
      "page_number": 585,
      "verse_key": "80:32",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَّتَٰعٗا لَّكُمۡ وَلِأَنۡعَٰمِكُمۡ",
      "translation": "Isiniifi beylada keessanis qanansiisuuf.",
      "footnotes": ""
    },
    {
      "id": 33,
      "verse_number": 33,
      "page_number": 585,
      "verse_key": "80:33",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِذَا جَآءَتِ ٱلصَّآخَّةُ",
      "translation": "Yeroo gurra duuchituun (Qiyaamaan) dhufte,",
      "footnotes": ""
    },
    {
      "id": 34,
      "verse_number": 34,
      "page_number": 585,
      "verse_key": "80:34",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَوۡمَ يَفِرُّ ٱلۡمَرۡءُ مِنۡ أَخِيهِ",
      "translation": "Guyyaa namni obboleessa isaa irraa dheessu,",
      "footnotes": ""
    },
    {
      "id": 35,
      "verse_number": 35,
      "page_number": 585,
      "verse_key": "80:35",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأُمِّهِۦ وَأَبِيهِ",
      "translation": "Haadha isaatiifi abbaa isaa irraas,",
      "footnotes": ""
    },
    {
      "id": 36,
      "verse_number": 36,
      "page_number": 585,
      "verse_key": "80:36",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَصَٰحِبَتِهِۦ وَبَنِيهِ",
      "translation": "Niitii isaatiifi ijoollee isaa irraas (baqatu san dhufti).",
      "footnotes": ""
    },
    {
      "id": 37,
      "verse_number": 37,
      "page_number": 585,
      "verse_key": "80:37",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لِكُلِّ ٱمۡرِيٕٖ مِّنۡهُمۡ يَوۡمَئِذٖ شَأۡنٞ يُغۡنِيهِ",
      "translation": "Nama Isaan irraa ta’e hundaaf guyyaa sana dhimma isa duroomsutu jira.",
      "footnotes": ""
    },
    {
      "id": 38,
      "verse_number": 38,
      "page_number": 585,
      "verse_key": "80:38",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وُجُوهٞ يَوۡمَئِذٖ مُّسۡفِرَةٞ",
      "translation": "Fuulli gariin guyyaa san ni ifti.",
      "footnotes": ""
    },
    {
      "id": 39,
      "verse_number": 39,
      "page_number": 585,
      "verse_key": "80:39",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ضَاحِكَةٞ مُّسۡتَبۡشِرَةٞ",
      "translation": "Kolfituu, gammadduudha.",
      "footnotes": ""
    },
    {
      "id": 40,
      "verse_number": 40,
      "page_number": 585,
      "verse_key": "80:40",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَوُجُوهٞ يَوۡمَئِذٍ عَلَيۡهَا غَبَرَةٞ",
      "translation": "Fuulli gariin immoo guyyaa san dhukkeetu (quttootu) irra jira.",
      "footnotes": ""
    },
    {
      "id": 41,
      "verse_number": 41,
      "page_number": 585,
      "verse_key": "80:41",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تَرۡهَقُهَا قَتَرَةٌ",
      "translation": "Gurraachinni ishee haguuga.",
      "footnotes": ""
    },
    {
      "id": 42,
      "verse_number": 42,
      "page_number": 585,
      "verse_key": "80:42",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أُوْلَٰٓئِكَ هُمُ ٱلۡكَفَرَةُ ٱلۡفَجَرَةُ",
      "translation": "Isaan sun warra Rabbitti kafaranii, balleessa hojjataniidha.",
      "footnotes": ""
    }
  ]
}