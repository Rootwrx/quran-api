{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 577,
      "verse_key": "75:1",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَآ أُقۡسِمُ بِيَوۡمِ ٱلۡقِيَٰمَةِ",
      "translation": "Miti Guyyaa Qiyaamaatiinin kakadha.",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 577,
      "verse_key": "75:2",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَآ أُقۡسِمُ بِٱلنَّفۡسِ ٱللَّوَّامَةِ",
      "translation": "Lubbuu of komattuu taateenis nan kakadha; (mindaa galfamuuf jirtu).",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 577,
      "verse_key": "75:3",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَيَحۡسَبُ ٱلۡإِنسَٰنُ أَلَّن نَّجۡمَعَ عِظَامَهُۥ",
      "translation": "Sila namni lafee isaa walitti hin qabnuu yaadaa?",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 577,
      "verse_key": "75:4",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بَلَىٰ قَٰدِرِينَ عَلَىٰٓ أَن نُّسَوِّيَ بَنَانَهُۥ",
      "translation": "Eeyyeen (walitti ni qabna) (Nuti) qubbeen isaatuu (akka turanitti deebifnee) sirreessuu irratti danda'oodha.",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 577,
      "verse_key": "75:5",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بَلۡ يُرِيدُ ٱلۡإِنسَٰنُ لِيَفۡجُرَ أَمَامَهُۥ",
      "translation": "Dhugumatti namni fuuldura isaa (Qiyaamaa) sobsiisuu fedha.",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 577,
      "verse_key": "75:6",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَسۡـَٔلُ أَيَّانَ يَوۡمُ ٱلۡقِيَٰمَةِ",
      "translation": "\"Guyyaan Qiyaamaa yoomii?\"gaafata.",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 577,
      "verse_key": "75:7",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِذَا بَرِقَ ٱلۡبَصَرُ",
      "translation": "(Guyyaan sun) yeroo ijji dhamaatu.",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 577,
      "verse_key": "75:8",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَخَسَفَ ٱلۡقَمَرُ",
      "translation": "(Yeroo) jiini ifa dhabu (gaddiddaa’u)",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 577,
      "verse_key": "75:9",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَجُمِعَ ٱلشَّمۡسُ وَٱلۡقَمَرُ",
      "translation": "Aduufi jiinis walitti qabamu.",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 577,
      "verse_key": "75:10",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَقُولُ ٱلۡإِنسَٰنُ يَوۡمَئِذٍ أَيۡنَ ٱلۡمَفَرُّ",
      "translation": "Guyyaa san namni \"bakki baqaa eessa?\" jedha.",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 577,
      "verse_key": "75:11",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّا لَا وَزَرَ",
      "translation": "Dhorgamaa! (Guyyaa san) iddoon baqaa hin jiru.",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 577,
      "verse_key": "75:12",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَىٰ رَبِّكَ يَوۡمَئِذٍ ٱلۡمُسۡتَقَرُّ",
      "translation": "Guyyaa san bakki qubannaa gara Gooftaa keetiiti.",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 577,
      "verse_key": "75:13",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يُنَبَّؤُاْ ٱلۡإِنسَٰنُ يَوۡمَئِذِۭ بِمَا قَدَّمَ وَأَخَّرَ",
      "translation": "Namni guyyaa san wanti inni durseefii booda aanse itti himama.",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 577,
      "verse_key": "75:14",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بَلِ ٱلۡإِنسَٰنُ عَلَىٰ نَفۡسِهِۦ بَصِيرَةٞ",
      "translation": "Dhugumatti, namni lubbuu isaa irratti ragaadha.",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 577,
      "verse_key": "75:15",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَوۡ أَلۡقَىٰ مَعَاذِيرَهُۥ",
      "translation": "Odoma rakkoo adda addaa dhiyeeffateeyyuu.",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 577,
      "verse_key": "75:16",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَا تُحَرِّكۡ بِهِۦ لِسَانَكَ لِتَعۡجَلَ بِهِۦٓ",
      "translation": "Isa (qabachuu)tti ariifachuuf jecha arraba kee isaan hin sochoosin.",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 577,
      "verse_key": "75:17",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ عَلَيۡنَا جَمۡعَهُۥ وَقُرۡءَانَهُۥ",
      "translation": "Isa (onnee kee keessatti) walitti qabuufi isa dubbisuun kee nurra jira.",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 577,
      "verse_key": "75:18",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِذَا قَرَأۡنَٰهُ فَٱتَّبِعۡ قُرۡءَانَهُۥ",
      "translation": "Yeroo Nuti isa qaraane qiraatii isaa hordofi.",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 577,
      "verse_key": "75:19",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ إِنَّ عَلَيۡنَا بَيَانَهُۥ",
      "translation": "Ergasii isa ibsuun Nurra jira.",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 578,
      "verse_key": "75:20",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّا بَلۡ تُحِبُّونَ ٱلۡعَاجِلَةَ",
      "translation": "Miti isin ariifattuu (duuniyaa) jaalattu.",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 578,
      "verse_key": "75:21",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَتَذَرُونَ ٱلۡأٓخِرَةَ",
      "translation": "Aakhiraa ni dhiiftu.",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 578,
      "verse_key": "75:22",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وُجُوهٞ يَوۡمَئِذٖ نَّاضِرَةٌ",
      "translation": "Fuulleen gariin Guyyaa san iftuudha.",
      "footnotes": ""
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 578,
      "verse_key": "75:23",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَىٰ رَبِّهَا نَاظِرَةٞ",
      "translation": "Gara Gooftaa ishee ilaaltuudha.",
      "footnotes": ""
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 578,
      "verse_key": "75:24",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَوُجُوهٞ يَوۡمَئِذِۭ بَاسِرَةٞ",
      "translation": "Fuulli gariin Guyyaa san akkaan suntuurtuudha.",
      "footnotes": ""
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 578,
      "verse_key": "75:25",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تَظُنُّ أَن يُفۡعَلَ بِهَا فَاقِرَةٞ",
      "translation": "Balaan guddaan ishee irratti raawwatamuu yaaddi.",
      "footnotes": ""
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 578,
      "verse_key": "75:26",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّآ إِذَا بَلَغَتِ ٱلتَّرَاقِيَ",
      "translation": "Miti Dhaga’aa! Yeroo (lubbuun ba'uuf) kokkee geesse,",
      "footnotes": ""
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 578,
      "verse_key": "75:27",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَقِيلَ مَنۡۜ رَاقٖ",
      "translation": "(Yeroo) \"eenyutu isa yaala?\" jedhame,",
      "footnotes": ""
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 578,
      "verse_key": "75:28",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَظَنَّ أَنَّهُ ٱلۡفِرَاقُ",
      "translation": "(Namni du'aa jiru sun) inni (addunyaa kana irraa) adda ba'uu dha’ jechuu yeroo mirkaneeffate;",
      "footnotes": ""
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 578,
      "verse_key": "75:29",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡتَفَّتِ ٱلسَّاقُ بِٱلسَّاقِ",
      "translation": "(Yeroo) mogoleen mogoleetti maramte.",
      "footnotes": ""
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 578,
      "verse_key": "75:30",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَىٰ رَبِّكَ يَوۡمَئِذٍ ٱلۡمَسَاقُ",
      "translation": "Guyyaa san oofamiinsi gara Gooftaa keetiiti.",
      "footnotes": ""
    },
    {
      "id": 31,
      "verse_number": 31,
      "page_number": 578,
      "verse_key": "75:31",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلَا صَدَّقَ وَلَا صَلَّىٰ",
      "translation": "Hin amanne; hin salaanne.",
      "footnotes": ""
    },
    {
      "id": 32,
      "verse_number": 32,
      "page_number": 578,
      "verse_key": "75:32",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَٰكِن كَذَّبَ وَتَوَلَّىٰ",
      "translation": "Garuu kijibsiisee irraa gara gale.",
      "footnotes": ""
    },
    {
      "id": 33,
      "verse_number": 33,
      "page_number": 578,
      "verse_key": "75:33",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ ذَهَبَ إِلَىٰٓ أَهۡلِهِۦ يَتَمَطَّىٰٓ",
      "translation": "Ergasii boonaa gara maatii isaa deeme.",
      "footnotes": ""
    },
    {
      "id": 34,
      "verse_number": 34,
      "page_number": 578,
      "verse_key": "75:34",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَوۡلَىٰ لَكَ فَأَوۡلَىٰ",
      "translation": "Badiin siif haa ta’u! Ammas badiin!",
      "footnotes": ""
    },
    {
      "id": 35,
      "verse_number": 35,
      "page_number": 578,
      "verse_key": "75:35",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ أَوۡلَىٰ لَكَ فَأَوۡلَىٰٓ",
      "translation": "Sana booda badiin siif haa ta’u! Ammas badiin!",
      "footnotes": ""
    },
    {
      "id": 36,
      "verse_number": 36,
      "page_number": 578,
      "verse_key": "75:36",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَيَحۡسَبُ ٱلۡإِنسَٰنُ أَن يُتۡرَكَ سُدًى",
      "translation": "Sila namni akkanumaan dhiifamuu yaadaa?",
      "footnotes": ""
    },
    {
      "id": 37,
      "verse_number": 37,
      "page_number": 578,
      "verse_key": "75:37",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَمۡ يَكُ نُطۡفَةٗ مِّن مَّنِيّٖ يُمۡنَىٰ",
      "translation": "Sila inni bishaan saalaa kan dhangalaafamu irraa ta'ee hin turree?",
      "footnotes": ""
    },
    {
      "id": 38,
      "verse_number": 38,
      "page_number": 578,
      "verse_key": "75:38",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ كَانَ عَلَقَةٗ فَخَلَقَ فَسَوَّىٰ",
      "translation": "Ergasii dhiiga ititaa ta'eeti (nama godhee) uumee guute.",
      "footnotes": ""
    },
    {
      "id": 39,
      "verse_number": 39,
      "page_number": 578,
      "verse_key": "75:39",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَجَعَلَ مِنۡهُ ٱلزَّوۡجَيۡنِ ٱلذَّكَرَ وَٱلۡأُنثَىٰٓ",
      "translation": "Isa irraas cimdii lama, dhiiraafi dhalaa, taasise.",
      "footnotes": ""
    },
    {
      "id": 40,
      "verse_number": 40,
      "page_number": 578,
      "verse_key": "75:40",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَيۡسَ ذَٰلِكَ بِقَٰدِرٍ عَلَىٰٓ أَن يُحۡـِۧيَ ٱلۡمَوۡتَىٰ",
      "translation": "Sila (Rabbiin akkas godhe) sun du'aa jiraachisuu irratti danda'aa mitii?",
      "footnotes": ""
    }
  ]
}