{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 600,
      "verse_key": "101:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلۡقَارِعَةُ",
      "translation": "1.Garaacdada (qalbiyada waa Qiyaamada).",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 600,
      "verse_key": "101:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَا ٱلۡقَارِعَةُ",
      "translation": "2. Maxay tahay Garaacdadu?.",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 600,
      "verse_key": "101:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَآ أَدۡرَىٰكَ مَا ٱلۡقَارِعَةُ",
      "translation": "3. Maxaa ku ogeysiiyeyna waxay tahay Garaacdadu?",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 600,
      "verse_key": "101:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَوۡمَ يَكُونُ ٱلنَّاسُ كَٱلۡفَرَاشِ ٱلۡمَبۡثُوثِ",
      "translation": "4. Waa Maalintay dadku noqon  sida baalallay kala firdhay.",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 600,
      "verse_key": "101:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَتَكُونُ ٱلۡجِبَالُ كَٱلۡعِهۡنِ ٱلۡمَنفُوشِ",
      "translation": "5. Oo buurahu noqon sida cudbi la dhilay.",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 600,
      "verse_key": "101:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَمَّا مَن ثَقُلَتۡ مَوَٰزِينُهُۥ",
      "translation": "6. Qofka miisaankiisa (camallada wanaagsan) uu culeys yahay.",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 600,
      "verse_key": "101:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَهُوَ فِي عِيشَةٖ رَّاضِيَةٖ",
      "translation": "7. Wuxuu ku noolaan nolol barwaaqo iyo farxad leh oo raalli lagu yahay (Janno).",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 600,
      "verse_key": "101:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَمَّا مَنۡ خَفَّتۡ مَوَٰزِينُهُۥ",
      "translation": "8. Qofkase uu miisaankiisa (camallada wanaagsan) fudeyd yahay.",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 600,
      "verse_key": "101:9",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأُمُّهُۥ هَاوِيَةٞ",
      "translation": "9. Hoygiisu waa Haawiya.",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 600,
      "verse_key": "101:10",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَآ أَدۡرَىٰكَ مَا هِيَهۡ",
      "translation": "10. Maxaa ku ogeysiiyeyna waxay tahay?",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 600,
      "verse_key": "101:11",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "نَارٌ حَامِيَةُۢ",
      "translation": "11. (Waa) Naar hureysa oo kuleyl badan.",
      "footnotes": ""
    }
  ]
}