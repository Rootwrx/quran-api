{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 596,
      "verse_key": "94:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَمۡ نَشۡرَحۡ لَكَ صَدۡرَكَ",
      "translation": "Não te dilatamos o peito?",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 596,
      "verse_key": "94:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَوَضَعۡنَا عَنكَ وِزۡرَكَ",
      "translation": "E não te depusemos o fardo,",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 596,
      "verse_key": "94:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِيٓ أَنقَضَ ظَهۡرَكَ",
      "translation": "Que te vergava as costas?",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 596,
      "verse_key": "94:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَرَفَعۡنَا لَكَ ذِكۡرَكَ",
      "translation": "E não te elevamos a fama?",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 596,
      "verse_key": "94:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِنَّ مَعَ ٱلۡعُسۡرِ يُسۡرًا",
      "translation": "Então, por certo, com a dificuldade, há facilidade!",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 596,
      "verse_key": "94:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ مَعَ ٱلۡعُسۡرِ يُسۡرٗا",
      "translation": "Por certo, com a dificuldade, há facilidade!",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 596,
      "verse_key": "94:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِذَا فَرَغۡتَ فَٱنصَبۡ",
      "translation": "Então, quando estiverdes livre, esforça-te em orar,",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 596,
      "verse_key": "94:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِلَىٰ رَبِّكَ فَٱرۡغَب",
      "translation": "E dirige-te a teu Senhor em rogos.",
      "footnotes": ""
    }
  ]
}