{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 599,
      "verse_key": "100:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡعَٰدِيَٰتِ ضَبۡحٗا",
      "translation": "Allāhu fi àwọn ẹṣin tó ń sáré tó ń mí hẹlẹhẹlẹ ní ojú-ogun búra.",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 599,
      "verse_key": "100:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَٱلۡمُورِيَٰتِ قَدۡحٗا",
      "translation": "Ó tún fi àwọn ẹṣin tí pátákò ẹsẹ̀ wọn ń ṣáná (níbi eré sísá) búra.",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 599,
      "verse_key": "100:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَٱلۡمُغِيرَٰتِ صُبۡحٗا",
      "translation": "Ó tún fi àwọn ẹṣin tó ń kọlu ọ̀tá ẹ̀sìn ní òwúrọ̀ kùtùkùtù búra.",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 599,
      "verse_key": "100:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَثَرۡنَ بِهِۦ نَقۡعٗا",
      "translation": "Wọ́n sì fi (pátákò ẹsẹ̀ wọn) tu eruku (ilẹ̀ ọ̀tá) sókè.",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 599,
      "verse_key": "100:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَوَسَطۡنَ بِهِۦ جَمۡعًا",
      "translation": "Wọ́n tún bẹ́ gìjà pẹ̀lú eruku ẹsẹ̀ wọn sáààrin àkójọ ọ̀tá.",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 599,
      "verse_key": "100:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ ٱلۡإِنسَٰنَ لِرَبِّهِۦ لَكَنُودٞ",
      "translation": "Dájúdájú ènìyàn ni aláìmoore sí Olúwa rẹ̀.",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 599,
      "verse_key": "100:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِنَّهُۥ عَلَىٰ ذَٰلِكَ لَشَهِيدٞ",
      "translation": "Dájúdájú Allāhu sì ń jẹ́ Ẹlẹ́rìí lórí ìyẹn.",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 599,
      "verse_key": "100:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِنَّهُۥ لِحُبِّ ٱلۡخَيۡرِ لَشَدِيدٌ",
      "translation": "Àti pé dájúdájú ènìyàn le gan-an níbi ìfẹ́ oore ayé.",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 599,
      "verse_key": "100:9",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "۞ أَفَلَا يَعۡلَمُ إِذَا بُعۡثِرَ مَا فِي ٱلۡقُبُورِ",
      "translation": "Ṣé kò mọ̀ pé nígbà tí wọ́n bá tú ohun tó wà nínú sàréè jáde (fún àjíǹde),",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 600,
      "verse_key": "100:10",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَحُصِّلَ مَا فِي ٱلصُّدُورِ",
      "translation": "tí wọ́n sì tú ohun tó wà nínú igbá-àyà ẹ̀dá síta pátápátá,",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 600,
      "verse_key": "100:11",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ رَبَّهُم بِهِمۡ يَوۡمَئِذٖ لَّخَبِيرُۢ",
      "translation": "dájúdájú Olúwa wọn ni Alámọ̀tán nípa wọn ní Ọjọ́ yẹn?",
      "footnotes": ""
    }
  ]
}