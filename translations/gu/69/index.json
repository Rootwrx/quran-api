{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 566,
      "verse_key": "69:1",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلۡحَآقَّةُ",
      "translation": "૧. સાબિત થવાવાળી",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 566,
      "verse_key": "69:2",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَا ٱلۡحَآقَّةُ",
      "translation": "૨. સાબિત થવાવાળી શું છે?",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 566,
      "verse_key": "69:3",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَآ أَدۡرَىٰكَ مَا ٱلۡحَآقَّةُ",
      "translation": "૩. અને તમને શું ખબર કે તે સાબિત થનાર શું છે?",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 566,
      "verse_key": "69:4",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَذَّبَتۡ ثَمُودُ وَعَادُۢ بِٱلۡقَارِعَةِ",
      "translation": "૪. ષમૂદ અને આદની કોમે ખખડાવી દેનારી (કયામત)ને જુઠલાવી હતી.",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 566,
      "verse_key": "69:5",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَمَّا ثَمُودُ فَأُهۡلِكُواْ بِٱلطَّاغِيَةِ",
      "translation": "૫. (જેના પરિણામ રૂપે) ષમૂદને ભયજનક અવાજ વડે નષ્ટ કરવામાં આવ્યા.",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 566,
      "verse_key": "69:6",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَمَّا عَادٞ فَأُهۡلِكُواْ بِرِيحٖ صَرۡصَرٍ عَاتِيَةٖ",
      "translation": "૬. અને આદને તીવ્ર તોફાની આંધીથી નષ્ટ કરી દેવામાં આવ્યા.",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 566,
      "verse_key": "69:7",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "سَخَّرَهَا عَلَيۡهِمۡ سَبۡعَ لَيَالٖ وَثَمَٰنِيَةَ أَيَّامٍ حُسُومٗاۖ فَتَرَى ٱلۡقَوۡمَ فِيهَا صَرۡعَىٰ كَأَنَّهُمۡ أَعۡجَازُ نَخۡلٍ خَاوِيَةٖ",
      "translation": "૭. જેને તેમના પર નિરંતર સાત રાત્રિઓ અને આઠ દિવસ સુધી (અલ્લાહએ) છવાયેલી રાખી. (જો તમે ત્યાં હોત તો) તમે જોતા કે તે લોકો જમીન પર એવી રીતે ઊંધા પડયા હતા, જેવી રીતે કે ખજૂરના ખોખલા થડ હોય.",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 566,
      "verse_key": "69:8",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَهَلۡ تَرَىٰ لَهُم مِّنۢ بَاقِيَةٖ",
      "translation": "૮. શું તમે તેમના માંથી કોઇ પણ બાકી જુઓ છો?",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 567,
      "verse_key": "69:9",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَجَآءَ فِرۡعَوۡنُ وَمَن قَبۡلَهُۥ وَٱلۡمُؤۡتَفِكَٰتُ بِٱلۡخَاطِئَةِ",
      "translation": "૯. ફિરઔન અને તેનાથી પહેલાના લોકો અને જેમની વસ્તીઓ પલટાવી નાખી, સૌ ગુનાહના કાર્યો કરતા હતા.",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 567,
      "verse_key": "69:10",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَعَصَوۡاْ رَسُولَ رَبِّهِمۡ فَأَخَذَهُمۡ أَخۡذَةٗ رَّابِيَةً",
      "translation": "૧૦. તે સૌએ પોતાના પાલનહારના પયગંબરની અવગણના કરી. (છેવટે) અલ્લાહ એ તેઓને (પણ) સખત પકડમાં લઇ લીધા.",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 567,
      "verse_key": "69:11",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّا لَمَّا طَغَا ٱلۡمَآءُ حَمَلۡنَٰكُمۡ فِي ٱلۡجَارِيَةِ",
      "translation": "૧૧. જ્યારે પાણીનું તોફાન વધારે થયું તો તે સમયે અમે જ તમને હોળીમાં સવાર કરી દીધા હતા.",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 567,
      "verse_key": "69:12",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لِنَجۡعَلَهَا لَكُمۡ تَذۡكِرَةٗ وَتَعِيَهَآ أُذُنٞ وَٰعِيَةٞ",
      "translation": "૧૨. જેથી અમે તેને તમારા માટે શિખામણ અને યાદગાર બનાવી દઈએ અને (જેથી) યાદ રાખવાવાળા કાન તેને યાદ રાખે.",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 567,
      "verse_key": "69:13",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِذَا نُفِخَ فِي ٱلصُّورِ نَفۡخَةٞ وَٰحِدَةٞ",
      "translation": "૧૩. બસ! જ્યારે સૂરમાં એક ફૂંક મારવામાં આવશે.",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 567,
      "verse_key": "69:14",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَحُمِلَتِ ٱلۡأَرۡضُ وَٱلۡجِبَالُ فَدُكَّتَا دَكَّةٗ وَٰحِدَةٗ",
      "translation": "૧૪. અને જમીન તથા પર્વતો ઉઠાવી લેવામાં આવશે. અને એક જ પ્રહારમાં ચૂરેચૂરા કરી દેવામાં આવશે.",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 567,
      "verse_key": "69:15",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَيَوۡمَئِذٖ وَقَعَتِ ٱلۡوَاقِعَةُ",
      "translation": "૧૫. તે દિવસે સાબિત થવાવાળી (કયામત) થઇને રહેશે.",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 567,
      "verse_key": "69:16",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱنشَقَّتِ ٱلسَّمَآءُ فَهِيَ يَوۡمَئِذٖ وَاهِيَةٞ",
      "translation": "૧૬. અને આકશ ફાટી જશે, અને તે દિવસે ઘણું જ નબળુ પડી જશે.",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 567,
      "verse_key": "69:17",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡمَلَكُ عَلَىٰٓ أَرۡجَآئِهَاۚ وَيَحۡمِلُ عَرۡشَ رَبِّكَ فَوۡقَهُمۡ يَوۡمَئِذٖ ثَمَٰنِيَةٞ",
      "translation": "૧૭. તેના કિનારાઓ પર ફરિશ્તાઓ હશે, અને તે દિવસે આઠ (ફરિશ્તાઓ) તમારા પાલનાહારના અર્શને પોતાના ઉપર ઉઠાવીને રાખ્યું હશે.",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 567,
      "verse_key": "69:18",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَوۡمَئِذٖ تُعۡرَضُونَ لَا تَخۡفَىٰ مِنكُمۡ خَافِيَةٞ",
      "translation": "૧૮. તે દિવસે તમે સૌ (અલ્લાહની સમક્ષ) રજૂ કરવામાં આવશો, તમારુ કોઇ રહસ્ય છૂપાયેલું નહી રહે.",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 567,
      "verse_key": "69:19",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَمَّا مَنۡ أُوتِيَ كِتَٰبَهُۥ بِيَمِينِهِۦ فَيَقُولُ هَآؤُمُ ٱقۡرَءُواْ كِتَٰبِيَهۡ",
      "translation": "૧૯. પછી જેને તેનું કર્મપત્ર જમણા હાથમાં આપવામાં આવશે, તો તે કહેવા લાગશે કે “ લો મારુ કર્મપત્ર વાંચી લો”.",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 567,
      "verse_key": "69:20",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنِّي ظَنَنتُ أَنِّي مُلَٰقٍ حِسَابِيَهۡ",
      "translation": "૨૦. મને તો સંપૂર્ણ વિશ્ર્વાસ હતો કે મને મારો હિસાબ મળવાનો છે.",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 567,
      "verse_key": "69:21",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَهُوَ فِي عِيشَةٖ رَّاضِيَةٖ",
      "translation": "૨૧. બસ! તે એક મનગમતા જીવનમાં હશે.",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 567,
      "verse_key": "69:22",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِي جَنَّةٍ عَالِيَةٖ",
      "translation": "૨૨. ઉચ્ચ દરજ્જાવાળી જન્નતમાં",
      "footnotes": ""
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 567,
      "verse_key": "69:23",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قُطُوفُهَا دَانِيَةٞ",
      "translation": "૨૩. જેના ગુચ્છા નમી પડેલા હશે.",
      "footnotes": ""
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 567,
      "verse_key": "69:24",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كُلُواْ وَٱشۡرَبُواْ هَنِيٓـَٔۢا بِمَآ أَسۡلَفۡتُمۡ فِي ٱلۡأَيَّامِ ٱلۡخَالِيَةِ",
      "translation": "૨૪. (તેમને કહેવામાં આવશે) કે આનંદથી ખાઓ પીઓ, પોતાના તે કર્મોના બદલામાં, જે તમે વિતેલા દિવસોમાં કર્યા.",
      "footnotes": ""
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 567,
      "verse_key": "69:25",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَمَّا مَنۡ أُوتِيَ كِتَٰبَهُۥ بِشِمَالِهِۦ فَيَقُولُ يَٰلَيۡتَنِي لَمۡ أُوتَ كِتَٰبِيَهۡ",
      "translation": "૨૫. પરંતુ જેનું કર્મપત્ર તેના ડાબા હાથમાં આપવામાં આવશે તો તે કહશે કે “ કાશ મને મારુ કર્મપત્ર આપવામાં જ ન આવ્યુ હોત”.",
      "footnotes": ""
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 567,
      "verse_key": "69:26",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَمۡ أَدۡرِ مَا حِسَابِيَهۡ",
      "translation": "૨૬. અને હું જાણતો જ ન હોત કે હિસાબ શું છે.",
      "footnotes": ""
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 567,
      "verse_key": "69:27",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَٰلَيۡتَهَا كَانَتِ ٱلۡقَاضِيَةَ",
      "translation": "૨૭. કાશ! કે મૃત્યુ (મારુ) કામ પુરૂ કરી દેત.",
      "footnotes": ""
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 567,
      "verse_key": "69:28",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَآ أَغۡنَىٰ عَنِّي مَالِيَهۡۜ",
      "translation": "૨૮. મારૂ ધન પણ મને કંઇ ફાયદો ન પહોંચાડી શક્યુ.",
      "footnotes": ""
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 567,
      "verse_key": "69:29",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "هَلَكَ عَنِّي سُلۡطَٰنِيَهۡ",
      "translation": "૨૯. મારી સત્તા પણ બરબાદ થઇ ગઈ.",
      "footnotes": ""
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 567,
      "verse_key": "69:30",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "خُذُوهُ فَغُلُّوهُ",
      "translation": "૩૦. આદેશ આપવામાં આવશે, તેને પકડી લો પછી તેને ગાળિયું પહેરાવી દો,",
      "footnotes": ""
    },
    {
      "id": 31,
      "verse_number": 31,
      "page_number": 567,
      "verse_key": "69:31",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ ٱلۡجَحِيمَ صَلُّوهُ",
      "translation": "૩૧. પછી તેને જહન્નમમાં નાખી દો.",
      "footnotes": ""
    },
    {
      "id": 32,
      "verse_number": 32,
      "page_number": 567,
      "verse_key": "69:32",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ فِي سِلۡسِلَةٖ ذَرۡعُهَا سَبۡعُونَ ذِرَاعٗا فَٱسۡلُكُوهُ",
      "translation": "૩૨. પછી તેને સિત્તેર હાથ લાંબી સાંકળમાં બાંધી દો.",
      "footnotes": ""
    },
    {
      "id": 33,
      "verse_number": 33,
      "page_number": 567,
      "verse_key": "69:33",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُۥ كَانَ لَا يُؤۡمِنُ بِٱللَّهِ ٱلۡعَظِيمِ",
      "translation": "૩૩. નિ:શંક તે મહાનતાવાળા અલ્લાહ પર ઇમાન નહતો રાખતો.",
      "footnotes": ""
    },
    {
      "id": 34,
      "verse_number": 34,
      "page_number": 567,
      "verse_key": "69:34",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَا يَحُضُّ عَلَىٰ طَعَامِ ٱلۡمِسۡكِينِ",
      "translation": "૩૪. અને લાચારને ખવડાવવા માટે પ્રોત્સાહન નહતો આપતો.",
      "footnotes": ""
    },
    {
      "id": 35,
      "verse_number": 35,
      "page_number": 568,
      "verse_key": "69:35",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلَيۡسَ لَهُ ٱلۡيَوۡمَ هَٰهُنَا حَمِيمٞ",
      "translation": "૩૫. બસ! આજે તેનો કોઈ મિત્ર નહીં હોય.",
      "footnotes": ""
    },
    {
      "id": 36,
      "verse_number": 36,
      "page_number": 568,
      "verse_key": "69:36",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَا طَعَامٌ إِلَّا مِنۡ غِسۡلِينٖ",
      "translation": "૩૬. અને પરૂ સિવાય તેને કઈ ભોજન નહીં મળે.",
      "footnotes": ""
    },
    {
      "id": 37,
      "verse_number": 37,
      "page_number": 568,
      "verse_key": "69:37",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَّا يَأۡكُلُهُۥٓ إِلَّا ٱلۡخَٰطِـُٔونَ",
      "translation": "૩૭. જેને ગુનેગાર સિવાય કોઇપણ નહીં ખાય.",
      "footnotes": ""
    },
    {
      "id": 38,
      "verse_number": 38,
      "page_number": 568,
      "verse_key": "69:38",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلَآ أُقۡسِمُ بِمَا تُبۡصِرُونَ",
      "translation": "૩૮. બસ! હું તે વસ્તુની પણ કસમ ખાઉ છું, જે તમે જુવો છો.",
      "footnotes": ""
    },
    {
      "id": 39,
      "verse_number": 39,
      "page_number": 568,
      "verse_key": "69:39",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا لَا تُبۡصِرُونَ",
      "translation": "૩૯. અને તે વસ્તુઓની પણ, જે તમે જોતા નથી.",
      "footnotes": ""
    },
    {
      "id": 40,
      "verse_number": 40,
      "page_number": 568,
      "verse_key": "69:40",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُۥ لَقَوۡلُ رَسُولٖ كَرِيمٖ",
      "translation": "૪૦. કે નિ:શંક આ (કુરઆન) પ્રતિષ્ઠિત પયગંબરની જબાન વડે પહોચાડવામાં આવ્યું છે.",
      "footnotes": ""
    },
    {
      "id": 41,
      "verse_number": 41,
      "page_number": 568,
      "verse_key": "69:41",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا هُوَ بِقَوۡلِ شَاعِرٖۚ قَلِيلٗا مَّا تُؤۡمِنُونَ",
      "translation": "૪૧. આ કોઇ કવિનું કથન નથી (અફસોસ) તમે ભાગ્યે જ ઇમાન લાવો છો.",
      "footnotes": ""
    },
    {
      "id": 42,
      "verse_number": 42,
      "page_number": 568,
      "verse_key": "69:42",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَا بِقَوۡلِ كَاهِنٖۚ قَلِيلٗا مَّا تَذَكَّرُونَ",
      "translation": "૪૨. અને ન તો આ કોઇ જયોતિષનું કથન છે. (અફસોસ) તમે ભાગ્યે જ શિખામણ પ્રાપ્ત કરો છો.",
      "footnotes": ""
    },
    {
      "id": 43,
      "verse_number": 43,
      "page_number": 568,
      "verse_key": "69:43",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تَنزِيلٞ مِّن رَّبِّ ٱلۡعَٰلَمِينَ",
      "translation": "૪૩. (આ તો) જગતના પાલનહારે ઉતાર્યું છે.",
      "footnotes": ""
    },
    {
      "id": 44,
      "verse_number": 44,
      "page_number": 568,
      "verse_key": "69:44",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَوۡ تَقَوَّلَ عَلَيۡنَا بَعۡضَ ٱلۡأَقَاوِيلِ",
      "translation": "૪૪. અને જો આ (મુહમ્મદ પયગંબર) અમારા પર કોઇપણ વાત ઘડી લેતો,",
      "footnotes": ""
    },
    {
      "id": 45,
      "verse_number": 45,
      "page_number": 568,
      "verse_key": "69:45",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَأَخَذۡنَا مِنۡهُ بِٱلۡيَمِينِ",
      "translation": "૪૫. તો જરૂરથી અમે તેનો જમણો હાથ પકડી લેતા.",
      "footnotes": ""
    },
    {
      "id": 46,
      "verse_number": 46,
      "page_number": 568,
      "verse_key": "69:46",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ لَقَطَعۡنَا مِنۡهُ ٱلۡوَتِينَ",
      "translation": "૪૬. પછી તેની ધોરી નસ કાપી નાખતા.",
      "footnotes": ""
    },
    {
      "id": 47,
      "verse_number": 47,
      "page_number": 568,
      "verse_key": "69:47",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَمَا مِنكُم مِّنۡ أَحَدٍ عَنۡهُ حَٰجِزِينَ",
      "translation": "૪૭. પછી તમારામાંથી કોઇ પણ અમને આ કામથી રોકનાર ન હોત.",
      "footnotes": ""
    },
    {
      "id": 48,
      "verse_number": 48,
      "page_number": 568,
      "verse_key": "69:48",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِنَّهُۥ لَتَذۡكِرَةٞ لِّلۡمُتَّقِينَ",
      "translation": "૪૮. નિ:શંક આ કુરઆન ડરવાવાળાઓ માટે શિખામણ છે.",
      "footnotes": ""
    },
    {
      "id": 49,
      "verse_number": 49,
      "page_number": 568,
      "verse_key": "69:49",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِنَّا لَنَعۡلَمُ أَنَّ مِنكُم مُّكَذِّبِينَ",
      "translation": "૪૯. અને અમને સારી રીતે જાણીએ છીએ કે તમારામાંથી કેટલાક તેને જુઠલાવનારા છે.",
      "footnotes": ""
    },
    {
      "id": 50,
      "verse_number": 50,
      "page_number": 568,
      "verse_key": "69:50",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِنَّهُۥ لَحَسۡرَةٌ عَلَى ٱلۡكَٰفِرِينَ",
      "translation": "૫૦. નિ:શંક (આ જુઠલાવવુ) ઇન્કારીઓ ઉપર ખેદ છે.",
      "footnotes": ""
    },
    {
      "id": 51,
      "verse_number": 51,
      "page_number": 568,
      "verse_key": "69:51",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِنَّهُۥ لَحَقُّ ٱلۡيَقِينِ",
      "translation": "૫૧. અને નિ:શંક આ તદ્દન વિશ્ર્વાસનિય સત્ય છે.",
      "footnotes": ""
    },
    {
      "id": 52,
      "verse_number": 52,
      "page_number": 568,
      "verse_key": "69:52",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَسَبِّحۡ بِٱسۡمِ رَبِّكَ ٱلۡعَظِيمِ",
      "translation": "૫૨. બસ! (હે નબી) તમે પોતાના મહાન પાલનહારની પવિત્રતા બયાન કરો.",
      "footnotes": ""
    }
  ]
}