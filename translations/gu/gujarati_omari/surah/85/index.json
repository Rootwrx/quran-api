{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 590,
      "verse_key": "85:1",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلسَّمَآءِ ذَاتِ ٱلۡبُرُوجِ",
      "translation": "૧. બુરૂજોવાળા આકાશની કસમ!",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 590,
      "verse_key": "85:2",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡيَوۡمِ ٱلۡمَوۡعُودِ",
      "translation": "૨. અને તે દિવસની, જેનું વચન આપવામાં આવ્યું છે.",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 590,
      "verse_key": "85:3",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَشَاهِدٖ وَمَشۡهُودٖ",
      "translation": "૩. હાજર થવાવાળા અને હાજર કરેલાની કસમ!",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 590,
      "verse_key": "85:4",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قُتِلَ أَصۡحَٰبُ ٱلۡأُخۡدُودِ",
      "translation": "૪. અલ્લાહની લઅનત છે, તે ખાડા (ખોદનાર) લોકો પર.",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 590,
      "verse_key": "85:5",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلنَّارِ ذَاتِ ٱلۡوَقُودِ",
      "translation": "૫. જેમાં ઇંધણવાળી આગ હતી.",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 590,
      "verse_key": "85:6",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذۡ هُمۡ عَلَيۡهَا قُعُودٞ",
      "translation": "૬. જ્યારે કે તે લોકો તેની આજુબાજુ બેઠા હતા.",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 590,
      "verse_key": "85:7",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَهُمۡ عَلَىٰ مَا يَفۡعَلُونَ بِٱلۡمُؤۡمِنِينَ شُهُودٞ",
      "translation": "૭. અને જે કઈ ઈમાનવાળાઓ સાથે કરી રહ્યા હતા, તેને પોતાની સામે જોઇ રહ્યા હતા.",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 590,
      "verse_key": "85:8",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا نَقَمُواْ مِنۡهُمۡ إِلَّآ أَن يُؤۡمِنُواْ بِٱللَّهِ ٱلۡعَزِيزِ ٱلۡحَمِيدِ",
      "translation": "૮. અને તે લોકોને ઇમાનવાળાઓની આ જ વાત ખરાબ લાગતી હતી કે તેઓ અલ્લાહ પર ઈમાન લાવ્યા હતા, જે પ્રભુત્વશાળી અને દરેક પ્રશંસાને લાયક છે.",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 590,
      "verse_key": "85:9",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِي لَهُۥ مُلۡكُ ٱلسَّمَٰوَٰتِ وَٱلۡأَرۡضِۚ وَٱللَّهُ عَلَىٰ كُلِّ شَيۡءٖ شَهِيدٌ",
      "translation": "૯. આકાશો અને જમીન પર બાદશાહત તેની જ છે અને દરેક વસ્તુ અલ્લાહ તઆલાની સામે જ છે.",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 590,
      "verse_key": "85:10",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ ٱلَّذِينَ فَتَنُواْ ٱلۡمُؤۡمِنِينَ وَٱلۡمُؤۡمِنَٰتِ ثُمَّ لَمۡ يَتُوبُواْ فَلَهُمۡ عَذَابُ جَهَنَّمَ وَلَهُمۡ عَذَابُ ٱلۡحَرِيقِ",
      "translation": "૧૦. જે લોકોએ મોમિન પુરૂષો અને સ્ત્રીઓ ઉપર અત્યાચાર કર્યો, પછી તૌબા (પણ) ન કરી તો તેમના માટે જહન્નમનો અઝાબ છે અને તેમના માટે એવો અઝાબ છે, જે તેમને ભષ્મ કરી દેશે.",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 590,
      "verse_key": "85:11",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ ٱلَّذِينَ ءَامَنُواْ وَعَمِلُواْ ٱلصَّٰلِحَٰتِ لَهُمۡ جَنَّٰتٞ تَجۡرِي مِن تَحۡتِهَا ٱلۡأَنۡهَٰرُۚ ذَٰلِكَ ٱلۡفَوۡزُ ٱلۡكَبِيرُ",
      "translation": "૧૧. નિ:શંક ઇમાન લાવ્યા અને નેક કામ કર્યા, તેમના માટે એવા બગીચા છે, જેની નીચે નહેરો વહી રહી છે. આ જ ભવ્ય સફળતા છે.",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 590,
      "verse_key": "85:12",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ بَطۡشَ رَبِّكَ لَشَدِيدٌ",
      "translation": "૧૨. નિ:શંક તારા પાલનહારની પકડ ખુબ જ સખત છે.",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 590,
      "verse_key": "85:13",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُۥ هُوَ يُبۡدِئُ وَيُعِيدُ",
      "translation": "૧૩. તે જ પહેલી વાર સર્જન કરે છે અને તે જ ફરીવાર સર્જન કરશે.",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 590,
      "verse_key": "85:14",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَهُوَ ٱلۡغَفُورُ ٱلۡوَدُودُ",
      "translation": "૧૪. તે ખૂબ માફ કરવાવાળો અને ખુબ જ મોહબ્બત કરનાર છે.",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 590,
      "verse_key": "85:15",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ذُو ٱلۡعَرۡشِ ٱلۡمَجِيدُ",
      "translation": "૧૫. અર્શનો માલિક ઉચ્ચ પ્રતિષ્ઠા વાળો છે.",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 590,
      "verse_key": "85:16",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَعَّالٞ لِّمَا يُرِيدُ",
      "translation": "૧૬. જે ઇચ્છે, તેને કરી નાખનાર છે.",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 590,
      "verse_key": "85:17",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "هَلۡ أَتَىٰكَ حَدِيثُ ٱلۡجُنُودِ",
      "translation": "૧૭. શું તમારી પાસે સેનાઓની સુચના પહોંચી છે.?",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 590,
      "verse_key": "85:18",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِرۡعَوۡنَ وَثَمُودَ",
      "translation": "૧૮. (એટલે કે) ફિરઔન અને ષમૂદના (લશ્કરોની)",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 590,
      "verse_key": "85:19",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بَلِ ٱلَّذِينَ كَفَرُواْ فِي تَكۡذِيبٖ",
      "translation": "૧૯. પરંતુ કાફિરો તો જુઠલાવવામાં લાગેલા છે.",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 590,
      "verse_key": "85:20",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱللَّهُ مِن وَرَآئِهِم مُّحِيطُۢ",
      "translation": "૨૦. અને અલ્લાહ તઆલાએ પણ તેઓને દરેક બાજુથી ઘેરી રાખ્યા છે.",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 590,
      "verse_key": "85:21",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بَلۡ هُوَ قُرۡءَانٞ مَّجِيدٞ",
      "translation": "૨૧. પરંતુ આ કુરઆન છે. ઉચ્ચ દરજ્જાવાળુ.",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 590,
      "verse_key": "85:22",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِي لَوۡحٖ مَّحۡفُوظِۭ",
      "translation": "૨૨. લૌહે મહફૂઝ માં (લખેલું) છે.",
      "footnotes": ""
    }
  ]
}