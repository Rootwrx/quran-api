{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 568,
      "verse_key": "70:1",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "سَأَلَ سَآئِلُۢ بِعَذَابٖ وَاقِعٖ",
      "translation": "૧. એક સવાલ કરનારાએ તે અઝાબ વિશે સવાલ કર્યો, જે સાબિત થઈને રહેશે.",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 568,
      "verse_key": "70:2",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لِّلۡكَٰفِرِينَ لَيۡسَ لَهُۥ دَافِعٞ",
      "translation": "૨. કાફિરો પરથી જેને કોઇ ટાળનાર નથી.",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 568,
      "verse_key": "70:3",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مِّنَ ٱللَّهِ ذِي ٱلۡمَعَارِجِ",
      "translation": "૩. (આ અઝાબ) અલ્લાહ તરફથી (આવશે) જે ઉચ્ચતા વાળો છે.",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 568,
      "verse_key": "70:4",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تَعۡرُجُ ٱلۡمَلَٰٓئِكَةُ وَٱلرُّوحُ إِلَيۡهِ فِي يَوۡمٖ كَانَ مِقۡدَارُهُۥ خَمۡسِينَ أَلۡفَ سَنَةٖ",
      "translation": "૪. જેના તરફ ફરિશ્તાઓ અને રૂહ એક દિવસમાં ચઢે છે. જેની સમયમર્યાદા પચાસ હજાર વર્ષ છે.",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 568,
      "verse_key": "70:5",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَٱصۡبِرۡ صَبۡرٗا جَمِيلًا",
      "translation": "૫. બસ! તુ સારી રીતે ધીરજ રાખ.",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 568,
      "verse_key": "70:6",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُمۡ يَرَوۡنَهُۥ بَعِيدٗا",
      "translation": "૬. નિ:શંક આ લોકો તે (અઝાબ) ને દૂર સમજી રહયા છે,",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 568,
      "verse_key": "70:7",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَنَرَىٰهُ قَرِيبٗا",
      "translation": "૭. પરંતુ અમે તેને નજીક જોઇ રહ્યા છીએ.",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 568,
      "verse_key": "70:8",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَوۡمَ تَكُونُ ٱلسَّمَآءُ كَٱلۡمُهۡلِ",
      "translation": "૮. જે દિવસે આકાશ ઊકળતા તાંબા જેવુ થઇ જશે.",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 568,
      "verse_key": "70:9",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَتَكُونُ ٱلۡجِبَالُ كَٱلۡعِهۡنِ",
      "translation": "૯. અને પર્વત રંગીન ઊન જેવા થઇ જશે.",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 568,
      "verse_key": "70:10",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَا يَسۡـَٔلُ حَمِيمٌ حَمِيمٗا",
      "translation": "૧૦. તે દિવસે કોઇ મિત્ર બીજા મિત્રને નહી પૂછે.",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 569,
      "verse_key": "70:11",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يُبَصَّرُونَهُمۡۚ يَوَدُّ ٱلۡمُجۡرِمُ لَوۡ يَفۡتَدِي مِنۡ عَذَابِ يَوۡمِئِذِۭ بِبَنِيهِ",
      "translation": "૧૧. જો કે તેઓ એકબીજાને દેખાડવામાં આવશે, તે દિવસે અઝાબથી બચવા માટે ગુનેગાર એવું ઈચ્છશે કે પોતાના દીકરાઓને મુક્તિદંડ રૂપે આપી દે.",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 569,
      "verse_key": "70:12",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَصَٰحِبَتِهِۦ وَأَخِيهِ",
      "translation": "૧૨. પોતાની પત્નિને અને પોતાના ભાઇને",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 569,
      "verse_key": "70:13",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَفَصِيلَتِهِ ٱلَّتِي تُـٔۡوِيهِ",
      "translation": "૧૩. અને પોતાના તે કુટુંબીજનોને, જેઓ તેને આશરો આપતા હતા.",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 569,
      "verse_key": "70:14",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَن فِي ٱلۡأَرۡضِ جَمِيعٗا ثُمَّ يُنجِيهِ",
      "translation": "૧૪. અને જે કઈ જમીનમાં છે તે બધું જ આપી પોતાને બચાવી લે.",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 569,
      "verse_key": "70:15",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّآۖ إِنَّهَا لَظَىٰ",
      "translation": "૧૫. (પરંતુ) કદાપિ આવું નહીં થાય, નિ:શંક તે ભડકતી (આગ) હશે.",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 569,
      "verse_key": "70:16",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "نَزَّاعَةٗ لِّلشَّوَىٰ",
      "translation": "૧૬. જે મોં અને માથાની ચામડીને ખેંચી લાવનારી છે.",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 569,
      "verse_key": "70:17",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تَدۡعُواْ مَنۡ أَدۡبَرَ وَتَوَلَّىٰ",
      "translation": "૧૭. તે (આગ) તે દરેક વ્યક્તિને પોકારશે, જે પાછળ ફરનાર અને પીઠ બતાવનાર છે.",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 569,
      "verse_key": "70:18",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَجَمَعَ فَأَوۡعَىٰٓ",
      "translation": "૧૮. અને ભેગુ કરીને સંભાળી રાખતો હશે.",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 569,
      "verse_key": "70:19",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "۞ إِنَّ ٱلۡإِنسَٰنَ خُلِقَ هَلُوعًا",
      "translation": "૧૯. ખરેખર મનુષ્ય ખુબ જ કાચા મનનો બનાવેલો છે.",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 569,
      "verse_key": "70:20",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذَا مَسَّهُ ٱلشَّرُّ جَزُوعٗا",
      "translation": "૨૦. જ્યારે તેને પરેશાની પહોંચે છે, તો ગભરાઇ જાય છે.",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 569,
      "verse_key": "70:21",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا مَسَّهُ ٱلۡخَيۡرُ مَنُوعًا",
      "translation": "૨૧. અને જ્યારે રાહત મળે છે, તો કંજુસી કરવા લાગે છે.",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 569,
      "verse_key": "70:22",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَّا ٱلۡمُصَلِّينَ",
      "translation": "૨૨. પરંતુ નમાઝ પઢવાવાળા.",
      "footnotes": ""
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 569,
      "verse_key": "70:23",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِينَ هُمۡ عَلَىٰ صَلَاتِهِمۡ دَآئِمُونَ",
      "translation": "૨૩. જેઓ પોતાની નમાઝ પર હંમેશા પાંબદી કરનાર છે.",
      "footnotes": ""
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 569,
      "verse_key": "70:24",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّذِينَ فِيٓ أَمۡوَٰلِهِمۡ حَقّٞ مَّعۡلُومٞ",
      "translation": "૨૪. અને જેમના ધનમાં નક્કી કરેલો ભાગ છે.",
      "footnotes": ""
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 569,
      "verse_key": "70:25",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لِّلسَّآئِلِ وَٱلۡمَحۡرُومِ",
      "translation": "૨૫. માંગવાવાળા માટે પણ અને સવાલથી બચનારાનો પણ.",
      "footnotes": ""
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 569,
      "verse_key": "70:26",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّذِينَ يُصَدِّقُونَ بِيَوۡمِ ٱلدِّينِ",
      "translation": "૨૬. અને જે કયામતના દિવસની પુષ્ટિ કરે છે.",
      "footnotes": ""
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 569,
      "verse_key": "70:27",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّذِينَ هُم مِّنۡ عَذَابِ رَبِّهِم مُّشۡفِقُونَ",
      "translation": "૨૭. અને જે પોતાના પાલનહારના અઝાબથી ડરતા રહે છે.",
      "footnotes": ""
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 569,
      "verse_key": "70:28",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ عَذَابَ رَبِّهِمۡ غَيۡرُ مَأۡمُونٖ",
      "translation": "૨૮. કારણકે તેમના પાલનહારનો અઝાબ નીડર થવા જેવી વસ્તુ નથી.",
      "footnotes": ""
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 569,
      "verse_key": "70:29",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّذِينَ هُمۡ لِفُرُوجِهِمۡ حَٰفِظُونَ",
      "translation": "૨૯. અને જે લોકો પોતાના ગુંપ્તાગની (હરામથી) રક્ષા કરે છે..",
      "footnotes": ""
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 569,
      "verse_key": "70:30",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَّا عَلَىٰٓ أَزۡوَٰجِهِمۡ أَوۡ مَا مَلَكَتۡ أَيۡمَٰنُهُمۡ فَإِنَّهُمۡ غَيۡرُ مَلُومِينَ",
      "translation": "૩૦. હા! તેમની પત્નિઓ અને બાંદીઓ વિશે જેમના તેઓ માલિક છે, તેમના પર કોઇ નિંદા નથી.",
      "footnotes": ""
    },
    {
      "id": 31,
      "verse_number": 31,
      "page_number": 569,
      "verse_key": "70:31",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَمَنِ ٱبۡتَغَىٰ وَرَآءَ ذَٰلِكَ فَأُوْلَٰٓئِكَ هُمُ ٱلۡعَادُونَ",
      "translation": "૩૧. હવે જે કોઇ તેના સિવાય અન્ય (રસ્તો) શોધશે, તો આવા લોકો હદ વટાવી જનારા છે.",
      "footnotes": ""
    },
    {
      "id": 32,
      "verse_number": 32,
      "page_number": 569,
      "verse_key": "70:32",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّذِينَ هُمۡ لِأَمَٰنَٰتِهِمۡ وَعَهۡدِهِمۡ رَٰعُونَ",
      "translation": "૩૨. અને જે પોતાની નિષ્ઠાનું અને પોતાના વચનોનું ધ્યાન રાખે છે.",
      "footnotes": ""
    },
    {
      "id": 33,
      "verse_number": 33,
      "page_number": 569,
      "verse_key": "70:33",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّذِينَ هُم بِشَهَٰدَٰتِهِمۡ قَآئِمُونَ",
      "translation": "૩૩. અને જે પોતાની સાક્ષીઓ પર સીધા અને મક્કમ રહે છે.",
      "footnotes": ""
    },
    {
      "id": 34,
      "verse_number": 34,
      "page_number": 569,
      "verse_key": "70:34",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّذِينَ هُمۡ عَلَىٰ صَلَاتِهِمۡ يُحَافِظُونَ",
      "translation": "૩૪. અને જેઓ પોતાની નમાઝોની રક્ષા કરે છે.",
      "footnotes": ""
    },
    {
      "id": 35,
      "verse_number": 35,
      "page_number": 569,
      "verse_key": "70:35",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أُوْلَٰٓئِكَ فِي جَنَّٰتٖ مُّكۡرَمُونَ",
      "translation": "૩૫. આ જ લોકો જન્નતોમાં ઇઝઝતવાળા હશે.",
      "footnotes": ""
    },
    {
      "id": 36,
      "verse_number": 36,
      "page_number": 569,
      "verse_key": "70:36",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَمَالِ ٱلَّذِينَ كَفَرُواْ قِبَلَكَ مُهۡطِعِينَ",
      "translation": "૩૬. બસ! આ કાફિરોને શુ થઇ ગયુ છે કે તે તમારી તરફ દોડતા આવી રહ્યા છે.",
      "footnotes": ""
    },
    {
      "id": 37,
      "verse_number": 37,
      "page_number": 569,
      "verse_key": "70:37",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَنِ ٱلۡيَمِينِ وَعَنِ ٱلشِّمَالِ عِزِينَ",
      "translation": "૩૭. જમણે અને ડાબેથી, જૂથના જૂથ (આવી રહ્યા છે)",
      "footnotes": ""
    },
    {
      "id": 38,
      "verse_number": 38,
      "page_number": 569,
      "verse_key": "70:38",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَيَطۡمَعُ كُلُّ ٱمۡرِيٕٖ مِّنۡهُمۡ أَن يُدۡخَلَ جَنَّةَ نَعِيمٖ",
      "translation": "૩૮. શું તેમના માંથી દરેક આશા રાખે છે કે તેને નેઅમતો વાળી જન્નતમાં દાખલ કરવામાં આવશે?",
      "footnotes": ""
    },
    {
      "id": 39,
      "verse_number": 39,
      "page_number": 569,
      "verse_key": "70:39",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّآۖ إِنَّا خَلَقۡنَٰهُم مِّمَّا يَعۡلَمُونَ",
      "translation": "૩૯. (આવું) કદાપિ નહી થાય અમે તેમનુ તે (વસ્તુ) થી સર્જન કર્યુ છે, જેને તેઓ પોતે પણ જાણે છે.",
      "footnotes": ""
    },
    {
      "id": 40,
      "verse_number": 40,
      "page_number": 570,
      "verse_key": "70:40",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلَآ أُقۡسِمُ بِرَبِّ ٱلۡمَشَٰرِقِ وَٱلۡمَغَٰرِبِ إِنَّا لَقَٰدِرُونَ",
      "translation": "૪૦. બસ! હું પશ્ર્ચિમો અને પૂર્વના પાલનહારની કસમ ખાઈને કહું છું (કે) અમે ખરેખર આ વાત પર કુદરત ધરાવીએ છીએ.",
      "footnotes": ""
    },
    {
      "id": 41,
      "verse_number": 41,
      "page_number": 570,
      "verse_key": "70:41",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلَىٰٓ أَن نُّبَدِّلَ خَيۡرٗا مِّنۡهُمۡ وَمَا نَحۡنُ بِمَسۡبُوقِينَ",
      "translation": "૪૧. કે અમે તેમના બદલે તેમનાથી સારા લોકોને લઇને આવીએ, અને અમે અક્ષમ કરનાર કોઈ નથી.",
      "footnotes": ""
    },
    {
      "id": 42,
      "verse_number": 42,
      "page_number": 570,
      "verse_key": "70:42",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَذَرۡهُمۡ يَخُوضُواْ وَيَلۡعَبُواْ حَتَّىٰ يُلَٰقُواْ يَوۡمَهُمُ ٱلَّذِي يُوعَدُونَ",
      "translation": "૪૨. બસ! તુ એમને લડતા-ઝઘડતા અને ખેલકૂદ કરતા છોડી દે અહીં સુધી કે તેઓ તે દિવસ જોઈ લે, જેનું તેમને વચન આપવામાં આવે છે.",
      "footnotes": ""
    },
    {
      "id": 43,
      "verse_number": 43,
      "page_number": 570,
      "verse_key": "70:43",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَوۡمَ يَخۡرُجُونَ مِنَ ٱلۡأَجۡدَاثِ سِرَاعٗا كَأَنَّهُمۡ إِلَىٰ نُصُبٖ يُوفِضُونَ",
      "translation": "૪૩. જે દિવસે તે લોકો પોતાની કબરોમાંથી નીકળી એવી રીતે દોડતા જઈ રહ્યા હશે, જેવું કે પોતાના પૂજકો પાસે દોડતા જઈ રહ્યા હોય.",
      "footnotes": ""
    },
    {
      "id": 44,
      "verse_number": 44,
      "page_number": 570,
      "verse_key": "70:44",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "خَٰشِعَةً أَبۡصَٰرُهُمۡ تَرۡهَقُهُمۡ ذِلَّةٞۚ ذَٰلِكَ ٱلۡيَوۡمُ ٱلَّذِي كَانُواْ يُوعَدُونَ",
      "translation": "૪૪. તેમની આંખો નમેલી હશે, તેમના પર બદનામી છવાયેલી હશે. આ છે, તે દિવસ જેનું તેમને વચન આપવામાં આવ્યુ હતું.",
      "footnotes": ""
    }
  ]
}