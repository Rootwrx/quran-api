{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 603,
      "verse_key": "109:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قُلۡ يَٰٓأَيُّهَا ٱلۡكَٰفِرُونَ",
      "translation": "૧. તમે કહી દો કે હે કાફિરો!",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 603,
      "verse_key": "109:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَآ أَعۡبُدُ مَا تَعۡبُدُونَ",
      "translation": "૨. જેની તમે ઈબાદત કરો છો હું તેમની ઈબાદત નથી કરી શકતો.",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 603,
      "verse_key": "109:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَآ أَنتُمۡ عَٰبِدُونَ مَآ أَعۡبُدُ",
      "translation": "૩. અને ન તો તમે તેની ઈબાદત કરવાવાળા છો, જેની હું ઈબાદત કરી રહ્યો છું.",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 603,
      "verse_key": "109:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَآ أَنَا۠ عَابِدٞ مَّا عَبَدتُّمۡ",
      "translation": "૪. અને ન હું તેમની ઈબાદત કરવાવાળો છું, જેમની તમે અને તમારા (પૂર્વજો) ઈબાદત કરે છે.",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 603,
      "verse_key": "109:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَآ أَنتُمۡ عَٰبِدُونَ مَآ أَعۡبُدُ",
      "translation": "૫. અને ન તો તમે ઈબાદત કરવાવાળા છો, જેની ઈબાદત હું કરી રહ્યો છું.",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 603,
      "verse_key": "109:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَكُمۡ دِينُكُمۡ وَلِيَ دِينِ",
      "translation": "૬. તમારા માટે તમારો દીન છે અને મારા માટે મારો દીન છે.",
      "footnotes": ""
    }
  ]
}