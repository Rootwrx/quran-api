{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 604,
      "verse_key": "113:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قُلۡ أَعُوذُ بِرَبِّ ٱلۡفَلَقِ",
      "translation": "Katakanlah, \"Aku berlindung kepada Tuhan yang menguasai subuh (fajar),",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 604,
      "verse_key": "113:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مِن شَرِّ مَا خَلَقَ",
      "translation": "dari kejahatan (makhluk yang) Dia ciptakan,",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 604,
      "verse_key": "113:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمِن شَرِّ غَاسِقٍ إِذَا وَقَبَ",
      "translation": "dan dari kejahatan malam apabila telah gelap gulita,",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 604,
      "verse_key": "113:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمِن شَرِّ ٱلنَّفَّٰثَٰتِ فِي ٱلۡعُقَدِ",
      "translation": "dan dari kejahatan (perempuan-perempuan) penyihir yang meniup pada buhul-buhul (talinya),",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 604,
      "verse_key": "113:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمِن شَرِّ حَاسِدٍ إِذَا حَسَدَ",
      "translation": "dan dari kejahatan orang yang dengki apabila dia dengki.\"",
      "footnotes": ""
    }
  ]
}