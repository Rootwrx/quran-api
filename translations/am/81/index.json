{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 586,
      "verse_key": "81:1",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذَا ٱلشَّمۡسُ كُوِّرَتۡ",
      "translation": "ፀሐይ በተጠቀለለች ጊዜ፤",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 586,
      "verse_key": "81:2",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلنُّجُومُ ٱنكَدَرَتۡ",
      "translation": "ከዋክብትም በረገፉ ጊዜ፤",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 586,
      "verse_key": "81:3",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡجِبَالُ سُيِّرَتۡ",
      "translation": "ተራራዎችም በተነዱ ጊዜ፤",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 586,
      "verse_key": "81:4",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡعِشَارُ عُطِّلَتۡ",
      "translation": "የዐሥር ወር እርጉዞች ግመሎችም (ያለጠባቂ) በተተዉ ጊዜ፤",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 586,
      "verse_key": "81:5",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡوُحُوشُ حُشِرَتۡ",
      "translation": "እንስሳትም ሁሉ በተሰበሰቡ ጊዜ፤",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 586,
      "verse_key": "81:6",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡبِحَارُ سُجِّرَتۡ",
      "translation": "ባሕሮችም በተቃጠሉ ጊዜ፤",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 586,
      "verse_key": "81:7",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلنُّفُوسُ زُوِّجَتۡ",
      "translation": "ነፍሶችም (በየአካሎቻቸው) በተቆራኙ ጊዜ፤",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 586,
      "verse_key": "81:8",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡمَوۡءُۥدَةُ سُئِلَتۡ",
      "translation": "በሕይወቷ የተቀበረችውም ሴት ልጅ በተጠየቀች ጊዜ፤",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 586,
      "verse_key": "81:9",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بِأَيِّ ذَنۢبٖ قُتِلَتۡ",
      "translation": "በምን ወንጀል እንደ ተገደለች፤",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 586,
      "verse_key": "81:10",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلصُّحُفُ نُشِرَتۡ",
      "translation": "ጽሑፎችም በተዘረጉ ጊዜ፤",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 586,
      "verse_key": "81:11",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلسَّمَآءُ كُشِطَتۡ",
      "translation": "ሰማይም በተገሸለጠች ጊዜ፤",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 586,
      "verse_key": "81:12",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡجَحِيمُ سُعِّرَتۡ",
      "translation": "ገሀነምም በተነደደች ጊዜ፤",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 586,
      "verse_key": "81:13",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡجَنَّةُ أُزۡلِفَتۡ",
      "translation": "ገነትም በተቀረበች ጊዜ፤",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 586,
      "verse_key": "81:14",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلِمَتۡ نَفۡسٞ مَّآ أَحۡضَرَتۡ",
      "translation": "ነፍስ ሁሉ (ከሥራ) ያቀረበችውን ታውቃለች፤",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 586,
      "verse_key": "81:15",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلَآ أُقۡسِمُ بِٱلۡخُنَّسِ",
      "translation": "ተመላሾችም በኾኑት (ዐዋድያት) እምላለሁ፡፡",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 586,
      "verse_key": "81:16",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلۡجَوَارِ ٱلۡكُنَّسِ",
      "translation": "ኺያጆች ገቢዎች በኾኑት (ከዋክብት)፡፡",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 586,
      "verse_key": "81:17",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّيۡلِ إِذَا عَسۡعَسَ",
      "translation": "በሌሊቱም (ጨለማውን) በጠነሰሰ ጊዜ፤",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 586,
      "verse_key": "81:18",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلصُّبۡحِ إِذَا تَنَفَّسَ",
      "translation": "በንጋቱም (በብርሃን) በተነፈሰ ጊዜ፤ (እምላለሁ)፡፡",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 586,
      "verse_key": "81:19",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُۥ لَقَوۡلُ رَسُولٖ كَرِيمٖ",
      "translation": "እርሱ (ቁርኣን) የክቡር መልክተኛ ቃል ነው፡፡",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 586,
      "verse_key": "81:20",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ذِي قُوَّةٍ عِندَ ذِي ٱلۡعَرۡشِ مَكِينٖ",
      "translation": "የኀይል ባለቤት በዙፋኑ ባለ ቤት ዘንድ ባለሟል የኾነ፡፡",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 586,
      "verse_key": "81:21",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مُّطَاعٖ ثَمَّ أَمِينٖ",
      "translation": "በዚያ ስፍራ ትእዛዙ ተሰሚ፤ ታማኝ የኾነ (መልክተኛ ቃል) ነው፡፡",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 586,
      "verse_key": "81:22",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا صَاحِبُكُم بِمَجۡنُونٖ",
      "translation": "ነቢያችሁም በፍጹም ዕብድ አይደለም፡፡",
      "footnotes": ""
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 586,
      "verse_key": "81:23",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَقَدۡ رَءَاهُ بِٱلۡأُفُقِ ٱلۡمُبِينِ",
      "translation": "በግልጹ አድማስም በእርግጥ አይቶታል፡፡",
      "footnotes": ""
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 586,
      "verse_key": "81:24",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا هُوَ عَلَى ٱلۡغَيۡبِ بِضَنِينٖ",
      "translation": "እርሱም በሩቁ ወሬ ላይ ሰሳች (ንፉግ) አይደለም፡፡",
      "footnotes": ""
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 586,
      "verse_key": "81:25",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا هُوَ بِقَوۡلِ شَيۡطَٰنٖ رَّجِيمٖ",
      "translation": "እርሱም (ቁርኣን) የእርጉም ሰይጣን ቃል አይደለም፡፡",
      "footnotes": ""
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 586,
      "verse_key": "81:26",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَيۡنَ تَذۡهَبُونَ",
      "translation": "ታዲያ ወዴት ትኼዳላችሁ?",
      "footnotes": ""
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 586,
      "verse_key": "81:27",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنۡ هُوَ إِلَّا ذِكۡرٞ لِّلۡعَٰلَمِينَ",
      "translation": "እርሱ የዓለማት መገሠጫ እንጅ ሌላ አይደለም፡፡",
      "footnotes": ""
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 586,
      "verse_key": "81:28",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لِمَن شَآءَ مِنكُمۡ أَن يَسۡتَقِيمَ",
      "translation": "ከናንተ ቀጥተኛ መኾንን ለሻ ሰው (መገሰጫ ነው)፡፡",
      "footnotes": ""
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 586,
      "verse_key": "81:29",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا تَشَآءُونَ إِلَّآ أَن يَشَآءَ ٱللَّهُ رَبُّ ٱلۡعَٰلَمِينَ",
      "translation": "የዓለማት ጌታ አላህ ካልሻም አትሹም፡፡",
      "footnotes": ""
    }
  ]
}