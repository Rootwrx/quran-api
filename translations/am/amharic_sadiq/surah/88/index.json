{
  "result": [
    {
      "id": "5968",
      "sura": "88",
      "aya": "1",
      "arabic_text": "هَلۡ أَتَىٰكَ حَدِيثُ ٱلۡغَٰشِيَةِ",
      "translation": "የሸፋኝቱ (ትንሣኤ) ወሬ መጣህን?",
      "footnotes": ""
    },
    {
      "id": "5969",
      "sura": "88",
      "aya": "2",
      "arabic_text": "وُجُوهٞ يَوۡمَئِذٍ خَٰشِعَةٌ",
      "translation": "ፊቶች በዚያ ቀን ተዋራጆች ናቸው፡፡",
      "footnotes": ""
    },
    {
      "id": "5970",
      "sura": "88",
      "aya": "3",
      "arabic_text": "عَامِلَةٞ نَّاصِبَةٞ",
      "translation": "ሠሪዎች ለፊዎች ናቸው፡፡",
      "footnotes": ""
    },
    {
      "id": "5971",
      "sura": "88",
      "aya": "4",
      "arabic_text": "تَصۡلَىٰ نَارًا حَامِيَةٗ",
      "translation": "ተኳሳን እሳት ይገባሉ፡፡",
      "footnotes": ""
    },
    {
      "id": "5972",
      "sura": "88",
      "aya": "5",
      "arabic_text": "تُسۡقَىٰ مِنۡ عَيۡنٍ ءَانِيَةٖ",
      "translation": "በጣም ከፈላች ምንጭ ይጋታሉ፡፡",
      "footnotes": ""
    },
    {
      "id": "5973",
      "sura": "88",
      "aya": "6",
      "arabic_text": "لَّيۡسَ لَهُمۡ طَعَامٌ إِلَّا مِن ضَرِيعٖ",
      "translation": "ለእነርሱ ዶሪዕ ከሚባል (እሾሃም) ዛፍ እንጅ ሌላ ምግብ የላቸውም፡፡",
      "footnotes": ""
    },
    {
      "id": "5974",
      "sura": "88",
      "aya": "7",
      "arabic_text": "لَّا يُسۡمِنُ وَلَا يُغۡنِي مِن جُوعٖ",
      "translation": "የማያሰባ ከረኃብም የማያብቃቃ ከኾነው፡፡",
      "footnotes": ""
    },
    {
      "id": "5975",
      "sura": "88",
      "aya": "8",
      "arabic_text": "وُجُوهٞ يَوۡمَئِذٖ نَّاعِمَةٞ",
      "translation": "ፊቶች በዚያ ቀን ተቀማጣዮች ናቸው፡፡",
      "footnotes": ""
    },
    {
      "id": "5976",
      "sura": "88",
      "aya": "9",
      "arabic_text": "لِّسَعۡيِهَا رَاضِيَةٞ",
      "translation": "ለሥራቸው ተደሳቾች ናቸው፡፡",
      "footnotes": ""
    },
    {
      "id": "5977",
      "sura": "88",
      "aya": "10",
      "arabic_text": "فِي جَنَّةٍ عَالِيَةٖ",
      "translation": "በከፍተኛ ገነት ውስጥ ናቸው፡፡",
      "footnotes": ""
    },
    {
      "id": "5978",
      "sura": "88",
      "aya": "11",
      "arabic_text": "لَّا تَسۡمَعُ فِيهَا لَٰغِيَةٗ",
      "translation": "በውስጧ ውድቅን ነገር አይሰሙም፡፡",
      "footnotes": ""
    },
    {
      "id": "5979",
      "sura": "88",
      "aya": "12",
      "arabic_text": "فِيهَا عَيۡنٞ جَارِيَةٞ",
      "translation": "በውስጧ ፈሳሾች ምንጮች አልሉ፡፡",
      "footnotes": ""
    },
    {
      "id": "5980",
      "sura": "88",
      "aya": "13",
      "arabic_text": "فِيهَا سُرُرٞ مَّرۡفُوعَةٞ",
      "translation": "በውስጧ ከፍ የተደረጉ አልጋዎች አልሉ፡፡",
      "footnotes": ""
    },
    {
      "id": "5981",
      "sura": "88",
      "aya": "14",
      "arabic_text": "وَأَكۡوَابٞ مَّوۡضُوعَةٞ",
      "translation": "በተርታ የተኖሩ ብርጭቆዎችም፡፡",
      "footnotes": ""
    },
    {
      "id": "5982",
      "sura": "88",
      "aya": "15",
      "arabic_text": "وَنَمَارِقُ مَصۡفُوفَةٞ",
      "translation": "የተደረደሩ መከዳዎችም፡፡",
      "footnotes": ""
    },
    {
      "id": "5983",
      "sura": "88",
      "aya": "16",
      "arabic_text": "وَزَرَابِيُّ مَبۡثُوثَةٌ",
      "translation": "የተነጠፉ ስጋጃዎችም (አልሉ)፡፡",
      "footnotes": ""
    },
    {
      "id": "5984",
      "sura": "88",
      "aya": "17",
      "arabic_text": "أَفَلَا يَنظُرُونَ إِلَى ٱلۡإِبِلِ كَيۡفَ خُلِقَتۡ",
      "translation": "(ከሓዲዎች) አይመለከቱምን? ወደ ግመል እንዴት እነደተፈጠረች!",
      "footnotes": ""
    },
    {
      "id": "5985",
      "sura": "88",
      "aya": "18",
      "arabic_text": "وَإِلَى ٱلسَّمَآءِ كَيۡفَ رُفِعَتۡ",
      "translation": "ወደ ሰማይም እንዴት ከፍ እንደ ተደረገች!",
      "footnotes": ""
    },
    {
      "id": "5986",
      "sura": "88",
      "aya": "19",
      "arabic_text": "وَإِلَى ٱلۡجِبَالِ كَيۡفَ نُصِبَتۡ",
      "translation": "ወደ ተራራዎችም እንዴት እንደ ተቸከሉ!",
      "footnotes": ""
    },
    {
      "id": "5987",
      "sura": "88",
      "aya": "20",
      "arabic_text": "وَإِلَى ٱلۡأَرۡضِ كَيۡفَ سُطِحَتۡ",
      "translation": "ወደ ምድርም እንዴ እንደተዘረጋች (አይመለከቱምን?)",
      "footnotes": ""
    },
    {
      "id": "5988",
      "sura": "88",
      "aya": "21",
      "arabic_text": "فَذَكِّرۡ إِنَّمَآ أَنتَ مُذَكِّرٞ",
      "translation": "አስታውስም፤ አንተ አስታዋሽ ብቻ ነህና፡፡",
      "footnotes": ""
    },
    {
      "id": "5989",
      "sura": "88",
      "aya": "22",
      "arabic_text": "لَّسۡتَ عَلَيۡهِم بِمُصَيۡطِرٍ",
      "translation": "በእነርሱ ላይ ተሿሚ (አስገዳጅ) አይደለህም፡፡",
      "footnotes": ""
    },
    {
      "id": "5990",
      "sura": "88",
      "aya": "23",
      "arabic_text": "إِلَّا مَن تَوَلَّىٰ وَكَفَرَ",
      "translation": "ግን (ከእውነት) የዞረና የካደ ሰው፤",
      "footnotes": ""
    },
    {
      "id": "5991",
      "sura": "88",
      "aya": "24",
      "arabic_text": "فَيُعَذِّبُهُ ٱللَّهُ ٱلۡعَذَابَ ٱلۡأَكۡبَرَ",
      "translation": "አላህ ታላቁን ቅጣት ይቀጣዋል፡፡",
      "footnotes": ""
    },
    {
      "id": "5992",
      "sura": "88",
      "aya": "25",
      "arabic_text": "إِنَّ إِلَيۡنَآ إِيَابَهُمۡ",
      "translation": "መመለሻቸው ወደእኛ ብቻ ነው፡፡",
      "footnotes": ""
    },
    {
      "id": "5993",
      "sura": "88",
      "aya": "26",
      "arabic_text": "ثُمَّ إِنَّ عَلَيۡنَا حِسَابَهُم",
      "translation": "ከዚያም ምርመራቸው በእኛ ላይ ብቻ ነው፡፡",
      "footnotes": ""
    }
  ]
}