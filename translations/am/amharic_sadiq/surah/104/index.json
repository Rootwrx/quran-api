  "result": [
    {
      "id": "6180",
      "sura": "104",
      "aya": "1",
      "arabic_text": "وَيۡلٞ لِّكُلِّ هُمَزَةٖ لُّمَزَةٍ",
      "translation": "ለሃሜተኛ ለዘላፊ ሁሉ ወዮለት፡፡",
      "footnotes": ""
    },
    {
      "id": "6181",
      "sura": "104",
      "aya": "2",
      "arabic_text": "ٱلَّذِي جَمَعَ مَالٗا وَعَدَّدَهُۥ",
      "translation": "ለዚያ ገንዘብን የሰበሰበና የቆጣጠረው (ያደለበውም) ለኾነ ፤(ወዮለት)፡፡",
      "footnotes": ""
    },
    {
      "id": "6182",
      "sura": "104",
      "aya": "3",
      "arabic_text": "يَحۡسَبُ أَنَّ مَالَهُۥٓ أَخۡلَدَهُۥ",
      "translation": "ገንዘቡ የሚያዘወትረው መኾኑን ያስባል፡፡",
      "footnotes": ""
    },
    {
      "id": "6183",
      "sura": "104",
      "aya": "4",
      "arabic_text": "كَلَّاۖ لَيُنۢبَذَنَّ فِي ٱلۡحُطَمَةِ",
      "translation": "ይከልከል፤ በሰባሪይቱ (እሳት) ውስጥ በእርግጥ ይጣላል፡፡",
      "footnotes": ""
    },
    {
      "id": "6184",
      "sura": "104",
      "aya": "5",
      "arabic_text": "وَمَآ أَدۡرَىٰكَ مَا ٱلۡحُطَمَةُ",
      "translation": "ሰባሪይቱም ምን እንደ ኾነች ምን አሳወቀህ?",
      "footnotes": ""
    },
    {
      "id": "6185",
      "sura": "104",
      "aya": "6",
      "arabic_text": "نَارُ ٱللَّهِ ٱلۡمُوقَدَةُ",
      "translation": "የተነደደችው የአላህ እሳት ናት፡፡",
      "footnotes": ""
    },
    {
      "id": "6186",
      "sura": "104",
      "aya": "7",
      "arabic_text": "ٱلَّتِي تَطَّلِعُ عَلَى ٱلۡأَفۡـِٔدَةِ",
      "translation": "ያቺ በልቦች ላይ የምትዘልቅ የኾነችው፡፡",
      "footnotes": ""
    },
    {
      "id": "6187",
      "sura": "104",
      "aya": "8",
      "arabic_text": "إِنَّهَا عَلَيۡهِم مُّؤۡصَدَةٞ",
      "translation": "እርሷ በእርግጥ በእነርሱ ላይ የተዘጋጀች ናት፡፡",
      "footnotes": ""
    },
    {
      "id": "6188",
      "sura": "104",
      "aya": "9",
      "arabic_text": "فِي عَمَدٖ مُّمَدَّدَةِۭ",
      "translation": "በተዘረጋች አዕማድ ውስጥ (የተዘጋች ናት)፡፡",
      "footnotes": ""
    }
  ]
}