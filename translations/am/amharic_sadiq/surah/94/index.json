{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 596,
      "verse_key": "94:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَمۡ نَشۡرَحۡ لَكَ صَدۡرَكَ",
      "translation": "ልብህን ለአንተ አላሰፋንልህምን? (አስፍተንልሃል)፡፡",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 596,
      "verse_key": "94:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَوَضَعۡنَا عَنكَ وِزۡرَكَ",
      "translation": "ሸክምህንም ካንተ አውርደንልሃል፡፡",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 596,
      "verse_key": "94:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِيٓ أَنقَضَ ظَهۡرَكَ",
      "translation": "ያንን ጀርባህን ያከበደውን (ሸክም)፡፡",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 596,
      "verse_key": "94:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَرَفَعۡنَا لَكَ ذِكۡرَكَ",
      "translation": "መወሳትህንም ላንተ ከፍ አድርገንልሃል፡፡",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 596,
      "verse_key": "94:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِنَّ مَعَ ٱلۡعُسۡرِ يُسۡرًا",
      "translation": "ከችግርም ጋር ምቾት አልለ፡፡",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 596,
      "verse_key": "94:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ مَعَ ٱلۡعُسۡرِ يُسۡرٗا",
      "translation": "ከችግር ጋር በእርግጥ ምቾት አልለ፡፡",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 596,
      "verse_key": "94:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِذَا فَرَغۡتَ فَٱنصَبۡ",
      "translation": "በጨረስክም ጊዜ ልፋ፡፡ (ቀጥል)፡፡",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 596,
      "verse_key": "94:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِلَىٰ رَبِّكَ فَٱرۡغَب",
      "translation": "ወደ ጌታህም ብቻ ከጅል፡፡",
      "footnotes": ""
    }
  ]
}