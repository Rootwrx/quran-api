{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 591,
      "verse_key": "87:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "سَبِّحِ ٱسۡمَ رَبِّكَ ٱلۡأَعۡلَى",
      "translation": "ከሁሉ በላይ የኾነውን ጌታህን ስም አሞግሥ፡፡",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 591,
      "verse_key": "87:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِي خَلَقَ فَسَوَّىٰ",
      "translation": "የዚያን (ሁሉን ነገር) የፈጠረውን ያስተካከለውንም፡፡",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 591,
      "verse_key": "87:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّذِي قَدَّرَ فَهَدَىٰ",
      "translation": "የዚያንም የወሰነውን፡፡ (ለተፈጠረለት ነገር) የመራውንም፡፡",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 591,
      "verse_key": "87:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّذِيٓ أَخۡرَجَ ٱلۡمَرۡعَىٰ",
      "translation": "የዚያንም ግጦሽን (ለምለም አድርጎ) ያወጣውን፡፡",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 591,
      "verse_key": "87:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَجَعَلَهُۥ غُثَآءً أَحۡوَىٰ",
      "translation": "(ከዚያ) ደረቅ ጥቁር ያደረገውንም (አምላክ ስም አሞግስ)፡፡",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 591,
      "verse_key": "87:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "سَنُقۡرِئُكَ فَلَا تَنسَىٰٓ",
      "translation": "(ቁርኣንን) በእርግጥ እናስነብብሃለን፤ አትረሳምም፡፡",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 591,
      "verse_key": "87:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَّا مَا شَآءَ ٱللَّهُۚ إِنَّهُۥ يَعۡلَمُ ٱلۡجَهۡرَ وَمَا يَخۡفَىٰ",
      "translation": "አላህ ከሻው ነገር በስተቀር፡፡ እርሱ ግልጹን የሚሸሸገውንም ሁሉ ያውቃልና፡፡",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 591,
      "verse_key": "87:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَنُيَسِّرُكَ لِلۡيُسۡرَىٰ",
      "translation": "ለገሪቱም (ሕግጋት) እንገጥምሃለን፡፡",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 591,
      "verse_key": "87:9",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَذَكِّرۡ إِن نَّفَعَتِ ٱلذِّكۡرَىٰ",
      "translation": "ግሣጼይቱ ብትጠቅም (ሰዎችን) ገሥጽም፡፡",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 591,
      "verse_key": "87:10",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "سَيَذَّكَّرُ مَن يَخۡشَىٰ",
      "translation": "(አላህን) የሚፈራ ሰው በእርግጥ ይገሠጻል፡፡",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 591,
      "verse_key": "87:11",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيَتَجَنَّبُهَا ٱلۡأَشۡقَى",
      "translation": "መናጢውም ይርቃታል፡፡",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 591,
      "verse_key": "87:12",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِي يَصۡلَى ٱلنَّارَ ٱلۡكُبۡرَىٰ",
      "translation": "ያ ታላቂቱን እሳት የሚገባው፡፡",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 591,
      "verse_key": "87:13",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ لَا يَمُوتُ فِيهَا وَلَا يَحۡيَىٰ",
      "translation": "ከዚያም በእርሷ ውስጥ አይሞትም ሕያውም አይኾንም፡፡",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 591,
      "verse_key": "87:14",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قَدۡ أَفۡلَحَ مَن تَزَكَّىٰ",
      "translation": "የተጥራራ ሰው በእርግጥ ዳነ፡፡",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 591,
      "verse_key": "87:15",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَذَكَرَ ٱسۡمَ رَبِّهِۦ فَصَلَّىٰ",
      "translation": "የጌታውንም ስም ያወሳና የሰገደ፡፡",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 592,
      "verse_key": "87:16",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بَلۡ تُؤۡثِرُونَ ٱلۡحَيَوٰةَ ٱلدُّنۡيَا",
      "translation": "ይልቁንም ቅርቢቱን ሕይወት ትመርጣላችሁ፡፡",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 592,
      "verse_key": "87:17",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡأٓخِرَةُ خَيۡرٞ وَأَبۡقَىٰٓ",
      "translation": "መጨረሻይቱ (ሕይወት) በላጭ ሁል ጊዜ ዘውታሪም ስትኾን፡፡",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 592,
      "verse_key": "87:18",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ هَٰذَا لَفِي ٱلصُّحُفِ ٱلۡأُولَىٰ",
      "translation": "ይህ በፊተኞቹ መጻሕፍት ውስጥ ያልለ ነው፡፡",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 592,
      "verse_key": "87:19",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "صُحُفِ إِبۡرَٰهِيمَ وَمُوسَىٰ",
      "translation": "በኢብራሂምና በሙሳ ጽሑፎች ውስጥ፡፡",
      "footnotes": ""
    }
  ]
}