      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "47.Toysa seehadaa kee jinnik sin Rabbi siinih yecee niqmatittek dirabbossaanam ma niqmataay?",
      "footnotes": ""
    },
    {
      "id": 48,
      "verse_number": 48,
      "page_number": 533,
      "verse_key": "55:48",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ذَوَاتَآ أَفۡنَانٖ",
      "translation": "48.Caxâ mixuu kee mexexu le luud Íe namma jannata.",
      "footnotes": ""
    },
