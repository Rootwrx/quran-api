{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 595,
      "verse_key": "91:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلشَّمۡسِ وَضُحَىٰهَا",
      "translation": "Ndahiye izuba n’urumuri rwaryo,",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 595,
      "verse_key": "91:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡقَمَرِ إِذَا تَلَىٰهَا",
      "translation": "N’ukwezi igihe kurikurikiye,",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 595,
      "verse_key": "91:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلنَّهَارِ إِذَا جَلَّىٰهَا",
      "translation": "N’amanywa igihe agaragaje urumuri (rw’izuba),",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 595,
      "verse_key": "91:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّيۡلِ إِذَا يَغۡشَىٰهَا",
      "translation": "N’ijoro igihe riritwikiriye,",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 595,
      "verse_key": "91:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلسَّمَآءِ وَمَا بَنَىٰهَا",
      "translation": "N’ikirere ndetse n’Uwagihanze,",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 595,
      "verse_key": "91:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡأَرۡضِ وَمَا طَحَىٰهَا",
      "translation": "N’isi n’Uwayirambuye,",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 595,
      "verse_key": "91:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَنَفۡسٖ وَمَا سَوَّىٰهَا",
      "translation": "Na roho n’uwayitunganyije;",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 595,
      "verse_key": "91:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَلۡهَمَهَا فُجُورَهَا وَتَقۡوَىٰهَا",
      "translation": "Hanyuma akayigaragariza ikibi n’icyiza,",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 595,
      "verse_key": "91:9",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قَدۡ أَفۡلَحَ مَن زَكَّىٰهَا",
      "translation": "Mu by’ukuri uweza roho ye (akubaha Allah kandi agakora ibikorwa byiza) yaratsinze,",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 595,
      "verse_key": "91:10",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَقَدۡ خَابَ مَن دَسَّىٰهَا",
      "translation": "Kandi rwose uwatereranye roho ye ari mu gihombo,",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 595,
      "verse_key": "91:11",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَذَّبَتۡ ثَمُودُ بِطَغۡوَىٰهَآ",
      "translation": "Abantu ba Thamudu bahinyuye (Intumwa yabo) kubera kwigomeka,",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 595,
      "verse_key": "91:12",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذِ ٱنۢبَعَثَ أَشۡقَىٰهَا",
      "translation": "Ubwo ubarusha ububi yahagurukanaga ibakwe (akajya kwica ingamiya y’imbyeyi),",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 595,
      "verse_key": "91:13",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَقَالَ لَهُمۡ رَسُولُ ٱللَّهِ نَاقَةَ ٱللَّهِ وَسُقۡيَٰهَا",
      "translation": "Nuko Intumwa ya Allah (Swaleh) irababwira iti “Muramenye! Iyo ni ingamiya ya Allah (ntimuyigirire nabi), kandi ntimuyibuze gushoka!”",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 595,
      "verse_key": "91:14",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَكَذَّبُوهُ فَعَقَرُوهَا فَدَمۡدَمَ عَلَيۡهِمۡ رَبُّهُم بِذَنۢبِهِمۡ فَسَوَّىٰهَا",
      "translation": "Ariko baramuhinyuye (ya ngamiya) barayica. Nuko Nyagasani wabo arabarimbura ku bw’icyaha cyabo, ndetse bose abarimbura mu buryo bureshya (baba abakuru n’abato, abakire n’abakene, ndetse n’abakomeye n’abaciriritse).",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 595,
      "verse_key": "91:15",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَا يَخَافُ عُقۡبَٰهَا",
      "translation": "Kandi (Allah) ntatinya ingaruka zabyo.",
      "footnotes": ""
    }
  ]
}