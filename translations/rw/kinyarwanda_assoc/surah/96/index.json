{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 597,
      "verse_key": "96:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱقۡرَأۡ بِٱسۡمِ رَبِّكَ ٱلَّذِي خَلَقَ",
      "translation": "Soma ku izina rya Nyagasani wawe waremye (ibiriho byose),",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 597,
      "verse_key": "96:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "خَلَقَ ٱلۡإِنسَٰنَ مِنۡ عَلَقٍ",
      "translation": "Yaremye umuntu mu rusoro rw’amaraso,",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 597,
      "verse_key": "96:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱقۡرَأۡ وَرَبُّكَ ٱلۡأَكۡرَمُ",
      "translation": "Soma! Kandi Nyagasani wawe ni Nyirubuntu uhebuje,",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 597,
      "verse_key": "96:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِي عَلَّمَ بِٱلۡقَلَمِ",
      "translation": "We wigishije (umuntu kwandika) akoresheje ikaramu,",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 597,
      "verse_key": "96:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلَّمَ ٱلۡإِنسَٰنَ مَا لَمۡ يَعۡلَمۡ",
      "translation": "Yigishije umuntu ibyo atari azi.",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 597,
      "verse_key": "96:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّآ إِنَّ ٱلۡإِنسَٰنَ لَيَطۡغَىٰٓ",
      "translation": "Oya! Mu by’ukuri umuntu ararengera (arahakana kandi agakora ibibi),",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 597,
      "verse_key": "96:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَن رَّءَاهُ ٱسۡتَغۡنَىٰٓ",
      "translation": "Iyo amaze kubona ko yihagije (kubera ibyo atunze).",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 597,
      "verse_key": "96:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ إِلَىٰ رَبِّكَ ٱلرُّجۡعَىٰٓ",
      "translation": "Mu by’ukuri (umenye ko) kwa Nyagasani wawe ari ho garukiro.",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 597,
      "verse_key": "96:9",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَرَءَيۡتَ ٱلَّذِي يَنۡهَىٰ",
      "translation": "Ese (yewe Muhamadi) wabonye wa wundi (Abu Jahl) ubuza",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 597,
      "verse_key": "96:10",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَبۡدًا إِذَا صَلَّىٰٓ",
      "translation": "Umugaragu (wacu Muhamadi) kugaragira?",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 597,
      "verse_key": "96:11",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَرَءَيۡتَ إِن كَانَ عَلَى ٱلۡهُدَىٰٓ",
      "translation": "Ese (yabuza ate umugaragu wacu) aramutse ari mu nzira y’ukuri,",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 597,
      "verse_key": "96:12",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَوۡ أَمَرَ بِٱلتَّقۡوَىٰٓ",
      "translation": "Cyangwa ari gukangurira (abantu) kugandukira Allah (mbwira icyaba iherezo ry’umubuza)?",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 597,
      "verse_key": "96:13",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَرَءَيۡتَ إِن كَذَّبَ وَتَوَلَّىٰٓ",
      "translation": "Ngaho mbwira (Abu Jahl) aramutse ahinyura (ukuri) akanatera umugongo,",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 597,
      "verse_key": "96:14",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَمۡ يَعۡلَم بِأَنَّ ٱللَّهَ يَرَىٰ",
      "translation": "Ese ntabwo azi ko Allah abona (ibyo akora)?",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 597,
      "verse_key": "96:15",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّا لَئِن لَّمۡ يَنتَهِ لَنَسۡفَعَۢا بِٱلنَّاصِيَةِ",
      "translation": "Oya! Natarekera aho, tuzamufata uruhanga rwe turukururire mu muriro,",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 597,
      "verse_key": "96:16",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "نَاصِيَةٖ كَٰذِبَةٍ خَاطِئَةٖ",
      "translation": "Uruhanga rubeshya, rw’urunyabyaha.",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 597,
      "verse_key": "96:17",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلۡيَدۡعُ نَادِيَهُۥ",
      "translation": "Ngaho nahamagare abambari be,",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 597,
      "verse_key": "96:18",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "سَنَدۡعُ ٱلزَّبَانِيَةَ",
      "translation": "Natwe tuzahamagara abamalayika bacu barinda umuriro (kugira ngo bamukanire urumukwiye).",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 597,
      "verse_key": "96:19",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": "obligatory",
      "sajdah_number": 14,
      "text": "كَلَّا لَا تُطِعۡهُ وَٱسۡجُدۡۤ وَٱقۡتَرِب۩",
      "translation": "Oya! (Yewe Muhamadi), ntukamwumvire (Abu Jahali), ahubwo ujye wubama (usali) uniyegereze Allah",
      "footnotes": ""
    }
  ]
}