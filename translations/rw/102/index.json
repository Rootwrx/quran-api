{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 600,
      "verse_key": "102:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلۡهَىٰكُمُ ٱلتَّكَاثُرُ",
      "translation": "Mwarangajwe no gushaka kwigwizaho ubutunzi,",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 600,
      "verse_key": "102:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "حَتَّىٰ زُرۡتُمُ ٱلۡمَقَابِرَ",
      "translation": "Kugeza ubwo mugeze mu mva,",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 600,
      "verse_key": "102:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّا سَوۡفَ تَعۡلَمُونَ",
      "translation": "Oya ntibikwiye! Bidatinze muzamenya (ko ubuzima bw’imperuka ari bwo bwiza kuri mwe).",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 600,
      "verse_key": "102:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ كَلَّا سَوۡفَ تَعۡلَمُونَ",
      "translation": "Oya na none! Bidatinze muzamenya.",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 600,
      "verse_key": "102:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّا لَوۡ تَعۡلَمُونَ عِلۡمَ ٱلۡيَقِينِ",
      "translation": "Oya! Iyo muza kugira ubumenyi budashidikanywaho (bw’ibijyanye n’iherezo ryo kurangazwa no kwigwizaho imitungo, ntimwari guhugira mu by’isi).",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 600,
      "verse_key": "102:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَتَرَوُنَّ ٱلۡجَحِيمَ",
      "translation": "Mu by’ukuri muzabona umuriro wa Jahiim!",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 600,
      "verse_key": "102:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ لَتَرَوُنَّهَا عَيۡنَ ٱلۡيَقِينِ",
      "translation": "Rwose muzawibonera imbonankubone.",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 600,
      "verse_key": "102:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ لَتُسۡـَٔلُنَّ يَوۡمَئِذٍ عَنِ ٱلنَّعِيمِ",
      "translation": "Kandi rwose, kuri uwo munsi muzabazwa ku ngabire (mwahawe muri ku isi).",
      "footnotes": ""
    }
  ]
}