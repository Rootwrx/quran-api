{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 604,
      "verse_key": "113:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قُلۡ أَعُوذُ بِرَبِّ ٱلۡفَلَقِ",
      "translation": "Vuga (yewe Muhamadi) uti “Niragije (Allah), Nyagasani w’umuseke”,",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 604,
      "verse_key": "113:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مِن شَرِّ مَا خَلَقَ",
      "translation": "“Ngo andinde inabi y’ibyo yaremye,”",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 604,
      "verse_key": "113:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمِن شَرِّ غَاسِقٍ إِذَا وَقَبَ",
      "translation": "“Anandinde ikibi cyo mu ijoro ryijimye igihe riguye,”",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 604,
      "verse_key": "113:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمِن شَرِّ ٱلنَّفَّٰثَٰتِ فِي ٱلۡعُقَدِ",
      "translation": "“Anandinde ikibi cy’abarozikazi bahuha ku mapfundo,”",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 604,
      "verse_key": "113:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمِن شَرِّ حَاسِدٍ إِذَا حَسَدَ",
      "translation": "“Ndetse anandinde inabi y’umunyeshyari igihe arigize.”",
      "footnotes": ""
    }
  ]
}