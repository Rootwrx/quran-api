{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 604,
      "verse_key": "112:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قُلۡ هُوَ ٱللَّهُ أَحَدٌ",
      "translation": "Sema: Yeye Mwenyezi Mungu ni wa pekee.",
      "footnotes": "Ewe Muhammad! Hao wanao sema kwa kejeli, \"Tusifie Mola wako Mlezi.\": waambie: Yeye ni Mwenyezi Mungu wa pekee hapana mwenginewe, wala hana mshirika."
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 604,
      "verse_key": "112:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱللَّهُ ٱلصَّمَدُ",
      "translation": "Mwenyezi Mungu Mkusudiwa.",
      "footnotes": "Mwenyezi Mungu Mwenye kukusudiwa peke yake katika haja zote, na yote yanayo takikana."
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 604,
      "verse_key": "112:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَمۡ يَلِدۡ وَلَمۡ يُولَدۡ",
      "translation": "Hakuzaa wala hakuzaliwa.",
      "footnotes": "Hana mtoto wala hakuzaliwa na baba wala mama,"
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 604,
      "verse_key": "112:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَمۡ يَكُن لَّهُۥ كُفُوًا أَحَدُۢ",
      "translation": "Wala hana anaye fanana naye hata mmoja.",
      "footnotes": "Wala hana yeyote aliye mshabihi, au mfano wake."
    }
  ]
}