{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 603,
      "verse_key": "109:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قُلۡ يَٰٓأَيُّهَا ٱلۡكَٰفِرُونَ",
      "translation": "Sema: Enyi makafiri!",
      "footnotes": "Ewe Muhammad! Sema: Enyi makafiri, mlio shikilia juu ya ukafiri wenu."
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 603,
      "verse_key": "109:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَآ أَعۡبُدُ مَا تَعۡبُدُونَ",
      "translation": "Siabudu mnacho kiabudu;",
      "footnotes": "Mimi simuabudu mnaye muabudu nyinyi badala ya Mwenyezi Mungu."
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 603,
      "verse_key": "109:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَآ أَنتُمۡ عَٰبِدُونَ مَآ أَعۡبُدُ",
      "translation": "Wala nyinyi hamuabudu ninaye muabudu.",
      "footnotes": "Wala nyinyi si wenye kumuabudu huyo ambaye mimi namuabudu, naye ni Mwenyezi Mungu wa pekee."
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 603,
      "verse_key": "109:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَآ أَنَا۠ عَابِدٞ مَّا عَبَدتُّمۡ",
      "translation": "Wala sitaabudu mnacho abudu.",
      "footnotes": "Wala mimi si mwenye kuabudu mfano wa ibada yenu, kwani nyinyi ni washirikina."
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 603,
      "verse_key": "109:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَآ أَنتُمۡ عَٰبِدُونَ مَآ أَعۡبُدُ",
      "translation": "Wala nyinyi hamuabudu ninaye muabudu.",
      "footnotes": "Wala nyinyi si wenye kuabudu mfano wa ibada yangu, kwani hii ni Tawhidi, ibada ya Mungu Mmoja."
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 603,
      "verse_key": "109:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَكُمۡ دِينُكُمۡ وَلِيَ دِينِ",
      "translation": "Nyinyi mna dini yenu, nami nina Dini yangu.",
      "footnotes": "Nyinyi mna dini yenu mnayo itakidi, na mimi nina Dini yangu aliyo niteulia Mwenyezi Mungu niishike."
    }
  ]
}