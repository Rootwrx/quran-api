{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 604,
      "verse_key": "114:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قُلۡ أَعُوذُ بِرَبِّ ٱلنَّاسِ",
      "translation": "Sema: Ninajikinga kwa Mola Mlezi wa wanaadamu,",
      "footnotes": "Sema: Najikinga na Mola Mlezi wa wanaadamu, Mwenye kuendesha mambo yao."
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 604,
      "verse_key": "114:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَلِكِ ٱلنَّاسِ",
      "translation": "Mfalme wa wanaadamu,",
      "footnotes": "Mfalme wa wanaadamu, kwa ufalme ulio timia, kuwamiliki wote wafalme wao na maraia."
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 604,
      "verse_key": "114:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَٰهِ ٱلنَّاسِ",
      "translation": "Mungu wa wanaadamu,",
      "footnotes": "Mungu wa wanaadamu, Mwenye uweza kwa kuwasarifu kwa ukamilifu atakavyo."
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 604,
      "verse_key": "114:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مِن شَرِّ ٱلۡوَسۡوَاسِ ٱلۡخَنَّاسِ",
      "translation": "Na shari ya wasiwasi wa Shetani, Khannas,",
      "footnotes": "Na shari ya mwenye kutia wasiwasi watu, ambaye hurudi nyuma ukiomba msaada kwa Mwenyezi Mungu dhidi yake."
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 604,
      "verse_key": "114:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِي يُوَسۡوِسُ فِي صُدُورِ ٱلنَّاسِ",
      "translation": "Anaye tia wasiwasi katika vifua vya watu,",
      "footnotes": "Ambaye hutia kwa kificho katika vifua vya watu ya kuwaachisha wasende kwenye njia ya uwongofu."
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 604,
      "verse_key": "114:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مِنَ ٱلۡجِنَّةِ وَٱلنَّاسِ",
      "translation": "Kutokana na majini na wanaadamu.",
      "footnotes": "Kutokana na majini na wanaadamu."
    }
  ]
}