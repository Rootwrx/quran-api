{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 602,
      "verse_key": "107:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَرَءَيۡتَ ٱلَّذِي يُكَذِّبُ بِٱلدِّينِ",
      "translation": "Je! Umemwona anaye kadhibisha Malipo?",
      "footnotes": "Unamjua anaye kanusha malipo na hisabu ya Akhera?"
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 602,
      "verse_key": "107:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَذَٰلِكَ ٱلَّذِي يَدُعُّ ٱلۡيَتِيمَ",
      "translation": "Huyo ndiye anaye msukuma yatima,",
      "footnotes": "Ukitaka kumjua basi ni huyo ambaye anaye msukuma yatima kwa nguvu, na anamtumilia mabavu, na anamdhulumu,"
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 602,
      "verse_key": "107:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَا يَحُضُّ عَلَىٰ طَعَامِ ٱلۡمِسۡكِينِ",
      "translation": "Wala hahimizi kumlisha masikini.",
      "footnotes": "Na wala hahimizi kulishwa masikini."
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 602,
      "verse_key": "107:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَوَيۡلٞ لِّلۡمُصَلِّينَ",
      "translation": "Basi, ole wao wanao swali,",
      "footnotes": "Maangamio yatawashukia wanao sali na wakasifika kwa sifa hizi, na wakaghafilika na Swala zao wasinafiike nazo."
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 602,
      "verse_key": "107:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِينَ هُمۡ عَن صَلَاتِهِمۡ سَاهُونَ",
      "translation": "Ambao wanapuuza Sala zao;",
      "footnotes": "Maangamio yatawashukia wanao swali na wakasifika kwa sifa hizi, na wakaghafilika na Swala zao wasinafiike nazo."
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 602,
      "verse_key": "107:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِينَ هُمۡ يُرَآءُونَ",
      "translation": "Ambao wanajionyesha,",
      "footnotes": "Ambao huonyesha kwa watu vitendo vyao, ili wapate cheo katika nyoyo za hao watu, na wasifiwe."
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 602,
      "verse_key": "107:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيَمۡنَعُونَ ٱلۡمَاعُونَ",
      "translation": "Nao huku wanazuia msaada.",
      "footnotes": "Na huku wanazuia hisani zao na msaada wao kwa watu"
    }
  ]
}