{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 604,
      "verse_key": "113:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قُلۡ أَعُوذُ بِرَبِّ ٱلۡفَلَقِ",
      "translation": "Sag: Ich nehme Zuflucht beim Herrn des Tagesanbruchs",
      "footnotes": null
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 604,
      "verse_key": "113:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مِن شَرِّ مَا خَلَقَ",
      "translation": "vor dem Übel dessen, was Er erschaffen hat,",
      "footnotes": null
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 604,
      "verse_key": "113:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمِن شَرِّ غَاسِقٍ إِذَا وَقَبَ",
      "translation": "und vor dem Übel der Dunkelheit, wenn sie zunimmt,",
      "footnotes": null
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 604,
      "verse_key": "113:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمِن شَرِّ ٱلنَّفَّٰثَٰتِ فِي ٱلۡعُقَدِ",
      "translation": "und vor dem Übel der Knotenanbläserinnen",
      "footnotes": null
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 604,
      "verse_key": "113:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمِن شَرِّ حَاسِدٍ إِذَا حَسَدَ",
      "translation": "und vor dem Übel eines (jeden) Neidenden, wenn er neidet.",
      "footnotes": null
    }
  ]
}