{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 586,
      "verse_key": "81:1",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذَا ٱلشَّمۡسُ كُوِّرَتۡ",
      "translation": "ߣߌ߫ ߕߋ߬ߟߋ ( ߢߊ ) ߓߎߙߎ߲ߞߐ߫ ߘߊ߫ ߕߎ߬ߡߊ ߡߍ߲",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 586,
      "verse_key": "81:2",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلنُّجُومُ ٱنكَدَرَتۡ",
      "translation": "ߊ߬ ߣߌ߫ ߣߌ߫ ߟߏ߬ߟߏ ߟߎ߬ ( ߦߋߟߋ߲ ) ߖߏ߬ߛߌ߬ ߘߊ߫ ߕߎ߬ߡߊ ߡߍ߲",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 586,
      "verse_key": "81:3",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡجِبَالُ سُيِّرَتۡ",
      "translation": "ߊ߬ ߣߌ߫ ߣߌ߫ ߞߎ߬ߙߎ ߟߎ߬ ߟߊߕߊ߯ ߘߊ߫ ߕߎ߬ߡߊ ߡߍ߲",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 586,
      "verse_key": "81:4",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡعِشَارُ عُطِّلَتۡ",
      "translation": "ߊ߬ ߣߌ߫ ߣߌ߫ ߓߊ߲߬ߘߊ߬ߕߊ ߟߎ߫ ߞߢߊ߬ ߘߊ߫ ( ߞߣߐߕߊ߬ ߟߊ߫ ) ߕߎ߬ߡߊ ߡߍ߲ ߜߟߊߞߏ߫",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 586,
      "verse_key": "81:5",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡوُحُوشُ حُشِرَتۡ",
      "translation": "ߊ߬ ߣߌ߫ ߣߌ߫ ߞߏ߲ߞߏ߫ ߓߌߦߊ߲ ߠߎ߬ ߟߊߘߍ߬ ߘߊ߫ ߕߎ߬ߡߊ ߡߍ߲",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 586,
      "verse_key": "81:6",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡبِحَارُ سُجِّرَتۡ",
      "translation": "ߊ߬ ߣߌ߫ ߣߌ߫ ߟߌ߲ߓߊ߲ ߠߎ߬ ߟߊߖߌ߯ߖߌ߯ ߘߊ߫ ߕߎ߬ߡߊ ߡߍ߲",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 586,
      "verse_key": "81:7",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلنُّفُوسُ زُوِّجَتۡ",
      "translation": "ߊ߬ ߣߌ߫ ߣߌ߫ ߣߌ ߟߎ߬ ߘߊߓߍ߲߫ ߘߊ߫ ( ߊ߬ ߝߊ߬ߘߌ ߟߎ߫ ߝߍ߬ ) ߕߎ߬ߡߊ ߡߍ߲",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 586,
      "verse_key": "81:8",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡمَوۡءُۥدَةُ سُئِلَتۡ",
      "translation": "ߊ߬ ߣߌ߫ ߣߌ߫ ߘߋ߲ߡߛߏ߫ ߢߣߊߡߊ߫ ߓߙߌ߬ߣߍ߲ ߢߌ߬ߣߌ߲߬ߞߊ߬ ߘߊ߫ ߕߎ߬ߡߊ ߡߍ߲",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 586,
      "verse_key": "81:9",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بِأَيِّ ذَنۢبٖ قُتِلَتۡ",
      "translation": "ߞߏ߫ ߊ߬ ߝߊ߰ ߘߊ߫ ߞߎ߲߬ ߞߋߟߋ߲ ߢߌ߬ߡߊ߲߫ ߠߋ߬ ߟߊ߫؟",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 586,
      "verse_key": "81:10",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلصُّحُفُ نُشِرَتۡ",
      "translation": "ߊ߬ ߣߌ߫ ߣߌ߫ ( ߗߋߥߊߟߌ ߛߓߍ߫ ) ߟߍߙߊ ߟߎ߬ ߘߐߝߍ߲߬ߛߍ߲߬ ߘߊ߫ ߕߎ߬ߡߊ ߡߍ߲",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 586,
      "verse_key": "81:11",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلسَّمَآءُ كُشِطَتۡ",
      "translation": "ߊ߬ ߣߌ߫ ߣߌ߫ ߡߐ߲ߕߐ߲ ߟߊߦߌ߬ߙߌ߲߬ߘߌ߬ ߘߊ߫ ߕߎ߬ߡߊ ߡߍ߲ ( ߞߊ߬ ߓߐ߫ ߊ߬ ߣߐ߭ ߘߐ߫ )",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 586,
      "verse_key": "81:12",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡجَحِيمُ سُعِّرَتۡ",
      "translation": "ߊ߬ ߣߌ߫ ߣߌ߫ ߥߋ߲߯ߓߋ ߟߊߡߍߣߍ߲߫ ߘߊ߫ ߕߎ߬ߡߊ ߡߍ߲",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 586,
      "verse_key": "81:13",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡجَنَّةُ أُزۡلِفَتۡ",
      "translation": "ߊ߬ ߣߌ߫ ߣߌ߫ ߡߍ߲ߘߌߦߊ߫ ߡߊߛߎ߬ߘߎ߲߬ߧߊ߬ ߘߊ߫ ߕߎ߬ߡߊ ߡߍ߲",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 586,
      "verse_key": "81:14",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلِمَتۡ نَفۡسٞ مَّآ أَحۡضَرَتۡ",
      "translation": "ߣߌߡߊ ߘߴߊ߬ ߟߐ߲߫ ( ߏ ߕߎ߬ߡߊ ) ߊ߬ ߞߊ߬ ߡߍ߲ ߠߊߣߊ߬ ( ߗߋߥߊߟߌ߫ ߘߐ߫ )",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 586,
      "verse_key": "81:15",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلَآ أُقۡسِمُ بِٱلۡخُنَّسِ",
      "translation": "ߍ߲߬ߍ߲߫ ߸ ߒ ߧߋ߫ ߒ ߞߊ߬ߟߌ߬ ߟߊ߫ ߟߋ߬ ߘߏ߲߬ߘߏ߲߬ߠߌ߲߬ ߟߏߟߏ ߟߎ߬ ߟߊ߫ ( ߊ߬ ߞߐߙߐ߲߫ ߘߐ߫ )",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 586,
      "verse_key": "81:16",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلۡجَوَارِ ٱلۡكُنَّسِ",
      "translation": "ߣߊߟߌ߬ߡߎ߲߫ ߓߏ߬ߙߌ߬ߟߊ߫ ߘߏ߲߰ߠߌ߲߬ߠߊ ߟߎ߬ ߘߌ߫ ( ߗߙߎ߫ )",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 586,
      "verse_key": "81:17",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّيۡلِ إِذَا عَسۡعَسَ",
      "translation": "ߊ߬ ߣߌ߫ ߛߎ ߞߏ߬ߕߐ ߝߣߊ߫ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 586,
      "verse_key": "81:18",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلصُّبۡحِ إِذَا تَنَفَّسَ",
      "translation": "ߊ߬ ߣߌ߫ ߞߣߍ ߓߕߐ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 586,
      "verse_key": "81:19",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُۥ لَقَوۡلُ رَسُولٖ كَرِيمٖ",
      "translation": "( ߘߐ߬ߛߊߙߌ ) ߦߋ߫ ߞߋߟߊ߫ ߞߊ߬ߡߙߊ߬ߒߞߋ ( ߟߊ߫ ߟߊ߬ߛߋ߬ߟߌ߬ ) ߞߊ߲ ߠߋ߬ ߘߌ߫ ߘߋ߬",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 586,
      "verse_key": "81:20",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ذِي قُوَّةٍ عِندَ ذِي ٱلۡعَرۡشِ مَكِينٖ",
      "translation": "ߛߍ߲ߓߍߕߌ߯ ߡߝߊߣߍ߲ ߠߋ߫ ߛߍ߬ߡߍ ߕߌ߱ ߕߍ߬ߟߊ ߘߐ߫",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 586,
      "verse_key": "81:21",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مُّطَاعٖ ثَمَّ أَمِينٖ",
      "translation": "ߞߊ߲ߡߌߘߕߊ ߟߴߊ߬ ߘߌ߫ ߦߋ߲߬ ߟߊߒߠߊߒߕߋ ߟߋ߬",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 586,
      "verse_key": "81:22",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا صَاحِبُكُم بِمَجۡنُونٖ",
      "translation": "ߊߟߎ߫ ߘߍ߬ߢߐ߲߮ ߕߍ߫ ߝߊ߬ߕߐ߬ ߘߌ߫ ߞߏߦߌ߫",
      "footnotes": ""
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 586,
      "verse_key": "81:23",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَقَدۡ رَءَاهُ بِٱلۡأُفُقِ ٱلۡمُبِينِ",
      "translation": "ߊ߬ ߞߊ߬ ‹ ߖߌ߬ߓߙߌߟߎ߫ › ߦߋ߫ ߢߊߡߌߟߏ߫ ߘߐߜߍ ߘߐ߫ ߟߋ߬ ߛߎߋ߫",
      "footnotes": ""
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 586,
      "verse_key": "81:24",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا هُوَ عَلَى ٱلۡغَيۡبِ بِضَنِينٖ",
      "translation": "ߊ߬ ߘߏ߲߬ ߕߍ߫ ߞߣߐߘߐߖߎ߯ ߘߌ߫ ߞߏ߫ ߘߏ߲߰ߣߍ߲߫ ߞߊ߲߬",
      "footnotes": ""
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 586,
      "verse_key": "81:25",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا هُوَ بِقَوۡلِ شَيۡطَٰنٖ رَّجِيمٖ",
      "translation": "‹ ߘߐ߬ߛߊߙߌ ߘߏ߲߬ › ߕߍ߫ ߜߍ߬ߘߍ߫ ߞߙߎ߬ߟߊ߬ߓߏ߲߬ߕߊ ߟߊ߫ ߟߊ߬ߛߋ߬ߟߌ߬ ߞߊ߲߫ ߘߌ߫",
      "footnotes": ""
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 586,
      "verse_key": "81:26",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَيۡنَ تَذۡهَبُونَ",
      "translation": "ߒ߬ߓߵߊߟߎ߫ ߥߊߕߐ߫ ߡߌ߲߫ ( ߘߐ߬ߛߊߙߌ ߟߊ߫ ߞߊ߲ߘߊ߫ ߛߌߟߊ ߞߐ߫) ߛߊ߫؟",
      "footnotes": ""
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 586,
      "verse_key": "81:27",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنۡ هُوَ إِلَّا ذِكۡرٞ لِّلۡعَٰلَمِينَ",
      "translation": "ߊ߬ ߘߏ߲߬ ߕߍ߫ ߘߏ߫ ߜߘߍ߫ ߘߌ߫ ߝߏ߫ ߖߊ߯ߓߊ ߟߎ߫ ߖߊ߲߬ߓߌߟߊ",
      "footnotes": ""
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 586,
      "verse_key": "81:28",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لِمَن شَآءَ مِنكُمۡ أَن يَسۡتَقِيمَ",
      "translation": "ߡߐ߰ ߦߋ߫ ߞߊ߬ߣߌ߲߫ ߖߘߍ߬ ߟߊ߬ߕߋߟߋ߲ ߓߊ߯ ߘߌߦߊ߫ ߡߍ߲ ߦߋ߫ ߊߟߎ߫ ߘߐ߫",
      "footnotes": ""
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 586,
      "verse_key": "81:29",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا تَشَآءُونَ إِلَّآ أَن يَشَآءَ ٱللَّهُ رَبُّ ٱلۡعَٰلَمِينَ",
      "translation": "ߊ߬ ߘߏ߲߬ ߕߍ߫ ߊߟߎ߫ ߘߌߦߊ߫ ߝߋߎ߫ ߝߏ߫ ߊ߬ ߓߊ߯ ߖߊ߯ߓߊ ߟߎ߫ ߡߊ߰ߙߌ ߊߟߊ߫ ߘߌߦߊ߫",
      "footnotes": ""
    }
  ]
}