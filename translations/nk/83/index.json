{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 587,
      "verse_key": "83:1",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيۡلٞ لِّلۡمُطَفِّفِينَ",
      "translation": "ߜߊ߬ߟߏ߬ ߦߋ߫ ߕߊ߲ߓߏ߲ߕߏߟߊ ߟߎ߬ ߦߋ߫ ߞߟߋ߫",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 587,
      "verse_key": "83:2",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِينَ إِذَا ٱكۡتَالُواْ عَلَى ٱلنَّاسِ يَسۡتَوۡفُونَ",
      "translation": "ߡߍ߲ ߠߎ߬ ߓߊ߯ ߞߍ߫ ߛߎߡߊ߲ߠߌ߲ ߕߊ߬ ߟߊ߫ ߡߐ߱ ߟߎ߫ ߞߎ߲߬ ( ߊ߬ߟߎ߫ ߖߘߍ߬ ߦߋ߫ ) ߊ߬ߟߎ߬ ߘߴߏ߬ ߟߝߊ߫ ߞߏ ߢߌߣߌ߲߫",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 587,
      "verse_key": "83:3",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا كَالُوهُمۡ أَو وَّزَنُوهُمۡ يُخۡسِرُونَ",
      "translation": "ߊ߬ߟߎ߬ ߓߊ߯ ߞߍ߫ ߛߎߡߊ߲ߠߌ߲ ߕߊ߬ ߟߊ߫ ( ߊ߬ߟߎ߫ ߖߍ߬ߘߍ ߞߎ߲߬ ) ߡߐ߱ ߟߎ߬ ߦߋ߫ ߛߊ߫ ߥߟߊ߫ ߞߵߊ߬ ߦߟߍ߬ ߖߊ߭ ߟߴߊ߬ߟߎ߬ ߦߋ߫ ߸ ߊ߬ߟߎ߬ ߘߴߏ߬ ߟߎ߬ ߟߊߘߛߏ߫",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 587,
      "verse_key": "83:4",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَا يَظُنُّ أُوْلَٰٓئِكَ أَنَّهُم مَّبۡعُوثُونَ",
      "translation": "ߦߊ߯ߟߊ߫ ߏ߬ ߟߎ߬ ߕߍ߫ ߊ߬ߟߎ߬ ߟߊߥߟߌ߬ߞߏ ߖߌ߰ ߟߊ߫ ߟߋ߫ ߓߊ߬؟",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 587,
      "verse_key": "83:5",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لِيَوۡمٍ عَظِيمٖ",
      "translation": "ߟߏ߲ߓߊ ߘߏ߫ ߘߐ߫",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 587,
      "verse_key": "83:6",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَوۡمَ يَقُومُ ٱلنَّاسُ لِرَبِّ ٱلۡعَٰلَمِينَ",
      "translation": "ߡߐ߱ ߟߎ߫ ߟߐ߬ߕߐ߫ ߖߊ߯ߓߊ ߟߎ߫ ߡߊ߰ߙߌ ( ߟߊ߫ ߥߊߟߋߦߊ ) ߡߊ߬ ߟߏ߲ ߡߍ߲",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 588,
      "verse_key": "83:7",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّآ إِنَّ كِتَٰبَ ٱلۡفُجَّارِ لَفِي سِجِّينٖ",
      "translation": "ߤߊ߲߫ ߸ ߡߐ߰ ߖߎ߮ ߟߎ߬ ߟߊ߫ ߟߍߙߊ ߦߋ߫ ߔߊ߬ߔߊߟߌ߬ߔߊ ߟߋ߬ ߘߐ߫ ߘߋ߬",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 588,
      "verse_key": "83:8",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَآ أَدۡرَىٰكَ مَا سِجِّينٞ",
      "translation": "ߡߎ߲߬ ߞߵߌߟߋ ߟߊߟߐ߲ߠߌ߲߫ ߔߊ߬ߔߊߟߌ߬ߔߊ ߟߊ߫؟",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 588,
      "verse_key": "83:9",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كِتَٰبٞ مَّرۡقُومٞ",
      "translation": "ߏ߬ ߦߋ߫ ߟߍߙߊ߫ ߛߓߍߣߍ߲ ߘߏ߫ ߟߋ߬ ߘߌ߫",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 588,
      "verse_key": "83:10",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ",
      "translation": "ߜߊ߬ߟߏ߬ ߦߋ߫ ߏ߬ ߟߏ߲ ߘߐ߫ ߡߊ߬ߛߐ߬ߛߐ߬ߟߌ߬ߟߊ ߟߎ߬ ߦߋ߫",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 588,
      "verse_key": "83:11",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِينَ يُكَذِّبُونَ بِيَوۡمِ ٱلدِّينِ",
      "translation": "ߡߍ߲ ߠߎ߬ ߦߋ߫ ߛߙߊ߬ߟߌ ߟߏ߲ ߡߊߛߐ߬ߛߐ߬ ߟߊ߫ ߞߊ߬ߣߌ߲߫",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 588,
      "verse_key": "83:12",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا يُكَذِّبُ بِهِۦٓ إِلَّا كُلُّ مُعۡتَدٍ أَثِيمٍ",
      "translation": "ߡߊ߬ߛߐ߬ߛߐ߬ߓߊ߰ ߛߌ߫ ߘߏ߲߬ ߕߍ߫ ߏ߬ ߟߊ߫ ߝߏ߫ ߘߊ߲߬ߠߕߊ߬ߡߌ߲߬ߓߊ߯ ߖߟߎ߬ߡߎ߲߬ߕߐ ߘߊ߬ߡߊ",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 588,
      "verse_key": "83:13",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذَا تُتۡلَىٰ عَلَيۡهِ ءَايَٰتُنَا قَالَ أَسَٰطِيرُ ٱلۡأَوَّلِينَ",
      "translation": "ߊ߲ ߠߊ߫ ߟߝߊߙߌ ߟߎ߬ ߓߊ߯ ߘߐߛߊߙߌ߫ ߏ߬ ߟߋ ߦߋ߫ ߸ ߊ߬ ߘߴߊ߬ ߝߐ߫ ߞߏ߫ ߝߟߐ߫ ߡߐ߱ ߟߎ߬ ߟߊ߫ ߛߌߙߌ߲ ߠߎ߫ ߟߋ߬",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 588,
      "verse_key": "83:14",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّاۖ بَلۡۜ رَانَ عَلَىٰ قُلُوبِهِم مَّا كَانُواْ يَكۡسِبُونَ",
      "translation": "ߍ߲߬ߍ߲߫ ߸ ߊ߬ ߕߍ߫ ߕߋ߲߬ ߞߟߋ߫ ߸ ߊ߬ߟߎ߬ ߗߋߥߊߟߌ߫ ( ߖߎ߮ ߛߊ߬ߣߊ߲ ) ߠߋ߬ ߓߘߊ߫ ߟߴߊ߬ߟߎ߫ ߛߣߐ߬ߡߍ ߟߎ߬ ߢߍߡߊ߬",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 588,
      "verse_key": "83:15",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّآ إِنَّهُمۡ عَن رَّبِّهِمۡ يَوۡمَئِذٖ لَّمَحۡجُوبُونَ",
      "translation": "ߝߋ߲ ߡߍ߲ ߠߎ߫ ߝߊ߲߭ ߥߣߊ߬ߙߌ߬ߕߐ߫ ߊ߬ߟߎ߫ ߡߊ߰ߙߌ ( ߟߊ߫ ߤߌߣߊ ) ߡߊ߬ ߏ߬ ߟߏ߲ ߞߊ߬ߣߌ߲߫",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 588,
      "verse_key": "83:16",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ إِنَّهُمۡ لَصَالُواْ ٱلۡجَحِيمِ",
      "translation": "ߏ߬ ߓߍ߯ ߞߐ߫ ߸ ߊ߬ߟߎ߬ ߦߋ߫ ߥߋ߲߯ߓߋ ߘߐ߫ ߘߏ߲߬ߓߊ߮ ߟߎ߫ ߟߋ߬ ߘߌ߫",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 588,
      "verse_key": "83:17",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ يُقَالُ هَٰذَا ٱلَّذِي كُنتُم بِهِۦ تُكَذِّبُونَ",
      "translation": "ߊ߬ ߘߌ߫ ߓߊ߲߫ ߞߊ߬ ߝߐ߫ ߊ߬ߟߎ߬ ߦߋ߫ ߞߏ߫ ߊߟߎ߫ ߕߎ߲߬ ߦߋ߫ ߡߍ߲ ߡߊߛߐ߬ߛߐ߬ ߟߊ߫ ߏ߬ ߟߋ ߦߋ߫ ߣߌ߲߬",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 588,
      "verse_key": "83:18",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّآ إِنَّ كِتَٰبَ ٱلۡأَبۡرَارِ لَفِي عِلِّيِّينَ",
      "translation": "ߢߌ߬ߡߦߊ߬ߟߊ ߟߎ߬ ߟߊ߫ ߟߍߙߊ ߛߊ߫ ߸ ߏ߬ ߦߋ߫ ߛߊ߲ߡߊߞߎ߬ߟߋߞߎ߬ߟߋ ߟߎ߫ ߟߋ߬ ߘߐ߫",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 588,
      "verse_key": "83:19",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَآ أَدۡرَىٰكَ مَا عِلِّيُّونَ",
      "translation": "ߡߎ߲߬ ߞߵߌ ߟߊߟߐ߲ߠߌ߲߫ ߛߊ߲ߡߊߞߎ߬ߟߋߞߎ߬ߟߋ ߟߎ߬ ߟߊ߫ ( ߞߎߟߋߞߎߟߋ߫ )؟",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 588,
      "verse_key": "83:20",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كِتَٰبٞ مَّرۡقُومٞ",
      "translation": "ߏ߬ ߦߋ߫ ߟߍߙߊ߫ ߛߓߍߣߍ߲ ߘߏ߫ ߟߋ߬ ߘߌ߫",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 588,
      "verse_key": "83:21",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَشۡهَدُهُ ٱلۡمُقَرَّبُونَ",
      "translation": "ߡߊ߬ߛߎ߬ߘߎ߲߬ߧߊ߬ߓߊ߮ ߟߎ߫ ߟߋ߬ ߛߙߋߦߊ߫ ߟߴߏ߬ ߡߊ߬",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 588,
      "verse_key": "83:22",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ ٱلۡأَبۡرَارَ لَفِي نَعِيمٍ",
      "translation": "ߢߌ߬ߡߦߊ߬ߟߊ ߟߎ߫ ߞߏ߬ߣߌ߲ ߣߍߡߊ ߟߋ߬ ߘߐ߫ ߘߋ߬",
      "footnotes": ""
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 588,
      "verse_key": "83:23",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلَى ٱلۡأَرَآئِكِ يَنظُرُونَ",
      "translation": "ߊ߬ߟߎ߬ ߦߋ߫ ߞߙߊߞߊ ߟߎ߫ ߞߊ߲߬ ߡߊ߬ߝߟߍ߬ߟߌ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 588,
      "verse_key": "83:24",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تَعۡرِفُ فِي وُجُوهِهِمۡ نَضۡرَةَ ٱلنَّعِيمِ",
      "translation": "ߌ ߘߌ߫ ( ߡߍ߲ߘߌߦߊ ) ߡߊ߬ߣߍ߬ߡߊ߬ߟߌ ߟߐ߲߫ ߊ߬ߟߎ߬ ߞߏߟߘߊ ߟߎ߬ ߘߐ߫",
      "footnotes": ""
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 588,
      "verse_key": "83:25",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يُسۡقَوۡنَ مِن رَّحِيقٖ مَّخۡتُومٍ",
      "translation": "ߊ߬ߟߎ߬ ߦߋ߫ ߟߊߡߌ߲߬ ߠߊ߫ ߡߌ߲߬ߕߊ߫ ߔߌߦߐߔߌߦߐ߫ ߘߊ߫ ߣߘߐߣߍ߲ ߠߊ߫",
      "footnotes": ""
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 588,
      "verse_key": "83:26",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "خِتَٰمُهُۥ مِسۡكٞۚ وَفِي ذَٰلِكَ فَلۡيَتَنَافَسِ ٱلۡمُتَنَٰفِسُونَ",
      "translation": "ߏ߬ ߘߊߕߎ߲߮ ߦߋ߫ ߛߎߡߊߙߋ ߟߋ߬ ߘߌ߫ ߸ ߒ߬ߓߊ߬ ߛߊ߲ߞߊߢߐ߲߯ߦߟߊ ߟߎ߬ ߦߋ߫ ߛߊ߲ߞߊߢߐ߲߯ߦߊ ߞߍ߫ ߟߴߏ߬ ߟߋ ( ߢߌߣߌ߲ ) ߘߐ߫ ߛߊ߫",
      "footnotes": ""
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 588,
      "verse_key": "83:27",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمِزَاجُهُۥ مِن تَسۡنِيمٍ",
      "translation": "ߏ߬ ߘߞߐߟߌ߲ߟߊ߲ ߦߋ߫ ߓߐ߫ ߟߊ߫ ߖߌߥߊ߬ߛߊߥߊ߬ߛߊ ߟߋ߬ ߘߐ߫",
      "footnotes": ""
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 588,
      "verse_key": "83:28",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَيۡنٗا يَشۡرَبُ بِهَا ٱلۡمُقَرَّبُونَ",
      "translation": "ߞߐ߬ߢߊ߫ ߟߴߏ߬ ߘߌ߫ ߸ ߡߊ߬ߛߎ߬ߘߎ߲߬ߧߊ߬ߓߊ߮ ߟߎ߬ ߦߴߊ߬ߟߎ߫ ߡߌ߲߬ ߠߊ߫ ߡߍ߲ ߠߊ߫",
      "footnotes": ""
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 588,
      "verse_key": "83:29",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ ٱلَّذِينَ أَجۡرَمُواْ كَانُواْ مِنَ ٱلَّذِينَ ءَامَنُواْ يَضۡحَكُونَ",
      "translation": "ߒ߬ߓߊ߬ ߡߍ߲ ߠߎ߬ ߞߍߣߍ߲߫ ߛߐ߲߬ߡߊ ߟߎ߬ ߘߌ߫ ߛߊ߫ ߸ ߏ߬ ߟߎ߫ ߟߋ߫ ߕߎ߲߬ ߦߴߊ߬ߟߎ߬ ߦߟߍߓߐ߫ ߟߊ߫ ߟߊߒߠߦߊߓߊ߮ ߟߎ߬ ߘߐ߫ ߔߙߏ߫",
      "footnotes": ""
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 588,
      "verse_key": "83:30",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا مَرُّواْ بِهِمۡ يَتَغَامَزُونَ",
      "translation": "ߊ߬ߟߎ߬ ߓߊ߯ ߞߍ߫ ߕߊ߬ߡߌ߲߬ ߠߴߊ߬ߟߎ߬ ߟߊ߫ ߞߍ߬ߣߍ߲ ߊ߬ߟߎ߬ ߘߴߊ߬ߟߎ߬ ߢߊߥߎߟߎ߲ ߠߎ߫ ߟߐ߬ ߊ߬ߟߎ߬ ߘߐ߫",
      "footnotes": ""
    },
    {
      "id": 31,
      "verse_number": 31,
      "page_number": 588,
      "verse_key": "83:31",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱنقَلَبُوٓاْ إِلَىٰٓ أَهۡلِهِمُ ٱنقَلَبُواْ فَكِهِينَ",
      "translation": "ߣߴߊ߬ߟߎ߫ ߘߏ߲߬ ߛߊ߬ߦߌ߲߬ ߘߴߊ߬ߟߎ߬ ߞߐߙߍ ߟߎ߫ ߡߵߊ߬ߟߎ߬ ߦߋ߫ ߛߊ߬ߦߌ߲߬ ߠߴߊ߬ߟߎ߬ ߘߝߐ ߟߋ߬ ߘߌ߫",
      "footnotes": ""
    },
    {
      "id": 32,
      "verse_number": 32,
      "page_number": 588,
      "verse_key": "83:32",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا رَأَوۡهُمۡ قَالُوٓاْ إِنَّ هَٰٓؤُلَآءِ لَضَآلُّونَ",
      "translation": "ߊ߬ߟߎ߬ ߓߊ߯ ߊ߬ߟߎ߬ ߦߋ߫ ߌߞߘߐ߫ ߊ߬ߟߎ߬ ߘߴߊ߬ ߝߐ߫ ߞߏ߫ ߤߋ߫ ߸ ߝߟߌ߬ߓߊ߮ ߟߎ߫ ߟߋ߬ ߢߌ߲߬ ߠߎ߬ ߘߌ߫ ߘߋ߬",
      "footnotes": ""
    },
    {
      "id": 33,
      "verse_number": 33,
      "page_number": 588,
      "verse_key": "83:33",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَآ أُرۡسِلُواْ عَلَيۡهِمۡ حَٰفِظِينَ",
      "translation": "ߊ߬ߟߎ ߘߏ߲߬ ߡߊ߫ ߗߋ߫ ߏ߬ ߟߎ߫ ߞߣߐ߬ߜߍ߲߬ ߞߊ߲ߡߊ߬ ߥߍ߫",
      "footnotes": ""
    },
    {
      "id": 34,
      "verse_number": 34,
      "page_number": 588,
      "verse_key": "83:34",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَٱلۡيَوۡمَ ٱلَّذِينَ ءَامَنُواْ مِنَ ٱلۡكُفَّارِ يَضۡحَكُونَ",
      "translation": "ߒ߬ߓߊ߬ ߓߌ߬ ߛߊ߫߸ ߡߍ߲ ߠߎ߬ ߞߍߣߍ߲߫ ߟߊߒߠߦߊߓߊ߮ ߘߌ߫ ߏ߬ ߟߎ߬ ߦߋ߫ ߦߟߍߓߐ߫ ߟߊ߫ ߟߋ߬ ߓߊ߲߬ߓߊ߮ ߟߎ߬ ߘߐ߫",
      "footnotes": ""
    },
    {
      "id": 35,
      "verse_number": 35,
      "page_number": 589,
      "verse_key": "83:35",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلَى ٱلۡأَرَآئِكِ يَنظُرُونَ",
      "translation": "ߊ߬ߟߎ߬ ߦߋ߫ ߞߙߊߞߊ ߟߎ߫ ߞߊ߲߬ ߡߊ߬ߝߟߍ߬ߟߌ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 36,
      "verse_number": 36,
      "page_number": 589,
      "verse_key": "83:36",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "هَلۡ ثُوِّبَ ٱلۡكُفَّارُ مَا كَانُواْ يَفۡعَلُونَ",
      "translation": "( ߞߏ߫ ) ߦߊ߯ߟߊ߫ ߓߊ߲߬ߓߊ߮ ߟߎ߫ ߛߙߊ߬ ߘߴߊ߬ߟߎ߬ ߗߋߥߊߟߌ ߟߊ߫ ߓߊ߬؟",
      "footnotes": ""
    }
  ]
}