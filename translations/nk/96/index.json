{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 597,
      "verse_key": "96:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱقۡرَأۡ بِٱسۡمِ رَبِّكَ ٱلَّذِي خَلَقَ",
      "translation": "ߌ ߡߊ߰ߙߌ ߕߐ߮ ߝߐ߫ ( ߘߊߡߌߣߊ ߟߊ߫ ) ߡߍ߲ ߞߊ߬ ߘߊ߲ߠߌ߲ ߞߍ߫",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 597,
      "verse_key": "96:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "خَلَقَ ٱلۡإِنسَٰنَ مِنۡ عَلَقٍ",
      "translation": "ߊ߬ ߞߊ߬ ߡߐ߱ ߘߊ߲߫ ߖߋ߬ߟߌ߬ ߛߌ߲ߓߌ ߟߋ߬ ߘߐ߫",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 597,
      "verse_key": "96:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱقۡرَأۡ وَرَبُّكَ ٱلۡأَكۡرَمُ",
      "translation": "ߊ߬ ( ߕߐ߮ ) ߝߐ߫ ߘߋ߬ ߸ ߓߊߏ߬ ߌ ߡߊ߰ߙߌ ߟߋ߫ ߞߊ߬ߡߙߊ߬ߒ߬ߞߋ߬ߓߊ ߘߌ߫",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 597,
      "verse_key": "96:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِي عَلَّمَ بِٱلۡقَلَمِ",
      "translation": "ߊ߬ߟߋ ߟߋ߫ ߞߊ߬ ߟߊ߬ߟߐ߲߬ߠߌ߲߬ߠߌ߲ ߞߍ߫ ߞߊ߬ߟߊ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 597,
      "verse_key": "96:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلَّمَ ٱلۡإِنسَٰنَ مَا لَمۡ يَعۡلَمۡ",
      "translation": "ߊ߬ ߞߊ߬ ߡߐ߱ ߟߊߟߐ߲ߠߌ߲߫ ߞߏ߫ ߟߴߊ߬ ߕߎ߲߬ ߡߊ߫ ߡߍ߲ ߟߐ߲߫",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 597,
      "verse_key": "96:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّآ إِنَّ ٱلۡإِنسَٰنَ لَيَطۡغَىٰٓ",
      "translation": "ߤߊ߲߫ ߸ ߡߐ߱ ߦߋ߫ ߘߊ߲߬ߠߕߊ߬ߡߌ߲߬ߓߊ߰ߕߐ ߟߋ߬ ߘߌ߫ ߘߋ߬",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 597,
      "verse_key": "96:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَن رَّءَاهُ ٱسۡتَغۡنَىٰٓ",
      "translation": "ߣߴߊ߬ ߞߵߊ߬ ߦߋ߫ ߊ߬ ߓߘߊ߫ ߓߣߊ߬ߦߊ߫",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 597,
      "verse_key": "96:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ إِلَىٰ رَبِّكَ ٱلرُّجۡعَىٰٓ",
      "translation": "ߞߐߛߊߦߌ߲ ߘߏ߲߬ ߞߍߕߐ߫ ߌ ߡߊ߰ߙߌ ߟߋ߫ ߡߊ߬ ߘߋ߬",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 597,
      "verse_key": "96:9",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَرَءَيۡتَ ٱلَّذِي يَنۡهَىٰ",
      "translation": "ߌ ߞߵߊ߬ ( ߟߋ߬ ߖߊ߲߬ߘߊ߬ߓߊ߮ ) ߦߋ߫ ߓߊ߬ ߡߍ߲ ߦߋ߫ ߜߍ߲ߠߌ߲ ߞߍ߫ ߟߊ߫؟",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 597,
      "verse_key": "96:10",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَبۡدًا إِذَا صَلَّىٰٓ",
      "translation": "( ߊߟߊ߫ ߟߊ߫ ) ߖߐ߲߭ ߠߊ߫ ߏ߬ ߛߊߟߌ߫ ߕߎߡߊ",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 597,
      "verse_key": "96:11",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَرَءَيۡتَ إِن كَانَ عَلَى ٱلۡهُدَىٰٓ",
      "translation": "ߌ ߞߵߊ߬ ߦߋ߫ ߓߊ߬ ߣߴߊ߬ ߦߋ߫ ߞߊ߲ߘߦߊ ߟߋ߫ ߞߊ߲߬؟",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 597,
      "verse_key": "96:12",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَوۡ أَمَرَ بِٱلتَّقۡوَىٰٓ",
      "translation": "ߥߟߴߊ߬ ߞߊ߬ ߟߐ߬ ߞߍ߫ ߝߊ߲ߕߊ߲ߞߊ ߟߋ߬ ߟߊ߫؟",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 597,
      "verse_key": "96:13",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَرَءَيۡتَ إِن كَذَّبَ وَتَوَلَّىٰٓ",
      "translation": "ߌ ߞߵߊ߬ ߦߋ߫ ߓߊ߬ ߣߴߊ߬ ߞߊ߬ ߡߊ߬ߛߐ߬ߛߐ߬ߟߌ ߞߍ߫ ߞߵߊ߬ ߞߐߘߏ߲߬؟",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 597,
      "verse_key": "96:14",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَمۡ يَعۡلَم بِأَنَّ ٱللَّهَ يَرَىٰ",
      "translation": "ߦߊ߯ߟߴߊ߬ ߡߴߊ߬ ߟߐ߲߫ ߞߏ߫ ߊߟߊ߫ ߓߍ߫ ߦߋߟߌ ߞߍ߫ ߟߊ߫ ߘߍ߫؟",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 597,
      "verse_key": "96:15",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّا لَئِن لَّمۡ يَنتَهِ لَنَسۡفَعَۢا بِٱلنَّاصِيَةِ",
      "translation": "ߒ߬ߓߊ߬ ߣߴߊ߬ ߡߊ߫ ߕߏ߫ ߸ ߊ߲ ߘߴߊ߬ ߛߊ߬ߡߊ߲߬ ߊ߬ ߕߋ߲ߛߌ ߡߊ߬ ( ߕߊߦߌ߲߫ )",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 597,
      "verse_key": "96:16",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "نَاصِيَةٖ كَٰذِبَةٍ خَاطِئَةٖ",
      "translation": "ߕߋ߲ߛߌ߫ ߥߎߦߊ߫ ߝߐߟߊ߫ ߝߎ߬ߕߎ߲߬ߕߌ߬ߟߊ ߞߊ߬ߣߌ߲߫",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 597,
      "verse_key": "96:17",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلۡيَدۡعُ نَادِيَهُۥ",
      "translation": "ߊ߬ ߦߴߊ߬ ߟߊ߫ ߞߍߙߍ߲ ߡߊߞߟߌ߫ ( ߊ߬ ߘߍ߬ߡߍ߲߬ ߞߊ߲ ߡߊ߬ ) ߝߛߊߦߌ߫",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 597,
      "verse_key": "96:18",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "سَنَدۡعُ ٱلزَّبَانِيَةَ",
      "translation": "ߒ߬ ߝߣߊ߫ ߘߌ߫ ߕߐ߲ߓߟߏߡߊ ߟߎ߬ ߡߊߞߟߌ߫",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 597,
      "verse_key": "96:19",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": "obligatory",
      "sajdah_number": 14,
      "text": "كَلَّا لَا تُطِعۡهُ وَٱسۡجُدۡۤ وَٱقۡتَرِب۩",
      "translation": "ߞߏ߬ߣߌ߲߬ ߍ߲߬ߍ߲߫ ߸ ߌ ߞߴߊ߬ ߞߊ߲ߡߌ߬ߘߊ߬ ߞߟߋ߫ ߸ ߌ ߦߴߌ ߕߌ߲ ߓߌߘߌ߲߫ ߠߋ߬ ߞߵߌ ߡߊߛߎ߬ߘߎ߲߬ߧߊ߫",
      "footnotes": ""
    }
  ]
}