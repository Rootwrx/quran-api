{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 564,
      "verse_key": "68:1",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "نٓۚ وَٱلۡقَلَمِ وَمَا يَسۡطُرُونَ",
      "translation": "ߣ ( ߣߎ߯ߣ )߸ ߊ߰ ߒ ߣߌ߫ ߞߊ߬ߟߊ ߕߍ߫ ߸ ߊ߬ ߣߌ߫ ( ߞߋ߲ߛߐ߲ ߠߎ߬ ) ߦߋ߫ ߡߍ߲ ߛߓߍ߫ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 564,
      "verse_key": "68:2",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَآ أَنتَ بِنِعۡمَةِ رَبِّكَ بِمَجۡنُونٖ",
      "translation": "ߌ ߡߊ߰ߙߌ ߟߊ߫ ߣߍߡߊ ߛߘߊ߫ ߘߐ߫ ߌ ߕߍ߫ ߝߊ߬ߕߐ߫ ߘߌ߫ ߝߋߎ߫",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 564,
      "verse_key": "68:3",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِنَّ لَكَ لَأَجۡرًا غَيۡرَ مَمۡنُونٖ",
      "translation": "ߛߊ߬ߙߊ߫ ߟߋ߫ ߝߊ߲߭ ߓߍ߫ ߌ ߦߋ߫ ߡߍ߲ ߕߍ߫ ߓߟߏߕߍ߰ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 564,
      "verse_key": "68:4",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِنَّكَ لَعَلَىٰ خُلُقٍ عَظِيمٖ",
      "translation": "ߌߟߋ ߞߏ߬ߣߌ߲ ߌ ߦߋ߫ ߖߏ߯ ߓߟߋߓߟߋ߫ ߞߊ߲߬ ߠߋ߬ ߞߟߋ߫",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 564,
      "verse_key": "68:5",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَسَتُبۡصِرُ وَيُبۡصِرُونَ",
      "translation": "ߍ߲ߤߤߍ߲߫ ߸ ߌߟߋ ߘߌߣߵߊ߬ ߦߋ߫ ߊ߬ߟߎ߬ ߝߣߊ߫ ߓߍߣߵߊ߬ ߦߋ߫ ( ߊ߬ ߕߍ߫ ߡߍ߲߫ )",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 564,
      "verse_key": "68:6",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بِأَييِّكُمُ ٱلۡمَفۡتُونُ",
      "translation": "ߡߍ߲ ߞߎ߲߭ ߕߴߦߋ߲߬ ߊߟߎ߫ ߘߐ߫",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 564,
      "verse_key": "68:7",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ رَبَّكَ هُوَ أَعۡلَمُ بِمَن ضَلَّ عَن سَبِيلِهِۦ وَهُوَ أَعۡلَمُ بِٱلۡمُهۡتَدِينَ",
      "translation": "ߓߊ ߌ ߡߊ߰ߙߌ ߟߋ߫ ߞߵߊ߬ ߟߐ߲߫ ߘߋ߬ ߡߍ߲ ߠߎ߫ ߝߟߌ߬ߣߍ߲߫ ߊ߬ ߟߊ߫ ߛߌߟߊ ߡߊ߬ ߸ ߊ߬ߟߋ ߟߋ߫ ߞߊ߬ ߞߊ߲ߘߊߣߍ߲ ߠߎ߬ ߝߣߊ߫ ߟߐ߲߫",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 564,
      "verse_key": "68:8",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلَا تُطِعِ ٱلۡمُكَذِّبِينَ",
      "translation": "ߌ ߞߣߊ߫ ߡߊ߬ߛߐ߬ߛߐ߬ߟߌ߬ߟߊ ߟߎ߬ ߞߊ߲ߡߌ߬ߘߊ߬ ߞߟߋ߫",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 564,
      "verse_key": "68:9",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَدُّواْ لَوۡ تُدۡهِنُ فَيُدۡهِنُونَ",
      "translation": "ߊ߬ ߘߌߡߴߊ߬ߟߎ߬ ߦߋ߫ ߟߋ߬ ߌ ߕߘߍ߬ ߦߴߊ߬ߟߎ߬ ߡߊߡߣߎ߬ߡߣߍ߫ ߊ߬ߟߎ߬ ߝߣߊ߫ ߦߴߌ ߡߊߡߣߎ߬ߡߣߍ߫",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 564,
      "verse_key": "68:10",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَا تُطِعۡ كُلَّ حَلَّافٖ مَّهِينٍ",
      "translation": "ߌ ߘߏ߲߬ ߞߣߊ߫ ߞߊ߬ߟߌ߬ߟߌ߬ߟߓߊ߬ ߝߎ߮ ߘߊ߬ߡߊ ߞߊ߲ ߡߌ߬ߘߊ߬ ߝߋߎ߫",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 564,
      "verse_key": "68:11",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "هَمَّازٖ مَّشَّآءِۭ بِنَمِيمٖ",
      "translation": "ߡߊ߬ߕߋ߰ߙߎ߬ߟߊ߫ ߡߙߍ߬ߕߍ߬ߡߙߍߕߍ ߡߍ߲߬ߘߍ߬ߦߊ ߟߊ߫ ߞߊ߬ߣߌ߲߫",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 564,
      "verse_key": "68:12",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَّنَّاعٖ لِّلۡخَيۡرِ مُعۡتَدٍ أَثِيمٍ",
      "translation": "ߡߊ߬ߞߏ߬ߛߊ߬ߟߊ ߟߋ߬ ߸ ߖߊ߲߬ߘߊ߬ߓߊ߯ ߖߟߎ߬ߡߎ߲߬ߕߐ߬ߓߊ ߟߋ߬",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 564,
      "verse_key": "68:13",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عُتُلِّۭ بَعۡدَ ذَٰلِكَ زَنِيمٍ",
      "translation": "ߣߌߝߙߊ ߟߋ߬ ߸ ߏ߬ ߓߍ߯ ߞߐ߫ ߡߐ߰ ߞߏߙߏ߲ ߠߋ߬",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 564,
      "verse_key": "68:14",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَن كَانَ ذَا مَالٖ وَبَنِينَ",
      "translation": "ߊ߬ ߞߍߣߍ߲߫ ߓߊ߬ߣߊ ߣߌ߫ ߘߋ߲ߞߍ߫ ߛߌߦߊߡߊ߲ߕߌ߮ ߘߌ߫ ߡߍ߲ߞߍ߭ ߸",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 564,
      "verse_key": "68:15",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذَا تُتۡلَىٰ عَلَيۡهِ ءَايَٰتُنَا قَالَ أَسَٰطِيرُ ٱلۡأَوَّلِينَ",
      "translation": "ߊ߲ ߠߊ߫ ߟߝߊߙߌ ߟߎ߬ ߓߊ߯ ߞߊ߬ߙߊ߲߫ ߊ߬ ߦߋ߫ ( ߏ߬ ߟߋ ߘߐ߫ )߸ ߊ߬ ߘߴߊ߬ ߝߐ߫ ߞߏ߫ ߝߟߐ߫ ߡߐ߱ ߟߎ߬ ߟߊ߫ ߛߌߙߌ߲ ߠߎ߫ ߟߴߏ߬ ߘߌ߫",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 565,
      "verse_key": "68:16",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "سَنَسِمُهُۥ عَلَى ٱلۡخُرۡطُومِ",
      "translation": "ߊ߲ ߘߏ߲߬ ߘߌߣߵߊ߬ ߥߍ߬ߢߍ߲߫ ߊ߬ ߛߏ߲ߜߊ߲ ߞߊ߲߬",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 565,
      "verse_key": "68:17",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّا بَلَوۡنَٰهُمۡ كَمَا بَلَوۡنَآ أَصۡحَٰبَ ٱلۡجَنَّةِ إِذۡ أَقۡسَمُواْ لَيَصۡرِمُنَّهَا مُصۡبِحِينَ",
      "translation": "ߒ߬ߠߎ߫ ߟߋ߫ ߣߵߊ߬ߟߎ߬ ߞߘߐߓߐ߫ ߟߊ߫ ߦߏ߫ ߊ߲ ߞߊ߬ ߣߊߞߐߕߌ߮ ߟߎ߬ ߞߘߐߓߐ߫ ߢߊ ߡߍ߲ ߸ ߏ߬ ߟߎ߫ ߞߵߊ߬ߟߎ߫ ߞߊ߬ߟߌ߬ ߡߍ߲ߞߍ߭ ߞߏ߫ ߊ߬ߟߎ߬ ߘߌ߫ ߛߏ߬ߙߌ߬ ߣߊߞߐ ߦߙߌߘߋ߲ ߕߍ߰ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 565,
      "verse_key": "68:18",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَا يَسۡتَثۡنُونَ",
      "translation": "ߊ߬ߟߎ߫ ߝߊ߲߭ ߡߊ߫ ߢߌ߬ߣߊ߬ߓߐ߬ߟߌ߫ ߞߍ߫ ( ߞߏ߫ ߞߵߊ߬ ߛߙߵߊߟߊ߫ ߟߊ߫ )",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 565,
      "verse_key": "68:19",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَطَافَ عَلَيۡهَا طَآئِفٞ مِّن رَّبِّكَ وَهُمۡ نَآئِمُونَ",
      "translation": "ߏ߬ ߞߍ ߸ ߡߊ߬ߡߣߌ߬ߡߌ߬ߣߌ߲߬ߠߊ ߘߏ߫ ߞߵߊ߬ ߡߊߡߣߌߡߌߣߌ߲߫ ߞߵߊ߬ߟߎ߬ ߕߏ߫ ߛߎ߬ߣߐ߮ ߘߐ߫ ߞߊ߬ ߝߘߊ߫ ߌ ߡߊ߰ߙߌ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 565,
      "verse_key": "68:20",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَصۡبَحَتۡ كَٱلصَّرِيمِ",
      "translation": "ߊ߬ ߞߍ߫ ߘߊ߫ ߛߐ߰ߡߊ߬ ߏ߬ ߘߐ߫ ߦߏ߫ ߜߊ߲߬ ߥߎߣߍߡߊ ߠߋ߬",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 565,
      "verse_key": "68:21",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَتَنَادَوۡاْ مُصۡبِحِينَ",
      "translation": "ߊ߬ߟߎ߫ ߞߏ߬ߣߌ߲ ߛߏ߬ߙߌ߬ ߘߊ߫ ߢߐ߲߮ ߞߟߌ߫ ߟߊ߫ ߟߋ߬",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 565,
      "verse_key": "68:22",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَنِ ٱغۡدُواْ عَلَىٰ حَرۡثِكُمۡ إِن كُنتُمۡ صَٰرِمِينَ",
      "translation": "ߞߏ߫ ߊߟߎ߫ ߦߋ߫ ߛߏ߬ߙߌ߬ ߊߟߎ߫ ߟߊ߫ ߛߍ߬ߣߍ ߡߊ߬ ߔߌߟߌ߲ߔߌߟߌ߲߫ ߣߴߊߟߎ߫ ߞߍ߫ ߘߴߊ߬ ߕߍ߰ߓߊ߯ ߘߌ߫",
      "footnotes": ""
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 565,
      "verse_key": "68:23",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَٱنطَلَقُواْ وَهُمۡ يَتَخَٰفَتُونَ",
      "translation": "ߊ߬ߟߎ߬ ߕߊ߯ ߘߊ߫ ߏ߬ ߘߐ߫ ߞߵߊ߬ ߕߘߍ߬ ߊ߬ߟߎ߰ ߘߊߞߘߐ߫ ߞߎߡߊ ߟߊ߫ ߢߐ߲߮ ߦߋ߫",
      "footnotes": ""
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 565,
      "verse_key": "68:24",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَن لَّا يَدۡخُلَنَّهَا ٱلۡيَوۡمَ عَلَيۡكُم مِّسۡكِينٞ",
      "translation": "ߞߏ߫ ߘߐ߰ߕߐ߫ ߛߌ߫ ߕߍߣߊ߫ ߘߏ߲߬ ߠߴߊ߬ ߞߣߐ߫ ߊߟߎ߫ ߞߊ߲߬ ߓߌ߬ ߝߋߎ߫",
      "footnotes": ""
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 565,
      "verse_key": "68:25",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَغَدَوۡاْ عَلَىٰ حَرۡدٖ قَٰدِرِينَ",
      "translation": "ߊ߬ߟߎ߫ ߛߏ߬ߙߌ߬ ߘߊ߫ ߕߊ߯ ߟߊ߫ ߞߏ߫ ߊ߬ߟߎ߬ ߘߌ߫ ߛߴߊ߬ߟߎ߬ ߡߙߌߘߐ ߏ߬ ߞߍ߫ ߟߊ߫ ߝߊ߯ߛߌ߫",
      "footnotes": ""
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 565,
      "verse_key": "68:26",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلَمَّا رَأَوۡهَا قَالُوٓاْ إِنَّا لَضَآلُّونَ",
      "translation": "ߒ߬ߞߵߊ߬ߟߎ߬ ߥߊߣߍ߫ ߊ߬ ( ߥߎߣߍߡߊ ) ߦߋ߫ ߟߊ߫ ߸ ߊ߬ߟߎ߬ ߞߊ߲߫ ߞߏ߫ ߊ߲ ߝߟߌ߬ߣߍ߲߫ ߣߊ߬ ߞߍ߫ ߟߋ߬",
      "footnotes": ""
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 565,
      "verse_key": "68:27",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بَلۡ نَحۡنُ مَحۡرُومُونَ",
      "translation": "ߊ߬ߦߌ߫ ߸ ( ߊ߬ߟߋ߬ ߣߊߞߐ ߟߋ߫ ߣߊ߬ ߞߍ߫ ߖߐ߫ ߸ ߞߏ߬ߣߌ߲߬ ) ߒ ߝߐߣߴߊ߬ ߟߊ߫ ߟߋ߬",
      "footnotes": ""
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 565,
      "verse_key": "68:28",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قَالَ أَوۡسَطُهُمۡ أَلَمۡ أَقُل لَّكُمۡ لَوۡلَا تُسَبِّحُونَ",
      "translation": "ߊ߬ߟߎ߬ ߘߐ߫ ߕߍߡߞߊߒߞߋ ߞߊ߲߫ ( ߊ߬ߟߎ߫ ߖߙߊ߬ߞߌ߬ ߞߊ߲߫ ߘߐ߫ ) ߞߏ߫ ߒߠߋ ߡߴߊ߬ ߝߐ߫ ߊߟߎ߫ ߦߋ߫ ߒ ߞߏ߫ ߥߟߊߞߏ߫ ߊߟߎ߫ ߕߘߍ߬ ߦߋ߫ ߛߊߣߌ߲ߧߊ ߞߎߡߊ ߝߐ߫ ( ߞߵߊߟߎ߫ ߖߊ߲߬ߓߌ߬ߟߊ߬ ߊߟߊ߫ ߘߐ߫ )؟",
      "footnotes": ""
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 565,
      "verse_key": "68:29",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قَالُواْ سُبۡحَٰنَ رَبِّنَآ إِنَّا كُنَّا ظَٰلِمِينَ",
      "translation": "ߊ߬ߟߎ߬ ߞߊ߲߫ ߏ߬ ߘߐ߫ ߞߏ߫ ߛߊߣߌ߲ߧߊ ߒ߬ ߡߊ߰ߙߌ ߦߋ߫ ߸ ߒ߬ߠߎ߫ ߟߋ߬ ߞߍߣߍ߲߫ ߕߐ߬ߢߍ߰ߒߕߋ ߟߎ߬ ߘߌ߫ ߓߏ߬",
      "footnotes": ""
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 565,
      "verse_key": "68:30",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَقۡبَلَ بَعۡضُهُمۡ عَلَىٰ بَعۡضٖ يَتَلَٰوَمُونَ",
      "translation": "ߘߊ߬ߡߊ߲߬ߛߊ߫ ߸ ߊ߬ߟߎ߫ ߞߵߊ߬ߟߎ߬ ߢߊߕߋߟߋ߲߫ ߢߐ߲߮ ߠߊ߫ ߢߐ߲߯ ߖߙߊߞߌ ߘߌ߫",
      "footnotes": ""
    },
    {
      "id": 31,
      "verse_number": 31,
      "page_number": 565,
      "verse_key": "68:31",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قَالُواْ يَٰوَيۡلَنَآ إِنَّا كُنَّا طَٰغِينَ",
      "translation": "ߊ߬ߟߎ߬ ߞߊ߲߫ ߞߏ߫ ߥߊߦߌ߯ ߸ ߒ߬ߠߎ߫ ߟߋ߬ ߞߍߣߍ߲߫ ߘߊ߲߬ߠߕߊ߬ߡߌ߲߬ߓߊ߮ ߟߎ߬ ߘߌ߫ ߘߐߙߐ߲߫߹",
      "footnotes": ""
    },
    {
      "id": 32,
      "verse_number": 32,
      "page_number": 565,
      "verse_key": "68:32",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَسَىٰ رَبُّنَآ أَن يُبۡدِلَنَا خَيۡرٗا مِّنۡهَآ إِنَّآ إِلَىٰ رَبِّنَا رَٰغِبُونَ",
      "translation": "ߊ߬ߢߐ߲߰ߘߐ߫ ߒ߬ ߡߊ߰ߙߌ ߘߌ߫ ߣߌ߲߬ ߝߊ߬ߟߋ߲߬ ߒ߬ ߧߋ߫ ߝߋ߲߫ ߠߊ߫ ߡߍ߲ ߦߊ߯ ߝߌ߬ߛߴߊ߬ ߘߌ߫ ߸ ߏ߬ ߟߋ ߟߐ߮ ߦߋ߫ ߒ߬ ߠߊ߫ ߒ߬ ߡߊ߰ߙߌ ߝߍ߬ ߘߋ߬",
      "footnotes": ""
    },
    {
      "id": 33,
      "verse_number": 33,
      "page_number": 565,
      "verse_key": "68:33",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَذَٰلِكَ ٱلۡعَذَابُۖ وَلَعَذَابُ ٱلۡأٓخِرَةِ أَكۡبَرُۚ لَوۡ كَانُواْ يَعۡلَمُونَ",
      "translation": "( ߊ߬ߟߎ߫ ) ߖߊ߲߰ߞߕߊ ߞߍ߫ ߘߊ߫ ߕߋ߲߬ ߠߋ߬ ߸ ߛߏߟߊߓߊ߲ ߖߊ߲߰ߞߕߊ ߟߋ߫ ߘߏ߲߬ ߓߏ߲߬ߓߊ߬ߓߊ ߦߋ߫ ߣߴߊ߬ߟߎ߫ ߕߎ߲߬ ߓߍ߫ ߏ߬ ߟߐ߲߫ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 34,
      "verse_number": 34,
      "page_number": 565,
      "verse_key": "68:34",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ لِلۡمُتَّقِينَ عِندَ رَبِّهِمۡ جَنَّٰتِ ٱلنَّعِيمِ",
      "translation": "ߡߍ߲ߘߌߦߊ߫ ( ߛߏ ) ߟߎ߫ ߟߋ߬ ߝߊ߲ߕߊ߲ߞߟߊ ߟߎ߬ ߦߋ߫ ߊ߬ߟߎ߫ ߡߊ߰ߙߌ ߕߍ߰ߟߊ ߘߐ߫ ߘߋ߬",
      "footnotes": ""
    },
    {
      "id": 35,
      "verse_number": 35,
      "page_number": 565,
      "verse_key": "68:35",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَنَجۡعَلُ ٱلۡمُسۡلِمِينَ كَٱلۡمُجۡرِمِينَ",
      "translation": "ߊ߬ߝߐ߫ ߒ߬ߠߎ߬ ߘߌ߫ ߖߘߍ߬ߞߟߏ߬ߦߊ߬ߓߊ߮ ߟߎ߬ ߞߍ߫ ߏ߬ ߘߐ߫ ߦߏ߫ ߛߐ߲߬ߡߊ ߟߎ߬؟",
      "footnotes": ""
    },
    {
      "id": 36,
      "verse_number": 36,
      "page_number": 565,
      "verse_key": "68:36",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَا لَكُمۡ كَيۡفَ تَحۡكُمُونَ",
      "translation": "ߡߋ߬ߘߋ ߦߴߊߟߎ߫ ߟߊ߫ ߊߟߎ߫ ߦߋ߫ ߞߕߌ ( ߘߎ߬ߣߍ߲߬ ߛߎ߮ ߏ߬ ) ߟߊ߫ ߟߊ߫ ߛߊ߫؟",
      "footnotes": ""
    },
    {
      "id": 37,
      "verse_number": 37,
      "page_number": 565,
      "verse_key": "68:37",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَمۡ لَكُمۡ كِتَٰبٞ فِيهِ تَدۡرُسُونَ",
      "translation": "ߥߟߊ߫ ߟߍߙߊ߫ ߟߵߊߟߎ߫ ߓߟߏ߫ ߓߊ߬ ߊߟߎ߫ ߦߋ߫ ߞߊ߬ߙߊ߲ ߞߍ߫ ߟߊ߫ ߡߍ߲ ߘߐ߫؟",
      "footnotes": ""
    },
    {
      "id": 38,
      "verse_number": 38,
      "page_number": 565,
      "verse_key": "68:38",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ لَكُمۡ فِيهِ لَمَا تَخَيَّرُونَ",
      "translation": "ߝߋ߲߫ ߠߵߊߟߎ߫ ߦߋ߫ ߏ߬ ߘߐ߫ ߘߋ߬ ߊߟߎ߫ ߦߋ߫ ߡߍ߲ ߛߎߥߊ߲ߘߌ߫ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 39,
      "verse_number": 39,
      "page_number": 565,
      "verse_key": "68:39",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَمۡ لَكُمۡ أَيۡمَٰنٌ عَلَيۡنَا بَٰلِغَةٌ إِلَىٰ يَوۡمِ ٱلۡقِيَٰمَةِ إِنَّ لَكُمۡ لَمَا تَحۡكُمُونَ",
      "translation": "ߥߟߊ߫ ߘߐ߬ߛߘߊ߬ߞߊ߲߫ ߠߎ߫ ߟߋ߬ ߊߟߎ߫ ߓߟߏ߫ ߒ߬ߠߎ߫ ߝߍ߬ ߞߵߊ߬ ߛߌ߰ ߟߐ߬ߓߊ ߟߏ߲ ߠߊ߫ ߓߊ߬ ߸ ߞߏ߫ ߝߋ߲߫ ߠߋ߬ ߊߟߎ߫ ߦߋ߫ ߊߟߎ߫ ߓߊ߯ ߞߕߌ߫ ߡߍ߲ ߠߊ߫؟",
      "footnotes": ""
    },
    {
      "id": 40,
      "verse_number": 40,
      "page_number": 565,
      "verse_key": "68:40",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "سَلۡهُمۡ أَيُّهُم بِذَٰلِكَ زَعِيمٌ",
      "translation": "ߊ߬ߟߎ߫ ߢߌ߬ߣߌ߲߬ߞߊ߬ ߓߊ߫ ߞߏ߫ ߖߍߣߍ߲ ߦߴߏ߬ ߓߟߏߡߊ ߘߌ߫؟",
      "footnotes": ""
    },
    {
      "id": 41,
      "verse_number": 41,
      "page_number": 565,
      "verse_key": "68:41",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَمۡ لَهُمۡ شُرَكَآءُ فَلۡيَأۡتُواْ بِشُرَكَآئِهِمۡ إِن كَانُواْ صَٰدِقِينَ",
      "translation": "ߥߟߊ߫ ߜߙߊ߬ߢߐ߲߯ ߠߎ߫ ߟߴߊ߬ߟߎ߬ ߟߊ߫ ߓߊ߬ ߸ ߒ߬ߓߵߊ߬ߟߎ߬ ߦߋ߫ ߣߵߊ߬ߟߎ߫ ߜߙߊ߬ߢߐ߲߮ ߠߎ߬ ߘߌ߫ ߣߴߊ߬ߟߎ߬ ߦߋ߫ ߕߎ߬ߢߊ߬ߕߌ߯ ߟߎ߫ ߘߌ߫",
      "footnotes": ""
    },
    {
      "id": 42,
      "verse_number": 42,
      "page_number": 565,
      "verse_key": "68:42",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَوۡمَ يُكۡشَفُ عَن سَاقٖ وَيُدۡعَوۡنَ إِلَى ٱلسُّجُودِ فَلَا يَسۡتَطِيعُونَ",
      "translation": "ߒ߬ߓߊ߬ ߜߍ߬ߟߍ߲ ߡߊߞߊ߬ߕߐ߫ ߟߏ߲ ߡߍ߲ ߞߵߊ߬ߟߎ߬ ߞߟߌ߫ ߕߌ߲ߓߌߘߌ߲ ߡߊ߬ ߏ߬ ߘߐ߫ ߛߊ߫ ߫߸ ߊ߬ߟߎ߫ ߘߏ߲߬ ߕߍߣߵߏ߬ ߟߊߣߐ߬ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 43,
      "verse_number": 43,
      "page_number": 566,
      "verse_key": "68:43",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "خَٰشِعَةً أَبۡصَٰرُهُمۡ تَرۡهَقُهُمۡ ذِلَّةٞۖ وَقَدۡ كَانُواْ يُدۡعَوۡنَ إِلَى ٱلسُّجُودِ وَهُمۡ سَٰلِمُونَ",
      "translation": "ߊ߬ߟߎ߬ ߢߊ ߟߎ߬ ߦߋ߫ ߞߌߛߐ߫ ߸ ߟߊ߬ߝߍߦߊ ߦߴߊ߬ߟߎ߬ ߢߡߊߣߐ߮ ߞߊ߲߬ ߸ ߊ߬ߟߎ߫ ߘߏ߲߬ ߕߘߍ߬ ߞߟߌ߫ ߟߊ߫ ߕߌ߲ߓߌߘߌ߲ ߡߊ߬ ߗߐ߫ ߞߵߊ߬ ߕߘߍ߬ ߊ߬ߟߎ߬ ߞߍ߲ߘߍߜߍ ߦߋ߫",
      "footnotes": ""
    },
    {
      "id": 44,
      "verse_number": 44,
      "page_number": 566,
      "verse_key": "68:44",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَذَرۡنِي وَمَن يُكَذِّبُ بِهَٰذَا ٱلۡحَدِيثِۖ سَنَسۡتَدۡرِجُهُم مِّنۡ حَيۡثُ لَا يَعۡلَمُونَ",
      "translation": "ߒ߬ߓߊ߬ ߒ ߣߌ߫ ߛߙߊ߬ߣߎ߲߬ߒ߬ߞߊ߲߬ߠߊ ߢߌ߲߬ ߡߊߛߐ߬ߛߐ߬ߓߊ߮ ߕߏ߫ ߢߐ߲߮ ߠߊ߫ ߕߋ߲߬ ߗߐ߫ ߸ ߒ߬ ߘߌߣߵߊ߬ߟߎ߫ ߘߐ߲߬ߘߐ߰ߣߍ߲߬ ߠߊߞߎ߬ߘߎ߫ ߊ߬ߟߎ߬ ߟߐ߲ߓߊߟߌߦߊ߫ ߦߙߐ ߘߏ߫ ߘߐ߫ ߖߐ߫",
      "footnotes": ""
    },
    {
      "id": 45,
      "verse_number": 45,
      "page_number": 566,
      "verse_key": "68:45",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأُمۡلِي لَهُمۡۚ إِنَّ كَيۡدِي مَتِينٌ",
      "translation": "ߒ ߧߴߊ߬ߟߎ߬ ߡߊߕߡߊߣߊ߫ ߟߊ߫ ߟߋ߬ ߡߎߣߎ߲߬ ߸ ߓߊ ߒ߬ ߝߍ߬ߙߍ ߟߊߝߊ߬ߛߊ߲߬ߣߍ߲߫ ߠߋ߫ ߘߋ߬",
      "footnotes": ""
    },
    {
      "id": 46,
      "verse_number": 46,
      "page_number": 566,
      "verse_key": "68:46",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَمۡ تَسۡـَٔلُهُمۡ أَجۡرٗا فَهُم مِّن مَّغۡرَمٖ مُّثۡقَلُونَ",
      "translation": "ߓߊ߬ ߌ ߦߴߊ߬ߟߎ߬ ߕߙߊ߫ ߟߊ߫ ߛߊ߬ߙߊ߫ ߟߋ߬ ߟߊ߫ ߞߏߘߌ߬ ߣߴߊ߬ߟߎ߬ ߜߟߌߦߊߣߍ߲߫ ߏ߬ ߖߎ߬ߟߎ ( ߛߙߊ߬ߞߏ ) ߓߟߏ߫؟",
      "footnotes": ""
    },
    {
      "id": 47,
      "verse_number": 47,
      "page_number": 566,
      "verse_key": "68:47",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَمۡ عِندَهُمُ ٱلۡغَيۡبُ فَهُمۡ يَكۡتُبُونَ",
      "translation": "ߥߟߊ߫ ߞߏߘߏ߲߰ߣߍ߲ ( ߟߐ߲ߠߌ߲ ) ߠߴߊ߬ߟߎ߬ ߓߟߏ߫ ߓߊ߬ ߸ ߣߴߊ߬ߟߎ߬ ߦߴߏ߬ ߛߓߍ߫ ߟߊ߫؟",
      "footnotes": ""
    },
    {
      "id": 48,
      "verse_number": 48,
      "page_number": 566,
      "verse_key": "68:48",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَٱصۡبِرۡ لِحُكۡمِ رَبِّكَ وَلَا تَكُن كَصَاحِبِ ٱلۡحُوتِ إِذۡ نَادَىٰ وَهُوَ مَكۡظُومٞ",
      "translation": "ߒ߬ߓߊ߬ ߌߟߋ ߌ ߡߎ߬ߢߎ߲߬ ߌ ߡߊ߰ߙߌ ߟߊ߫ ߞߕߌ ߦߋ߫ ( ߊ߬ߟߎ߬ ߡߊߕߡߊߣߊ ߘߐ߫ )، ߌ ߞߊߣߊ߬ ߞߍ߫ ߘߋ߬ ߦߏ߫ ߖߍ߯ߕߌ߮ ߸ ߕߎ߬ߡߊ ߡߍ߲ ߏ߬ ߘߌߡߌ߲ߣߍ߲߫ ߞߊߘߌߞߊߘߌ ߞߵߊߟߊ߫ ߞߟߌ߫",
      "footnotes": ""
    },
    {
      "id": 49,
      "verse_number": 49,
      "page_number": 566,
      "verse_key": "68:49",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَّوۡلَآ أَن تَدَٰرَكَهُۥ نِعۡمَةٞ مِّن رَّبِّهِۦ لَنُبِذَ بِٱلۡعَرَآءِ وَهُوَ مَذۡمُومٞ",
      "translation": "ߣߌ߫ ( ߊ߬ ߟߊ߫ ߖߘߍ߬ߟߊ߬ߛߊߦߌ ) ߣߍߡߊ ߕߎ߲߬ ߡߊ߫ ߘߋ߬ߦߌ߫ ߊ߬ ߡߊ߬ ߞߊ߬ ߝߘߴߊ߬ ߡߊ߰ߙߌ ߟߊ߫ ߸ ߊ߬ ߞߎ߲߬ߘߐߦߌ߬ߘߊ߬ߣߍ߲ ߕߌ߲߬ ߘߌ߫ ߟߊߝߌ߬ߟߌ߫ ߥߊ߬ߢߊ߲ ߘߐ߫",
      "footnotes": ""
    },
    {
      "id": 50,
      "verse_number": 50,
      "page_number": 566,
      "verse_key": "68:50",
      "juz_number": 29,
