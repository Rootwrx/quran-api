{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 595,
      "verse_key": "91:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلشَّمۡسِ وَضُحَىٰهَا",
      "translation": "ߊ߰ ߒ ߣߌ߫ ߕߋ߬ߟߋ ߕߍ߫ ߸ ߊ߬ ߣߴߊ߬ ߞߣߍߦߊ",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 595,
      "verse_key": "91:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡقَمَرِ إِذَا تَلَىٰهَا",
      "translation": "ߊ߰ ߒ ߣߌ߫ ߞߊߙߏ ߕߍ߫ ߸ ߣߴߏ߬ ߞߵߊ߬ ߣߐ߬ߡߌ߬ߣߊ߬ ( ߞߣߍߦߊ ߘߐ߫ ) ߕߎ߬ߡߊ ߡߍ߲",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 595,
      "verse_key": "91:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلنَّهَارِ إِذَا جَلَّىٰهَا",
      "translation": "ߊ߰ ߒ ߣߌ߫ ߕߟߋ߬ߘߐ߬ߟߊ ߕߍ߫ ߸ ߣߴߊ߬ ߟߊ߲ߞߣߍߡߊ߫ ߘߊ߫ ߕߎ߬ߡߊ ߡߍ߲",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 595,
      "verse_key": "91:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّيۡلِ إِذَا يَغۡشَىٰهَا",
      "translation": "ߊ߰ ߒ ߣߌ߫ ߛߎ ߕߍ߫ ߸ ߣߴߏ߬ ߞߵߊ߬ ߞߎ߲߬ߠߊ߬ߓߌߘߌ߲߫ ߕߎ߬ߡߊ ߡߍ߲",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 595,
      "verse_key": "91:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلسَّمَآءِ وَمَا بَنَىٰهَا",
      "translation": "ߊ߰ ߒ ߣߌ߫ ߛߊ߲ ߕߍ߫ ߸ ߊ߬ ߣߌ߫ ߡߍ߲ ߞߵߊ߬ ߟߐ߬",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 595,
      "verse_key": "91:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡأَرۡضِ وَمَا طَحَىٰهَا",
      "translation": "ߊ߰ ߒ ߣߌ߫ ߘߎ߱ ߕߍ߫ ߸ ߊ߬ ߣߌ߫ ߡߍ߲ ߞߵߊ߬ ߘߐߝߎߟߋ߲߫",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 595,
      "verse_key": "91:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَنَفۡسٖ وَمَا سَوَّىٰهَا",
      "translation": "ߊ߰ ߒ ߣߌ߫ ߣߌ ߕߍ߫ ߸ ߊ߬ ߣߌ߫ ߡߍ߲ ߞߵߊ߬ ߟߊߞߢߊ߫",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 595,
      "verse_key": "91:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَلۡهَمَهَا فُجُورَهَا وَتَقۡوَىٰهَا",
      "translation": "ߞߵߊ߬ ߓߌ߬ߟߊ߬ߒ߬ߞߣߐ߬ߟߐ߲ߠߌ߲߫ ߊ߬ ߟߊ߫ ߞߏߙߏ߲ߧߊ ߣߴߊ߬ ߟߊ߫ ߝߊ߲ߕߊ߲ߞߊ ߟߊ߫ ߏ߬ ߘߐ߫",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 595,
      "verse_key": "91:9",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قَدۡ أَفۡلَحَ مَن زَكَّىٰهَا",
      "translation": "( ߒ߬ߓߊ߬ ) ߡߍ߲ ߓߊ߯ ߊ߬ ( ߕߊ ߏ߬ ) ߛߊߣߌ߲ߧߊ߫ ߸ ߏ߬ ߓߘߊ߫ ߛߎߘߊ߲߫ ߞߟߋ߫",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 595,
      "verse_key": "91:10",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَقَدۡ خَابَ مَن دَسَّىٰهَا",
      "translation": "ߡߍ߲ ߓߊ߯ ߊ߬ ( ߕߊ ߏ߬ ) ߡߊߣߐ߯ ߸ ߏ߬ ߓߘߊ߫ ߖߋ߰ߘߐ߬ߞߏߟߏ߲ߧߊ߫ ߞߟߋ߫",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 595,
      "verse_key": "91:11",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَذَّبَتۡ ثَمُودُ بِطَغۡوَىٰهَآ",
      "translation": "ߛߊ߬ߡߎߘߊ ߟߎ߫ ߞߊ߬ ߡߊ߬ߛߐ߬ߛߐ߬ߟߌ ߞߍ߫ ߊ߬ߟߎ߬ ߟߊ߫ ߘߊ߲߬ߠߊ߬ߕߊߡߌ߲ ߠߋ߬ ߞߏߛߐ߲߬",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 595,
      "verse_key": "91:12",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذِ ٱنۢبَعَثَ أَشۡقَىٰهَا",
      "translation": "ߕߎ߬ߡߊ ߡߍ߲ ߊ߬ߟߎ߬ ߘߐ߫ ߞߎ߲߬ߠߊ߲߬ߜߏ ߞߵߊ߬ ߖߘߍ߬ߓߐ߫ ( ߜߐ߲߬ߝߛߊ ߕߍ߰ ߞߊ߲ ߡߊ߬ )",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 595,
      "verse_key": "91:13",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَقَالَ لَهُمۡ رَسُولُ ٱللَّهِ نَاقَةَ ٱللَّهِ وَسُقۡيَٰهَا",
      "translation": "ߊߟߊ߫ ߟߊ߫ ߞߋߟߊ ߘߏ߲߬ ߞߵߊ߬ ߝߐ߫ ߊ߬ߟߎ߬ ߦߋ߫ ߸ ߞߏ߫ ߊߟߎ߯ ߊߟߊ߫ ߟߊ߫ ߢߐ߯ߡߍߡߛߏ ߟߊߕߏ߫ ߘߋ߬ ߸ ߊ߬ ߣߴߊ߬ ߡߌ߲߬ ߟߏ߲",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 595,
      "verse_key": "91:14",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَكَذَّبُوهُ فَعَقَرُوهَا فَدَمۡدَمَ عَلَيۡهِمۡ رَبُّهُم بِذَنۢبِهِمۡ فَسَوَّىٰهَا",
      "translation": "ߒ߬ߞߵߊ߬ߟߎ߫ ߞߵߏ߬ ߡߊߛߐ߬ߛߐ߫ ߞߵߊ߬ ߥߙߐ߬ߛߐ ߟߎ߬ ߓߎ߲ߘߊ߲߫ ߸ ߏ߬ ߞߍ ߊ߬ߟߎ߫ ߡߊ߰ߙߌ ߞߵߊ߬ߟߎ߬ ( ߟߊ߫ ߘߎ߱ ) ߘߊߝߘߌ߫ ߊ߬ߟߎ߫ ߞߎ߲߬ߞߏ ߞߏߛߐ߲߬ ߞߵߊ߬ ߓߍ߬ߛߍ߲߬ߞߍ߫ ߊ߬ߟߎ߫ ߡߊ߬ ߟߋߎ߫",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 595,
      "verse_key": "91:15",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَا يَخَافُ عُقۡبَٰهَا",
      "translation": "( ߊߟߊ߫ ) ߘߏ߲߬ ߕߍ߫ ߛߌߟߊ߲߫ ߠߴߏ߬ ( ߛߎ߮ ) ߞߐߝߟߌ ߟߊ߫",
      "footnotes": ""
    }
  ]
}