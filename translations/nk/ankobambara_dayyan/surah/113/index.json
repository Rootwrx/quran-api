{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 604,
      "verse_key": "113:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قُلۡ أَعُوذُ بِرَبِّ ٱلۡفَلَقِ",
      "translation": "ߊ߬ ߝߐ߫ ߞߏ߫ ߒ ߧߋ߫ ߒ ߞߊ߬ߙߌ߬ߝߊ߬ ߟߊ߫ ߘߎ߰ߛߊ߬ߜߍ ߡߊ߰ߙߌ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 604,
      "verse_key": "113:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مِن شَرِّ مَا خَلَقَ",
      "translation": "ߞߊ߬ ߓߐ߫ ߊ߬ ߟߊ߫ ߘߊ߲ߝߋ߲ ߘߐ߫ ߖߎ߯ߡߊ߲ ߡߊ߬",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 604,
      "verse_key": "113:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمِن شَرِّ غَاسِقٍ إِذَا وَقَبَ",
      "translation": "ߊ߬ ߣߌ߫ ߞߊ߬ ߓߐ߫ ߛߎߞߏ߭ ߟߊ߫ ߖߎ߯ߦߊ ߡߊ߬ ߣߴߊ߬ ߞߍ߫ ߘߊ߫ ߘߍ߲ߘߍ߲߫",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 604,
      "verse_key": "113:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمِن شَرِّ ٱلنَّفَّٰثَٰتِ فِي ٱلۡعُقَدِ",
      "translation": "ߊ߬ ߣߌ߫ ߞߊ߬ ߓߐ߫ ߘߊߖߌ-ߕߎߓߊ߮ ߟߊ߫ ߖߎ߯ߦߊ ߡߊ߬ ߕߊ߬ߝߏ ߘߐ߫",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 604,
      "verse_key": "113:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمِن شَرِّ حَاسِدٍ إِذَا حَسَدَ",
      "translation": "ߊ߬ ߣߌ߫ ߞߊ߬ ߓߐ߫ ߓߍ߬ߛߍ߲߬ߞߐߣߌ߲ ߠߊ߫ ߖߎ߯ߦߊ ߡߊ߬ ߓߍ߬ߛߍ߲߬ߞߐ߬ߣߌ߲߬ߧߊ ߟߊ߫",
      "footnotes": ""
    }
  ]
}