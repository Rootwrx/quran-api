{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 566,
      "verse_key": "69:1",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلۡحَآقَّةُ",
      "translation": "( ߟߏ߲߫ ) ߘߐߛߊ߬ߓߊߟߌ߹",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 566,
      "verse_key": "69:2",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَا ٱلۡحَآقَّةُ",
      "translation": "ߡߋ߬ߘߋ ߘߐߛߊ߬ߓߊߟߌ ߘߌ߫؟",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 566,
      "verse_key": "69:3",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَآ أَدۡرَىٰكَ مَا ٱلۡحَآقَّةُ",
      "translation": "ߡߎ߲߬ ߞߵߌ ߟߊߟߐ߲ߠߌ߲߫ ߘߐߛߊ߬ߓߊߟߌ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 566,
      "verse_key": "69:4",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَذَّبَتۡ ثَمُودُ وَعَادُۢ بِٱلۡقَارِعَةِ",
      "translation": "ߛߊ߬ߡߎߘߊ ߟߎ߬ ߣߌ߫ ߊ߯ߘߌ ߟߎ߫ ߞߊ߬ ( ߟߏ߲߫ ) ߖߛߎ߬-ߞߟߎ߬ߟߊ ߡߊߛߐ߬ߛߐ߬",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 566,
      "verse_key": "69:5",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَمَّا ثَمُودُ فَأُهۡلِكُواْ بِٱلطَّاغِيَةِ",
      "translation": "ߛߊ߬ߡߎ߯ߘߊ ߟߎ߫ ߘߏ߲߬ ߸ ߏ߬ ߟߎ߬ ߛߌߟߊߛߊ߬ ߘߊ߫ ( ߡߊ߲߬ߞߊ߲߬ ߜߍߟߍ߲߫ ) ߘߊ߲߬ߠߊߕߊ߬ߡߌ߲߬ߣߍ߲ ߠߋ߬ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 566,
      "verse_key": "69:6",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَمَّا عَادٞ فَأُهۡلِكُواْ بِرِيحٖ صَرۡصَرٍ عَاتِيَةٖ",
      "translation": "ߊ߯ߘߌ ߟߎ߬ ߝߣߊ߫ ߘߏ߲߬ ߸ ߏ߬ ߟߎ߬ ߛߌߟߊߛߊ߬ ߘߊ߫ ߝߢߐ߫ ߞߊ߲-ߜߍߟߍ߲߫ ߝߊ߲߬ߞߊ߬ߡߊ ߟߋ߬ ߟߊ߫ ߝߏߋ߯",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 566,
      "verse_key": "69:7",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "سَخَّرَهَا عَلَيۡهِمۡ سَبۡعَ لَيَالٖ وَثَمَٰنِيَةَ أَيَّامٍ حُسُومٗاۖ فَتَرَى ٱلۡقَوۡمَ فِيهَا صَرۡعَىٰ كَأَنَّهُمۡ أَعۡجَازُ نَخۡلٍ خَاوِيَةٖ",
      "translation": "ߏ߬ ߗߋ߫ ߘߴߏ߬ ߟߎ߬ ߞߊߡߊ߬ ߛߎ߫ ߥߐߙߐ߲ߥߌ߬ߟߊ߬ ߣߌ߫ ߕߟߋ߬ ߛߋߦߌ߲߫ ߠߋ߬ ߞߘߐ߫ ߝߙߍߕߍߕߍ߫ ߸ ߝߏ߫ ߌ ߘߌ߫ ߡߌ߬ߙߌ߲߬ߘߌ ߓߋ߬ߓߋ߬ߣߍ߲ ߦߴߊ߬ ߞߐ߫ ߦߏ߫ ߕߌ߲߬ߖߐ߲߬ ߦߙߌߞߎߘߎ߲ ߠߎ߬ ߟߊߥߟߏߥߟߏߣߍ߲ ߠߎ߫ ߟߋ߬",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 566,
      "verse_key": "69:8",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَهَلۡ تَرَىٰ لَهُم مِّنۢ بَاقِيَةٖ",
      "translation": "ߦߊ߯ߟߊ߫ ߌ ߞߵߏ߬ ߟߎ߫ ߕߐ߬ ߦߋ߫ ߓߊ߬؟",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 567,
      "verse_key": "69:9",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَجَآءَ فِرۡعَوۡنُ وَمَن قَبۡلَهُۥ وَٱلۡمُؤۡتَفِكَٰتُ بِٱلۡخَاطِئَةِ",
      "translation": "ߝߋߙߊߥߎ߲߫ ߣߴߊ߬ ߢߍߓߊ߮ ߟߎ߬ ߣߌ߫ ߘߎ߰ߦߟߍߡߊ߲ ߘߏ߲߬ߞߏ ߟߎ߫ ߣߊ߬ ߘߊ߫ ߞߎ߲߬ߞߏ ߟߎ߬ ߘߌ߫",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 567,
      "verse_key": "69:10",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَعَصَوۡاْ رَسُولَ رَبِّهِمۡ فَأَخَذَهُمۡ أَخۡذَةٗ رَّابِيَةً",
      "translation": "ߏ߬ ߟߎ߫ ߞߵߊ߬ߟߎ߫ ߡߊ߰ߙߌ ߟߊ߫ ߞߋߟߊ ߛߐ߬ߛߐ߫ ߟߋ߬ ߸ ߏ߬ ߝߣߊ߫ ߞߵߊ߬ߟߎ߫ ߡߌ߬ߘߊ߬ ߕߍߕߊ ߟߊ߫ ߥߊߡߌ߫",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 567,
      "verse_key": "69:11",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّا لَمَّا طَغَا ٱلۡمَآءُ حَمَلۡنَٰكُمۡ فِي ٱلۡجَارِيَةِ",
      "translation": "ߒ߬ߓߎ߫ ߞߏ߬ߣߌ߲ ߖߌ ߘߊ߲߬ߠߊߕߊ߬ߡߌ߲߬ߣߍ߲ ( ߝߐ߬ߝߐ ߓߟߏ߫ )߸ ߊ߲ ߞߵߊߟߎ߫ ߕߊ߬ ߞߎߟߎ߲ ߘߐ߫",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 567,
      "verse_key": "69:12",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لِنَجۡعَلَهَا لَكُمۡ تَذۡكِرَةٗ وَتَعِيَهَآ أُذُنٞ وَٰعِيَةٞ",
      "translation": "ߛߴߊ߲ ߘߴߏ߬ ߞߍ߫ ߊߟߎ߫ ߦߋ߫ ߕߊ߰ߛߌ߬ߞߏ߫ ߘߌ߫ ߊ߬ ߣߌ߫ ߛߊ߫ ߕߟߏ߫ ߕߊ߲߬ߞߊ߬ߟߌ߬ߟߊ ߘߴߊ߬ ߕߊ߲߬ߞߊ߬",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 567,
      "verse_key": "69:13",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِذَا نُفِخَ فِي ٱلصُّورِ نَفۡخَةٞ وَٰحِدَةٞ",
      "translation": "ߒ߬ߓߊ߬ ߣߌ߫ ߓߘߎ ߝߍ߫ ߘߊ߫ ߕߎ߬ߡߊ ߡߍ߲ ߝߍߟߌ߫ ߞߏ߫ ߞߋߟߋ߲߫ ߔߋ߫ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 567,
      "verse_key": "69:14",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَحُمِلَتِ ٱلۡأَرۡضُ وَٱلۡجِبَالُ فَدُكَّتَا دَكَّةٗ وَٰحِدَةٗ",
      "translation": "ߊ߬ ߣߌ߫ ߞߊ߬ ߘߎ߱ ߣߌ߫ ߞߎ߬ߙߎ ߟߎ߫ ߕߊ߬ ߞߵߊ߬ߟߎ߬ ߡߐߢߐ߲ߞߐ߫ ߞߋߟߋ߲ߘߌ߫ ߓߘߎߓߘߍ߫",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 567,
      "verse_key": "69:15",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَيَوۡمَئِذٖ وَقَعَتِ ٱلۡوَاقِعَةُ",
      "translation": "ߒ߬ߓߊ߬ ߞߍߕߊ߫ ( ߘߐߛߊ߬ߓߊߟߌ ) ߞߍߕߐ߫ ߏ߬ ߟߏ߲ ߠߋ߬",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 567,
      "verse_key": "69:16",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱنشَقَّتِ ٱلسَّمَآءُ فَهِيَ يَوۡمَئِذٖ وَاهِيَةٞ",
      "translation": "ߊ߬ ߣߌ߫ ߛߊ߲ ߘߌ߫ ߥߦߍߥߦߍ߫ ߓߊߏ߬ ߊ߬ ߘߐߓߊ߲ߣߍ߲߫ ߠߴߏ߬ ߟߏ߲ ߘߋ߬",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 567,
      "verse_key": "69:17",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡمَلَكُ عَلَىٰٓ أَرۡجَآئِهَاۚ وَيَحۡمِلُ عَرۡشَ رَبِّكَ فَوۡقَهُمۡ يَوۡمَئِذٖ ثَمَٰنِيَةٞ",
      "translation": "ߞߋ߲ߛߐ߲ ߠߎ߬ ߘߌ߫ ߞߍ߫ ߊ߬ ߝߊ߲߭ ߠߎ߬ ߘߐ߫، ߊ߬ߟߎ߬ ߛߋߦߌ߲߫ ߠߋ߬ ߦߴߌ ߡߊ߰ߙߌ ߟߊ߫ ߛߍ߬ߡߍ ߕߊ߬ ߟߴߊ߬ߟߎ߫ ߞߎ߲߬ߘߐ߫ ߏ߬ ߟߏ߲",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 567,
      "verse_key": "69:18",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَوۡمَئِذٖ تُعۡرَضُونَ لَا تَخۡفَىٰ مِنكُمۡ خَافِيَةٞ",
      "translation": "ߏ߬ ߟߏ߲ ߸ ߊߟߎ߫ ߘߌ߫ ߦߌߘߊߞߊ߬ ߊߟߎ߫ ߟߊ߫ ߞߏ߫ ߛߌ߫ ߕߍ߫ ߕߏ߫ ߘߏ߲߰ ߘߐ߫",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 567,
      "verse_key": "69:19",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَمَّا مَنۡ أُوتِيَ كِتَٰبَهُۥ بِيَمِينِهِۦ فَيَقُولُ هَآؤُمُ ٱقۡرَءُواْ كِتَٰبِيَهۡ",
      "translation": "ߒ߬ߓߊ߬ ߣߌ߫ ߡߍ߲ ߠߊ߫ ߟߍߙߊ ߘߌ߫ ߘߴߊ߬ ߡߵߊ߬ ߞߌߣߌ߲ ߝߍ߬ ߏ߬ ߘߴߊ߬ ߝߐ߫ ߞߏ߫ ߊߟߎ߯ ߣߊ߬ ߒ ߠߊ߫ ߟߍߙߊ ߞߊ߬ߙߊ߲߬ ߞߟߋ߫",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 567,
      "verse_key": "69:20",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنِّي ظَنَنتُ أَنِّي مُلَٰقٍ حِسَابِيَهۡ",
      "translation": "ߒߠߋ ߞߵߊ߬ ߖߌ߰ ߟߋ߬ ߒ ߞߏ߫ ߒ ߘߌߣߊ߬ ߒ ߠߊ߫ ߖߊ߬ߕߋ߬ߓߐ ߓߍ߲߬ߢߐ߲߰ߦߊ߬ ߗߐ߫",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 567,
      "verse_key": "69:21",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَهُوَ فِي عِيشَةٖ رَّاضِيَةٖ",
      "translation": "ߒ߬ߓߊ߬ ߏ ߕߌ߰ ߦߋ߫ ߘߌ߬ߢߍ߬ߡߊ߬ ߓߊߟߏ߫ ߟߋ߬ ߘߐ߫",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 567,
      "verse_key": "69:22",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِي جَنَّةٍ عَالِيَةٖ",
      "translation": "ߡߍ߲ߘߌߦߊ߫ ( ߦߙߐ߫ ߡߐ߬ߟߐ߲߬ ) ߞߘߐߕߊ߬ߣߍ߲ ߘߐ߫",
      "footnotes": ""
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 567,
      "verse_key": "69:23",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قُطُوفُهَا دَانِيَةٞ",
      "translation": "ߏ߬ ߦߙߌߘߋ߲߫ ߞߊߘߌ ߦߋ߫ ( ߞߍ߫ ߟߊ߫ ) ߘߎ߲߬ߘߊ ߟߋ߬ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 567,
      "verse_key": "69:24",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كُلُواْ وَٱشۡرَبُواْ هَنِيٓـَٔۢا بِمَآ أَسۡلَفۡتُمۡ فِي ٱلۡأَيَّامِ ٱلۡخَالِيَةِ",
      "translation": "ߞߏ߫ ߊߟߎ߫ ߘߊߥߎ߲ߠߌ߲ ߞߍ߫ ߞߵߊߟߎ߫ ߡߌ߲߬ ߛߎߡߊߒߞߣߐ ߘߌ߫ ߕߐߟߍ߫ ߸ ߏ߬ ߡߊߛߐ߬ߘߐ߬ߣߍ߲߫ ߊߟߎ߫ ߟߊ߫ ߓߊ߯ߙߊ߫ ߞߍߣߍ߲ ߠߎ߫ ߟߋ߫ ߝߍ߬ ( ߘߎߢߊ߫ ) ߟߏ߲߫ ߕߊ߬ߡߌ߲߬ߣߍ߲ ߠߎ߬ ߘߐ߫",
      "footnotes": ""
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 567,
      "verse_key": "69:25",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَمَّا مَنۡ أُوتِيَ كِتَٰبَهُۥ بِشِمَالِهِۦ فَيَقُولُ يَٰلَيۡتَنِي لَمۡ أُوتَ كِتَٰبِيَهۡ",
      "translation": "ߒ߬ߓߊ߬ ߣߌ߫ ߡߍ߲ ߠߋ߬ ߟߊ߫ ߟߍߙߊ ߘߌ߫ ߘߴߊ߬ ߡߵߊ߬ ߡߊ߬ߙߊ߲ ߝߍ߬ ߛߊ߫ ߸ ߏ߬ ߘߴߊ߬ ߝߐ߫ ߞߏ߫ ߥߊߦߌ߯ ߤߊߟߌ߫ ߒ ߠߊ߫ ߟߍߙߊ ߕߎ߲߬ ߞߊߣߊ߬ ߘߌ߫ ߒ ߡߊ߬",
      "footnotes": ""
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 567,
      "verse_key": "69:26",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَمۡ أَدۡرِ مَا حِسَابِيَهۡ",
      "translation": "ߒ ߕߎ߲߬ ߡߴߊ߬ ߟߐ߲߫ ߡߍ߲ ߦߋ߫ ߒ ߠߊ߫ ߖߊ߬ߕߋ߬ߓߐ ߘߌ߫",
      "footnotes": ""
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 567,
      "verse_key": "69:27",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَٰلَيۡتَهَا كَانَتِ ٱلۡقَاضِيَةَ",
      "translation": "ߥߟߊߞߏ߫ ߛߊ߬ߦߊ ߕߘߍ߬ ߦߋ߫ ߞߍ߫ ( ߒ ߠߊ߫ ߞߏ ߓߍ߯ ߓߊ߲ߞߏ ) ߟߊߕߍ߰ߣߍ߲ ߘߌ߫ ߞߘߊߎ߫",
      "footnotes": ""
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 567,
      "verse_key": "69:28",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَآ أَغۡنَىٰ عَنِّي مَالِيَهۡۜ",
      "translation": "ߒ ߠߊ߫ ߣߊ߲߬ߝߟߏ ߡߊ߫ ߝߏߦߌ߬ ߟߊߘߊ߲߬ ߒ ߠߊ߫ ߡߎ߬ߕߎ߲߬",
      "footnotes": ""
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 567,
      "verse_key": "69:29",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "هَلَكَ عَنِّي سُلۡطَٰنِيَهۡ",
      "translation": "ߒ ߠߊ߫ ߣߐ߬ߓߊ߰ߦߊ ߓߍ߯ ߓߘߊ߫ ߕߌߢߍ߫ ߒ ߡߊ߬",
      "footnotes": ""
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 567,
      "verse_key": "69:30",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "خُذُوهُ فَغُلُّوهُ",
      "translation": "( ߊ߬ ߘߌ߫ ߝߐ߫ ) ߞߏ߫ ߊߟߎ߫ ߦߴߏ߬ ߕߍߕߊ߬ ߞߵߊ߬ ߞߏߟߏ߲ߜߊ߲߫",
      "footnotes": ""
    },
    {
      "id": 31,
      "verse_number": 31,
      "page_number": 567,
      "verse_key": "69:31",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ ٱلۡجَحِيمَ صَلُّوهُ",
      "translation": "ߊߟߎ߫ ߦߋ߫ ߓߊ߲߫ ߞߵߊ߬ ߟߊߘߏ߲߬ ߥߋ߲߯ߓߋ ߘߐ߫",
      "footnotes": ""
    },
    {
      "id": 32,
      "verse_number": 32,
      "page_number": 567,
      "verse_key": "69:32",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ فِي سِلۡسِلَةٖ ذَرۡعُهَا سَبۡعُونَ ذِرَاعٗا فَٱسۡلُكُوهُ",
      "translation": "ߊߟߎ߫ ߦߋ߫ ߓߊ߲߫ ߞߵߊ߬ ߓߏ߲߫ ߖߟߐ߬ߞߐ߬ ߘߐ߫ ߡߍ߲ ߖߊ߲߬ߧߊ ߦߋ߫ ߣߐ߲߬ߞߐ߲ ߢߊ߫ ߓߌߥߐߙߐ߲ߥߌ߬ߟߊ߬ ߘߌ߫",
      "footnotes": ""
    },
    {
      "id": 33,
      "verse_number": 33,
      "page_number": 567,
      "verse_key": "69:33",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُۥ كَانَ لَا يُؤۡمِنُ بِٱللَّهِ ٱلۡعَظِيمِ",
      "translation": "ߓߊߏ߬ ߊ߬ ߕߘߍ߬ ߡߊ߫ ߟߴߊߟߊ߫ ߘߊ߲߬ߞߘߐ߬ߓߊ ߟߊ߫ ߟߋ߬",
      "footnotes": ""
    },
    {
      "id": 34,
      "verse_number": 34,
      "page_number": 567,
      "verse_key": "69:34",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَا يَحُضُّ عَلَىٰ طَعَامِ ٱلۡمِسۡكِينِ",
      "translation": "ߊ߬ ߘߏ߲߬ ߕߎ߲߬ ߕߍ߫ ߘߐ߬ߛߎ߬ߟߌ߫ ߞߍ߫ ߟߊ߫ ߘߐ߰ߕߐ ߘߡߊߢߌߣߌ߲ ߡߊ߬",
      "footnotes": ""
    },
    {
      "id": 35,
      "verse_number": 35,
      "page_number": 568,
      "verse_key": "69:35",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلَيۡسَ لَهُ ٱلۡيَوۡمَ هَٰهُنَا حَمِيمٞ",
      "translation": "ߡߊ߬ߡߐ߯ ߕߍ߫ ߊ߬ ߟߊ߫ ߦߊ߲߬ ߏ߬ ߟߋ ߘߐ߫ ߓߌ߬",
      "footnotes": ""
    },
    {
      "id": 36,
      "verse_number": 36,
      "page_number": 568,
      "verse_key": "69:36",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَا طَعَامٌ إِلَّا مِنۡ غِسۡلِينٖ",
      "translation": "ߛߎ߬ߡߊ߲߬ ߝߣߊ߫ ߕߍ߫ ( ߊ߬ ߦߋ߫ ) ߝߏ߫ ߡߍ߲ ߓߐߣߍ߲߫ ߞߐߟߐ߲ߞߐߟߐ߲ ߘߐ߫",
      "footnotes": ""
    },
    {
      "id": 37,
      "verse_number": 37,
      "page_number": 568,
      "verse_key": "69:37",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَّا يَأۡكُلُهُۥٓ إِلَّا ٱلۡخَٰطِـُٔونَ",
      "translation": "ߡߐ߰ ߛߌ߫ ߘߏ߲߬ ߕߍ߫ ߏ߬ ߘߊߥߎ߲߫ ߠߊ߫ ߝߏ߫ ߞߎ߲߬ߞߏ߬ߕߐ ߟߎ߬",
      "footnotes": ""
    },
    {
      "id": 38,
      "verse_number": 38,
      "page_number": 568,
      "verse_key": "69:38",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلَآ أُقۡسِمُ بِمَا تُبۡصِرُونَ",
      "translation": "ߍ߲߬ߍ߲ߍ߲߬ ߸ ߒ ߧߋ߫ ߒ ߞߊ߬ߟߌ߬ ߟߊ߫ ߟߋ߬ ߊߟߎ߫ ߦߋߟߌߞߏ ߟߎ߬ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 39,
      "verse_number": 39,
      "page_number": 568,
      "verse_key": "69:39",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا لَا تُبۡصِرُونَ",
      "translation": "ߊ߬ ߣߴߊߟߎ߫ ߦߋߓߊߟߌߦߊߞߏ ߟߎ߬ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 40,
      "verse_number": 40,
      "page_number": 568,
      "verse_key": "69:40",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُۥ لَقَوۡلُ رَسُولٖ كَرِيمٖ",
      "translation": "ߤߋ߫ ߸ ‹ ߘߐ߬ߛߊߙߌ › ߦߋ߫ ߞߋߟߊߘߋ߲߫ ߞߊ߬ߡߙߊ߬ߒߞߋ ߟߊ߫ ߟߊ߬ߛߋ߬ߟߌ߬ߞߊ߲ ߠߋ߬ ߘߌ߫ ߘߋ߬",
      "footnotes": ""
    },
    {
      "id": 41,
      "verse_number": 41,
      "page_number": 568,
      "verse_key": "69:41",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا هُوَ بِقَوۡلِ شَاعِرٖۚ قَلِيلٗا مَّا تُؤۡمِنُونَ",
      "translation": "ߊ߬ ߕߍ߫ ߛߞߎ߬ߦߟߊ߬ ߞߎߡߊߞߊ߲߫ ߘߌ߫ ߝߋߎ߫ ߸ ߊߟߎ߫ ߟߊߒߠߊߞߏ ߘߏ߲߬ ߕߜߋ߫ ߟߋ߫ ߘߋ߬",
      "footnotes": ""
    },
    {
      "id": 42,
      "verse_number": 42,
      "page_number": 568,
      "verse_key": "69:42",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَا بِقَوۡلِ كَاهِنٖۚ قَلِيلٗا مَّا تَذَكَّرُونَ",
      "translation": "ߊ߬ ߘߏ߲߬ ߕߍ߫ ߓߟߏߟߊߝߍߣߍ߲ߠߊ߫ ߝߣߊ߫ ߞߎߡߊߞߊ߲߫ ߘߌ߫ ߸ ߊߟߎ߫ ߕߊ߰ߛߌ߬ ߞߏ ߘߏ߲߬ ߕߜߋ߫ ߟߋ߬ ߘߋ߬",
      "footnotes": ""
    },
    {
      "id": 43,
      "verse_number": 43,
      "page_number": 568,
      "verse_key": "69:43",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تَنزِيلٞ مِّن رَّبِّ ٱلۡعَٰلَمِينَ",
      "translation": "ߊ߬ ߟߊߖߌ߰ߣߍ߲߫ ߞߊ߬ ߝߘߊ߫ ߖߊ߯ߓߊ ߟߎ߫ ߡߊ߰ߙߌ ߟߋ߬ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 44,
      "verse_number": 44,
      "page_number": 568,
      "verse_key": "69:44",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَوۡ تَقَوَّلَ عَلَيۡنَا بَعۡضَ ٱلۡأَقَاوِيلِ",
      "translation": "ߣߌ߫ ‹ ߞߋߟߊ › ߕߎ߲߬ ߞߊ߬ ߞߎߡߊ ߘߏ߫ ߟߊ߫ ߒ߬ߠߎ߬ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 45,
      "verse_number": 45,
      "page_number": 568,
      "verse_key": "69:45",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَأَخَذۡنَا مِنۡهُ بِٱلۡيَمِينِ",
      "translation": "ߊ߲ ߕߎ߲߬ ߘߴߊ߬ ߕߍߕߊ߬ ߝߊ߲߬ߞߊ ߟߊ߫ ߏ߬ ߘߐ߫ ߟߋ߬ ߘߍ߯",
      "footnotes": ""
    },
    {
      "id": 46,
      "verse_number": 46,
      "page_number": 568,
      "verse_key": "69:46",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ لَقَطَعۡنَا مِنۡهُ ٱلۡوَتِينَ",
      "translation": "ߏ߬ ߞߐ߫ ߸ ߊ߲ ߕߘߍ߬ ߘߴߊ߬ ߣߊ߬ߝߏ߬ߟߏ߲߬ߓߊ ߕߍ߰ ߜߊߘߌ߫",
      "footnotes": ""
    },
    {
      "id": 47,
      "verse_number": 47,
      "page_number": 568,
      "verse_key": "69:47",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَمَا مِنكُم مِّنۡ أَحَدٍ عَنۡهُ حَٰجِزِينَ",
      "translation": "ߡߐ߰ ߛߌ߫ ߘߏ߲߬ ߕߍ߫ ߊߟߎ߫ ߘߐ߫ ߡߍ߲ ߘߌ߫ ߘߏ߲߬ ߊ߬ ߣߴߏ߬ ߕߍ߫",
      "footnotes": ""
    },
    {
      "id": 48,
      "verse_number": 48,
      "page_number": 568,
      "verse_key": "69:48",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِنَّهُۥ لَتَذۡكِرَةٞ لِّلۡمُتَّقِينَ",
      "translation": "‹ ߘߐ߬ߛߊߙߌ › ߘߏ߲߬ ߦߋ߫ ߕߊ߰ߛߌ߬ߞߏ߫ ߟߋ߬ ߘߌ߫ ߝߊ߲ߕߊ߲ߞߟߊ ߟߎ߬ ߦߋ߫ ߘߋ߬",
      "footnotes": ""
    },
    {
      "id": 49,
      "verse_number": 49,
      "page_number": 568,
      "verse_key": "69:49",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِنَّا لَنَعۡلَمُ أَنَّ مِنكُم مُّكَذِّبِينَ",
      "translation": "ߊ߲ߠߎ߫ ߘߏ߲߬ ߞߵߊ߬ ߟߐ߲߫ ߗߐ߫ ߸ ߒ ߞߏ߫ ߡߊ߬ߛߐ߬ߛߐ߬ߟߌ߬ߟߊ ߟߎ߬ ߦߴߊߟߎ߫ ߘߐ߫",
      "footnotes": ""
    },
    {
      "id": 50,
      "verse_number": 50,
      "page_number": 568,
      "verse_key": "69:50",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِنَّهُۥ لَحَسۡرَةٌ عَلَى ٱلۡكَٰفِرِينَ",
      "translation": "ߞߏ߬ߣߌ߲߬ ߊ߬ ߘߌߣߊ߬ ߞߍ߫ ߣߡߌߛߊ ߘߌ߫ ߓߊ߲߬ߓߊ߮ ߟߎ߫ ߞߊ߲߬ ߞߟߋ߫",
      "footnotes": ""
    },
    {
      "id": 51,
      "verse_number": 51,
      "page_number": 568,
      "verse_key": "69:51",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِنَّهُۥ لَحَقُّ ٱلۡيَقِينِ",
      "translation": "ߓߊߏ߬ ߕߎ߬ߢߊ߬ ߔߊߞߌߛߊ ߟߋ߬",
      "footnotes": ""
    },
    {
      "id": 52,
      "verse_number": 52,
      "page_number": 568,
      "verse_key": "69:52",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَسَبِّحۡ بِٱسۡمِ رَبِّكَ ٱلۡعَظِيمِ",
      "translation": "ߒ߬ߓߊ߬ ߌߟߋ ߦߴߌ ߡߊ߰ߙߌ ߕߐ߮ ߛߊߣߌ߲ߧߊ߫ ߟߋ߬ ߞߟߋ߫",
      "footnotes": ""
    }
  ]
}