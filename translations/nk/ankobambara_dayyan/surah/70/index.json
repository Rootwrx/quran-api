{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 568,
      "verse_key": "70:1",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "سَأَلَ سَآئِلُۢ بِعَذَابٖ وَاقِعٖ",
      "translation": "ߡߊ߬ߞߟߌ߬ߟߊ ߘߏ߫ ߓߘߊ߫ ߖߊ߲߰ߞߕߊ߫ ߞߍߕߊ ߡߊߞߟߌ߫",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 568,
      "verse_key": "70:2",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لِّلۡكَٰفِرِينَ لَيۡسَ لَهُۥ دَافِعٞ",
      "translation": "ߓߊ߲߬ߓߊ߮ ߟߎ߬ ߞߊߡߊ߬ ߘߐ߬ߛߊ߬ߓߊ߯ ߕߴߏ߬ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 568,
      "verse_key": "70:3",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مِّنَ ٱللَّهِ ذِي ٱلۡمَعَارِجِ",
      "translation": "ߏ߬ ߝߘߊߕߐ߫ ߊߟߊ߫ ߟߋ߬ ߟߊ߫ ߦߟߍ߬ߦߟߍ ߟߎ߫ ߕߌ߱ ߞߊ߬ߣߌ߲߫",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 568,
      "verse_key": "70:4",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تَعۡرُجُ ٱلۡمَلَٰٓئِكَةُ وَٱلرُّوحُ إِلَيۡهِ فِي يَوۡمٖ كَانَ مِقۡدَارُهُۥ خَمۡسِينَ أَلۡفَ سَنَةٖ",
      "translation": "ߞߋ߲ߛߐ߲ ߠߎ߬ ߣߌ߫ ߛߟߐߡߌ߲ ߘߌ߫ ߦߟߍ߬ ߊ߬ ߡߊ߬ ߟߏ߲߫ ߘߐ߫ ߡߍ߲ ߞߎ߲߬ߕߊ߮ ߦߋ߫ ߛߊ߲߬ ߥߊ߯ߓߌߟߏ߯ߟߎ߫ ߘߌ߫",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 568,
      "verse_key": "70:5",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَٱصۡبِرۡ صَبۡرٗا جَمِيلًا",
      "translation": "ߒ߬ߓߊ߬ ߌߟߋ ߌ ߡߎ߬ߢߎ߲߫ ߡߎ߬ߢߎ߲߫ ߗߋ߲߬ ߠߊ߫ ߖߐ߫",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 568,
      "verse_key": "70:6",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُمۡ يَرَوۡنَهُۥ بَعِيدٗا",
      "translation": "ߓߊ ߊ߬ߟߎ߬ ߦߴߊ߬ ߦߋ߫ ߟߊ߫ ( ߞߏ߫ ) ߞߎ߲߬ߘߐ߬ߖߊ߲߫ ߠߋ߬ ߘߌ߫",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 568,
      "verse_key": "70:7",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَنَرَىٰهُ قَرِيبٗا",
      "translation": "ߒ߬ߠߎ߫ ߘߏ߲߬ ߦߴߊ߬ ߦߋ߫ ߟߊ߫ ( ߞߏ߫ ) ߞߎ߲߬ߘߐ߬ߛߎߘߎ߲ ߠߋ߬ ߘߌ߫",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 568,
      "verse_key": "70:8",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَوۡمَ تَكُونُ ٱلسَّمَآءُ كَٱلۡمُهۡلِ",
      "translation": "ߛߊ߲ ߦߋ߫ ߞߍ߫ ߟߴߏ߬ ߟߏ߲ ߠߋ߬ ߦߏ߫ ( ߥߊߘߌߺ)ߺߜߍ߫ ߦߋ߬ߟߋ߲߬ߣߍ߲",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 568,
      "verse_key": "70:9",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَتَكُونُ ٱلۡجِبَالُ كَٱلۡعِهۡنِ",
      "translation": "ߞߎ߬ߙߎ ߟߎ߬ ߘߌ߫ ߞߍ߫ ߦߏ߫ ߖߎ߯ߓߊ ߘߓߐߣߍ߲",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 568,
      "verse_key": "70:10",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَا يَسۡـَٔلُ حَمِيمٌ حَمِيمٗا",
      "translation": "ߡߊ߬ߡߐ߮ ߘߏ߫ ߕߍ߫ ߡߊߢߌ߬ߣߌ߲߬ߞߊ߫ ߡߊ߬ߡߐ߰ ߜߘߍ߫ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 569,
      "verse_key": "70:11",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يُبَصَّرُونَهُمۡۚ يَوَدُّ ٱلۡمُجۡرِمُ لَوۡ يَفۡتَدِي مِنۡ عَذَابِ يَوۡمِئِذِۭ بِبَنِيهِ",
      "translation": "ߊ߬ߟߎ߫ ߘߏ߲߬ ߢߊ߯ ߢߐ߲߮ ߠߊ߫ ߗߐ߫ ߸ ߛߐ߲߬ߡߊ ߘߴߊ߬ ߣߕߊ߬ ߞߏ߫ ߤߊߟߵߊ߬ߟߋ߬ ߓߊ߯ ߞߎ߲߬ߞߊ߬ ߞߊ߬ ߓߐ߫ ߏ߬ ߟߏ߲ ߖߊ߲߰ߞߕߊ ߞߘߐ߫ ߊ߬ ߖߍ߬ߘߍ ߘߋ߲ߞߍ ߟߎ߬ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 569,
      "verse_key": "70:12",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَصَٰحِبَتِهِۦ وَأَخِيهِ",
      "translation": "ߊ߬ ߣߴߊ߬ ߡߏ߬ߛߏ ߟߊ߫ ߊ߬ ߣߴߊ߬ ߓߊߘߋ߲ߞߍ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 569,
      "verse_key": "70:13",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَفَصِيلَتِهِ ٱلَّتِي تُـٔۡوِيهِ",
      "translation": "ߊ߬ ߣߴߊ߬ ߓߊߓߏ߲ ߠߊ߫ ߡߍ߲ ߦߴߊ߬ ߟߊߘߊ߬ߙߏ߬ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 569,
      "verse_key": "70:14",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَن فِي ٱلۡأَرۡضِ جَمِيعٗا ثُمَّ يُنجِيهِ",
      "translation": "ߊ߬ ߣߌ߫ ߡߍ߲ ߓߍ߯ ߦߋ߫ ߘߎ߰ߞߟߏ ߞߊ߲߬ ߞߊ߬ ߓߊ߲߫ ߞߵߊ߬ߟߋ߬ ߟߊߞߛߌ߫",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 569,
      "verse_key": "70:15",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّآۖ إِنَّهَا لَظَىٰ",
      "translation": "ߊ߬ߦߌ߫ ߸ ߊ߬ ( ߕߊ ) ߦߋ߫ ߡߢߊߞߌ ߟߋ߬ ߘߌ߫",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 569,
      "verse_key": "70:16",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "نَزَّاعَةٗ لِّلشَّوَىٰ",
      "translation": "ߏ߬ ߦߋ߫ ߛߐ߰ߙߊ߲ ߠߎ߫ ߡߊ߬ߣߊ߲߬ߞߌ߬ ߟߊ߫ ߟߋ߬",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 569,
      "verse_key": "70:17",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تَدۡعُواْ مَنۡ أَدۡبَرَ وَتَوَلَّىٰ",
      "translation": "ߊ߬ ߦߋ߫ ߡߐ߰ ߟߋ߬ ߡߊߞߟߌ߫ ߟߊ߫ ߡߍ߲ ߓߊ߯ ߊ߬ ߞߐߘߏ߲߬ ߞߵߊ߬ ߡߙߎ߫",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 569,
      "verse_key": "70:18",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَجَمَعَ فَأَوۡعَىٰٓ",
      "translation": "ߊ߬ ߣߴߊ߬ ߞߊ߬ ‹ ߣߊ߲߬ߝߟߏ › ߟߊߘߍ߬ ߞߵߊ߬ ߞߍ߫ ߞߏ߲߬ߘߏ ߘߐ߫",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 569,
      "verse_key": "70:19",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "۞ إِنَّ ٱلۡإِنسَٰنَ خُلِقَ هَلُوعًا",
      "translation": "ߤߋ߫ ߸ ߡߐ߱ ߘߊ߲ߣߍ߲߫ ߦߟߌߖߎ߯ߦߊ ߟߋ߬ ߟߊ߫ ߘߋ߬",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 569,
      "verse_key": "70:20",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذَا مَسَّهُ ٱلشَّرُّ جَزُوعٗا",
      "translation": "ߣߌ߫ ߕߣߊ߬ߡߊ߫ ߡߊ߰ ߘߴߊ߬ ߟߊ߫ ߘߌߡߌ߲ߓߊ߮ ߟߋ߬ ߞߊߘߌߞߊߘߌ߫",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 569,
      "verse_key": "70:21",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا مَسَّهُ ٱلۡخَيۡرُ مَنُوعًا",
      "translation": "ߣߌ߫ ߤߙߊ߫ ߡߊ߰ ߘߴߊ߬ ߟߊ߫ ߓߏ߬ߞߎߣߊ߲ ߠߋ߬ ߞߙߊߕߊߕߊ߫",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 569,
      "verse_key": "70:22",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَّا ٱلۡمُصَلِّينَ",
      "translation": "ߝߏ߫ ߕߌ߲ߞߌߟߊ ߟߎ߫ ߘߋ߬",
      "footnotes": ""
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 569,
      "verse_key": "70:23",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِينَ هُمۡ عَلَىٰ صَلَاتِهِمۡ دَآئِمُونَ",
      "translation": "ߡߍ߲ ߠߎ߬ ߦߋ߫ ߕߏ߫ ߟߴߊ߬ߟߎ߬ ߟߊ߫ ߛߊߟߌ ( ߟߎ߫ ) ߡߊ߬ ߞߊ߬ߣߌ߲߫",
      "footnotes": ""
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 569,
      "verse_key": "70:24",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّذِينَ فِيٓ أَمۡوَٰلِهِمۡ حَقّٞ مَّعۡلُومٞ",
      "translation": "ߊ߬ ߣߌ߫ ߖߏ߫ ߡߊߟߐ߲ߣߍ߲߫ ߏ߬ ߟߎ߬ ߡߍ߲ ߠߎ߬ ߓߟߏ߫ ߊ߬ߟߎ߬ ߟߊ߫ ߣߊ߲߬ߝߟߏ ߟߎ߬ ߘߐ߫",
      "footnotes": ""
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 569,
      "verse_key": "70:25",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لِّلسَّآئِلِ وَٱلۡمَحۡرُومِ",
      "translation": "ߕߙߊߟߌߟߊ ߣߌ߫ ߖߘߍ߬ߘߐ߬ߡߌ߬ߘߊ߬ߟߊ ߟߎ߬ ߦߋ߫ ( ߞߊ߬ ߓߐ߫ ߡߊ߬ߕߙߊ߬ߟߌ ߡߊ߬ )",
      "footnotes": ""
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 569,
      "verse_key": "70:26",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّذِينَ يُصَدِّقُونَ بِيَوۡمِ ٱلدِّينِ",
      "translation": "ߊ߬ ߣߌ߫ ߡߍ߲ ߠߎ߬ ߦߋ߫ ߟߊ߫ ߟߊ߫ ߛߙߊ߬ߟߌ߬ ߟߏ߲߬ ( ߞߏ ) ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 569,
      "verse_key": "70:27",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّذِينَ هُم مِّنۡ عَذَابِ رَبِّهِم مُّشۡفِقُونَ",
      "translation": "ߊ߬ ߣߌ߫ ߡߍ߲ ߠߎ߬ ߜߎ ߛߌߟߊ߲ߣߍ߲߫ ߊ߬ߟߎ߫ ߡߊ߰ߙߌ ߟߊ߫ ߖߊ߲߰ߞߕߊ ߢߍ߫",
      "footnotes": ""
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 569,
      "verse_key": "70:28",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ عَذَابَ رَبِّهِمۡ غَيۡرُ مَأۡمُونٖ",
      "translation": "ߓߊ ߊ߬ߟߎ߫ ߡߊ߰ߙߌ ߟߊ߫ ߖߊ߲߰ߞߕߊ ߦߋ߫ ߡߊߕߊ߲߬ߞߊ߬ߜߏ ߟߋ߬ ߘߌ߫",
      "footnotes": ""
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 569,
      "verse_key": "70:29",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّذِينَ هُمۡ لِفُرُوجِهِمۡ حَٰفِظُونَ",
      "translation": "ߊ߬ ߣߌ߫ ߡߍ߲ ߠߎ߬ ߦߴߊ߬ߟߎ߫ ߥߏ߬ߘߊ ߟߎ߬ ߟߊߕߊ߲߬ߞߊ߬ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 569,
      "verse_key": "70:30",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَّا عَلَىٰٓ أَزۡوَٰجِهِمۡ أَوۡ مَا مَلَكَتۡ أَيۡمَٰنُهُمۡ فَإِنَّهُمۡ غَيۡرُ مَلُومِينَ",
      "translation": "ߝߏ߫ ߊ߬ߟߎ߬ ߝߘߎߡߊ ߟߎ߫ ߡߊ߬ ߥߟߴߊ߬ߟߎ߬ ߓߟߏߡߐ߮ ߟߎ߫ ߡߊ߬ ߸ ߒ߬ߓߊ߬ ߏ߬ ߟߎ߫ ߟߋ߫ ߖߙߊ߬ߞߌ߬ߓߊߟߌ ߟߎ߬ ߘߌ߫",
      "footnotes": ""
    },
    {
      "id": 31,
      "verse_number": 31,
      "page_number": 569,
      "verse_key": "70:31",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَمَنِ ٱبۡتَغَىٰ وَرَآءَ ذَٰلِكَ فَأُوْلَٰٓئِكَ هُمُ ٱلۡعَادُونَ",
      "translation": "ߡߍ߲ ߠߋ߬ ߓߊ߯ ߘߏ߫ ߜߘߍ߫ ߢߌߣߌ߲߫ ߏ߬ ( ߟߎ߬ ) ߞߐ߫ ߛߊ߫ ߸ ߒ߬ߓߊ߬ ߏ߬ ߟߎ߫ ߟߋ߫ ߘߊ߲߬ߠߕߊ߬ߡߌ߲߬ߓߊ߮ ߟߎ߬ ߘߌ߫",
      "footnotes": ""
    },
    {
      "id": 32,
      "verse_number": 32,
      "page_number": 569,
      "verse_key": "70:32",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّذِينَ هُمۡ لِأَمَٰنَٰتِهِمۡ وَعَهۡدِهِمۡ رَٰعُونَ",
      "translation": "ߊ߬ ߣߌ߫ ‹ ߕߌ߲ߞߌߟߊ › ߟߎ߫ ߟߴߊ߬ߟߎ߬ ߟߊ߫ ߞߊ߬ߙߌ߬ߝߊ ߟߎ߬ ߣߴߊ߬ߟߎ߬ ߟߊ߫ ߛߘߊߒߡߊߞߊ߲ ߠߎ߬ ߟߊߣߍ߰ߣߍ߰ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 33,
      "verse_number": 33,
      "page_number": 569,
      "verse_key": "70:33",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّذِينَ هُم بِشَهَٰدَٰتِهِمۡ قَآئِمُونَ",
      "translation": "ߊ߬ ߣߌ߫ ߡߍ߲ ߠߎ߬ ߦߴߊ߬ߟߎ߫ ߟߐ߬ ߟߴߊ߬ߟߎ߬ ߟߊ߫ ߛߙߋߦߊ ߟߎ߬ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 34,
      "verse_number": 34,
      "page_number": 569,
      "verse_key": "70:34",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّذِينَ هُمۡ عَلَىٰ صَلَاتِهِمۡ يُحَافِظُونَ",
      "translation": "ߊ߬ ߣߌ߫ ߡߍ߲ ߠߎ߬ ߦߴߊ߬ߟߎ߬ ߟߊ߫ ߛߊߟߌ ߟߎ߬ ߟߊߕߊ߲߬ߞߊ߬ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 35,
      "verse_number": 35,
      "page_number": 569,
      "verse_key": "70:35",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أُوْلَٰٓئِكَ فِي جَنَّٰتٖ مُّكۡرَمُونَ",
      "translation": "ߒ߬ߓߊ߬ ߏ߬ ߟߎ߫ ߟߋ߬ ߟߊߘߌߦߊߕߐ߫ ߡߍ߲ߘߌߦߊ߫ ( ߣߊߞߐ ) ߟߎ߬ ߘߐ߫",
      "footnotes": ""
    },
    {
      "id": 36,
      "verse_number": 36,
      "page_number": 569,
      "verse_key": "70:36",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَمَالِ ٱلَّذِينَ كَفَرُواْ قِبَلَكَ مُهۡطِعِينَ",
      "translation": "ߡߋ߬ߘߋ ߦߋ߫ ߓߊ߲߬ߓߊ߮ ߟߎ߬ ߟߊ߫ ߓߏ߲߰ ߊ߬ߟߎ߬ ߛߌߟߊ߲ߣߍ߲ ߞߊߟߌߦߊߡߍߣߍ߲ߕߐ ߦߴߌߟߋ ߓߘߊߟߊߝߊ߲߭ ߝߍ߬؟",
      "footnotes": ""
    },
    {
      "id": 37,
      "verse_number": 37,
      "page_number": 569,
      "verse_key": "70:37",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَنِ ٱلۡيَمِينِ وَعَنِ ٱلشِّمَالِ عِزِينَ",
      "translation": "ߞߙߎ ߟߎ߬ ߘߌ߫ ߞߌߣߌ߲߫ ߣߌ߫ ߣߎߡߊ߲߫ ( ߌ ߟߊߡߌߣߌ߲ ߘߐ߫ )",
      "footnotes": ""
    },
    {
      "id": 38,
      "verse_number": 38,
      "page_number": 569,
      "verse_key": "70:38",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَيَطۡمَعُ كُلُّ ٱمۡرِيٕٖ مِّنۡهُمۡ أَن يُدۡخَلَ جَنَّةَ نَعِيمٖ",
      "translation": "ߊ߬ ߟߐ߮ ߦߋ߫ ߡߐ߱ ߞߋ߬ߟߋ߲߬ߞߋ߬ߟߋ߲߬ߠߊ ߟߴߊ߬ߟߎ߬ ߘߐ߫ ߟߋ߬ ߓߊ߬ ߞߏ߫ ߞߵߊ߬ ߟߊߘߏ߲߬ ߣߍߡߊ ߣߊߞߐ ߘߐ߫ ( ߊ߬ ߡߊ߲߬ߞߕߎ߬ ߘߌߡߊ߲ ߝߍ߬ ߦߋ߲߬ ߕߋ߲߬ )؟",
      "footnotes": ""
    },
    {
      "id": 39,
      "verse_number": 39,
      "page_number": 569,
      "verse_key": "70:39",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّآۖ إِنَّا خَلَقۡنَٰهُم مِّمَّا يَعۡلَمُونَ",
      "translation": "ߏ߬ ߕߍ߫ ߞߍ߫ ߟߊ߫ ߓߊ ߒ߬ߠߎ߫ ߞߵߊ߬ߟߎ߬ ߘߊ߲߫ ߝߋ߲߫ ߠߋ߬ ߘߐ߫ ߸ ߊ߬ߟߎ߫ ߖߍ߬ߘߍ ߞߊ߬ ߡߍ߲ ߟߐ߲߫",
      "footnotes": ""
    },
    {
      "id": 40,
      "verse_number": 40,
      "page_number": 570,
      "verse_key": "70:40",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلَآ أُقۡسِمُ بِرَبِّ ٱلۡمَشَٰرِقِ وَٱلۡمَغَٰرِبِ إِنَّا لَقَٰدِرُونَ",
      "translation": "ߍ߲߬ߍ߲߫ ߸ ߒ ߧߋ߫ ߒ ߞߊ߬ߟߌ߬ ߟߊ߫ ߟߋ߬ ߕߟߋ߬ߓߐ ߟߎ߬ ߣߌ߫ ߕߟߋ߬ߓߋ ߟߎ߫ ߡߊ߰ߙߌ ߟߊ߫ ߒ ߞߏ߫ ߊ߬ ߛߋ߫ ߦߋ߫ ߒ߬ߠߎ߬ ߦߋ߫ ߝߋߎ߫",
      "footnotes": ""
    },
    {
      "id": 41,
      "verse_number": 41,
      "page_number": 570,
      "verse_key": "70:41",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلَىٰٓ أَن نُّبَدِّلَ خَيۡرٗا مِّنۡهُمۡ وَمَا نَحۡنُ بِمَسۡبُوقِينَ",
      "translation": "ߞߵߊ߬ߟߎ߫ ߡߊߝߊ߬ߟߋ߲߬ ( ߝߋ߲߫ ߜߘߍ߫ ߟߊ߫ ) ߡߍ߲ ߦߊ߯ ߝߌ߬ߛߴߊ߬ߟߎ߬ ߘߌ߫ ߸ ߒ߬ߠߎ߫ ߘߏ߲߬ ߕߍ߫ ߞߘߐߕߏߓߊ߯ ߘߌ߫ ( ߏ߬ ߛߎ߮ ߞߍ ߘߐ߫ )",
      "footnotes": ""
    },
    {
      "id": 42,
      "verse_number": 42,
      "page_number": 570,
      "verse_key": "70:42",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَذَرۡهُمۡ يَخُوضُواْ وَيَلۡعَبُواْ حَتَّىٰ يُلَٰقُواْ يَوۡمَهُمُ ٱلَّذِي يُوعَدُونَ",
      "translation": "ߒ߬ߓߵߊ߬ߟߎ߬ ߕߏ߫ ߦߋ߲߬ ߊ߬ߟߎ߬ ߦߴߊ߬ߟߎ߫ ߞߎ߬ߙߎ߲߬ߓߊ߫ ߊ߬ ߣߌ߫ ߞߵߊ߬ߟߎ߬ ߕߏߟߏ߲߫ ߸ ߝߏ߫ ߊ߬ߟߎ߬ ߦߴߊ߬ߟߎ߬ ߟߊ߫ ߟߏ߲ ߓߍ߲߬ߢߐ߲߰ߦߊ߫ ߞߐ߲߬ߞߐ߲߬ ߞߍߣߴߊ߬ߟߎ߫ ߡߊ߬ ߡߍ߲ ߠߊ߫",
      "footnotes": ""
    },
    {
      "id": 43,
      "verse_number": 43,
      "page_number": 570,
      "verse_key": "70:43",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَوۡمَ يَخۡرُجُونَ مِنَ ٱلۡأَجۡدَاثِ سِرَاعٗا كَأَنَّهُمۡ إِلَىٰ نُصُبٖ يُوفِضُونَ",
      "translation": "ߊ߬ߟߎ߬ ߞߊߟߌߦߊߡߍߣߍ߲ߕߐ ߟߎ߬ ߓߕߐ߫ ߛߊ߬ߙߌ ߟߎ߬ ߘߐ߫ ߏ߬ ߟߏ߲ ߠߋ߬ ߸ ߦߏ߫ ߊ߬ߟߎ߬ ߜߘߌߕߐ߫ ߞߊ߬ ߕߊ߯ ( ߊ߬ߟߎ߬ ߟߊ߫ ߘߎߢߊ߫ ߘߐ߫ ) ߝߋ߲߫ ߠߊߟߐ߬ߣߍ߲߫ ( ߓߊ߬ߕߏ߬ߕߊ ߛߎ߮ ) ߘߏ߫ ߟߋ߫ ߡߊ߬",
      "footnotes": ""
    },
    {
      "id": 44,
      "verse_number": 44,
      "page_number": 570,
      "verse_key": "70:44",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "خَٰشِعَةً أَبۡصَٰرُهُمۡ تَرۡهَقُهُمۡ ذِلَّةٞۚ ذَٰلِكَ ٱلۡيَوۡمُ ٱلَّذِي كَانُواْ يُوعَدُونَ",
      "translation": "ߊ߬ߟߎ߬ ߢߊ ߟߎ߬ ߦߋ߫ ߞߌߛߐ߫ ߸ ߟߊ߬ߝߍߦߊ ߦߴߊ߬ߟߎ߬ ߢߡߊߣߐ߮ ߞߊ߲߬ ߸ ߒ߬ߓߊ߬ ߟߏ߲߫ ߠߴߏ߬ ߘߌ߫ ߊ߬ߟߎ߬ ߦߟߌߡߊߜߏߦߊߣߍ߲߫ ߕߘߍ߬ ߡߍ߲ ߠߊ߫ ( ߘߎߢߊ ߞߣߐ߫ )",
      "footnotes": ""
    }
  ]
}