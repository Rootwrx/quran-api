{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 589,
      "verse_key": "84:1",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذَا ٱلسَّمَآءُ ٱنشَقَّتۡ",
      "translation": "ߣߌ߫ ߛߊ߲ ߛߍ߲ߞߍ߫ ߘߊ߫ ߕߎ߬ߡߊ ߡߍ߲ ( ߟߐ߬ߓߊ ߛߋ ߟߊ߫ )",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 589,
      "verse_key": "84:2",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَذِنَتۡ لِرَبِّهَا وَحُقَّتۡ",
      "translation": "ߞߵߊ߬ ߕߟߏߡߊߟߐ߬ ߊ߬ ߡߊ߰ߙߌ ߟߊ߫ ߸ ߊ߬ ߣߌ߫ ߞߵߊ߬ ߖߊ߯ߙߋ߫ ߏ߬ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 589,
      "verse_key": "84:3",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡأَرۡضُ مُدَّتۡ",
      "translation": "ߣߌ߫ ߘߎ߱ ߝߣߊ߫ ߘߐߝߎߟߋ߲߫ ߘߊ߫ ߕߎ߬ߡߊ ߡߍ߲",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 589,
      "verse_key": "84:4",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَلۡقَتۡ مَا فِيهَا وَتَخَلَّتۡ",
      "translation": "ߞߵߊ߬ ߞߣߐ߫ ߝߋ߲ ߠߎ߫ ߝߏ߬ߦߌ߫ ߞߵߊ߬ ߘߐߞߏߟߏ߲ߧߊ߫ ߝߏߦߊ߲߫",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 589,
      "verse_key": "84:5",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَذِنَتۡ لِرَبِّهَا وَحُقَّتۡ",
      "translation": "ߞߵߊ߬ ߕߟߏߡߊߟߐ߬ ߊ߬ ߡߊ߰ߙߌ ߟߊ߫ ߸ ߊ߬ ߣߌ߫ ߞߵߊ߬ ߖߊ߯ߙߋ߫ ߏ߬ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 589,
      "verse_key": "84:6",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَٰٓأَيُّهَا ٱلۡإِنسَٰنُ إِنَّكَ كَادِحٌ إِلَىٰ رَبِّكَ كَدۡحٗا فَمُلَٰقِيهِ",
      "translation": "ߤߍ߲߬ ߌߟߋ߬ ߡߐ߰ߣߍ߲ ߸ ߌ ߦߋ߫ ߓߊ߯ߙߊ ߘߏ߫ ߟߋ߫ ߞߊ߲߬ ߞߵߊ߬ ߟߐ߬ ߌ ߡߊ߰ߙߌ ߡߊ߬ ߛߍ߰ߞߍ߬ߓߟߏߡߊ߬ ߸ ߌ ߘߏ߲߬ ߦߴߏ߬ ( ߛߊ߬ߙߊ ) ߓߍ߲߬ߢߐ߲߰ߦߊ߬ ߟߊ߫ ߟߋ߫ ߘߋ߬",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 589,
      "verse_key": "84:7",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَمَّا مَنۡ أُوتِيَ كِتَٰبَهُۥ بِيَمِينِهِۦ",
      "translation": "ߒ߬ߓߊ߬ ߣߌ߫ ߡߍ߲ ߠߊ߫ ߟߍߙߊ ߘߌ߫ ߘߴߊ߬ ߡߵߊ߬ ߞߌߣߌ߲ ߝߍ߬",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 589,
      "verse_key": "84:8",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَسَوۡفَ يُحَاسَبُ حِسَابٗا يَسِيرٗا",
      "translation": "ߏ ߣߊ߬ ߖߊ߬ߕߋ߬ߓߐ߫ ߖߊ߬ߕߋ߬ߓߐ߬ ߣߐ߲߯ ߠߊ߫߶",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 589,
      "verse_key": "84:9",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيَنقَلِبُ إِلَىٰٓ أَهۡلِهِۦ مَسۡرُورٗا",
      "translation": "ߊ߬ ߣߴߏ߬ ߛߍߥߊߡߍߣߍ߲ߕߐ ߣߊ߬ ߛߊ߬ߦߌ߲߬ ߊ߬ ߞߐߙߍ ߟߎ߫ ߡߊ߬",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 589,
      "verse_key": "84:10",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَمَّا مَنۡ أُوتِيَ كِتَٰبَهُۥ وَرَآءَ ظَهۡرِهِۦ",
      "translation": "ߣߌ߫ ߡߍ߲ ߠߋ߬ ߟߊ߫ ߟߍߙߊ ߘߌ߫ ߘߴߊ߬ ߡߵߊ߬ ߞߕߐ߫ ߛߊ߫",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 589,
      "verse_key": "84:11",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَسَوۡفَ يَدۡعُواْ ثُبُورٗا",
      "translation": "ߒ߬ߓߊ߬ ߏ߬ ߣߊ߬ ߕߏߒߘߐ ߡߊߞߟߌ߫ ( ߢߊߣߌ߲ ߡߊ߬ )߶",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 589,
      "verse_key": "84:12",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيَصۡلَىٰ سَعِيرًا",
      "translation": "ߊ߬ ߣߴߊ߬ ߣߊ߬ ߘߏ߲߬ ߖߌ߯ߖߌ߮ ߘߐ߫",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 589,
      "verse_key": "84:13",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُۥ كَانَ فِيٓ أَهۡلِهِۦ مَسۡرُورًا",
      "translation": "ߏ ߘߏ߲߬ ߜߎ ߛߍߥߊߣߍ߲ ߠߋ߫ ߕߍ߬ߘߍ߫ ߊ߬ ߞߐߙߍ ߕߍߡߊ߬",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 589,
      "verse_key": "84:14",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُۥ ظَنَّ أَن لَّن يَحُورَ",
      "translation": "ߓߊ ߏ߬ ߞߵߊ߬ ߖߌ߰ ߟߋ߬ ߞߴߊ߬ ߕߍߣߊ߬ ߛߊ߬ߦߌ߬ ߟߊ߫ ( ߊ߬ ߡߊ߰ߙߌ ߡߵߊ߬ ߖߊ߬ߕߋ߬ߓߐ߬ߞߏ ߘߐ߫ )",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 589,
      "verse_key": "84:15",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بَلَىٰٓۚ إِنَّ رَبَّهُۥ كَانَ بِهِۦ بَصِيرٗا",
      "translation": "ߤߊ߯ߟߌ߫ ߸ ߓߊ ߊ߬ ߡߊ߰ߙߌ ߢߊ߫ ߕߎ߲߬ ߓߍ߫ ߊ߬ ߟߊ߫ ߛߎߋ߫",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 589,
      "verse_key": "84:16",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلَآ أُقۡسِمُ بِٱلشَّفَقِ",
      "translation": "ߊ߬ߦߌ߫ ߸ ߒ ߧߋ߫ ߒ ߞߊ߬ߟߌ߬ ߟߊ߫ ߥߎ߬ߟߋ߲ߥߎ߬ߟߋ߲ ߠߊ߫ ( ߕߋ߬ߟߋ ߓߋ߬ ߕߎߡߊ )",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 589,
      "verse_key": "84:17",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّيۡلِ وَمَا وَسَقَ",
      "translation": "ߊ߬ ߣߌ߫ ߛߎ ߝߣߊ߫ ߟߊ߫ ߸ ߊ߬ ߣߴߊ߬ ߞߏ߬ߣߍ߲߬ ߝߋ߲ ߡߍ߲ ߠߎ߫ ߡߊ߬",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 589,
      "verse_key": "84:18",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡقَمَرِ إِذَا ٱتَّسَقَ",
      "translation": "ߊ߬ ߣߌ߫ ߞߊߙߏ ߝߣߊ߫ ߟߊ߫ ߸ ߣߴߊ߬ ߘߝߊ߫ ߘߊ߫ ( ߊ߬ ߛߌ߰ߓߊ߲ߘߊ ߡߊ߬ )",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 589,
      "verse_key": "84:19",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَتَرۡكَبُنَّ طَبَقًا عَن طَبَقٖ",
      "translation": "ߊߟߎ߫ ߓߍߣߊ ߦߍ߬ߟߍ߫ ߝߣߍ߬ߝߣߍ ߘߏ߫ ߞߊ߲߬ ߝߣߍ߬ߝߣߍ ߘߏ߫ ߞߐ߫ ߘߋ߬",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 589,
      "verse_key": "84:20",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَمَا لَهُمۡ لَا يُؤۡمِنُونَ",
      "translation": "ߒ߬ߓߊ߬ ߡߋ߬ߘߋ ߦߴߊ߬ߟߎ߬ ߟߊ߫ ߓߏ߲߰ ߊ߬ߟߎ߬ ߕߍ߫ ߞߍ߫ ߟߊ߫ ߟߊߒߠߦߊߓߊ߯ ߘߌ߫؟",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 589,
      "verse_key": "84:21",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": "obligatory",
      "sajdah_number": 13,
      "text": "وَإِذَا قُرِئَ عَلَيۡهِمُ ٱلۡقُرۡءَانُ لَا يَسۡجُدُونَۤ۩",
      "translation": "ߣߌ߫ ߘߐ߬ߛߊߙߌ ߘߐߛߊߙߌ߫ ߘߴߊ߬ߟߎ߬ ߦߋ߫ ߸ ߊ߬ߟߎ߫ ߝߊ߲߭ ߕߍ߫ ߕߌ߲ߓߌߘߌ߲߫ ߞߍ߫ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 589,
      "verse_key": "84:22",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بَلِ ٱلَّذِينَ كَفَرُواْ يُكَذِّبُونَ",
      "translation": "ߊ߬ߟߎ߬ ߕߴߏ߬ ߞߍ߫ ߘߋ߬ ߸ ߓߊ ߓߊ߲߬ߓߊ߮ ߟߎ߫ ߟߋ߫ ߡߊ߬ߛߐ߬ߛߐ߬ߟߌ ߞߍ߫ ߟߊ߫ ߛߐ߲߬",
      "footnotes": ""
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 589,
      "verse_key": "84:23",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱللَّهُ أَعۡلَمُ بِمَا يُوعُونَ",
      "translation": "ߊߟߊ߫ ߘߏ߲߬ ߞߵߊ߬ ߟߐ߲߫ ߡߍ߲ ߦߴߊ߬ߟߎ߬ ߞߣߐ߫ ߗߐ߫",
      "footnotes": ""
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 589,
      "verse_key": "84:24",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبَشِّرۡهُم بِعَذَابٍ أَلِيمٍ",
      "translation": "ߒ߬ߓߊ߬ ߌ ߘߟߊߥߊ߬ߦߌ߲߫ ߊ߬ߟߎ߬ ߦߋ߫ ߖߊ߲߰ߞߕߊ߬ ߘߌߡߌ߲ߣߍ߲ ߠߊ߫",
      "footnotes": ""
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 589,
      "verse_key": "84:25",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَّا ٱلَّذِينَ ءَامَنُواْ وَعَمِلُواْ ٱلصَّٰلِحَٰتِ لَهُمۡ أَجۡرٌ غَيۡرُ مَمۡنُونِۭ",
      "translation": "ߝߏ߫ ߡߍ߲ ߠߎ߬ ߞߍߣߍ߲߫ ߟߊߒߠߦߊߓߊ߮ ߘߌ߫ ߞߊ߬ ߢߌ߬ߡߊ ߟߎ߬ ߞߍ߫ ߸ ߛߙߊ߬ ߓߟߏߕߍ߰ߓߊߟߌ ߦߴߏ߬ ߟߎ߫ ߟߋ߬ ߦߋ߫",
      "footnotes": ""
    }
  ]
}