{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 591,
      "verse_key": "87:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "سَبِّحِ ٱسۡمَ رَبِّكَ ٱلۡأَعۡلَى",
      "translation": "ߌ ߡߊ߰ߙߌ߬ ߞߘߐߕߊ߬ߣߍ߲߬ߓߊ ߕߐ߮ ߛߊߣߌ߲ߧߊ߫",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 591,
      "verse_key": "87:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِي خَلَقَ فَسَوَّىٰ",
      "translation": "ߡߍ߲ ߞߊ߬ ߘߊ߲ߠߌ߲ ߞߍ߫ ߞߵߊ߬ ߟߊߞߢߊ߫",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 591,
      "verse_key": "87:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّذِي قَدَّرَ فَهَدَىٰ",
      "translation": "ߊ߬ ߣߌ߫ ߡߍ߲ ߞߊ߬ ( ߝߋ߲ ߓߍ߯ ߢߊ߬ߞߏ߫ ) ߟߊߕߍ߰ ߞߵߊ߬ߟߎ߬ ߞߊ߲ߘߊ߫ ( ߏ ߡߊ߬ )",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 591,
      "verse_key": "87:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّذِيٓ أَخۡرَجَ ٱلۡمَرۡعَىٰ",
      "translation": "ߊ߬ ߣߌ߫ ߡߍ߲ ߞߊ߬ ߖߍߥߎߟߍ ߟߊߓߐ߫",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 591,
      "verse_key": "87:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَجَعَلَهُۥ غُثَآءً أَحۡوَىٰ",
      "translation": "ߊ߬ ߣߊ߬ ߘߴߏ߬ ߞߍ߫ ߢߊ߰ߛߊ߬ ߖߊߙߊ߲߫ ߝߌ߲߬ߣߍ߲ ߘߌ߫",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 591,
      "verse_key": "87:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "سَنُقۡرِئُكَ فَلَا تَنسَىٰٓ",
      "translation": "ߊ߲ ߘߌߣߵߌ ߟߊߓߌ߬ߟߊ߬ ( ߞߏ ߟߎ߬ ߟߊ߫ ) ߝߏ߫ ߌ ߕߍ߫ ߢߌ߬ߣߊ߬",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 591,
      "verse_key": "87:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَّا مَا شَآءَ ٱللَّهُۚ إِنَّهُۥ يَعۡلَمُ ٱلۡجَهۡرَ وَمَا يَخۡفَىٰ",
      "translation": "ߝߏ߫ ߡߍ߲ ( ߞߐߢߌ߬ߣߊ ) ߓߊ߯ ߊߟߊ߫ ߘߌߦߊ߫ ߸ ߓߊ ߏ߬ ߟߋ ߞߊ߬ ߟߊ߲ߞߣߍߡߊ ߟߐ߲߫ ߊ߬ ߣߌ߫ ߡߍ߲ ߦߋ߫ ߘߏ߲߰ ߠߊ߫",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 591,
      "verse_key": "87:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَنُيَسِّرُكَ لِلۡيُسۡرَىٰ",
      "translation": "ߊ߬ ߣߴߊ߲ ߘߌߣߵߌ ߓߍ߲߬ ߌ ߛߋ߲߬ߡߊ߬ ߣߐ߰ߦߊ߬ ߘߐ߫",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 591,
      "verse_key": "87:9",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَذَكِّرۡ إِن نَّفَعَتِ ٱلذِّكۡرَىٰ",
      "translation": "ߒ߬ߓߊ߬ ߌߟߋ ߦߋ߫ ߦߟߌߟߊߓߌߟߊ ߞߍ߫ ߗߐ߫ ߸ ߣߌ߫ ߦߟߌߟߊߓߌߟߊ ߞߊ߬ ߞߏߢߊ߬",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 591,
      "verse_key": "87:10",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "سَيَذَّكَّرُ مَن يَخۡشَىٰ",
      "translation": "( ߊߟߊߞߏ ) ߢߍ߬ߣߍ߲߬ߕߐ ߘߌ߫ ߞߊ߬ߥߊ߲߬ߘߌ߫ ( ߊ߬ ߟߊ߫ )",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 591,
      "verse_key": "87:11",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيَتَجَنَّبُهَا ٱلۡأَشۡقَى",
      "translation": "ߞߎ߬ߠߊ߲߬ߜߏ߬ߓߊ ߞߏ߬ߣߌ߲߬ ߘߴߊ߬ ߡߊߓߐ߫ ߊ߬ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 591,
      "verse_key": "87:12",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِي يَصۡلَى ٱلنَّارَ ٱلۡكُبۡرَىٰ",
      "translation": "ߏ߬ ߡߍ߲ ߘߏ߲߬ߕߐ߫ ߕߊ߫ ߓߟߋ߬ߓߟߋ ߘߐ߫",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 591,
      "verse_key": "87:13",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ لَا يَمُوتُ فِيهَا وَلَا يَحۡيَىٰ",
      "translation": "ߊ߬ ߕߍ߫ ߛߊ߬ ߦߋ߲߬ ߏ߬ ߞߐ߫ ߊ߬ ߘߏ߲߬ ߕߍ߫ ߢߣߊߡߦߊ߫",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 591,
      "verse_key": "87:14",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قَدۡ أَفۡلَحَ مَن تَزَكَّىٰ",
      "translation": "ߡߍ߲ ߓߊ߯ ߊ߬ ߖߘߍ߬ ߛߊߣߌ߲ߧߊ߫ ߏ߬ ߟߋ ߓߘߊ߫ ߛߎߘߊ߲߫",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 591,
      "verse_key": "87:15",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَذَكَرَ ٱسۡمَ رَبِّهِۦ فَصَلَّىٰ",
      "translation": "ߊ߬ ߣߌ߫ ߡߍ߲ ߞߵߊ߬ ߡߊ߰ߙߌ ߕߐ߮ ߝߐ߫ ߞߊ߬ ߛߊߟߌ߫",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 592,
      "verse_key": "87:16",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بَلۡ تُؤۡثِرُونَ ٱلۡحَيَوٰةَ ٱلدُّنۡيَا",
      "translation": "ߤߋߦߌ߬ ߸ ߊߟߎ߫ ߦߋ߫ ߘߎߢߊ߫ ߢߣߊߡߦߊ ߟߋ߬ ߛߎߥߊ߲ߘߌ߫ ߟߊ߫ ߓߊ߬؟",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 592,
      "verse_key": "87:17",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡأٓخِرَةُ خَيۡرٞ وَأَبۡقَىٰٓ",
      "translation": "ߟߊ߬ߓߊ߲ ( ߛߏ ) ߕߊ ߟߋ߬ ߘߏ߲߬ ߦߊ߯ ߝߌ߬ߛߊ߬ ߸ ߏ߬ ߟߋ ߦߋ߫ ߕߏ߫ ߟߴߊ߬ ߡߊ߬",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 592,
      "verse_key": "87:18",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ هَٰذَا لَفِي ٱلصُّحُفِ ٱلۡأُولَىٰ",
      "translation": "( ߞߎߡߊ ) ߣߌ߲߬ ߦߋ߫ ߞߝߊ߬ ߝߟߐߝߟߐ ߟߎ߬ ߞߣߐ߫",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 592,
      "verse_key": "87:19",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "صُحُفِ إِبۡرَٰهِيمَ وَمُوسَىٰ",
      "translation": "ߌߓߎ߬ߙߊ߬ߤߌߡߊ߫ ߟߊ߫ ߞߊ߬ߝߊ ߣߌ߫ ߡߎߛߊ߫ ߕߊ ߞߊ߬ߣߌ߲߫",
      "footnotes": ""
    }
  ]
}