{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 583,
      "verse_key": "79:1",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلنَّٰزِعَٰتِ غَرۡقٗا",
      "translation": "ߊ߰ ߒ ߣߌ߫ ߓߏ߬ߛߟߌ߬ߟߊ ߟߎ߬ ߕߍ߫ ߔߏߦߌ߫",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 583,
      "verse_key": "79:2",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلنَّٰشِطَٰتِ نَشۡطٗا",
      "translation": "ߊ߰ ߒ ߣߌ߫ ߣߌߘߐߘߌߦߟߊ ߟߎ߬ ߕߍ߫ ߝߊ߲ߛߊ߲߫",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 583,
      "verse_key": "79:3",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلسَّٰبِحَٰتِ سَبۡحٗا",
      "translation": "ߊ߰ ߒ ߣߌ߫ ߣߊߥߎ߲ߠߌ߲ߠߊ ߟߎ߬ ߕߍ߫ ߔߌߘߏ߲ߔߌߘߏ߲߫",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 583,
      "verse_key": "79:4",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَٱلسَّٰبِقَٰتِ سَبۡقٗا",
      "translation": "ߊ߰ ߒ ߣߌ߫ ߓߏ߬ߙߌ߬ߘߐ߬ߞߢߊ߬ߟߊ ߟߎ߬ ߕߍ߫ ߓߘߌߓߘߌ߫",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 583,
      "verse_key": "79:5",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَٱلۡمُدَبِّرَٰتِ أَمۡرٗا",
      "translation": "ߏ߬ ߞߐ߫ ߸ ߊ߰ ߒ ߣߌ߫ ߞߏߢߊ ( ߟߎ߫ ) ߝߙߍ߬ ߘߐ߬ߓߍ߲߬ߠߊ ߟߎ߬ ߕߍ߫",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 583,
      "verse_key": "79:6",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَوۡمَ تَرۡجُفُ ٱلرَّاجِفَةُ",
      "translation": "ߟߊ߬ߦߍ߬ߙߍ߲߬ߠߌ߲߬ߠߊ ߦߋ߫ ߟߊ߬ߦߍ߬ߙߍ߲߬ߠߌ߲ ߞߍ߫ ߟߊ߫ ߟߏ߲ ߡߍ߲ ( ߜߌߚߌ߫ )",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 583,
      "verse_key": "79:7",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تَتۡبَعُهَا ٱلرَّادِفَةُ",
      "translation": "ߛߊ߯ߘߊߟߌߟߊ ߘߴߏ߬ ߣߐ߬ߡߌ߬ߣߊ߬",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 583,
      "verse_key": "79:8",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قُلُوبٞ يَوۡمَئِذٖ وَاجِفَةٌ",
      "translation": "ߖߎ߬ߛߎ ߟߎ߬ ߦߋ߫ ߞߟߎ߫ ߟߴߏ߬ ߟߏ߲ ߠߋ߬ ( ߛߌߟߊ߲ ߡߊ߬ )",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 583,
      "verse_key": "79:9",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَبۡصَٰرُهَا خَٰشِعَةٞ",
      "translation": "ߊ߬ߟߎ߬ ߟߊ߫ ߦߋߟߌ ߟߎ߬ ߦߋ߫ ߞߌߛߐ߫",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 583,
      "verse_key": "79:10",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَقُولُونَ أَءِنَّا لَمَرۡدُودُونَ فِي ٱلۡحَافِرَةِ",
      "translation": "‹ ߟߊߥߟߌ߬ߞߏ ߡߊߛߐ߬ߛߐ߬ߓߊ߮ › ߟߎ߬ ߦߴߊ߬ ߝߐ ߞߊ߲߬ ߞߏ߫ ߊ߬ ߝߐ߫ ߊ߲ ߠߊߛߊ߬ߦߌ߲߬ߕߐ߫ ߟߋ߬ ߊ߲ ߢߊߞߘߐ ߡߊ߬ ߓߊ߬؟",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 583,
      "verse_key": "79:11",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَءِذَا كُنَّا عِظَٰمٗا نَّخِرَةٗ",
      "translation": "ߊ߲ ߞߍߣߍ߲ ߞߐ߫ ߞߟߏ߫ ߡߎ߰ߡߎ߰ߣߍ߲ ߠߎ߬ ߘߌ߫ ߞߊ߬ߣߌ߲߫؟",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 583,
      "verse_key": "79:12",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قَالُواْ تِلۡكَ إِذٗا كَرَّةٌ خَاسِرَةٞ",
      "translation": "ߞߴߏ߬ ߘߏ߲߬ ߦߋ߫ ߞߐߛߊߦߌ߲߫ ߜߊ߲ߛߊ߲߫ ߘߴߏ߬ ߘߐ߫ ߘߋ߬؟",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 583,
      "verse_key": "79:13",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِنَّمَا هِيَ زَجۡرَةٞ وَٰحِدَةٞ",
      "translation": "ߤߊ߫߸ ߏ߬ ߦߋ߫ ߟߊ߬ߜߎ߬ߝߎ߲߬ߠߌ߲߬ ߞߋߟߋ߲߫ ߔߋ߫ ߟߋ߬ ߘߌ߫ ߘߋ߬",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 583,
      "verse_key": "79:14",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِذَا هُم بِٱلسَّاهِرَةِ",
      "translation": "ߘߊ߬ߡߊ߲߬ߛߴߊ߬ߟߎ߬ ߓߍ߯ ߓߘߊ߫ ߛߋ߫ ߘߎ߰ߜߍ ߞߊ߲߬",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 583,
      "verse_key": "79:15",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "هَلۡ أَتَىٰكَ حَدِيثُ مُوسَىٰٓ",
      "translation": "ߦߊ߯ߟߊ߫ ߡߎߛߊߞߏ ߓߊ߬ߘߏ ߣߊ߬ ߘߴߌ ߡߊ߬ ߓߊ߬؟",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 584,
      "verse_key": "79:16",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذۡ نَادَىٰهُ رَبُّهُۥ بِٱلۡوَادِ ٱلۡمُقَدَّسِ طُوًى",
      "translation": "ߊ߬ ߡߊ߰ߙߌ ߞߵߊ߬ ߞߟߌ߫ ߕߏߥߊ߫ ߝߙߊ߬ ߛߊߣߌ߲ ߘߟߊ߫ ߕߎ߬ߡߊ ߡߍ߲",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 584,
      "verse_key": "79:17",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱذۡهَبۡ إِلَىٰ فِرۡعَوۡنَ إِنَّهُۥ طَغَىٰ",
      "translation": "( ߞߏ߫ ) ߕߊ߯ ߝߙߊߥߎ߲߫ ߕߘߍ߬ ߓߊ߫ ߓߊߏ߬ ߊ߬ ߓߘߊ߫ ߘߊ߲߬ߠߊߕߊ߬ߡߌ߲߫",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 584,
      "verse_key": "79:18",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَقُلۡ هَل لَّكَ إِلَىٰٓ أَن تَزَكَّىٰ",
      "translation": "ߌ ߦߴߊ߬ ߝߐ߫ ߞߴߌ ߦߴߊ߬ ߝߍ߬ ߞߊ߬ ߛߊߣߌ߲ߧߊ߫ ߓߊ߬؟",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 584,
      "verse_key": "79:19",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَهۡدِيَكَ إِلَىٰ رَبِّكَ فَتَخۡشَىٰ",
      "translation": "ߊ߬ ߣߌ߫ ߒ ߧߴߌ ߞߊ߲ߘߊ߫ ߌ ߡߊ߰ߙߌ ߡߵߊ߬ ߢߍߡߌߙߌ߲ ߦߋ߫ ߞߴߌ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 584,
      "verse_key": "79:20",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَرَىٰهُ ٱلۡأٓيَةَ ٱلۡكُبۡرَىٰ",
      "translation": "ߊ߬ ߞߊ߬ ߡߊ߲߬ߕߊ߬ߣߌ߲߬ ߓߟߋߓߟߋ ߦߌ߬ߘߴߊ߬ ߟߊ߫ ߏ߬ ߟߋ ߘߐ߫",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 584,
      "verse_key": "79:21",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَكَذَّبَ وَعَصَىٰ",
      "translation": "ߞߏ߬ߣߵߊ߬ ߞߵߊ߬ ߥߎߦߊߒߕߋߦߊ߫ ߞߵߊ߬ ߡߊߛߐ߬ߛߐ߬",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 584,
      "verse_key": "79:22",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ أَدۡبَرَ يَسۡعَىٰ",
      "translation": "ߏ߬ ߞߐ߫ ߸ ߞߵߊ߬ ߞߐߘߏ߲߬ ߕߊ߯ߡߊ ߘߌ߫ ߛߐߍ߫",
      "footnotes": ""
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 584,
      "verse_key": "79:23",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَحَشَرَ فَنَادَىٰ",
      "translation": "ߞߊ߬ ߟߊ߬ߘߍ ߞߍ߫ ߏ߬ ߘߐ߫ ߞߊ߬ ߞߟߋߞߟߋߡߊߦߟߍ ߓߍ߯ ߞߍ߫",
      "footnotes": ""
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 584,
      "verse_key": "79:24",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَقَالَ أَنَا۠ رَبُّكُمُ ٱلۡأَعۡلَىٰ",
      "translation": "ߞߵߊ߬ ߟߊߛߋ߫ ߏ߬ ߘߐ߫ ߞߏ߫ ߒߠߋ ߟߴߊߟߎ߫ ߡߊ߰ߙߌ߬ ߞߘߐߕߊ߬ߣߍ߲߬ߓߊ ߘߌ߫",
      "footnotes": ""
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 584,
      "verse_key": "79:25",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَخَذَهُ ٱللَّهُ نَكَالَ ٱلۡأٓخِرَةِ وَٱلۡأُولَىٰٓ",
      "translation": "ߊߟߊ߫ ߞߵߊ߬ ߕߍߕߊ߬ ߛߎ߬ߟߊ߫ ߣߌ߫ ߞߌߢߍ߲߫ ߓߌ߲߬ߓߊߏ ߟߊ߫ ߏ߬ ߟߋ ߘߐ߫",
      "footnotes": ""
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 584,
      "verse_key": "79:26",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ فِي ذَٰلِكَ لَعِبۡرَةٗ لِّمَن يَخۡشَىٰٓ",
      "translation": "ߤߊ߲߫ ߸ ߕߊ߰ߛߌ߬ߟߌ߫ ߟߴߏ߬ ߘߐ߫ ( ߊߟߊߞߏ ) ߢߍ߬ߣߍ߲߬ߕߐ ߦߋ߫ ߘߋ߬",
      "footnotes": ""
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 584,
      "verse_key": "79:27",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ءَأَنتُمۡ أَشَدُّ خَلۡقًا أَمِ ٱلسَّمَآءُۚ بَنَىٰهَا",
      "translation": "ߐ߲߬ߤߐ߲߫ ߸ ߊߟߎ ߟߋ߬ ߘߊ߲ ߞߊ߫ ߜߍ߬ߟߍ߲߫ ߓߊ߬ ߥߟߊ߫ ߛߊ߲ ߸ ߊ߬ ߘߏ߲߬ ߞߵߏ ߟߐ߬",
      "footnotes": ""
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 584,
      "verse_key": "79:28",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "رَفَعَ سَمۡكَهَا فَسَوَّىٰهَا",
      "translation": "ߞߵߏ߬ ߓߟߌ ߞߘߐߕߊ߬ ߞߵߊ߬ ߟߊߞߢߊ߫",
      "footnotes": ""
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 584,
      "verse_key": "79:29",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَغۡطَشَ لَيۡلَهَا وَأَخۡرَجَ ضُحَىٰهَا",
      "translation": "ߊ߬ ߣߴߊ߬ ߞߵߊ߬ ߛߎ ߘߌ߬ߓߌߘߏ߲߬ ߞߵߊ߬ ߛߎ߲߰ߕߟߋ ߞߣߍߦߊ ߟߊߓߐ߫",
      "footnotes": ""
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 584,
      "verse_key": "79:30",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡأَرۡضَ بَعۡدَ ذَٰلِكَ دَحَىٰهَآ",
      "translation": "ߊ߬ ߞߊ߬ ߘߎ߱ ߘߐߝߎߟߋ߲߫ ߏ߬ ߟߋ ߞߐ߫",
      "footnotes": ""
    },
    {
      "id": 31,
      "verse_number": 31,
      "page_number": 584,
      "verse_key": "79:31",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَخۡرَجَ مِنۡهَا مَآءَهَا وَمَرۡعَىٰهَا",
      "translation": "ߞߵߊ߬ ߖߌ ߟߊߓߐ߫ ߊ߬ ߞߣߐ߫ ߊ߬ ߣߴߊ߬ ߖߍߥߎߟߍ",
      "footnotes": ""
    },
    {
      "id": 32,
      "verse_number": 32,
      "page_number": 584,
      "verse_key": "79:32",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡجِبَالَ أَرۡسَىٰهَا",
      "translation": "ߊ߬ ߣߴߊ߬ ߞߊ߬ ߞߎ߬ߙߎ ߟߎ߬ ߕߘߎ߫",
      "footnotes": ""
    },
    {
      "id": 33,
      "verse_number": 33,
      "page_number": 584,
      "verse_key": "79:33",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَتَٰعٗا لَّكُمۡ وَلِأَنۡعَٰمِكُمۡ",
      "translation": "ߘߊ߰ߡߎ߲ ߘߌ߫ ߊߟߎ߫ ߣߴߊߟߎ߫ ߟߊ߫ ߓߌߦߊ߲ ߠߎ߬ ߦߋ߫",
      "footnotes": ""
    },
    {
      "id": 34,
      "verse_number": 34,
      "page_number": 584,
      "verse_key": "79:34",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِذَا جَآءَتِ ٱلطَّآمَّةُ ٱلۡكُبۡرَىٰ",
      "translation": "ߒ߬ߓߊ߬ ߣߌ߫ ߥߙߊ߬ߒ߬ߘߊ߬ߡߊ߫ ߓߟߋ߬ߓߟߋ ߣߊ߬ ߘߊ߫ ߕߎ߬ߡߊ ߡߍ߲",
      "footnotes": ""
    },
    {
      "id": 35,
      "verse_number": 35,
      "page_number": 584,
      "verse_key": "79:35",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَوۡمَ يَتَذَكَّرُ ٱلۡإِنسَٰنُ مَا سَعَىٰ",
      "translation": "ߡߐ߱ ߘߴߊ߬ ߦߟߌߓߌ߬ߟߴߊ߬ ߗߋߥߊߟߌ߫ ߞߍߣߍ߲ ߘߐ߫ ߏ߬ ߟߏ߲",
      "footnotes": ""
    },
    {
      "id": 36,
      "verse_number": 36,
      "page_number": 584,
      "verse_key": "79:36",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَبُرِّزَتِ ٱلۡجَحِيمُ لِمَن يَرَىٰ",
      "translation": "ߊ߬ ߣߌ߫ ߥߋ߲߯ߓߋ ߘߌ߫ ߟߊߓߐ߫ ߜߍ ߘߐ߫ ߦߋߟߌߟߊ ߢߊߞߘߐ߫",
      "footnotes": ""
    },
    {
      "id": 37,
      "verse_number": 37,
      "page_number": 584,
      "verse_key": "79:37",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَمَّا مَن طَغَىٰ",
      "translation": "ߒ߬ߓߊ߬ ߣߌ߫ ߡߍ߲ ߣߐ߬ ߞߍ߫ ߘߊ߫ ߘߊ߲߬ߠߊ߬ߕߊߡߌ߲ ߞߍ߫ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 38,
      "verse_number": 38,
      "page_number": 584,
      "verse_key": "79:38",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَءَاثَرَ ٱلۡحَيَوٰةَ ٱلدُّنۡيَا",
      "translation": "ߊ߬ ߣߴߊ߬ ߞߊ߬ ߘߎߢߊ߫ ߢߣߊߡߦߊ ߛߎߥߊ߲ߘߌ߫",
      "footnotes": ""
    },
    {
      "id": 39,
      "verse_number": 39,
      "page_number": 584,
      "verse_key": "79:39",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِنَّ ٱلۡجَحِيمَ هِيَ ٱلۡمَأۡوَىٰ",
      "translation": "ߥߋ߲߯ߓߋ ߟߴߏ߬ ߡߊߞߍ߫ ߦߌߟߊ ߘߌ߫ ߏ߬ ߘߐ߫ ߘߋ߬",
      "footnotes": ""
    },
    {
      "id": 40,
      "verse_number": 40,
      "page_number": 584,
      "verse_key": "79:40",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَمَّا مَنۡ خَافَ مَقَامَ رَبِّهِۦ وَنَهَى ٱلنَّفۡسَ عَنِ ٱلۡهَوَىٰ",
      "translation": "ߒ߬ߓߊ߬ ߣߌ߫ ߡߍ߲ ߛߌߟߊ߲ߣߍ߲߫ ߞߍ߫ ߘߴߊ߬ ߡߊ߰ߙߌ ߓߘߊ߫ ߟߐ ߢߍ߫ ߛߊ߫ ߸ ߊ߬ ߣߌ߫ ߞߵߊ߬ ߣߌ ߝߊߕߊ߲ߕߌ߫ ߣߋ߬ߦߌ ߟߎ߫ ߡߊ߬",
      "footnotes": ""
    },
    {
      "id": 41,
      "verse_number": 41,
      "page_number": 584,
      "verse_key": "79:41",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِنَّ ٱلۡجَنَّةَ هِيَ ٱلۡمَأۡوَىٰ",
      "translation": "ߛߋߚߋ߫ ߡߍ߲ߘߌߦߊ߫ ߟߴߏ߬ ߡߊߞߍ߫ ߦߌߟߊ ߘߌ߫",
      "footnotes": ""
    },
    {
      "id": 42,
      "verse_number": 42,
      "page_number": 584,
      "verse_key": "79:42",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَسۡـَٔلُونَكَ عَنِ ٱلسَّاعَةِ أَيَّانَ مُرۡسَىٰهَا",
      "translation": "ߊ߬ߟߎ߬ ߦߴߌߟߋ ߢߌ߬ߣߌ߲߬ߞߊ߬ ߟߊ߫ ( ߟߐ߬ߓߊ ) ߕߎ߬ߡߊ ߡߊ߬ ߸ ߞߴߏ߬ ߞߍߕߐ߫ ߕߎ߬ߡߊ ߢߌ߬ߡߊ߲߬؟",
      "footnotes": ""
    },
    {
      "id": 43,
      "verse_number": 43,
      "page_number": 584,
      "verse_key": "79:43",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِيمَ أَنتَ مِن ذِكۡرَىٰهَآ",
      "translation": "ߌߟߋ ߕߊ ߢߌ߬ߡߊ߲߫ ߠߴߏ߬ ߞߏߝߐ ߘߐ߫؟",
      "footnotes": ""
    },
    {
      "id": 44,
      "verse_number": 44,
      "page_number": 584,
      "verse_key": "79:44",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَىٰ رَبِّكَ مُنتَهَىٰهَآ",
      "translation": "ߏ߬ ߞߐߘߊ߲ ߦߋ߫ ߞߊߕߙߍ߬ ߌ ߡߊ߰ߙߌ ߟߋ߫ ߡߊ߬",
      "footnotes": ""
    },
    {
      "id": 45,
      "verse_number": 45,
      "page_number": 584,
      "verse_key": "79:45",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّمَآ أَنتَ مُنذِرُ مَن يَخۡشَىٰهَا",
      "translation": "ߊ߬ ߞߎ߬ߘߎ ߦߋ߫ ߡߍ߲ ߞߊ߲ߠߊ߫ ߌߟߋ ߦߋ߫ ߖߊ߬ߛߙߋ߬ߡߟߊ߬ߓߊ߮ ߘߴߏ߬ ߟߋ ߦߋ߫",
      "footnotes": ""
    },
    {
      "id": 46,
      "verse_number": 46,
      "page_number": 584,
      "verse_key": "79:46",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَأَنَّهُمۡ يَوۡمَ يَرَوۡنَهَا لَمۡ يَلۡبَثُوٓاْ إِلَّا عَشِيَّةً أَوۡ ضُحَىٰهَا",
      "translation": "ߊ߬ߟߎ߬ ߞߍߕߐ߫ ߊ߬ ߦߋ߫ ߟߊ߫ ߟߏ߲ ߡߍ߲ ߸ ߊ߬ߟߎ߬ ߘߌ߫ ߞߍ߫ ߦߏ߫ ߊ߬ߟߎ߬ ߡߊ߫ ߥߛߎ߬ ߛߌ߫ ߞߍ߫ ( ߘߎߢߊ߫ ߘߐ߫ ) ߝߏ߫ ߥߎ߬ߙߘߊ߬ߣߍ߲߬ ߞߋߟߋ߲߫ ߸ ߥߟߊ߫ ߏ߬ ߛߐ߰ߡߘߊ߬ߣߍ߲߫",
      "footnotes": ""
    }
  ]
}