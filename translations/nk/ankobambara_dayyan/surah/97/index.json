      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّآ أَنزَلۡنَٰهُ فِي لَيۡلَةِ ٱلۡقَدۡرِ",
      "translation": "ߒ߬ߠߎ߫ ߟߋ߫ ߞߵߊ߬ ߟߊߖߌ߰ ߟߊ߬ߕߍ߮ ߛߎ ߘߐ߫ ߘߋ߬",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 598,
      "verse_key": "97:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَآ أَدۡرَىٰكَ مَا لَيۡلَةُ ٱلۡقَدۡرِ",
      "translation": "ߡߋ߬ߘߋ ߞߵߌߟߋ ߟߊߟߐ߲ߠߌ߲߫ ߟߊ߬ߕߍ߮ ߛߎ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 598,
      "verse_key": "97:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَيۡلَةُ ٱلۡقَدۡرِ خَيۡرٞ مِّنۡ أَلۡفِ شَهۡرٖ",
      "translation": "ߟߊ߬ߕߍ߮ ߛߎ ߓߊ߬ ߸ ߏ߬ ߦߊ߯ ߝߌ߬ߛߊ߬ ߞߊߙߏ߫ ߥߊ߯ߞߋߟߋ߲߫ ߘߌ߫",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 598,
      "verse_key": "97:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تَنَزَّلُ ٱلۡمَلَٰٓئِكَةُ وَٱلرُّوحُ فِيهَا بِإِذۡنِ رَبِّهِم مِّن كُلِّ أَمۡرٖ",
      "translation": "ߞߋ߲ߛߐ߲ ߠߎ߬ ߣߌ߫ ߛߟߐߡߌ߲ ߦߋ߫ ߖߌ߰ ߟߴߊ߬ߟߋ ߟߋ߬ ߘߐ߫ ߊ߬ߟߎ߫ ߡߊ߰ߙߌ ߟߊ߫ ߘߌ߬ߢߍ ߟߊ߫ ߞߏߢߊ ߓߍ߯ ߘߐ߫",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 598,
      "verse_key": "97:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "سَلَٰمٌ هِيَ حَتَّىٰ مَطۡلَعِ ٱلۡفَجۡرِ",
      "translation": "ߖߐ ߟߴߏ߬ ߘߌ߫ ߝߎߚߎ߫ ߤߊ߲߯ ߞߣߍ ߖߌ߲ߞߌ߲߫ ߕߎߡߊ",
      "footnotes": ""
    }
  ]
}