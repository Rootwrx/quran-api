{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 601,
      "verse_key": "105:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَمۡ تَرَ كَيۡفَ فَعَلَ رَبُّكَ بِأَصۡحَٰبِ ٱلۡفِيلِ",
      "translation": "ߌ ߡߴߊ߬ ߦߋ߫ ߓߊ߬ ߌ ߡߊ߰ߙߌ ߞߊ߬ ߡߍ߲ ߞߍ߫ ߛߋ߲߬ߓߊ߬ ߕߌ߮ ߟߎ߬ ߟߊ߫؟",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 601,
      "verse_key": "105:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَمۡ يَجۡعَلۡ كَيۡدَهُمۡ فِي تَضۡلِيلٖ",
      "translation": "ߊ߬ ߡߴߊ߬ߟߎ߬ ߟߊ߫ ߝߍ߬ߙߍ ߢߡߊߞߍ߫ ߊ߬ߟߎ߫ ߡߊ߬ ߓߊ߬؟",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 601,
      "verse_key": "105:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَرۡسَلَ عَلَيۡهِمۡ طَيۡرًا أَبَابِيلَ",
      "translation": "ߊ߬ ߣߴߊ߬ ߞߊ߬ ߞߣߐ߬ ߓߌ߮ ߗߋ߫ ߊ߬ߟߎ߬ ߞߊߡߊ߬",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 601,
      "verse_key": "105:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تَرۡمِيهِم بِحِجَارَةٖ مِّن سِجِّيلٖ",
      "translation": "ߏ߬ ߟߎ߫ ߞߵߊ߬ߟߎ߬ ߡߊߓߏ߲ߓߏ߲߫ ߣߊ߰ߘߌ߬ߝߘߊ߬ ߓߙߍ ߟߎ߬ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 601,
      "verse_key": "105:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَجَعَلَهُمۡ كَعَصۡفٖ مَّأۡكُولِۭ",
      "translation": "ߞߵߊ߬ߟߎ߬ ߞߍ߫ ߦߏ߫ ߢߊ߯ߡߊ-ߞߟߊ߫ ߢߌߡߌ߲ߣߍ߲ ߠߋ߬",
      "footnotes": ""
    }
  ]
}