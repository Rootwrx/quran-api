{
  "result": [
    {
      "id": "5552",
      "sura": "75",
      "aya": "1",
      "arabic_text": "لَآ أُقۡسِمُ بِيَوۡمِ ٱلۡقِيَٰمَةِ",
      "translation": "ߒ ߧߋ߫ ߒ ߞߊ߬ߟߌ߬ ߟߊ߫ ߟߐ߬ߓߊ ߟߏ߲ ߠߊ߫ ߟߋ߬",
      "footnotes": ""
    },
    {
      "id": "5553",
      "sura": "75",
      "aya": "2",
      "arabic_text": "وَلَآ أُقۡسِمُ بِٱلنَّفۡسِ ٱللَّوَّامَةِ",
      "translation": "ߊ߬ ߣߌ߫ ߒ ߧߋ߫ ߒ ߞߊ߬ߟߌ߬ ߟߊ߫ ߣߌ߫ ߖߙߊ߬ߞߟߌ߬ߟߊ߬ߓߊ ߟߊ߫ ߟߋ߬",
      "footnotes": ""
    },
    {
      "id": "5554",
      "sura": "75",
      "aya": "3",
      "arabic_text": "أَيَحۡسَبُ ٱلۡإِنسَٰنُ أَلَّن نَّجۡمَعَ عِظَامَهُۥ",
      "translation": "ߤߋ߯ߋ߰ ߡߐ߱ ߦߴߊ߬ ߓߛߌ߬ߞߌ߬ ߟߊ߫ ߟߋ߫ ߓߊ߬ ߞߏ߫ ߒ߬ ߕߍߣߵߊ߬ ߞߟߏ ߟߎ߬ ߟߊߘߍ߬ ߟߊ߫؟",
      "footnotes": ""
    },
    {
      "id": "5555",
      "sura": "75",
      "aya": "4",
      "arabic_text": "بَلَىٰ قَٰدِرِينَ عَلَىٰٓ أَن نُّسَوِّيَ بَنَانَهُۥ",
      "translation": "ߔߙߏ߫ ߸ ߒ߬ߠߎ߫ ߖߍ߬ߘߍ ߛߋߣߍ߲߫ ߓߍ߫ ߊ߬ ߡߍ߲ߛߍ߲ ߠߎ߬ ߟߊߞߢߊ߫ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": "5556",
      "sura": "75",
      "aya": "5",
      "arabic_text": "بَلۡ يُرِيدُ ٱلۡإِنسَٰنُ لِيَفۡجُرَ أَمَامَهُۥ",
      "translation": "ߋ߬ߋ߬ߋ߯ ߸ ߡߐ߱ ߢߝߍߟߊ ߟߋ߬ ߕߌߢߍ߫ ߟߐ߮ ߦߴߊ߬ ߟߊ߫ ( ߟߐ߬ߓߊ߬ߞߏ ߡߊߛߐ߬ߛߐ ߘߌ߫ ) ߓߏ߬",
      "footnotes": ""
    },
    {
      "id": "5557",
      "sura": "75",
      "aya": "6",
      "arabic_text": "يَسۡـَٔلُ أَيَّانَ يَوۡمُ ٱلۡقِيَٰمَةِ",
      "translation": "ߊ߰ ߢߌ߬ߣߌ߲߬ߞߊ߬ߟߌ ߞߍ߫ ߟߊ߫ ( ߏ߬ ߟߋ ߘߐ߫ ) ߞߏ߫ ߟߐ߬ߓߊ ߦߋ߫ ߕߎ߬ߡߊ ߢߌ߬ߡߊ߲߬ ߘߌ߫؟",
      "footnotes": ""
    },
    {
      "id": "5558",
      "sura": "75",
      "aya": "7",
      "arabic_text": "فَإِذَا بَرِقَ ٱلۡبَصَرُ",
      "translation": "ߒ߬ߓߊ߬ ߣߌ߫ ߢߊ ߟߎ߬ ߘߐߡߊ߰ߡߊ߰ ߘߊ߫ ( ߛߌߟߊ߲ ߓߟߏ߫ ߕߎ߬ߡߊ ߡߍ߲ )",
      "footnotes": ""
    },
    {
      "id": "5559",
      "sura": "75",
      "aya": "8",
      "arabic_text": "وَخَسَفَ ٱلۡقَمَرُ",
      "translation": "ߞߊ߬ ߞߊߙߏ ( ߢߊ ) ߓߎߘߎ߲ߛߊ߫",
      "footnotes": ""
    },
    {
      "id": "5560",
      "sura": "75",
      "aya": "9",
      "arabic_text": "وَجُمِعَ ٱلشَّمۡسُ وَٱلۡقَمَرُ",
      "translation": "ߞߊ߬ ߕߋ߬ߟߋ ߣߌ߫ ߞߊߙߏ ߟߊߘߍ߬ ( ߓߐ߫ ߦߙߐ߫ ߞߋߟߋ߲߫ ߠߊ߫ )",
      "footnotes": ""
    },
    {
      "id": "5561",
      "sura": "75",
      "aya": "10",
      "arabic_text": "يَقُولُ ٱلۡإِنسَٰنُ يَوۡمَئِذٍ أَيۡنَ ٱلۡمَفَرُّ",
      "translation": "ߡߐ߱ ߘߴߊ߬ ߝߐ߫ ߏ߬ ߟߏ߲ ߞߏ߫ ߓߏ߬ߙߌ߬ ߦߙߐ ߦߋ߫ ߡߌ߲߫؟",
      "footnotes": ""
    },
    {
      "id": "5562",
      "sura": "75",
      "aya": "11",
      "arabic_text": "كَلَّا لَا وَزَرَ",
      "translation": "ߍ߲߬ߍ߲߫ ߸ ߞߎ߲߬ߞߊ߬ߙߌ߬ߝߊ߬ ߦߙߐ߫ ߖߘߍ߬ ߕߍ߫ ( ߡߍ߲ ߘߌ߫ ߡߊߜߍ߲߫ )",
      "footnotes": ""
    },
    {
      "id": "5563",
      "sura": "75",
      "aya": "12",
      "arabic_text": "إِلَىٰ رَبِّكَ يَوۡمَئِذٍ ٱلۡمُسۡتَقَرُّ",
      "translation": "ߡߊ߬ߞߍ߬ ߦߌߟߊ ߦߴߌ ߡߊ߰ߙߌ ( ߘߐߙߐ߲߫ ) ߠߋ߫ ߕߍ߬ߟߊ ߘߐ߫ ߏ߬ ߟߏ߲",
      "footnotes": ""
    },
    {
      "id": "5564",
      "sura": "75",
      "aya": "13",
      "arabic_text": "يُنَبَّؤُاْ ٱلۡإِنسَٰنُ يَوۡمَئِذِۭ بِمَا قَدَّمَ وَأَخَّرَ",
      "translation": "ߡߐ߱ ߗߋߥߊߟߌ ߢߍߕߊ ߣߴߊ߬ ߞߐߟߕߊ ߟߎ߬ ߘߌ߫ ߟߊߓߐ߫ ߊ߬ ߡߊ߬ ߏ߬ ߟߏ߲",
      "footnotes": ""
    },
    {
      "id": "5565",
      "sura": "75",
      "aya": "14",
      "arabic_text": "بَلِ ٱلۡإِنسَٰنُ عَلَىٰ نَفۡسِهِۦ بَصِيرَةٞ",
      "translation": "ߍ߲߬ߍ߲߫ ( ߤߊߟߌ߬ ߣߴߏ߬ ߕߍ߫ ) ߡߐ߱ ߦߴߊ߬ ߖߘߍ߬ ߟߊߛߊ߬ߓߊ߯ ߟߋ߬ ߘߌ߫ ( ߊ߬ ߞߍߕߊ ߟߎ߬ ߘߐ߫ )",
      "footnotes": ""
    },
    {
      "id": "5566",
      "sura": "75",
      "aya": "15",
      "arabic_text": "وَلَوۡ أَلۡقَىٰ مَعَاذِيرَهُۥ",
      "translation": "ߤߟߴߊ߬ ߞߵߊ߬ ߟߐ߬ߦߙߌ ߟߎ߬ ߝߏ߬ߦߌ߫ ߟߋ߬",
      "footnotes": ""
    },
    {
      "id": "5567",
      "sura": "75",
      "aya": "16",
      "arabic_text": "لَا تُحَرِّكۡ بِهِۦ لِسَانَكَ لِتَعۡجَلَ بِهِۦٓ",
      "translation": "ߌ ߞߴߌ ߣߍ߲߭ ߠߊߓߙߊ߬ߞߊ߬ߓߙߊߞߊ߫ ‹ ߘߐ߬ߛߊߙߌ › ߟߊ߫ ߞߌߢߍ ߡߵߊ߬ ߟߊ߫ ߘߋ߬",
      "footnotes": ""
    },
    {
      "id": "5568",
      "sura": "75",
      "aya": "17",
      "arabic_text": "إِنَّ عَلَيۡنَا جَمۡعَهُۥ وَقُرۡءَانَهُۥ",
      "translation": "ߓߊߏ߬ ߊ߬ ߟߊߘߍ߭ ߒ߬ߠߎ߫ ߟߋ߫ ߡߵߊ߬ ߣߴߊ߬ ߘߐߛߊߙߌ ߓߍ߯",
      "footnotes": ""
    },
    {
      "id": "5569",
      "sura": "75",
      "aya": "18",
      "arabic_text": "فَإِذَا قَرَأۡنَٰهُ فَٱتَّبِعۡ قُرۡءَانَهُۥ",
      "translation": "ߒ߬ߓߵߊ߲ ߓߊ߯ ߞߍ߫ ߊ߬ ߝߐ߫ ߟߊ ( ߌ ߦߋ߫ ) ߌ ߦߴߊ߬ ߝߐߟߌ ߣߐ߬ߡߌ߬ߣߊ߬ ( ߒ߬ ߞߐ߫ )",
      "footnotes": ""
    },
    {
      "id": "5570",
      "sura": "75",
      "aya": "19",
      "arabic_text": "ثُمَّ إِنَّ عَلَيۡنَا بَيَانَهُۥ",
      "translation": "ߊ߬ ߘߐߞߣߍ ( ߌ ߡߊ߬߸ ߏ߬ ߦߋ߫ ) ߒ߬ߠߎ߫ ߟߋ߫ ߡߊ߬ ߏ߬ ߞߐ߫ ߛߊ߫",
      "footnotes": ""
    },
    {
      "id": "5571",
      "sura": "75",
      "aya": "20",
      "arabic_text": "كَلَّا بَلۡ تُحِبُّونَ ٱلۡعَاجِلَةَ",
      "translation": "ߋ߬ߋ߬ߋ߯߹ ߊߟߎ߫ ߦߋ߫ ߞߌߢߍ߲߫ ߠߋ߬ ߘߐߙߐ߲߫ ߝߍ߬ ߞߟߋ߫",
      "footnotes": ""
    },
    {
      "id": "5572",
      "sura": "75",
      "aya": "21",
      "arabic_text": "وَتَذَرُونَ ٱلۡأٓخِرَةَ",
      "translation": "ߞߊ߬ ߞߐߟߊ߫ ߕߏ߫ ߦߋ߲߬ ( ߣߊߟߌ߬ߡߎ߲߫ ߛߎ߬ߟߊ߫ ߘߌ߫ )",
      "footnotes": ""
    },
    {
      "id": "5573",
      "sura": "75",
      "aya": "22",
      "arabic_text": "وُجُوهٞ يَوۡمَئِذٖ نَّاضِرَةٌ",
      "translation": "ߢߘߊ ߘߏ߫ ߟߎ߫ ߡߊߣߍߡߊߣߍ߲߫ ߏ߬ ߟߏ߲",
      "footnotes": ""
    },
    {
      "id": "5574",
      "sura": "75",
      "aya": "23",
      "arabic_text": "إِلَىٰ رَبِّهَا نَاظِرَةٞ",
      "translation": "ߊ߬ߟߎ߬ ߦߴߊ߬ߟߎ߫ ߡߊ߰ߙߌ ߡߊߝߟߍ ߞߊ߲߬",
      "footnotes": ""
    },
    {
      "id": "5575",
      "sura": "75",
      "aya": "24",
      "arabic_text": "وَوُجُوهٞ يَوۡمَئِذِۭ بَاسِرَةٞ",
      "translation": "ߢߘߊ ߘߏ߫ ߟߎ߫ ߝߣߊ߫ ߛߌ߲߬ߛߌ߲߬ߣߍ߲߫ ߏ߬ ߟߏ߲",
      "footnotes": ""
    },
    {
      "id": "5576",
      "sura": "75",
      "aya": "25",
      "arabic_text": "تَظُنُّ أَن يُفۡعَلَ بِهَا فَاقِرَةٞ",
      "translation": "ߏ߬ ߟߎ߬ ߦߴߊ߬ ߖߌ߰ ߟߊ߫ ߞߏ߫ ߞߐߣߍ߯ߙߋ߲߫ ߘߖߐߞߏ ߟߋ߬ ߞߍߕߐ߫ ߊ߬ߟߎ߬ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": "5577",
      "sura": "75",
      "aya": "26",
      "arabic_text": "كَلَّآ إِذَا بَلَغَتِ ٱلتَّرَاقِيَ",
      "translation": "ߤߊ߯ߟߌ߫ ߸ ߣߌ߫ ߣߌ ߛߋ߫ ߘߊ߫ ߞߊ߲ߥߐߙߐ߲ߞߐ߲ߕߐ߲ ߠߎ߬ ߘߐ߫ ߕߎ߬ߡߊ ߡߍ߲",
      "footnotes": ""
    },
    {
      "id": "5578",
      "sura": "75",
      "aya": "27",
      "arabic_text": "وَقِيلَ مَنۡۜ رَاقٖ",
      "translation": "ߘߏ߫ ߦߴߊ߬ ߝߐ߫ ( ߞߎ߲߬ߠߊ߬ߛߌ߰ߓߊ߮ ߕߐ߬ߡߊ ߟߎ߬ ߦߋ߫ ) ߞߏ߫ ߖߐ߲߫ ߘߴߊ߬ ߓߙߐ߫؟",
      "footnotes": ""
    },
    {
      "id": "5579",
      "sura": "75",
      "aya": "28",
      "arabic_text": "وَظَنَّ أَنَّهُ ٱلۡفِرَاقُ",
      "translation": "ߊ߬ ߓߘߴߊ߬ ߖߌ߰ ߛߊ߫ ߸ ߞߏ߫ ߝߘߊߟߌ ߟߋ߬ ( ߛߋߣߍ߲߫ )",
      "footnotes": ""
    },
    {
      "id": "5580",
      "sura": "75",
      "aya": "29",
      "arabic_text": "وَٱلۡتَفَّتِ ٱلسَّاقُ بِٱلسَّاقِ",
      "translation": "ߘߊ߬ߡߊ߲߬ߛߊ߫ ߜߍ߬ߟߍ߲ ߓߘߴߊ߬ ߡߙߐߡߙߐ߫ ߢߐ߲߮ ߡߊ߬",
      "footnotes": ""
    },
    {
      "id": "5581",
      "sura": "75",
      "aya": "30",
      "arabic_text": "إِلَىٰ رَبِّكَ يَوۡمَئِذٍ ٱلۡمَسَاقُ",
      "translation": "ߓߍ߯ ߜߍ߲߬ߜߍ߲߬ߕߐ߫ ߏ߬ ߟߏ߲ ߠߋ߬ ߞߊߕߙߍ߬ ߌ ߡߊ߰ߙߌ ߟߋ߫ ߡߊ߬",
      "footnotes": ""
    },
    {
      "id": "5582",
      "sura": "75",
      "aya": "31",
      "arabic_text": "فَلَا صَدَّقَ وَلَا صَلَّىٰ",
      "translation": "ߊ߬ߟߋ ߘߏ߲߬ ߡߊ߫ ߟߊ߫ ( ߞߋߟߊ ߣߌ߫ ߘߐ߬ߛߊ߬ߙߌ߬ ) ߞߏ߫ ߟߊ߫ ߸ ߊ߬ ߖߘߍ߬ ߡߊ߫ ߛߊߟߌ߫ ߟߋ߬",
      "footnotes": ""
    },
    {
      "id": "5583",
      "sura": "75",
      "aya": "32",
      "arabic_text": "وَلَٰكِن كَذَّبَ وَتَوَلَّىٰ",
      "translation": "ߞߏ߬ߣߵߊ߬ ߞߊ߬ ߡߊ߬ߛߐ߬ߛߐ߬ߟߌ ߞߍ߫ ߊ߬ ߣߌ߫ ߞߵߊ߬ ߞߐߘߏ߲߬",
      "footnotes": ""
    },
    {
      "id": "5584",
      "sura": "75",
      "aya": "33",
      "arabic_text": "ثُمَّ ذَهَبَ إِلَىٰٓ أَهۡلِهِۦ يَتَمَطَّىٰٓ",
      "translation": "ߊ߬ ߓߊ߲߫ ߘߊ߫ ߞߊ߬ ߥߴߊ߬ ߘߏ߲߬ߞߏ ߡߊ߬ ߊ߰ ߕߊ߬ߡߌ߲߬ ߠߴߊ߬ ߢߊ߲߬ߞߊ߲߬ ߠߊ߫",
      "footnotes": ""
    },
    {
      "id": "5585",
      "sura": "75",
      "aya": "34",
      "arabic_text": "أَوۡلَىٰ لَكَ فَأَوۡلَىٰ",
      "translation": "ߌ ߕߏ ߊ߬ ߘߐ߫ ߏ߬ ߞߊ߫ ߞߊ߲߫ ߞߵߌ ߕߏ߫ ߊ߬ ߘߐ߫ ߞߊ߬ߣߌ߲߫",
      "footnotes": ""
    },
    {
      "id": "5586",
      "sura": "75",
      "aya": "35",
      "arabic_text": "ثُمَّ أَوۡلَىٰ لَكَ فَأَوۡلَىٰٓ",
      "translation": "ߏ߬ ߞߐ߫ ߝߣߊ߫ ߸ ߌ ߕߏ ߊ߬ ߘߐ߫ ߏ߬ ߞߊ߫ ߞߊ߲߫ ߞߵߌ ߕߏ߫ ߊ߬ ߘߐ߫ ߞߊ߬ߣߌ߲߫",
      "footnotes": ""
    },
    {
      "id": "5587",
      "sura": "75",
      "aya": "36",
      "arabic_text": "أَيَحۡسَبُ ٱلۡإِنسَٰنُ أَن يُتۡرَكَ سُدًى",
      "translation": "ߊ߰ ߡߐ߱ ߢߣߊ߫ ߟߋ߫ ߓߊ߬ ߞߏ߫ ߞߵߊ߬ ߕߏ߫ ߊ߬ ߖߍ߬ߘߍߡߊ߬ ߕߋ߲߬ ߝߎ߫؟",
      "footnotes": ""
    },
    {
      "id": "5588",
      "sura": "75",
      "aya": "37",
      "arabic_text": "أَلَمۡ يَكُ نُطۡفَةٗ مِّن مَّنِيّٖ يُمۡنَىٰ",
      "translation": "ߊ߬ ߝߐ߫ ߊ߬ߟߋ߫ ߕߎ߲߬ ߕߍ߫ ߖߌ߫ ߔߌߦߐߔߌߦߐ ߘߌ߫ ߛߖߌ߫ ߟߊߓߐ߲߬ߕߊ ߘߐ߫ ߘߍ߫؟",
      "footnotes": ""
    },
    {
      "id": "5589",
      "sura": "75",
      "aya": "38",
      "arabic_text": "ثُمَّ كَانَ عَلَقَةٗ فَخَلَقَ فَسَوَّىٰ",
      "translation": "ߞߊ߬ ߞߍ߫ ߖߋ߬ߟߌ߬ ߛߌ߲ߓߌ ߘߴߏ߬ ߞߐ߫ ߸ ( ߊߟߊ߫ ) ߞߵߏ߬ ߟߋ ߘߊ߲߫ ߞߵߊ߬ ߟߊߞߢߊ߫",
      "footnotes": ""
    },
    {
      "id": "5590",
      "sura": "75",
      "aya": "39",
      "arabic_text": "فَجَعَلَ مِنۡهُ ٱلزَّوۡجَيۡنِ ٱلذَّكَرَ وَٱلۡأُنثَىٰٓ",
      "translation": "ߊ߬ ߣߊ߬ ߘߊ߫ ߝߌ߬ߟߊ߬ߓߊ ߞߴߏ߬ ߘߐ߫ ߸ ߗߍ߬ߡߊ߲ ߣߌ߫ ߡߛߏ߬ߡߊ߲ ߞߊ߬ߣߌ߲߫",
      "footnotes": ""
    },
    {
      "id": "5591",
      "sura": "75",
      "aya": "40",
      "arabic_text": "أَلَيۡسَ ذَٰلِكَ بِقَٰدِرٍ عَلَىٰٓ أَن يُحۡـِۧيَ ٱلۡمَوۡتَىٰ",
      "translation": "ߦߊ߯ߟߊ߫ ߏ߬ ( ߟߎ߬ ) ߞߍߓߊ߮ ߕߴߛߋ߫ ߛߎ߭ ߟߎ߬ ߟߊߞߎߣߎ߲߫ ߠߊ߫ ߓߊ߬؟",
      "footnotes": ""
    }
  ]
}