{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 526,
      "verse_key": "53:1",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلنَّجۡمِ إِذَا هَوَىٰ",
      "translation": "ߊ߰ ߒ ߣߌ߫ ߟߏ߬ߟߏ ߕߍ߫ ߸ ߣߴߊ߬ ߓߋ߬ ߘߊ߫",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 526,
      "verse_key": "53:2",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَا ضَلَّ صَاحِبُكُمۡ وَمَا غَوَىٰ",
      "translation": "ߊߟߎ߫ ߘߍ߬ߢߐ߲߮ ߡߊ߫ ߝߟߌ߬ ߸ ߊ߬ ߡߊ߫ ߝߋ߬ߟߋ߲߬ߝߋߟߋ߲߫",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 526,
      "verse_key": "53:3",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا يَنطِقُ عَنِ ٱلۡهَوَىٰٓ",
      "translation": "ߊ߬ ߘߏ߲߬ ߕߍ߫ ߞߎߡߊ߫ ߟߴߊ߬ ߞߎ߲߬ߝߍ߬",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 526,
      "verse_key": "53:4",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنۡ هُوَ إِلَّا وَحۡيٞ يُوحَىٰ",
      "translation": "ߝߏߦߌ߬ ߕߴߏ߬ ߘߌ߫ ߝߏ߫ ߦߟߌߘߏ߲ߞߊ߲߫ ߗߋߣߍ߲",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 526,
      "verse_key": "53:5",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلَّمَهُۥ شَدِيدُ ٱلۡقُوَىٰ",
      "translation": "ߛߍ߲ߓߍ߫ ߜߍߟߍ߲ ߠߋ߫ ߞߵߊ߬ ߟߊߓߌ߬ߟߴߊ߬ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 526,
      "verse_key": "53:6",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ذُو مِرَّةٖ فَٱسۡتَوَىٰ",
      "translation": "ߏ߬ ߦߋ߫ ߦߋߞߏ߫ ߢߌߡߊߕߌ߯ ߟߋ߬ ߘߌ߫ ߞߵߊ߬ ߟߊߞߢߊ߫",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 526,
      "verse_key": "53:7",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَهُوَ بِٱلۡأُفُقِ ٱلۡأَعۡلَىٰ",
      "translation": "ߏ ߕߎ߬ߡߊ ߊ߬ ߦߋ߫ ߝߏ߬ߣߊ߲߬ ߞߘߐߕߊ߬ߣߍ߲ ߘߐ߫ ߔߏ߲߫",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 526,
      "verse_key": "53:8",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ دَنَا فَتَدَلَّىٰ",
      "translation": "ߏ߬ ߞߐ߫ ߊ߬ ߞߵߊ߬ ߡߊߘߏ߲߬ ߸ ߞߊ߬ ߓߊ߲߫ ߞߵߊ߬ ߝߊ߲ߡߊߖߌ߰",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 526,
      "verse_key": "53:9",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَكَانَ قَابَ قَوۡسَيۡنِ أَوۡ أَدۡنَىٰ",
      "translation": "ߝߴߏ߬ ߞߍ߫ ߘߊ߫ ߣߐ߲߬ߞߐ߲ ߢߊ߫ ߝߌ߬ߟߊ߫ ߖߊ߬ߕߋ߬ ߘߌ߫ ( ߝߏ߬ߣߊ߲ ߘߐ߫ ) ߥߟߴߊ߬ ߦߋ߫ ߛߎ߬ߘߎ߲߬ߧߊ߫ ߏ߬ ߘߌ߫",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 526,
      "verse_key": "53:10",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَوۡحَىٰٓ إِلَىٰ عَبۡدِهِۦ مَآ أَوۡحَىٰ",
      "translation": "ߊ߬ ߞߊ߬ ߦߟߌߘߏ߲ ߟߐ߬ ߊ߬ ߟߊ߫ ߖߐ߲߭ ߡߊ߬ ߸ ߊ߬ ߟߊ߫ ߦߟߌߘߏ߲ߞߊ߲ ߠߊ߫",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 526,
      "verse_key": "53:11",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَا كَذَبَ ٱلۡفُؤَادُ مَا رَأَىٰٓ",
      "translation": "( ߊ߬ ) ߛߣߐ߬ߡߍ ߡߊ߫ ߝߘߏ߬ ߝߋ߲ ߡߊ߬ ߝߋߎ߫ ߊ߬ ߞߊ߬ ߡߍ߲ ߦߋ߫.",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 526,
      "verse_key": "53:12",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَتُمَٰرُونَهُۥ عَلَىٰ مَا يَرَىٰ",
      "translation": "ߌ߬ߦߏ߯ ߊߟߎ ߘߴߊ߬ ߘߊߘߐߖߊ߬ ߝߋ߲߫ ߡߵߊ߬ ߞߊ߬ ߡߍ߲ ߦߋ߫؟",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 526,
      "verse_key": "53:13",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَقَدۡ رَءَاهُ نَزۡلَةً أُخۡرَىٰ",
      "translation": "ߊ߬ ߘߏ߲߬ ߞߵߊ߬ ߦߋ߫ ߟߋ߬ ߖߌ߰ߟߌ߬ ߜߘߍ߫ ߘߐ߫",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 526,
      "verse_key": "53:14",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عِندَ سِدۡرَةِ ٱلۡمُنتَهَىٰ",
      "translation": "ߘߊ߲߭ ߕߡߐߙߐ߲ߛߎ߲ ߦߙߐ",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 526,
      "verse_key": "53:15",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عِندَهَا جَنَّةُ ٱلۡمَأۡوَىٰٓ",
      "translation": "ߡߊ߬ߞߍ ߣߊߞߐ ߦߴߏ߬ ߦߙߐ ߟߋ߬ ߘߐ߫",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 526,
      "verse_key": "53:16",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذۡ يَغۡشَى ٱلسِّدۡرَةَ مَا يَغۡشَىٰ",
      "translation": "ߕߎ߬ߡߊ ߡߍ߲ ߝߋ߲ ߘߏ߫ ߦߋ߫ ߕߡߐߙߐ߲ߛߎ߲ ߞߎ߲߬ߠߊ߬ߓߌߘߌ߲߫ ߠߊ߫ ߡߍ߲ ߦߋ߫ ߓߌߘߌ߲߫ ߠߴߊ߬ ߞߎ߲߬ߠߊ߫",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 526,
      "verse_key": "53:17",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَا زَاغَ ٱلۡبَصَرُ وَمَا طَغَىٰ",
      "translation": "ߊ߬ ߢߊ ߡߊ߫ ߖߍ߲߬ߞߍ߫ ( ߊ߬ ߦߋߞߏ ߡߊ߬ ) ߸ ߊ߬ ߘߏ߲߬ ߡߊ߫ ߝߘߏ߬ ( ߖߌ߬ߦߊ߬ߥߟߌ߫ ߓߟߏ߫ )",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 526,
      "verse_key": "53:18",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَقَدۡ رَأَىٰ مِنۡ ءَايَٰتِ رَبِّهِ ٱلۡكُبۡرَىٰٓ",
      "translation": "ߊ߬ ߞߵߊ߬ ߡߊ߰ߙߌ ߟߊ߫ ߡߊ߲߬ߕߊ߬ߣߌ߲߬ߓߊ ߟߎ߬ ߘߏ߫ ߟߎ߫ ߦߋ߫ ߟߋ߬ ߛߎߋ߫",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 526,
      "verse_key": "53:19",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَرَءَيۡتُمُ ٱللَّٰتَ وَٱلۡعُزَّىٰ",
      "translation": "ߒ߬ߓߵߊߟߎ߫ ߓߘߊ߫ ߟߊ߯ߕߊ߫ ߣߌ߫ ߊ߬ߟߌ߬ߺߊ߳ߎߖ߭ߊ߫ ߦߋ߫ ߓߊ߬؟",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 526,
      "verse_key": "53:20",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَنَوٰةَ ٱلثَّالِثَةَ ٱلۡأُخۡرَىٰٓ",
      "translation": "ߊ߬ ߣߴߊ߬ߟߎ߬ ߛߓߊߣߊ߲߫ ߜߘߍ ߡߊ߬ߣߊ߯ߕߊ߫",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 526,
      "verse_key": "53:21",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَكُمُ ٱلذَّكَرُ وَلَهُ ٱلۡأُنثَىٰ",
      "translation": "ߊ߬ ߝߐ߫ ߊߟߎ ߕߊ ߟߋ߬ ߗߍ߬ߡߊ߲ ߘߌ߫ ߸ ߞߊ߬ ߡߛߏ߬ߡߊ߲ ߞߍ߫ ߊ߬ߟߋ߬ ߕߊ ߘߌ߫؟",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 526,
      "verse_key": "53:22",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تِلۡكَ إِذٗا قِسۡمَةٞ ضِيزَىٰٓ",
      "translation": "ߏ ߕߎ߬ߡߊ ߝߍ߬ ߕߐ߬ߢߍ߰ ߕߟߊߟߌ ߟߴߏ߬ ߘߌ߫",
      "footnotes": ""
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 526,
      "verse_key": "53:23",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنۡ هِيَ إِلَّآ أَسۡمَآءٞ سَمَّيۡتُمُوهَآ أَنتُمۡ وَءَابَآؤُكُم مَّآ أَنزَلَ ٱللَّهُ بِهَا مِن سُلۡطَٰنٍۚ إِن يَتَّبِعُونَ إِلَّا ٱلظَّنَّ وَمَا تَهۡوَى ٱلۡأَنفُسُۖ وَلَقَدۡ جَآءَهُم مِّن رَّبِّهِمُ ٱلۡهُدَىٰٓ",
      "translation": "ߏ߬ ߟߎ߬ ߕߍ߫ ߝߏߦߌ߬ ߘߌ߫ ߝߏ߫ ߕߐ߯ ߟߎ߫ ߊߟߎ߫ ߣߴߊߟߎ߫ ߝߊ߬ ߟߎ߫ ߞߊ߬ ߡߍ߲ ߠߎ߬ ߕߐ߯ߟߊ߫ ߸ ߊߟߊ߫ ߡߊ߫ ߞߎ߲߬ ߛߌ߫ ߟߊߖߌ߰ ߊ߬ߟߎ߬ ߟߊ߫ ߊ߬ߟߎ߫ ߘߏ߲߬ ߕߍ߫ ߝߏߦߌ߬ ߟߊߓߊ߬ߕߏ߬ ߟߊ߫ ߝߏ߫ ߖߌ߰ߟߌ ߣߌ߫ ߣߌ ߟߎ߫ ߛߊ߬ߥߏ ߸ ߞߊ߲ߘߦߊ ߘߏ߲߬ ߜߎ ߓߘߊ߫ ߣߵߊ߬ߟߎ߫ ߡߊ߬ ߞߊ߬ ߝߘߴߊ߬ߟߎ߫ ߡߊ߰ߙߌ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 526,
      "verse_key": "53:24",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَمۡ لِلۡإِنسَٰنِ مَا تَمَنَّىٰ",
      "translation": "ߥߟߊ߫ ߡߐ߱ ߣߊ߬ߕߊ ߟߋ߬ ( ߞߍߕߐ߫ ) ߊ߬ ߦߋ߫ ߓߊ߬؟",
      "footnotes": ""
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 526,
      "verse_key": "53:25",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلِلَّهِ ٱلۡأٓخِرَةُ وَٱلۡأُولَىٰ",
      "translation": "ߒ߬ߓߵߊߟߊ߫ ߕߊ ߟߋ߬ ߛߎ߬ߟߊ߫ ߣߌ߫ ߘߎߢߊ߫ ߘߌ߫ ߥߍ߫",
      "footnotes": ""
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 526,
      "verse_key": "53:26",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "۞ وَكَم مِّن مَّلَكٖ فِي ٱلسَّمَٰوَٰتِ لَا تُغۡنِي شَفَٰعَتُهُمۡ شَيۡـًٔا إِلَّا مِنۢ بَعۡدِ أَن يَأۡذَنَ ٱللَّهُ لِمَن يَشَآءُ وَيَرۡضَىٰٓ",
      "translation": "ߞߋ߲ߛߐ߲߫ ߖߋ߬ߟߌߖߋ߬ߟߌ߫ ߟߋ߬ ߛߊ߲ ߠߎ߬ ߞߣߐ߫ ߸ ߏ߬ ߟߎ߬ ߟߊ߫ ߛߟߏ߬ߣߊ߬ߟߌ ߕߍ߫ ߝߏߦߌ߫ ߢߊ߬ ߝߏ߫ ߊߟߊ߫ ߛߐ߲߭ ߞߐ߫ ߡߐ߰ ߦߋ߫ ߡߍ߲ ߓߊ߯ ߊ߬ ߘߌߦߊ߫ ߞߊ߬ ߘߌ߬ߢߍ߫ (ߏ߬ ߦߋ߫ )",
      "footnotes": ""
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 527,
      "verse_key": "53:27",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ ٱلَّذِينَ لَا يُؤۡمِنُونَ بِٱلۡأٓخِرَةِ لَيُسَمُّونَ ٱلۡمَلَٰٓئِكَةَ تَسۡمِيَةَ ٱلۡأُنثَىٰ",
      "translation": "ߡߍ߲ ߠߎ߬ ߕߍ߫ ߟߊ߫ ߟߊ߫ ߛߎ߬ߟߊ߬ ( ߞߏ ) ߟߊ߫ ߸ ߏ߬ ߟߎ߫ ߟߋ߬ ߞߋ߲ߛߐ߲ ߠߎ߬ ߕߐ߯ߟߊ߫ ߟߊ߫ ߡߛߏ߬ߡߊ߲ ߠߎ߬ ߕߐ߯ߟߢߊ ߡߊ߬",
      "footnotes": ""
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 527,
      "verse_key": "53:28",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا لَهُم بِهِۦ مِنۡ عِلۡمٍۖ إِن يَتَّبِعُونَ إِلَّا ٱلظَّنَّۖ وَإِنَّ ٱلظَّنَّ لَا يُغۡنِي مِنَ ٱلۡحَقِّ شَيۡـٔٗا",
      "translation": "ߟߐ߲ߠߌ߲߫ ߘߏ߲߬ ߕߍ߫ ߊ߬ߟߎ߬ ߦߋ߫ ߏ߬ ߞߏߢߊ ߘߐ߫ ߥߍ߫ ߸ ߊ߬ߟߎ߬ ߕߍ߫ ߝߏߦߌ߬ ߟߊߓߊ߬ߕߏ ߞߊ߲߬ ߝߏ߫ ߖߌ߰ߟߌ ߸ ߖߌ߰ߟߌ ߘߏ߲߬ ߕߍ߫ ߝߏߦߌ߫ ߢߊ߬ ߟߊ߫ ߕߎ߬ߢߊ ߘߐ߫",
      "footnotes": ""
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 527,
      "verse_key": "53:29",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَعۡرِضۡ عَن مَّن تَوَلَّىٰ عَن ذِكۡرِنَا وَلَمۡ يُرِدۡ إِلَّا ٱلۡحَيَوٰةَ ٱلدُّنۡيَا",
      "translation": "ߒ߬ߓߊ߬ ߌߟߋ ߦߴߌ ߞߐߘߏ߲߬ ߡߐ߰ ߟߊ߫ ߡߍ߲ ߓߊ߯ ߊ߬ ߞߐߟߐ߬ ߊ߲ ߞߏߝߐ ߟߊ߫ ߸ ߊ߬ ߘߏ߲߬ ߡߊ߫ ߢߌߣߌ߲߫ ߘߏ߫ ߜߘߍ߫ ߡߊ߬ ߘߎߢߊ߫ ߢߣߊߡߦߊ ߞߐ߫",
      "footnotes": ""
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 527,
      "verse_key": "53:30",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ذَٰلِكَ مَبۡلَغُهُم مِّنَ ٱلۡعِلۡمِۚ إِنَّ رَبَّكَ هُوَ أَعۡلَمُ بِمَن ضَلَّ عَن سَبِيلِهِۦ وَهُوَ أَعۡلَمُ بِمَنِ ٱهۡتَدَىٰ",
      "translation": "ߏ߬ ߟߋ ߘߏ߲߬ ߦߴߊ߬ߟߎ߫ ߘߊ߲߭ ߘߌ߫ ߟߐ߲ߠߌ߲ ߘߐ߫ ، ߝߣߊ߫߸ ߌ ߡߊ߰ߙߌ ߟߋ߫ ߞߵߊ߬ ߟߐ߲ߓߊ ߞߍ߫ ߡߍ߲ ߝߟߌ߬ߣߍ߲߫ ߊ߬ ߟߊ߫ ߛߌߟߊ ߡߊ߬ ߸ ߊ߬ߟߋ ߟߋ߫ ߞߵߊ߬ ߟߐ߲ߓߊ ߞߍ߫ ߡߍ߲ ߞߊ߲ߘߊߣߍ߲߫",
      "footnotes": ""
    },
    {
      "id": 31,
      "verse_number": 31,
      "page_number": 527,
      "verse_key": "53:31",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلِلَّهِ مَا فِي ٱلسَّمَٰوَٰتِ وَمَا فِي ٱلۡأَرۡضِ لِيَجۡزِيَ ٱلَّذِينَ أَسَٰٓـُٔواْ بِمَا عَمِلُواْ وَيَجۡزِيَ ٱلَّذِينَ أَحۡسَنُواْ بِٱلۡحُسۡنَى",
      "translation": "ߊߟߊ߫ ߕߊ ߟߋ߬ ߛߊ߲ ߠߎ߬ ߞߣߐߝߋ߲ ߣߌ߫ ߘߎ߱ ߞߣߐߝߋ߲ ߘߌ߫ ߸ ߖߐ߲߬ߛߴߊ߬ ߘߌ߫ ߖߎ߯ߦߊߓߊ߮ ߟߎ߫ ߛߊ߬ߙߴߊ߬ߟߎ߬ ߗߋߥߊߟߌ ߟߎ߬ ߟߊ߫߸ ߊ߬ ߣߌ߫ ߞߊ߬ ߢߊ߬ߓߊ߮ ߟߎ߫ ߛߊ߬ߙߊ߫ ߢߌ߬ߡߊ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 32,
      "verse_number": 32,
      "page_number": 527,
      "verse_key": "53:32",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِينَ يَجۡتَنِبُونَ كَبَٰٓئِرَ ٱلۡإِثۡمِ وَٱلۡفَوَٰحِشَ إِلَّا ٱللَّمَمَۚ إِنَّ رَبَّكَ وَٰسِعُ ٱلۡمَغۡفِرَةِۚ هُوَ أَعۡلَمُ بِكُمۡ إِذۡ أَنشَأَكُم مِّنَ ٱلۡأَرۡضِ وَإِذۡ أَنتُمۡ أَجِنَّةٞ فِي بُطُونِ أُمَّهَٰتِكُمۡۖ فَلَا تُزَكُّوٓاْ أَنفُسَكُمۡۖ هُوَ أَعۡلَمُ بِمَنِ ٱتَّقَىٰٓ",
      "translation": "ߏ߬ ߟߎ߬ ߡߍ߲ ߠߎ߬ ߦߴߊ߬ߟߎ߬ ߡߊߓߐ߫ ߟߊ߫ ߞߎ߲߬ߞߏ߬ߓߊߓߊ ߟߎ߬ ߣߌ߫ ߞߏߙߏ߲ߧߊ ߟߎ߬ ߟߊ߫ ߸ ߝߏ߫ ߊ߬ ߝߕߌߣߍ߲ ߠߎ߬ ، ߌ ߡߊ߰ߙߌ ߞߏ߬ߣߌ߲߬ ߦߋ߫ ߡߊ߬ߞߕߏ߬ߟߌ ߘߐߥߛߊ߬ߓߊ߮ ߟߋ߬ ߘߌ߫ ߸ ߊ߬ߟߋ ߟߋ߫ ߘߏ߲߬ ߞߵߊߟߎ߫ ߟߐ߲ߓߊ ߞߍ߫ ߸ ߞߊ߬ߦߌ߯ ߊߟߎ߫ ߘߊ߲߫ ߕߎߡߊ ߊ߬ ߓߟߏ߫ ߘߎ߱ ߘߐ߫ ، ߊ߬ ߣߴߊߟߎ߫ ߦߋ߫ ߢߋ߬ߟߋ ߘߌ߫ ߕߎ߬ߡߊ ߡߍ߲ ߊߟߎ߫ ߣߊ ߟߎ߬ ߘߓߍ ߟߎ߬ ߘߐ߫ ، ߊߟߎ߫ ߞߊߣߵߊߟߎ߫ ߖߘߍ߬ ߟߊߜߍ߫ ߏ߬ ߘߐ߫ ߘߋ߬ ߸ ߓߊ ߊ߬ߟߋ ߟߋ߫ ߞߊ߬ ߡߐ߰ ߟߐ߲߫ ߡߍ߲ ߝߊ߲ߕߊ߲߬ߞߊ߬ߣߍ߲߫",
      "footnotes": ""
    },
    {
      "id": 33,
      "verse_number": 33,
      "page_number": 527,
      "verse_key": "53:33",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَرَءَيۡتَ ٱلَّذِي تَوَلَّىٰ",
      "translation": "ߌ ߞߵߊ߬ ߦߋ߫ ߓߊ߬ ߡߍ߲ ߣߵߊ߬ ߞߐߘߏ߲߬ ߠߊ߫؟",
      "footnotes": ""
    },
    {
      "id": 34,
      "verse_number": 34,
      "page_number": 527,
      "verse_key": "53:34",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَعۡطَىٰ قَلِيلٗا وَأَكۡدَىٰٓ",
      "translation": "ߊ߬ ߣߴߊ߬ ߞߊ߬ ߘߏߣߍ߲߫ ߘߌ߫ ߞߵߊ߬ ߕߍ߮ ߜߟߍ߬ߦߊ߫؟",
      "footnotes": ""
    },
    {
      "id": 35,
      "verse_number": 35,
      "page_number": 527,
      "verse_key": "53:35",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَعِندَهُۥ عِلۡمُ ٱلۡغَيۡبِ فَهُوَ يَرَىٰٓ",
      "translation": "ߞߏ߫ ߘߏ߲߰ߣߍ߲ ߟߐ߲ߠߌ߲ ߠߴߊ߬ ߓߟߏ߫ ߓߊ߬ ߣߴߊ߬ ߓߍ߫ ߏ߬ ߦߋ߫ ߟߊ߫؟",
      "footnotes": ""
    },
    {
      "id": 36,
      "verse_number": 36,
      "page_number": 527,
      "verse_key": "53:36",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَمۡ لَمۡ يُنَبَّأۡ بِمَا فِي صُحُفِ مُوسَىٰ",
      "translation": "ߥߟߴߊ߬ ߡߊ߫ ߟߊߞߍ߫ ߝߋ߲߫ ߞߊ߬ߟߊߡߊ߬ ߓߊ߬ ߡߍ߲ ߦߋ߫ ߡߎߛߊ߫ ߟߊ߫ ߟߍߙߊ ߟߎ߬ ߞߣߐ߫؟",
      "footnotes": ""
    },
    {
      "id": 37,
      "verse_number": 37,
      "page_number": 527,
      "verse_key": "53:37",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِبۡرَٰهِيمَ ٱلَّذِي وَفَّىٰٓ",
      "translation": "ߊ߬ ߣߴߌߓߎ߬ߙߊ߬ߤߌߡߊ߫ ߏ߬ ߡߍ߲ ߞߊ߬ ߘߝߊߟߌ ߞߍ߫؟",
      "footnotes": ""
    },
    {
      "id": 38,
      "verse_number": 38,
      "page_number": 527,
      "verse_key": "53:38",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَّا تَزِرُ وَازِرَةٞ وِزۡرَ أُخۡرَىٰ",
      "translation": "ߞߏ߫ ߘߏ߬ߣߌ߲߬ߕߌ߯ ߕߍ߫ ߘߏ߫ ߜߘߍ߫ ߘߏ߬ߣߌ߲ ߕߊ߬ ߟߊ߫ ߘߋ߬",
      "footnotes": ""
    },
    {
      "id": 39,
      "verse_number": 39,
      "page_number": 527,
      "verse_key": "53:39",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَن لَّيۡسَ لِلۡإِنسَٰنِ إِلَّا مَا سَعَىٰ",
      "translation": "ߊ߬ ߣߌ߫ ߞߏ߫ ߝߏߦߌ߬ ߕߍ߫ ߡߐ߱ ߦߋ߫ ߝߏ߫ ߊ߬ ߞߊ߬ ߡߍ߲ ߓߊ߯ߙߊ߫",
      "footnotes": ""
    },
    {
      "id": 40,
      "verse_number": 40,
      "page_number": 527,
      "verse_key": "53:40",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّ سَعۡيَهُۥ سَوۡفَ يُرَىٰ",
      "translation": "ߊ߬ ߣߌ߫ ߞߏ߫ ߊ߬ ߟߊ߫ ߓߊ߯ߙߊ ߣߊ߬ ߦߋ߫߶",
      "footnotes": ""
    },
    {
      "id": 41,
      "verse_number": 41,
      "page_number": 527,
      "verse_key": "53:41",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ يُجۡزَىٰهُ ٱلۡجَزَآءَ ٱلۡأَوۡفَىٰ",
      "translation": "ߏ߬ ߟߋ ߞߐ߫ ߊ߬ ߘߌ߫ ߛߊ߬ߙߊ߫ ߛߙߊ߬ ߘߝߊߣߍ߲ ߠߊ߫",
      "footnotes": ""
    },
    {
      "id": 42,
      "verse_number": 42,
      "page_number": 527,
      "verse_key": "53:42",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّ إِلَىٰ رَبِّكَ ٱلۡمُنتَهَىٰ",
      "translation": "ߊ߬ ߣߌ߫ ߞߏ߫ ߘߊ߲߬ߞߐ߯ߛߌ ߓߌ߬ߟߊ߬ߣߍ߲߫ ߌ ߡߊ߰ߙߌ ߟߋ߫ ߡߊ߬",
      "footnotes": ""
    },
    {
      "id": 43,
      "verse_number": 43,
      "page_number": 527,
      "verse_key": "53:43",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّهُۥ هُوَ أَضۡحَكَ وَأَبۡكَىٰ",
      "translation": "ߊ߬ ߣߌ߫ ߞߏ߫ ߊ߬ߟߋ ߟߋ߫ ߟߊ߬ߝߎ߬ߟߍ߬ߟߌ ߞߍ߫ ߟߊ߫ ߞߊ߬ ߟߞߊ߬ߛߌ߬ߟߌ ߞߍ߫",
      "footnotes": ""
    },
    {
      "id": 44,
      "verse_number": 44,
      "page_number": 527,
      "verse_key": "53:44",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّهُۥ هُوَ أَمَاتَ وَأَحۡيَا",
      "translation": "ߊ߬ ߣߴߊ߬ߟߋ ߟߋ߫ ߟߊ߬ߣߌ߬ߒ߬ߕߊ߲߬ߧߊ߬ߟߌ ߞߍ߫ ߟߊ߫ ߞߊ߬ ߟߊ߬ߣߌ߬ߡߦߊ߬ߟߌ ߞߍ߫",
      "footnotes": ""
    },
    {
      "id": 45,
      "verse_number": 45,
      "page_number": 528,
      "verse_key": "53:45",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّهُۥ خَلَقَ ٱلزَّوۡجَيۡنِ ٱلذَّكَرَ وَٱلۡأُنثَىٰ",
      "translation": "ߊ߬ ߣߌ߫ ߞߏ߫ ߊ߬ߟߋ ߟߋ߫ ߞߊ߬ ߝߌ߬ߟߊ߬ߓߊ ߘߊ߲߫ ߗߍ߬ߡߊ߲ ߣߌ߫ ߡߛߏ߬ߡߊ߲ ߘߌ߫",
      "footnotes": ""
    },
    {
      "id": 46,
      "verse_number": 46,
      "page_number": 528,
      "verse_key": "53:46",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مِن نُّطۡفَةٍ إِذَا تُمۡنَىٰ",
      "translation": "ߛߖߌ߫ ߞߎߌߞߎߌߟߊ ߘߐ߫",
      "footnotes": ""
    },
    {
      "id": 47,
      "verse_number": 47,
      "page_number": 528,
      "verse_key": "53:47",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّ عَلَيۡهِ ٱلنَّشۡأَةَ ٱلۡأُخۡرَىٰ",
      "translation": "ߊ߬ ߣߌ߫ ߞߏ߫ ߘߊ߲ߠߌ߲ ߞߐߟߕߊ ߦߴߊ߬ߟߋ ߟߋ߫ ߡߊ߬",
      "footnotes": ""
    },
    {
      "id": 48,
      "verse_number": 48,
      "page_number": 528,
      "verse_key": "53:48",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّهُۥ هُوَ أَغۡنَىٰ وَأَقۡنَىٰ",
      "translation": "ߊ߬ ߣߌ߫ ߞߏ߫ ߊ߬ߟߋ ߟߋ߫ ߟߊ߬ߞߊ߰ߙߌ߬ߟߌ ߞߍ߫ ߟߊ߫ ߞߊ߬ ߟߊ߬ߓߣߊ߬ߦߊ߬ߟߌ ߞߍ߫",
      "footnotes": ""
    },
    {
      "id": 49,
      "verse_number": 49,
      "page_number": 528,
      "verse_key": "53:49",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّهُۥ هُوَ رَبُّ ٱلشِّعۡرَىٰ",
      "translation": "ߊ߬ ߣߌ߫ ߞߏ߫ ߊ߬ߟߋ ߟߋ߫ ߛߌ߬ߟߏߟߏ ߡߊ߰ߙߌ ߘߌ߫",
      "footnotes": ""
    },
    {
      "id": 50,
      "verse_number": 50,
      "page_number": 528,
      "verse_key": "53:50",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّهُۥٓ أَهۡلَكَ عَادًا ٱلۡأُولَىٰ",
      "translation": "ߊ߬ ߣߌ߫ ߞߏ߫ ߊ߬ߟߋ ߟߋ߫ ߞߊ߬ ߊ߯ߘߌ߫ ߝߟߐ ߟߎ߬ ߛߌߟߊߛߊ߬",
      "footnotes": ""
    },
    {
      "id": 51,
      "verse_number": 51,
      "page_number": 528,
      "verse_key": "53:51",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَثَمُودَاْ فَمَآ أَبۡقَىٰ",
      "translation": "ߊ߬ ߣߌ߫ ߛߊ߬ߡߎߘߊ ߟߎ߬ ߸ ߏ߬ ߟߎ߫ ߕߐ߬ ߡߊ߫ ߕߏ߫ ߘߋ߬",
      "footnotes": ""
    },
    {
      "id": 52,
      "verse_number": 52,
      "page_number": 528,
      "verse_key": "53:52",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَقَوۡمَ نُوحٖ مِّن قَبۡلُۖ إِنَّهُمۡ كَانُواْ هُمۡ أَظۡلَمَ وَأَطۡغَىٰ",
      "translation": "ߊ߬ ߣߌ߫ ߣߎ߯ߤߎ߲߫ ߡߌ߬ߙߌ߲߬ߘߌ ߟߎ߫ ߞߊ߬ߞߘߐ߬ ߸ ߏ߬ ߟߎ߫ ߟߋ߫ ߕߐ߬ߢߍ߰ߒ߬ߕߋ߬ߓߊ ߕߘߍ߬ ߊ߬ ߣߌ߫ ߘߊ߲߬ߠߕߊ߬ߡߌ߲߬ߓߊ߯ ߓߟߋߓߟߋ",
      "footnotes": ""
    },
    {
      "id": 53,
      "verse_number": 53,
      "page_number": 528,
      "verse_key": "53:53",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡمُؤۡتَفِكَةَ أَهۡوَىٰ",
      "translation": "ߊ߬ ߣߌ߫ ߘߎ߰ߝߘߌ߬ߓߊ߰ߕߐ ߟߎ߬ ߞߵߏ߬ ߟߎ߬ ߟߊߝߘߌ߫",
      "footnotes": ""
    },
    {
      "id": 54,
      "verse_number": 54,
      "page_number": 528,
      "verse_key": "53:54",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَغَشَّىٰهَا مَا غَشَّىٰ",
      "translation": "ߊ߬ ߞߵߏ߬ ߟߎ߫ ߞߎ߲߬ߠߊ߬ߓߌ߬ߘߌ߲߬ ߝߋ߲߫ ߠߊ߫ ߏ߬ ߟߋ ߘߐ߫ ߡߍ߲ ߞߵߊ߬ߟߎ߫ ߞߎ߲߬ߠߊ߬ߓߌߘߌ߲߫",
      "footnotes": ""
    },
    {
      "id": 55,
      "verse_number": 55,
      "page_number": 528,
      "verse_key": "53:55",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكَ تَتَمَارَىٰ",
      "translation": "ߒ߬ߓߊ߬ ߌ ߦߋ߫ ߞߎߛߎ߲߫ ߠߴߌ ߡߊ߰ߙߌ ߟߊ߫ ߣߍߡߊ ߢߌ߬ߡߊ߲߫ ߡߊ߬ ߛߊ߫؟",
      "footnotes": ""
    },
    {
      "id": 56,
      "verse_number": 56,
      "page_number": 528,
      "verse_key": "53:56",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "هَٰذَا نَذِيرٞ مِّنَ ٱلنُّذُرِ ٱلۡأُولَىٰٓ",
      "translation": "ߖߊ߬ߛߙߋ߬ߡߊ߬ߟߌ߬ߟߊ߫ ߟߋ߬ ߣߌ߲߬ ߸ ߖߊ߬ߛߙߋ߬ߡߊ߬ߟߌ߬ߟߊ߬ ߝߟߐ ߟߎ߬ ( ߛߎ߯ߦߊ ) ߘߐ߫",
      "footnotes": ""
    },
    {
      "id": 57,
      "verse_number": 57,
      "page_number": 528,
      "verse_key": "53:57",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَزِفَتِ ٱلۡأٓزِفَةُ",
      "translation": "ߞߎߘߍߞߎߘߍߦߕߊ ( ߕߎ߬ߡߊ ) ߓߘߊ߫ ߛߎ߬ߘߎ߲߬ߧߊ߫",
      "footnotes": ""
    },
    {
      "id": 58,
      "verse_number": 58,
      "page_number": 528,
      "verse_key": "53:58",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَيۡسَ لَهَا مِن دُونِ ٱللَّهِ كَاشِفَةٌ",
      "translation": "ߞߎ߲߬ߠߞߊ߬ߓߊ߯ ߘߏ߲߬ ߕߍ߫ ߊ߬ ߟߊ߫ ߊߟߊ߫ ߞߐ߫",
      "footnotes": ""
    },
    {
      "id": 59,
      "verse_number": 59,
      "page_number": 528,
      "verse_key": "53:59",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَمِنۡ هَٰذَا ٱلۡحَدِيثِ تَعۡجَبُونَ",
      "translation": "ߊ߬ ߝߐ߫ ߊߟߎ߫ ߦߋ߫ ߞߓߊߞߏߦߊ߫ ߟߊ߫ ߞߎߡߊ ߢߌ߲߬ ߠߋ߬ ߟߊ߫ ߓߊ߬؟",
      "footnotes": ""
    },
    {
      "id": 60,
      "verse_number": 60,
      "page_number": 528,
      "verse_key": "53:60",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَتَضۡحَكُونَ وَلَا تَبۡكُونَ",
      "translation": "ߞߵߊ߬ ߞߍ߫ ߦߟߍ ߘߌ߫ ߊߟߎ߫ ߕߍ߫ ߞߊ߬ߛߌ߬ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 61,
      "verse_number": 61,
      "page_number": 528,
      "verse_key": "53:61",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنتُمۡ سَٰمِدُونَ",
      "translation": "ߊ߬ ߣߌ߫ ߞߵߊߟߎ߫ ߕߏ߫ ߡߣߊ߬ߡߣߊ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 62,
      "verse_number": 62,
      "page_number": 528,
      "verse_key": "53:62",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": "obligatory",
      "sajdah_number": 12,
      "text": "فَٱسۡجُدُواْۤ لِلَّهِۤ وَٱعۡبُدُواْ۩",
      "translation": "ߒ߬ߓߵߊߟߎ߫ ߦߋ߫ ߕߌ߲ߓߌߘߌ߲ ߞߍ߫ ߊߟߊ߫ ߦߋ߫ ߘߋ߬ ߊ߬ ߣߌ߫ ߞߵߊ߬ ߓߊ߬ߕߏ߬",
      "footnotes": ""
    }
  ]
}