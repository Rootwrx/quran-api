{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 531,
      "verse_key": "55:1",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلرَّحۡمَٰنُ",
      "translation": "ߡߊ߬ߤߌ߬ߣߊ߬ߟߊ",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 531,
      "verse_key": "55:2",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلَّمَ ٱلۡقُرۡءَانَ",
      "translation": "ߏ߬ ߟߋ ߞߊ߬ ߟߊ߬ߟߐ߲߬ߠߌ߲߬ߠߌ߲ ߞߍ߫ ߘߐ߬ߛߊߙߌ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 531,
      "verse_key": "55:3",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "خَلَقَ ٱلۡإِنسَٰنَ",
      "translation": "ߊ߬ ߞߊ߬ ߡߐ߱ ߘߊ߲߫",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 531,
      "verse_key": "55:4",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلَّمَهُ ٱلۡبَيَانَ",
      "translation": "ߞߵߊ߬ ߟߊߟߐ߲ߠߌ߲߫ ߡߟߋߞߎߦߊ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 531,
      "verse_key": "55:5",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلشَّمۡسُ وَٱلۡقَمَرُ بِحُسۡبَانٖ",
      "translation": "ߕߋ߬ߟߋ ߣߌ߫ ߞߊߙߏ ߦߋ߫ ߖߊ߬ߕߋ߬ߦߊ ߟߋ߬ ߘߐ߫",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 531,
      "verse_key": "55:6",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلنَّجۡمُ وَٱلشَّجَرُ يَسۡجُدَانِ",
      "translation": "ߓߌ߲ ߣߌ߫ ߦߙߌ ߟߎ߬ ߦߋ߫ ߕߌ߲ߓߌߘߌ߲ ߞߍ߫ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 531,
      "verse_key": "55:7",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلسَّمَآءَ رَفَعَهَا وَوَضَعَ ٱلۡمِيزَانَ",
      "translation": "ߊ߬ ߞߊ߬ ߛߊ߲ ߞߘߐߕߊ߬ ߟߋ߬ ߞߊ߬ ߖߊ߭ ߛߌ߰",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 531,
      "verse_key": "55:8",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَّا تَطۡغَوۡاْ فِي ٱلۡمِيزَانِ",
      "translation": "ߊߟߎ߫ ߞߊߣߊ߬ ߞߊ߬ߙߏ߲ ߞߍ߫ ߛߎߡߊ߲ߠߌ߲ ߘߐ߫ ߘߋ߬",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 531,
      "verse_key": "55:9",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَقِيمُواْ ٱلۡوَزۡنَ بِٱلۡقِسۡطِ وَلَا تُخۡسِرُواْ ٱلۡمِيزَانَ",
      "translation": "ߊߟߎ߫ ߦߋ߫ ߛߎߡߊ߲ߠߌ߲ ߠߊߟߐ߬ ߕߋߟߋ߲ ߠߊ߫ ߸ ߊߟߎ߫ ߞߊߣߊ߬ ߖߊ߭ ߘߐߘߛߏ߫",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 531,
      "verse_key": "55:10",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡأَرۡضَ وَضَعَهَا لِلۡأَنَامِ",
      "translation": "ߊ߬ ߞߊ߬ ߘߎ߱ ߟߊ߫ ߘߊ߲ߝߋ߲ ߠߎ߬ ߦߋ߫",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 531,
      "verse_key": "55:11",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِيهَا فَٰكِهَةٞ وَٱلنَّخۡلُ ذَاتُ ٱلۡأَكۡمَامِ",
      "translation": "ߦߙߌߘߋ߲ ߦߴߊ߬ ߘߐ߫ ߊ߬ ߣߌ߫ ߕߌ߲߬ߖߐ߲߫ ߝߎ߲ߘߌߡߊ ߟߎ߬",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 531,
      "verse_key": "55:12",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡحَبُّ ذُو ٱلۡعَصۡفِ وَٱلرَّيۡحَانُ",
      "translation": "ߊ߬ ߣߌ߫ ߢߊ߯ߡߊ߫ ߖߌߘߏ߲߬ߠߊ ߟߎ߬ ߣߌ߫ ߛߎߡߊߙߋߡߊ ߟߎ߬",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 531,
      "verse_key": "55:13",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "ߊߟߎ߫ ߦߴߊߟߎ߫ ߝߌ߬ߟߊ ߡߊ߰ߙߌ ߟߊ߫ ߣߍߡߊ ߢߌ߬ߡߊ߲߬ ߡߊߛߐ߬ߛߐ߬ ߟߊ߫ ߏ߬ ߘߐ߫ ߓߏ߲߰؟",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 531,
      "verse_key": "55:14",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "خَلَقَ ٱلۡإِنسَٰنَ مِن صَلۡصَٰلٖ كَٱلۡفَخَّارِ",
      "translation": "ߊ߬ ߞߊ߬ ߡߐ߱ ߘߊ߲߫ ߓߐ߮ ߟߋ߬ ߘߐ߫ ߸ ߦߏ߫ ߕߍ߯ߣߍ߲",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 531,
      "verse_key": "55:15",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَخَلَقَ ٱلۡجَآنَّ مِن مَّارِجٖ مِّن نَّارٖ",
      "translation": "ߞߊ߬ ߥߐ߬ߞߎߟߐ ߟߎ߬ ߘߊ߲߫ ߕߊ-ߡߍߣߍ߲ ߘߐ߫",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 531,
      "verse_key": "55:16",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "ߊߟߎ߫ ߦߋ߫ ߊߟߎ߫ ߝߌ߬ߟߊ ߡߊ߰ߙߌ ߟߊ߫ ߣߍߡߊ ߢߌ߬ߡߊ߲߬ ߡߊߛߐ߬ߛߐ߬ ߟߊ߫ ߏ߬ ߘߐ߫ ߓߏ߲߰؟",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 532,
      "verse_key": "55:17",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "رَبُّ ٱلۡمَشۡرِقَيۡنِ وَرَبُّ ٱلۡمَغۡرِبَيۡنِ",
      "translation": "ߕߟߋ߬ߓߐ߫ ߝߌ߬ߟߊ ߡߊ߰ߙߌ ߟߋ߬ ߸ ߊ߬ ߣߌ߫ ߕߟߋ߬ߓߋ߫ ߝߌ߬ߟߊ ߡߊ߰ߙߌ",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 532,
      "verse_key": "55:18",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "ߊߟߎ߫ ߦߴߊߟߎ߫ ߝߌ߬ߟߊ ߡߊ߰ߙߌ ߟߊ߫ ߣߍߡߊ ߢߌ߬ߡߊ߲߬ ߡߊߛߐ߬ߛߐ߬ ߟߊ߫ ߏ߬ ߘߐ߫ ߓߏ߲߰؟",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 532,
      "verse_key": "55:19",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَرَجَ ٱلۡبَحۡرَيۡنِ يَلۡتَقِيَانِ",
      "translation": "ߊ߬ ߞߊ߬ ߓߊ߫ ߝߌ߬ߟߊ ߟߊߥߦߏ߬ ߸ ߊ߬ߟߎ߬ ߦߋ߫ ߢߐ߲߮ ߓߍ߲߬ ߠߊ߫ ( ߝߛߊߦߌ߫ )",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 532,
      "verse_key": "55:20",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بَيۡنَهُمَا بَرۡزَخٞ لَّا يَبۡغِيَانِ",
      "translation": "ߓߊ߬ߟߊ߲߬ ߦߴߊ߬ߟߎ߬ ߕߍ߫ ߸ ߊ߬ߟߎ߬ ߕߍ߫ ߓߊ߬ߛߊ߲߬ ߠߊ߫ ߝߋߎ߫",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 532,
      "verse_key": "55:21",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "ߊߟߎ߫ ߦߴߊߟߎ߫ ߝߌ߬ߟߊ ߡߊ߰ߙߌ ߟߊ߫ ߣߍߡߊ ߢߌ߬ߡߊ߲߬ ߡߊߛߐ߬ߛߐ߬ ߟߊ߫ ߏ߬ ߘߐ߫ ߓߏ߲߰؟",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 532,
      "verse_key": "55:22",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَخۡرُجُ مِنۡهُمَا ٱللُّؤۡلُؤُ وَٱلۡمَرۡجَانُ",
      "translation": "ߟߓߊ߬-ߞߐߣߐ߲ ߠߎ߬ ߦߋ߫ ߓߐ߫ ߟߴߊ߬ߟߎ߬ ߘߐ߫ ߸ ߊ߬ ߣߌ߫ ߘߍ߯ߣߍ߲",
      "footnotes": ""
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 532,
      "verse_key": "55:23",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "ߊߟߎ߫ ߦߴߊߟߎ߫ ߝߌ߬ߟߊ ߡߊ߰ߙߌ ߟߊ߫ ߣߍߡߊ ߢߌ߬ߡߊ߲߬ ߡߊߛߐ߬ߛߐ߬ ߟߊ߫ ߏ߬ ߘߐ߫ ߓߏ߲߰؟",
      "footnotes": ""
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 532,
      "verse_key": "55:24",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَهُ ٱلۡجَوَارِ ٱلۡمُنشَـَٔاتُ فِي ٱلۡبَحۡرِ كَٱلۡأَعۡلَٰمِ",
      "translation": "ߞߎߟߎ߲ ߠߎ߬ ߦߴߊ߬ߟߋ ߟߋ߬ ߓߟߏ߫ ߡߍ߲ ߠߎ߬ ߘߊ߲ߣߍ߲ ߦߋ߫ ߓߊ ߞߊ߲߬ ߦߏ߫ ߞߐ߲߬ߞߍ ߟߎ߫ ߟߋ߬",
      "footnotes": ""
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 532,
      "verse_key": "55:25",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "ߊߟߎ߫ ߦߴߊߟߎ߫ ߝߌ߬ߟߊ ߡߊ߰ߙߌ ߟߊ߫ ߣߍߡߊ ߢߌ߬ߡߊ߲߬ ߡߊߛߐ߬ߛߐ߬ ߟߊ߫ ߏ߬ ߘߐ߫ ߓߏ߲߰؟",
      "footnotes": ""
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 532,
      "verse_key": "55:26",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كُلُّ مَنۡ عَلَيۡهَا فَانٖ",
      "translation": "ߣߌߡߊ ߝߋ߲-ߋ-ߝߋ߲߫ ߘߎ߱ ߞߊ߲߬ ߸ ߏ߬ ߓߍ߯ ߓߊ߲ߕߐ߫ ߟߋ߬",
      "footnotes": ""
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 532,
      "verse_key": "55:27",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيَبۡقَىٰ وَجۡهُ رَبِّكَ ذُو ٱلۡجَلَٰلِ وَٱلۡإِكۡرَامِ",
      "translation": "ߌ ߡߊ߰ߙߌ ߞߏߟߘߊ ߟߋ߬ ߕߏߕߐ߫ ߸ ߞߟߊ߬ߒ߬ߞߋ߬ߦߊ ߣߌ߫ ߞߊ߬ߡߙߊ߬ߒ߬ߞߋ߬ߦߊ ߕߌ߱",
      "footnotes": ""
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 532,
      "verse_key": "55:28",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "ߊߟߎ߫ ߦߴߊߟߎ߫ ߝߌ߬ߟߊ ߡߊ߰ߙߌ ߟߊ߫ ߣߍߡߊ ߢߌ߬ߡߊ߲߬ ߡߊߛߐ߬ߛߐ߬ ߟߊ߫ ߏ߬ ߘߐ߫ ߓߏ߲߰؟",
      "footnotes": ""
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 532,
      "verse_key": "55:29",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَسۡـَٔلُهُۥ مَن فِي ٱلسَّمَٰوَٰتِ وَٱلۡأَرۡضِۚ كُلَّ يَوۡمٍ هُوَ فِي شَأۡنٖ",
      "translation": "ߝߋ߲ ߓߍ߯ ߦߴߊ߬ߟߋ ߟߋ߬ ߡߊߕߙߊ߫ ߟߊ߫ ߛߊ߲ ߠߎ߬ ߣߌ߫ ߘߎ߱ ߞߣߐ߫ ߸ ߊ߬ߟߋ߬ ߦߋ߫ ߞߏ߫ ߟߋ߬ ߡߊ߬ ߟߏ߲ ߓߍ߯",
      "footnotes": ""
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 532,
      "verse_key": "55:30",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
