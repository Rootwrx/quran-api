{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 594,
      "verse_key": "90:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَآ أُقۡسِمُ بِهَٰذَا ٱلۡبَلَدِ",
      "translation": "ߒ ߧߋ߫ ߒ ߞߊ߬ߟߌ߬ ߟߊ߫ ߟߋ߬ ߛߏ ߢߌ߲߬ ߠߊ߫ ߛߎߋ߫",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 594,
      "verse_key": "90:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنتَ حِلُّۢ بِهَٰذَا ٱلۡبَلَدِ",
      "translation": "ߞߵߌߟߋ ߡߊߞߍߣߍ߲ ߕߏ߫ ߊ߬ߟߋ߬ ߛߏ ߢߌ߲߬ ߠߊ߫",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 594,
      "verse_key": "90:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَوَالِدٖ وَمَا وَلَدَ",
      "translation": "ߊ߬ ߣߌ߫ ߒ ߧߋ߫ ߒ ߞߊ߬ߟߌ߬ ߟߊ߫ ߡߏߦߌߓߊ߮ ߣߌ߫ ߡߏߦߌߘߋ߲ ߠߊ߫",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 594,
      "verse_key": "90:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَقَدۡ خَلَقۡنَا ٱلۡإِنسَٰنَ فِي كَبَدٍ",
      "translation": "ߒ ߞߏ߫ ߒ߬ߠߎ߫ ߞߊ߬ ߡߐ߱ ߘߊ߲߫ ߛߍ߱ ߟߋ߬ ߟߊ߫ ߘߋ߬",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 594,
      "verse_key": "90:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَيَحۡسَبُ أَن لَّن يَقۡدِرَ عَلَيۡهِ أَحَدٞ",
      "translation": "ߊ߬ ߦߴߊ߬ ߢߣߊ߫ ( ߏ߬ ߘߐ߫ ) ߟߋ߫ ߓߊ߬ ߞߏ߫ ߝߏߦߌ߬ ߕߍߣߊ߬ ߛߋ߫ ߟߴߊ߬ߟߋ߬ ߟߊ߫؟",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 594,
      "verse_key": "90:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَقُولُ أَهۡلَكۡتُ مَالٗا لُّبَدًا",
      "translation": "ߊ߬ ߦߴߊ߬ ߝߐ߫ ߟߊ߫ ߛߊ߫ ߞߏ߫ ߒ ߓߘߊ߫ ߣߊ߲߬ߝߟߏ߬ ߛߌߦߊߡߊ߲߫ ߘߏ߲߬ ߘߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 594,
      "verse_key": "90:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَيَحۡسَبُ أَن لَّمۡ يَرَهُۥٓ أَحَدٌ",
      "translation": "ߊ߬ ߦߴߊ߬ ߢߣߊ߫ ( ߏ߬ ߘߐ߫ ) ߟߋ߫ ߓߊ߬ ߞߏ߫ ߡߐ߰ ߛߌ߫ ߡߴߊ߬ ߦߋ߫؟",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 594,
      "verse_key": "90:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَمۡ نَجۡعَل لَّهُۥ عَيۡنَيۡنِ",
      "translation": "ߊ߬ ߝߐ߫ ߒ߬ߠߎ߫ ߟߋ߬ ߡߊ߫ ߢߊ߫ ߝߌ߬ߟߊ߬ ߞߍ߫ ߊ߬ ߟߊ߫ ߘߍ߫؟",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 594,
      "verse_key": "90:9",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلِسَانٗا وَشَفَتَيۡنِ",
      "translation": "ߊ߬ ߣߌ߫ ߣߍ߲߬ ߣߌ߫ ߘߊߜߟߏ߫ ߝߌ߬ߟߊ߫؟",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 594,
      "verse_key": "90:10",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَهَدَيۡنَٰهُ ٱلنَّجۡدَيۡنِ",
      "translation": "ߊ߬ ߣߴߊ߲ ߞߵߊ߬ ߟߊߛߌ߲߫ ߛߌߟߊ߫ ߝߌ߬ߟߊ߫ ߡߊ߬",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 594,
      "verse_key": "90:11",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلَا ٱقۡتَحَمَ ٱلۡعَقَبَةَ",
      "translation": "ߒ߬ߓߵߊ߬ ߕߍߣߊ߬ ߦߟߍ߬ ߟߊ߫ ߞߊߙߊ߲ߞߊߙߊ߲ ߠߊ߫ ߝߋߎ߫",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 594,
      "verse_key": "90:12",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَآ أَدۡرَىٰكَ مَا ٱلۡعَقَبَةُ",
      "translation": "ߡߋ߬ߘߋ ߘߏ߲߬ ߞߵߌ ߟߊߟߐ߲ߠߌ߲߫ ߞߊߙߊ߲ߞߊߙߊ߲ ߠߊ߫؟",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 594,
      "verse_key": "90:13",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَكُّ رَقَبَةٍ",
      "translation": "ߏ߬ ߦߋ߫ ߖߐ߲߬ ߞߊ߲ߤߙߐߦߊ ߟߋ߬ ߘߌ߫",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 594,
      "verse_key": "90:14",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَوۡ إِطۡعَٰمٞ فِي يَوۡمٖ ذِي مَسۡغَبَةٖ",
      "translation": "ߥߟߊ߫ ߞߊ߬ ߘߊߥߎ߲ߠߌ߲ ߘߌ߫ ߞߐ߲ߞߐ߫ ߟߏ߲ ߘߏ߫ ߡߊ߬",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 594,
      "verse_key": "90:15",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَتِيمٗا ذَا مَقۡرَبَةٍ",
      "translation": "ߡߊ߬ߡߐ߰ ߝߊߙߌߕߊ ߘߏ߫ ߡߊ߬",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 594,
      "verse_key": "90:16",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَوۡ مِسۡكِينٗا ذَا مَتۡرَبَةٖ",
      "translation": "ߥߟߊ߫ ߘߐ߰ߕߐ߫ ߓߎ߬ߙߎ߲߬ߓߎ߬ߙߎ߲߬ߕߐ ߘߏ߫ ߡߊ߬",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 594,
      "verse_key": "90:17",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ كَانَ مِنَ ٱلَّذِينَ ءَامَنُواْ وَتَوَاصَوۡاْ بِٱلصَّبۡرِ وَتَوَاصَوۡاْ بِٱلۡمَرۡحَمَةِ",
      "translation": "ߏ߬ ߞߐ߫ ߸ ߞߊ߬ ߞߍ߫ ߡߐ߱ ߟߎ߬ ߘߏ߫ ߘߌ߫ ߡߍ߲ ߠߎ߬ ߞߍߣߍ߲߫ ߟߊߒߠߦߊߓߊ߮ ߟߎ߬ ߘߌ߫ ߸ ߊ߬ ߣߴߊ߬ߟߎ߫ ߞߊ߬ ߢߐ߲߮ ߟߐ߬ ߡߎ߬ߢߎ߲ ߠߊ߫ ߸ ߊ߬ ߣߌ߫ ߞߊ߬ ߢߐ߲߮ ߟߐ߬ ߤߌߣߊ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 594,
      "verse_key": "90:18",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أُوْلَٰٓئِكَ أَصۡحَٰبُ ٱلۡمَيۡمَنَةِ",
      "translation": "ߏ߬ ߟߎ߫ ߟߋ߬ ߞߌߣߌ߲ߓߟߏߟߞߊ ߟߎ߬ ߘߌ߫ ߛߊ߫",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 594,
      "verse_key": "90:19",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّذِينَ كَفَرُواْ بِـَٔايَٰتِنَا هُمۡ أَصۡحَٰبُ ٱلۡمَشۡـَٔمَةِ",
      "translation": "ߡߍ߲ ߠߎ߫ ߣߵߊ߬ߟߎ߫ ߓߊ߲߬ ߠߊ߫ ߒ߬ ߠߊ߫ ߟߝߊߙߌ ߟߎ߬ ߘߐ߫ ߛߊ߫ ߸ ߏ߬ ߟߎ߫ ߟߋ߬ ߣߎߡߊ߲ߓߟߏߟߞߊ ߟߎ߬ ߘߌ߫",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 594,
      "verse_key": "90:20",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلَيۡهِمۡ نَارٞ مُّؤۡصَدَةُۢ",
      "translation": "ߕߊ ߜߊߛߏ߲ߕߐ߫ ߏ߬ ߟߎ߫ ߟߋ߫ ߡߊ߬ ߜߙߏ߫",
      "footnotes": ""
    }
  ]
}