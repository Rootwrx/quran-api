{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 595,
      "verse_key": "92:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّيۡلِ إِذَا يَغۡشَىٰ",
      "translation": "ߊ߰ ߒ ߣߌ߫ ߛߎ ߕߍ߫ ߸ ߣߴߊ߬ ߞߊ߬ ߞߎ߲߬ߠߊ߬ߓߌ߬ߘߌ߲߬ߠߌ߲ ߞߍ߫ ߕߎ߬ߡߊ ߡߍ߲",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 595,
      "verse_key": "92:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلنَّهَارِ إِذَا تَجَلَّىٰ",
      "translation": "ߊ߰ ߒ ߣߌ߫ ߕߟߋ߬ߘߐ߬ߟߊ ߕߍ߫ ߸ ߣߴߏ߬ ߟߊ߲ߞߣߍߡߊ߫ ߘߊ߫ ߕߎ߬ߡߊ ߡߍ߲",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 595,
      "verse_key": "92:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا خَلَقَ ٱلذَّكَرَ وَٱلۡأُنثَىٰٓ",
      "translation": "ߊ߬ ߣߌ߫ ߡߍ߲ ߞߊ߬ ߗߍ߬ߡߊ߲ ߣߌ߫ ߡߛߏ߬ߡߊ߲ ߘߊ߲߫ ( ߊ߰ ߒ ߣߴߏ߬ ߕߍ߫ )",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 595,
      "verse_key": "92:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ سَعۡيَكُمۡ لَشَتَّىٰ",
      "translation": "ߤߋ߫ ߸ ߊߟߎ߫ ߗߋߥߊߟߌ ( ߟߎ߬) ߝߘߊߝߘߊߣߍ߲߫ ߠߋ߫ ߘߋ߬",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 595,
      "verse_key": "92:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَمَّا مَنۡ أَعۡطَىٰ وَٱتَّقَىٰ",
      "translation": "ߏ߬ ߘߐ߫ ߸ ߣߌ߫ ߡߍ߲ ߞߊ߬ ߛߐߟߌ ߞߍ߫ ߞߵߊ߬ ߝߊ߲ߕߊ߲߬ߞߊ߫",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 595,
      "verse_key": "92:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَصَدَّقَ بِٱلۡحُسۡنَىٰ",
      "translation": "ߊ߬ ߣߌ߫ ߞߊ߬ ߢߌ߬ߡߊ ߟߊߛߙߋߦߊ߫",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 595,
      "verse_key": "92:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَسَنُيَسِّرُهُۥ لِلۡيُسۡرَىٰ",
      "translation": "ߏ߬ ߓߊ߯ ߞߍ߫ ߸ ߊ߲ ߘߌߣߵߏ ߣߐ߰ߦߊ߫ ߣߐ߰ߦߊ ߦߋ߫",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 595,
      "verse_key": "92:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَمَّا مَنۢ بَخِلَ وَٱسۡتَغۡنَىٰ",
      "translation": "ߣߌ߫ ߡߍ߲ ߘߏ߲߬ ߞߊ߬ ߖߎ߯ߡߊ߲ߒߕߋߦߊ ߞߍ߫ ߸ ߞߵߊ߬ ߖߘߍ߬ ߟߊߓߣߊ߬ߦߊ ߢߌߣߌ߲߫",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 595,
      "verse_key": "92:9",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَكَذَّبَ بِٱلۡحُسۡنَىٰ",
      "translation": "ߊ߬ ߣߌ߫ ߞߊ߬ ߢߌ߬ߡߊ ߡߊߛߐ߬ߛߐ߬",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 595,
      "verse_key": "92:10",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَسَنُيَسِّرُهُۥ لِلۡعُسۡرَىٰ",
      "translation": "ߏ߬ ߓߊ߯ ߞߍ߫ ߸ ߊ߲ ߘߌߣߵߏ ߣߐ߰ߦߊ߫ ߜߟߍ߬ߦߊ ߦߋ߫",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 595,
      "verse_key": "92:11",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا يُغۡنِي عَنۡهُ مَالُهُۥٓ إِذَا تَرَدَّىٰٓ",
      "translation": "ߏ߬ ߟߊ߫ ߣߊ߲߬ߝߟߏ ߕߍ߫ ߝߏߦߌ߫ ߢߵߊ߬ ߦߋ߫ ߊ߬ ߕߏ߫ ߕߎߡߊ ߊ߬ ߘߐ߫",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 595,
      "verse_key": "92:12",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ عَلَيۡنَا لَلۡهُدَىٰ",
      "translation": "ߓߊ ߞߊ߲ߘߦߊ ( ߦߌ߬ߘߊ ) ߦߴߊ߲ߠߎ߫ ߟߋ߫ ߡߊ߬ ߘߋ߬",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 595,
      "verse_key": "92:13",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِنَّ لَنَا لَلۡأٓخِرَةَ وَٱلۡأُولَىٰ",
      "translation": "ߊ߬ ߣߌ߫ ߒ߬ߠߎ߫ ߟߋ߬ ߕߊ ߦߋ߫ ߛߏߟߊߓߊ߲ ߣߌ߫ ߘߎߢߊ߫ ߘߌ߫ ߘߋ߬",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 595,
      "verse_key": "92:14",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَنذَرۡتُكُمۡ نَارٗا تَلَظَّىٰ",
      "translation": "ߒ߬ߓߊ߬ (ߞߊ߬ߦߌ߯ ߞߋߟߋ߲߬) ߒ ߓߘߊ߫ ߖߊ߬ߛߙߋ߬ߡߊ߬ߟߊ ߞߍ߫ ߊߟߎ߫ ߦߋ߫ ߕߊ߫ ߡߢߊߞߌ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 596,
      "verse_key": "92:15",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَا يَصۡلَىٰهَآ إِلَّا ٱلۡأَشۡقَى",
      "translation": "ߡߐ߰ ߛߌ߫ ߕߍ߫ ߘߏ߲߬ ߠߴߏ߬ ߘߐ߫ ߝߏ߫ ߞߎ߲߬ߠߊ߲߬ߜߏ߬ߓߊ",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 596,
      "verse_key": "92:16",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِي كَذَّبَ وَتَوَلَّىٰ",
      "translation": "ߡߍ߲ ߞߊ߬ ߡߊ߬ߛߐ߬ߛߐ߬ߟߌ ߞߍ߫ ߞߵߊ߬ ߞߐߘߏ߲߬",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 596,
      "verse_key": "92:17",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَسَيُجَنَّبُهَا ٱلۡأَتۡقَى",
      "translation": "ߝߊ߲ߕߊ߲ߞߊߟߓߊ ߟߋ߬ ߘߌߣߵߊ߬ ߡߊߕߊ߯ ߊ߬ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 596,
      "verse_key": "92:18",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِي يُؤۡتِي مَالَهُۥ يَتَزَكَّىٰ",
      "translation": "ߡߍ߲ ߦߴߊ߬ ߟߊ߫ ߣߊ߲߬ߝߟߏ ߘߌ߫ ߟߊ߫ ߞߵߊ߬ ߖߘߍ߬ ߟߛߊߣߌ߲ߧߊ߫ ( ߞߊ߬ ߓߐ߫ ߓߏ߬ߞߎ߬ߣߊ߲߬ߦߊ ߡߊ߬ )",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 596,
      "verse_key": "92:19",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا لِأَحَدٍ عِندَهُۥ مِن نِّعۡمَةٖ تُجۡزَىٰٓ",
      "translation": "ߥߊߟߢߌߡߊ߫ ߘߏ߲߬ ߕߴߊ߬ ߣߌ߫ ߡߐ߰ ߛߌ߫ ߕߍ߫ ߞߏ߫ ߊ߬ ߓߴߏ߬ ߕߊ ߛߙߊ߬ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 596,
      "verse_key": "92:20",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَّا ٱبۡتِغَآءَ وَجۡهِ رَبِّهِ ٱلۡأَعۡلَىٰ",
      "translation": "ߝߏ߫ ߊ߬ ߡߊ߰ߙߌ߬ ߞߘߐߕߊ߬ߣߍ߲߬ߓߊ ߞߏߟߘߊ ߢߌߣߌ߲ ߘߐߙߐ߲߫",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 596,
      "verse_key": "92:21",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَسَوۡفَ يَرۡضَىٰ",
      "translation": "ߒ߬ߓߊ߬ ߏ ߣߵߊ߬ ( ߛߊ߬ߥߏ ߣߴߊ߬ ) ߘߌ߬ߢߍ ߛߐ߬ߘߐ߲߬ ( ߊ߬ ߡߊ߰ߙߌ ߝߍ߬ )߶",
      "footnotes": ""
    }
  ]
}