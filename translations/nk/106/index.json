      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لِإِيلَٰفِ قُرَيۡشٍ",
      "translation": "ߣߌ߲߬ ߞߎ߲߭ ߠߋ߫ ߞߎ߬ߙߊߦߌ߬ߛߌ ߟߎ߫ ߘߋ߬ߙߌ߬ߞߏ ߘߌ߫",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 602,
      "verse_key": "106:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِۦلَٰفِهِمۡ رِحۡلَةَ ٱلشِّتَآءِ وَٱلصَّيۡفِ",
      "translation": "ߊ߬ߟߎ߫ ߘߋ߬ߙߌ߬ߣߍ߲߬ ߣߍߣߍ ߣߌ߫ ߕߙߊ ߘߐ߫ ߥߌ߬ߟߌ ߡߍ߲ ߠߊ߫",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 602,
      "verse_key": "106:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلۡيَعۡبُدُواْ رَبَّ هَٰذَا ٱلۡبَيۡتِ",
      "translation": "ߒ߬ߓߵߊ߬ߟߎ߬ ߦߋ߫ ߓߏ߲ ߣߌ߲߬ ߡߊ߰ߙߌ ߓߊ߬ߕߏ߬",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 602,
      "verse_key": "106:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِيٓ أَطۡعَمَهُم مِّن جُوعٖ وَءَامَنَهُم مِّنۡ خَوۡفِۭ",
      "translation": "ߏ߬ ߡߍ߲ ߞߵߊ߬ߟߎ߬ ߘߡߊߢߌߣߌ߲߫ ߞߐ߲ߞߐ ߘߐ߫ ߸ ߞߵߊ߬ߟߎ߫ ߞߎ߲߬ߠߊ߬ߝߎߟߋ߲߫ ߞߊ߬ ߓߐ߫ ߞߎ߲߬ߠߊ߬ߛߘߌ ߘߐ߫",
      "footnotes": ""
    }
  ]
}