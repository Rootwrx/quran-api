{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 577,
      "verse_key": "75:1",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَآ أُقۡسِمُ بِيَوۡمِ ٱلۡقِيَٰمَةِ",
      "translation": "ߒ ߧߋ߫ ߒ ߞߊ߬ߟߌ߬ ߟߊ߫ ߟߐ߬ߓߊ ߟߏ߲ ߠߊ߫ ߟߋ߬",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 577,
      "verse_key": "75:2",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَآ أُقۡسِمُ بِٱلنَّفۡسِ ٱللَّوَّامَةِ",
      "translation": "ߊ߬ ߣߌ߫ ߒ ߧߋ߫ ߒ ߞߊ߬ߟߌ߬ ߟߊ߫ ߣߌ߫ ߖߙߊ߬ߞߟߌ߬ߟߊ߬ߓߊ ߟߊ߫ ߟߋ߬",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 577,
      "verse_key": "75:3",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَيَحۡسَبُ ٱلۡإِنسَٰنُ أَلَّن نَّجۡمَعَ عِظَامَهُۥ",
      "translation": "ߤߋ߯ߋ߰ ߡߐ߱ ߦߴߊ߬ ߓߛߌ߬ߞߌ߬ ߟߊ߫ ߟߋ߫ ߓߊ߬ ߞߏ߫ ߒ߬ ߕߍߣߵߊ߬ ߞߟߏ ߟߎ߬ ߟߊߘߍ߬ ߟߊ߫؟",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 577,
      "verse_key": "75:4",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بَلَىٰ قَٰدِرِينَ عَلَىٰٓ أَن نُّسَوِّيَ بَنَانَهُۥ",
      "translation": "ߔߙߏ߫ ߸ ߒ߬ߠߎ߫ ߖߍ߬ߘߍ ߛߋߣߍ߲߫ ߓߍ߫ ߊ߬ ߡߍ߲ߛߍ߲ ߠߎ߬ ߟߊߞߢߊ߫ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 577,
      "verse_key": "75:5",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بَلۡ يُرِيدُ ٱلۡإِنسَٰنُ لِيَفۡجُرَ أَمَامَهُۥ",
      "translation": "ߋ߬ߋ߬ߋ߯ ߸ ߡߐ߱ ߢߝߍߟߊ ߟߋ߬ ߕߌߢߍ߫ ߟߐ߮ ߦߴߊ߬ ߟߊ߫ ( ߟߐ߬ߓߊ߬ߞߏ ߡߊߛߐ߬ߛߐ ߘߌ߫ ) ߓߏ߬",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 577,
      "verse_key": "75:6",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَسۡـَٔلُ أَيَّانَ يَوۡمُ ٱلۡقِيَٰمَةِ",
      "translation": "ߊ߰ ߢߌ߬ߣߌ߲߬ߞߊ߬ߟߌ ߞߍ߫ ߟߊ߫ ( ߏ߬ ߟߋ ߘߐ߫ ) ߞߏ߫ ߟߐ߬ߓߊ ߦߋ߫ ߕߎ߬ߡߊ ߢߌ߬ߡߊ߲߬ ߘߌ߫؟",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 577,
      "verse_key": "75:7",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِذَا بَرِقَ ٱلۡبَصَرُ",
      "translation": "ߒ߬ߓߊ߬ ߣߌ߫ ߢߊ ߟߎ߬ ߘߐߡߊ߰ߡߊ߰ ߘߊ߫ ( ߛߌߟߊ߲ ߓߟߏ߫ ߕߎ߬ߡߊ ߡߍ߲ )",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 577,
      "verse_key": "75:8",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَخَسَفَ ٱلۡقَمَرُ",
      "translation": "ߞߊ߬ ߞߊߙߏ ( ߢߊ ) ߓߎߘߎ߲ߛߊ߫",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 577,
      "verse_key": "75:9",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَجُمِعَ ٱلشَّمۡسُ وَٱلۡقَمَرُ",
      "translation": "ߞߊ߬ ߕߋ߬ߟߋ ߣߌ߫ ߞߊߙߏ ߟߊߘߍ߬ ( ߓߐ߫ ߦߙߐ߫ ߞߋߟߋ߲߫ ߠߊ߫ )",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 577,
      "verse_key": "75:10",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَقُولُ ٱلۡإِنسَٰنُ يَوۡمَئِذٍ أَيۡنَ ٱلۡمَفَرُّ",
      "translation": "ߡߐ߱ ߘߴߊ߬ ߝߐ߫ ߏ߬ ߟߏ߲ ߞߏ߫ ߓߏ߬ߙߌ߬ ߦߙߐ ߦߋ߫ ߡߌ߲߫؟",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 577,
      "verse_key": "75:11",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّا لَا وَزَرَ",
      "translation": "ߍ߲߬ߍ߲߫ ߸ ߞߎ߲߬ߞߊ߬ߙߌ߬ߝߊ߬ ߦߙߐ߫ ߖߘߍ߬ ߕߍ߫ ( ߡߍ߲ ߘߌ߫ ߡߊߜߍ߲߫ )",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 577,
      "verse_key": "75:12",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَىٰ رَبِّكَ يَوۡمَئِذٍ ٱلۡمُسۡتَقَرُّ",
      "translation": "ߡߊ߬ߞߍ߬ ߦߌߟߊ ߦߴߌ ߡߊ߰ߙߌ ( ߘߐߙߐ߲߫ ) ߠߋ߫ ߕߍ߬ߟߊ ߘߐ߫ ߏ߬ ߟߏ߲",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 577,
      "verse_key": "75:13",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يُنَبَّؤُاْ ٱلۡإِنسَٰنُ يَوۡمَئِذِۭ بِمَا قَدَّمَ وَأَخَّرَ",
      "translation": "ߡߐ߱ ߗߋߥߊߟߌ ߢߍߕߊ ߣߴߊ߬ ߞߐߟߕߊ ߟߎ߬ ߘߌ߫ ߟߊߓߐ߫ ߊ߬ ߡߊ߬ ߏ߬ ߟߏ߲",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 577,
      "verse_key": "75:14",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بَلِ ٱلۡإِنسَٰنُ عَلَىٰ نَفۡسِهِۦ بَصِيرَةٞ",
      "translation": "ߍ߲߬ߍ߲߫ ( ߤߊߟߌ߬ ߣߴߏ߬ ߕߍ߫ ) ߡߐ߱ ߦߴߊ߬ ߖߘߍ߬ ߟߊߛߊ߬ߓߊ߯ ߟߋ߬ ߘߌ߫ ( ߊ߬ ߞߍߕߊ ߟߎ߬ ߘߐ߫ )",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 577,
      "verse_key": "75:15",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَوۡ أَلۡقَىٰ مَعَاذِيرَهُۥ",
      "translation": "ߤߟߴߊ߬ ߞߵߊ߬ ߟߐ߬ߦߙߌ ߟߎ߬ ߝߏ߬ߦߌ߫ ߟߋ߬",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 577,
      "verse_key": "75:16",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَا تُحَرِّكۡ بِهِۦ لِسَانَكَ لِتَعۡجَلَ بِهِۦٓ",
      "translation": "ߌ ߞߴߌ ߣߍ߲߭ ߠߊߓߙߊ߬ߞߊ߬ߓߙߊߞߊ߫ ‹ ߘߐ߬ߛߊߙߌ › ߟߊ߫ ߞߌߢߍ ߡߵߊ߬ ߟߊ߫ ߘߋ߬",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 577,
      "verse_key": "75:17",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ عَلَيۡنَا جَمۡعَهُۥ وَقُرۡءَانَهُۥ",
      "translation": "ߓߊߏ߬ ߊ߬ ߟߊߘߍ߭ ߒ߬ߠߎ߫ ߟߋ߫ ߡߵߊ߬ ߣߴߊ߬ ߘߐߛߊߙߌ ߓߍ߯",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 577,
      "verse_key": "75:18",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِذَا قَرَأۡنَٰهُ فَٱتَّبِعۡ قُرۡءَانَهُۥ",
      "translation": "ߒ߬ߓߵߊ߲ ߓߊ߯ ߞߍ߫ ߊ߬ ߝߐ߫ ߟߊ ( ߌ ߦߋ߫ ) ߌ ߦߴߊ߬ ߝߐߟߌ ߣߐ߬ߡߌ߬ߣߊ߬ ( ߒ߬ ߞߐ߫ )",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 577,
      "verse_key": "75:19",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ إِنَّ عَلَيۡنَا بَيَانَهُۥ",
      "translation": "ߊ߬ ߘߐߞߣߍ ( ߌ ߡߊ߬߸ ߏ߬ ߦߋ߫ ) ߒ߬ߠߎ߫ ߟߋ߫ ߡߊ߬ ߏ߬ ߞߐ߫ ߛߊ߫",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 578,
      "verse_key": "75:20",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّا بَلۡ تُحِبُّونَ ٱلۡعَاجِلَةَ",
      "translation": "ߋ߬ߋ߬ߋ߯߹ ߊߟߎ߫ ߦߋ߫ ߞߌߢߍ߲߫ ߠߋ߬ ߘߐߙߐ߲߫ ߝߍ߬ ߞߟߋ߫",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 578,
      "verse_key": "75:21",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَتَذَرُونَ ٱلۡأٓخِرَةَ",
      "translation": "ߞߊ߬ ߞߐߟߊ߫ ߕߏ߫ ߦߋ߲߬ ( ߣߊߟߌ߬ߡߎ߲߫ ߛߎ߬ߟߊ߫ ߘߌ߫ )",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 578,
      "verse_key": "75:22",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وُجُوهٞ يَوۡمَئِذٖ نَّاضِرَةٌ",
      "translation": "ߢߘߊ ߘߏ߫ ߟߎ߫ ߡߊߣߍߡߊߣߍ߲߫ ߏ߬ ߟߏ߲",
      "footnotes": ""
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 578,
      "verse_key": "75:23",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَىٰ رَبِّهَا نَاظِرَةٞ",
      "translation": "ߊ߬ߟߎ߬ ߦߴߊ߬ߟߎ߫ ߡߊ߰ߙߌ ߡߊߝߟߍ ߞߊ߲߬",
      "footnotes": ""
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 578,
      "verse_key": "75:24",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَوُجُوهٞ يَوۡمَئِذِۭ بَاسِرَةٞ",
      "translation": "ߢߘߊ ߘߏ߫ ߟߎ߫ ߝߣߊ߫ ߛߌ߲߬ߛߌ߲߬ߣߍ߲߫ ߏ߬ ߟߏ߲",
      "footnotes": ""
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 578,
      "verse_key": "75:25",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تَظُنُّ أَن يُفۡعَلَ بِهَا فَاقِرَةٞ",
      "translation": "ߏ߬ ߟߎ߬ ߦߴߊ߬ ߖߌ߰ ߟߊ߫ ߞߏ߫ ߞߐߣߍ߯ߙߋ߲߫ ߘߖߐߞߏ ߟߋ߬ ߞߍߕߐ߫ ߊ߬ߟߎ߬ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 578,
      "verse_key": "75:26",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّآ إِذَا بَلَغَتِ ٱلتَّرَاقِيَ",
      "translation": "ߤߊ߯ߟߌ߫ ߸ ߣߌ߫ ߣߌ ߛߋ߫ ߘߊ߫ ߞߊ߲ߥߐߙߐ߲ߞߐ߲ߕߐ߲ ߠߎ߬ ߘߐ߫ ߕߎ߬ߡߊ ߡߍ߲",
      "footnotes": ""
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 578,
      "verse_key": "75:27",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَقِيلَ مَنۡۜ رَاقٖ",
      "translation": "ߘߏ߫ ߦߴߊ߬ ߝߐ߫ ( ߞߎ߲߬ߠߊ߬ߛߌ߰ߓߊ߮ ߕߐ߬ߡߊ ߟߎ߬ ߦߋ߫ ) ߞߏ߫ ߖߐ߲߫ ߘߴߊ߬ ߓߙߐ߫؟",
      "footnotes": ""
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 578,
      "verse_key": "75:28",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَظَنَّ أَنَّهُ ٱلۡفِرَاقُ",
      "translation": "ߊ߬ ߓߘߴߊ߬ ߖߌ߰ ߛߊ߫ ߸ ߞߏ߫ ߝߘߊߟߌ ߟߋ߬ ( ߛߋߣߍ߲߫ )",
      "footnotes": ""
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 578,
      "verse_key": "75:29",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡتَفَّتِ ٱلسَّاقُ بِٱلسَّاقِ",
      "translation": "ߘߊ߬ߡߊ߲߬ߛߊ߫ ߜߍ߬ߟߍ߲ ߓߘߴߊ߬ ߡߙߐߡߙߐ߫ ߢߐ߲߮ ߡߊ߬",
      "footnotes": ""
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 578,
      "verse_key": "75:30",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَىٰ رَبِّكَ يَوۡمَئِذٍ ٱلۡمَسَاقُ",
      "translation": "ߓߍ߯ ߜߍ߲߬ߜߍ߲߬ߕߐ߫ ߏ߬ ߟߏ߲ ߠߋ߬ ߞߊߕߙߍ߬ ߌ ߡߊ߰ߙߌ ߟߋ߫ ߡߊ߬",
      "footnotes": ""
    },
    {
      "id": 31,
      "verse_number": 31,
      "page_number": 578,
      "verse_key": "75:31",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلَا صَدَّقَ وَلَا صَلَّىٰ",
      "translation": "ߊ߬ߟߋ ߘߏ߲߬ ߡߊ߫ ߟߊ߫ ( ߞߋߟߊ ߣߌ߫ ߘߐ߬ߛߊ߬ߙߌ߬ ) ߞߏ߫ ߟߊ߫ ߸ ߊ߬ ߖߘߍ߬ ߡߊ߫ ߛߊߟߌ߫ ߟߋ߬",
      "footnotes": ""
    },
    {
      "id": 32,
      "verse_number": 32,
      "page_number": 578,
      "verse_key": "75:32",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَٰكِن كَذَّبَ وَتَوَلَّىٰ",
      "translation": "ߞߏ߬ߣߵߊ߬ ߞߊ߬ ߡߊ߬ߛߐ߬ߛߐ߬ߟߌ ߞߍ߫ ߊ߬ ߣߌ߫ ߞߵߊ߬ ߞߐߘߏ߲߬",
      "footnotes": ""
    },
    {
      "id": 33,
      "verse_number": 33,
      "page_number": 578,
      "verse_key": "75:33",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ ذَهَبَ إِلَىٰٓ أَهۡلِهِۦ يَتَمَطَّىٰٓ",
      "translation": "ߊ߬ ߓߊ߲߫ ߘߊ߫ ߞߊ߬ ߥߴߊ߬ ߘߏ߲߬ߞߏ ߡߊ߬ ߊ߰ ߕߊ߬ߡߌ߲߬ ߠߴߊ߬ ߢߊ߲߬ߞߊ߲߬ ߠߊ߫",
      "footnotes": ""
    },
    {
      "id": 34,
      "verse_number": 34,
      "page_number": 578,
      "verse_key": "75:34",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَوۡلَىٰ لَكَ فَأَوۡلَىٰ",
      "translation": "ߌ ߕߏ ߊ߬ ߘߐ߫ ߏ߬ ߞߊ߫ ߞߊ߲߫ ߞߵߌ ߕߏ߫ ߊ߬ ߘߐ߫ ߞߊ߬ߣߌ߲߫",
      "footnotes": ""
    },
    {
      "id": 35,
      "verse_number": 35,
      "page_number": 578,
      "verse_key": "75:35",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ أَوۡلَىٰ لَكَ فَأَوۡلَىٰٓ",
      "translation": "ߏ߬ ߞߐ߫ ߝߣߊ߫ ߸ ߌ ߕߏ ߊ߬ ߘߐ߫ ߏ߬ ߞߊ߫ ߞߊ߲߫ ߞߵߌ ߕߏ߫ ߊ߬ ߘߐ߫ ߞߊ߬ߣߌ߲߫",
      "footnotes": ""
    },
    {
      "id": 36,
      "verse_number": 36,
      "page_number": 578,
      "verse_key": "75:36",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَيَحۡسَبُ ٱلۡإِنسَٰنُ أَن يُتۡرَكَ سُدًى",
      "translation": "ߊ߰ ߡߐ߱ ߢߣߊ߫ ߟߋ߫ ߓߊ߬ ߞߏ߫ ߞߵߊ߬ ߕߏ߫ ߊ߬ ߖߍ߬ߘߍߡߊ߬ ߕߋ߲߬ ߝߎ߫؟",
      "footnotes": ""
    },
    {
      "id": 37,
      "verse_number": 37,
      "page_number": 578,
      "verse_key": "75:37",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَمۡ يَكُ نُطۡفَةٗ مِّن مَّنِيّٖ يُمۡنَىٰ",
      "translation": "ߊ߬ ߝߐ߫ ߊ߬ߟߋ߫ ߕߎ߲߬ ߕߍ߫ ߖߌ߫ ߔߌߦߐߔߌߦߐ ߘߌ߫ ߛߖߌ߫ ߟߊߓߐ߲߬ߕߊ ߘߐ߫ ߘߍ߫؟",
      "footnotes": ""
    },
    {
      "id": 38,
      "verse_number": 38,
      "page_number": 578,
      "verse_key": "75:38",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ كَانَ عَلَقَةٗ فَخَلَقَ فَسَوَّىٰ",
      "translation": "ߞߊ߬ ߞߍ߫ ߖߋ߬ߟߌ߬ ߛߌ߲ߓߌ ߘߴߏ߬ ߞߐ߫ ߸ ( ߊߟߊ߫ ) ߞߵߏ߬ ߟߋ ߘߊ߲߫ ߞߵߊ߬ ߟߊߞߢߊ߫",
      "footnotes": ""
    },
    {
      "id": 39,
      "verse_number": 39,
      "page_number": 578,
      "verse_key": "75:39",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَجَعَلَ مِنۡهُ ٱلزَّوۡجَيۡنِ ٱلذَّكَرَ وَٱلۡأُنثَىٰٓ",
      "translation": "ߊ߬ ߣߊ߬ ߘߊ߫ ߝߌ߬ߟߊ߬ߓߊ ߞߴߏ߬ ߘߐ߫ ߸ ߗߍ߬ߡߊ߲ ߣߌ߫ ߡߛߏ߬ߡߊ߲ ߞߊ߬ߣߌ߲߫",
      "footnotes": ""
    },
    {
      "id": 40,
      "verse_number": 40,
      "page_number": 578,
      "verse_key": "75:40",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَيۡسَ ذَٰلِكَ بِقَٰدِرٍ عَلَىٰٓ أَن يُحۡـِۧيَ ٱلۡمَوۡتَىٰ",
      "translation": "ߦߊ߯ߟߊ߫ ߏ߬ ( ߟߎ߬ ) ߞߍߓߊ߮ ߕߴߛߋ߫ ߛߎ߭ ߟߎ߬ ߟߊߞߎߣߎ߲߫ ߠߊ߫ ߓߊ߬؟",
      "footnotes": ""
    }
  ]
