{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 591,
      "verse_key": "86:1",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلسَّمَآءِ وَٱلطَّارِقِ",
      "translation": "ߊ߰ ߒ ߣߌ߫ ߡߐ߲ߕߐ߲ ߕߍ߫ ߸ ߊ߰ ߒ ߣߌ߫ ߛߎߡߐ߬ߡߐ ߕߍ߫",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 591,
      "verse_key": "86:2",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَآ أَدۡرَىٰكَ مَا ٱلطَّارِقُ",
      "translation": "ߡߎ߲߬ ߞߵߌ ߟߊߟߐ߲ߠߌ߲߫ ߛߎߡߐ߬ߡߐ ߟߊ߫؟",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 591,
      "verse_key": "86:3",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلنَّجۡمُ ٱلثَّاقِبُ",
      "translation": "ߒ߬ߓߊ߬ ߏ߬ ߦߋ߫ ߟߏ߬ߟߏ߫ ( ߘߓߌ߬ ) ߘߐ߬ߛߊ߲߬ߠߊ ߟߋ߬ ߘߌ߫",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 591,
      "verse_key": "86:4",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِن كُلُّ نَفۡسٖ لَّمَّا عَلَيۡهَا حَافِظٞ",
      "translation": "ߣߌߡߊ߫ ߛߌ߫ ߕߴߦߋ߲߬ ߸ ߝߏ߫ ߞߣߐ߬ߜߍ߲߬ߓߊ߯ ߦߋ߫ ߞߍ߫ ߊ߬ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 591,
      "verse_key": "86:5",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلۡيَنظُرِ ٱلۡإِنسَٰنُ مِمَّ خُلِقَ",
      "translation": "ߒ߬ߓߊ߬ ߡߐ߱ ߦߴߊ߬ ߝߟߍ߫ ߞߍ߬ ߸ ߊ߬ ߘߊ߲ߣߍ߲߫ ߝߋ߲ ߡߍ߲ ߘߐ߫",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 591,
      "verse_key": "86:6",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "خُلِقَ مِن مَّآءٖ دَافِقٖ",
      "translation": "ߊ߬ ߘߊ߲ߣߍ߲߫ ߖߌ߫ ߔߎߦߌߔߎߦߌߕߊ ߟߋ߬ ߘߐ߫",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 591,
      "verse_key": "86:7",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَخۡرُجُ مِنۢ بَيۡنِ ٱلصُّلۡبِ وَٱلتَّرَآئِبِ",
      "translation": "ߏ߬ ߦߋ߫ ߓߐ߫ ߟߊ߫ ߛߏ߬ߙߏ ߣߌ߫ ߛߌߛߌߞߟߏ ߟߎ߫ ߟߋ߬ ߕߍ߫",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 591,
      "verse_key": "86:8",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُۥ عَلَىٰ رَجۡعِهِۦ لَقَادِرٞ",
      "translation": "‹ ߊߟߊ߫ › ߛߋߣߍ߲߫ ߏ ߣߐ߬ߡߊߛߊ߬ߦߌ߬ ߟߊ߫ ( ߞߎߘߊ߫ ߞߍ߫ ) ߘߋ߬",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 591,
      "verse_key": "86:9",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَوۡمَ تُبۡلَى ٱلسَّرَآئِرُ",
      "translation": "ߜߎ߲߬ߘߎ ߟߎ߬ ߝߊߙߊ߲ߛߊ߫ ߟߏ߲ ߘߐ߫ ߞߊ߬ߣߌ߲߫",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 591,
      "verse_key": "86:10",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَمَا لَهُۥ مِن قُوَّةٖ وَلَا نَاصِرٖ",
      "translation": "ߏ ߕߎ߬ߡߊ ߝߊ߲߬ߞߊ߬ ߣߌ߫ ߘߍ߬ߡߍ߲߬ߓߊ߯ ߛߌ߫ ߕߍ߫ ߊ߬ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 591,
      "verse_key": "86:11",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلسَّمَآءِ ذَاتِ ٱلرَّجۡعِ",
      "translation": "ߊ߰ ߒ ߣߌ߫ ߡߐ߲ߕߐ߲߫ ( ߛߊ߲ߖߌ߫ ) ߡߛߊ߬ߦߌ߬ߛߊ߬ߦߌ߬ߟߊ ߕߍ߫",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 591,
      "verse_key": "86:12",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡأَرۡضِ ذَاتِ ٱلصَّدۡعِ",
      "translation": "ߊ߰ ߒ ߣߌ߫ ( ߛߣߍ߬ ) ߘߎ߯ ߛߍ߲ߞߍߟߊ ߕߍ߫ ( ߝߍߘߍ߲ߝߋ߲ ߠߎ߬ ߘߌ߫ )",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 591,
      "verse_key": "86:13",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُۥ لَقَوۡلٞ فَصۡلٞ",
      "translation": "‹ ߘߐ߬ߛߊߙߌ › ߦߋ߫ ߞߎߡߊ߫ ߝߊߙߊ߲ߝߊߛߌߣߍ߲ ߠߋ߬ ߘߌ߫ ߘߋ߬",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 591,
      "verse_key": "86:14",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا هُوَ بِٱلۡهَزۡلِ",
      "translation": "ߊ߬ ߕߍ߫ ߡߣߊ߬ߡߣߊ߬ ( ߞߊ߲߫ ) ߘߌ߫ ߞߏߦߌ߫",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 591,
      "verse_key": "86:15",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُمۡ يَكِيدُونَ كَيۡدٗا",
      "translation": "ߊ߬ߟߎ߬ ߦߋ߫ ߝߙߍ߬ߞߍ߫ ߝߍ߬ߙߍ ߘߏ߫ ߟߊ߫ ߝߛߊߦߌ߫",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 591,
      "verse_key": "86:16",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَكِيدُ كَيۡدٗا",
      "translation": "ߒ ߝߣߊ߫ ߦߋ߫ ߝߙߍ߬ߞߍ߫ ߝߍ߬ߙߍ ߘߏ߫ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 591,
      "verse_key": "86:17",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَمَهِّلِ ٱلۡكَٰفِرِينَ أَمۡهِلۡهُمۡ رُوَيۡدَۢا",
      "translation": "ߒ߬ߓߊ߬ ߌߟߋ ߦߋ߫ ߓߊ߲߬ߓߊ߮ ߟߎ߬ ߡߊߕߡߊߣߊ߫ ߡߎߣߎ߲߬ ، ߌ ߦߴߊ߬ߟߎ߬ ߡߊߕߡߊߣߊ߫ ߘߏߣߍ߲ߘߏߣߍ߲߫ ߗߐ߫",
      "footnotes": ""
    }
  ]
}