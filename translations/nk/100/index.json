{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 599,
      "verse_key": "100:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡعَٰدِيَٰتِ ضَبۡحٗا",
      "translation": "ߊ߰ ߒ ߣߌ߫ ߛߏ߬ ߓߏ߬ߙߌ߬ߟߊ߬ߓߊ ߟߎ߬ ߕߍ߫ ߞߣߐ-ߘߐ-ߞߊ߲ ߘߌ߫ ߤߙߌߓߊߤߙߌߓߊ߫",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 599,
      "verse_key": "100:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَٱلۡمُورِيَٰتِ قَدۡحٗا",
      "translation": "ߊ߰ ߒ ߣߌ߫ ߛߏ߬ ߕߐߙߐ߲-ߕߊߕߍ߯ߟߊ ߟߎ߬ ߕߍ߫ ߞߊ߲߬ߞߙߎ߬ߜߍ ߘߐ߫ ߗߊߙߏ߫",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 599,
      "verse_key": "100:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَٱلۡمُغِيرَٰتِ صُبۡحٗا",
      "translation": "ߊ߰ ߒ ߣߌ߫ ߝߌ߬ߙߌ߲߬ߕߊ߬ߥߏ߬ ߛߏ ߟߎ߬ ߕߍ߫ ߘߎ߰ߛߊ߬ߜߍ߬ߘߊ ߟߊ߫ ߔߙߌ߫",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 599,
      "verse_key": "100:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَثَرۡنَ بِهِۦ نَقۡعٗا",
      "translation": "ߞߊ߬ ߜߊ߲߬ߜߊ߲ ߘߎ߬ߙߎ߫ ߏ߬ ߟߊ߫ ߞߙߎߕߎߕߎ߫",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 599,
      "verse_key": "100:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَوَسَطۡنَ بِهِۦ جَمۡعًا",
      "translation": "ߞߵߊ߬ߟߎ߫ ߝߟߌ߬ ߖߊ߬ߡߊ ߕߍߡߊ߬ ߔߙߊߎ߫",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 599,
      "verse_key": "100:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ ٱلۡإِنسَٰنَ لِرَبِّهِۦ لَكَنُودٞ",
      "translation": "ߤߊ߲߫ ߸ ߡߐ߱ ߝߌߛߙߌߥߊߟߋ߫ ߊ߬ ߡߊ߰ߙߌ ߡߊ߬ ߟߋ߬ ߘߋ߬",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 599,
      "verse_key": "100:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِنَّهُۥ عَلَىٰ ذَٰلِكَ لَشَهِيدٞ",
      "translation": "ߊ߬ ߖߍ߬ߘߍ ߘߏ߲߬ ߦߴߏ߬ ߛߙߋ ߘߌ߫ ߟߋ߬ ߛߎߋ߫",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 599,
      "verse_key": "100:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِنَّهُۥ لِحُبِّ ٱلۡخَيۡرِ لَشَدِيدٌ",
      "translation": "ߊ߬ ( ߣߌ ) ߜߍ߬ߟߍ߲߬ߡߊ߲߬ߓߊ ߟߋ߫ ߘߏ߲߬ ߦߋ߫ ߤߙߊ ߟߊߘߌߦߊߢߍ ߘߐ߫",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 599,
      "verse_key": "100:9",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "۞ أَفَلَا يَعۡلَمُ إِذَا بُعۡثِرَ مَا فِي ٱلۡقُبُورِ",
      "translation": "ߊ߬ߝߐ߫ ߊ߬ ߡߴߊ߬ ( ߟߊ߫ ߞߏ ߟߊߓߊ߲߫ ߢߊ ) ߟߐ߲߫ ߸ ߣߌ߫ ߡߐ߱ ߟߎ߬ ߟߊߥߟߌ߬ ߘߊ߫ ߕߎ߬ߡߊ ߡߍ߲ ߞߊ߬ ߓߐ߫ ߛߊ߬ߙߌ ߟߎ߬ ߘߐ߫؟",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 600,
      "verse_key": "100:10",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَحُصِّلَ مَا فِي ٱلصُّدُورِ",
      "translation": "ߊ߬ ߣߌ߫ ߞߊ߬ ߛߌߛߌ ߟߎ߬ ߘߐߞߏ ߟߎ߬ ߟߊ߲ߞߣߍߡߊ߫ ( ߡߍ߲ ߦߴߊ߬ ߟߊ߫ ߟߍߙߊ ߞߣߐ߫ )",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 600,
      "verse_key": "100:11",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ رَبَّهُم بِهِمۡ يَوۡمَئِذٖ لَّخَبِيرُۢ",
      "translation": "ߤߋ߫ ߸ ߊ߬ߟߎ߫ ߡߊ߰ߙߌ ߦߴߊߟߎ߫ ߞߟߊ߬ߡߊ߬ߓߊ߯ ߟߋ߬ ߘߌ߫ ߏ߬ ߟߏ߲ ߘߋ߬؟",
      "footnotes": ""
    }
  ]
}