{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 590,
      "verse_key": "85:1",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلسَّمَآءِ ذَاتِ ٱلۡبُرُوجِ",
      "translation": "ߊ߰ ߒ ߣߌ߫ ߛߊ߲ ߛߘߍߟߏߟߏ߫ ߛߌߦߊߡߊ߲ߕߌ߮ ߕߍ߫",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 590,
      "verse_key": "85:2",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡيَوۡمِ ٱلۡمَوۡعُودِ",
      "translation": "ߊ߰ ߒ ߣߌ߫ ߟߏ߲߫ ߘߐߛߘߊߣߍ߲ ߕߍ߫ ( ߖߊ߬ߕߋ߬ߓߐ߬ ߞߊߡߊ߬ )",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 590,
      "verse_key": "85:3",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَشَاهِدٖ وَمَشۡهُودٖ",
      "translation": "ߊ߰ ߒ ߣߌ߫ ߛߙߋߦߊߓߊ߮ ߣߌ߫ ߛߙߋߦߊߓߊ߯ߕߐ ߕߍ߫",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 590,
      "verse_key": "85:4",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قُتِلَ أَصۡحَٰبُ ٱلۡأُخۡدُودِ",
      "translation": "ߝߏߟߏ߲߫ ߞߎߘߎ߲ߕߌ߮ ߟߎ߬ ߘߊ߲ߞߊߣߍ߲߫ ߠߋ߫ ߘߋ߬",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 590,
      "verse_key": "85:5",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلنَّارِ ذَاتِ ٱلۡوَقُودِ",
      "translation": "ߕߊ߫ ( ߝߏߟߏ߲߫ ) ߢߐ߲߬ߛߐ߲߬ߡߊ ߞߊ߬ߣߌ߲߫",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 590,
      "verse_key": "85:6",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذۡ هُمۡ عَلَيۡهَا قُعُودٞ",
      "translation": "ߞߵߊ߬ ߕߘߍ߬ ߊ߬ߟߎ߫ ߛߌ߰ߣߍ߲߫ ߊ߬ ߞߊ߲ߞߎ߲ ߠߊ߫",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 590,
      "verse_key": "85:7",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَهُمۡ عَلَىٰ مَا يَفۡعَلُونَ بِٱلۡمُؤۡمِنِينَ شُهُودٞ",
      "translation": "ߞߵߊ߬ ߕߘߍ߬ ߊ߬ߟߎ߬ ߦߋ߫ ߡߍ߲ ߞߍ ߞߊ߲߬ ߟߊߒߠߦߊߓߊ߮ ߟߎ߬ ߟߊ߫ ߊ߬ߟߎ߬ ߢߊ߯ ߏ߬ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 590,
      "verse_key": "85:8",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا نَقَمُواْ مِنۡهُمۡ إِلَّآ أَن يُؤۡمِنُواْ بِٱللَّهِ ٱلۡعَزِيزِ ٱلۡحَمِيدِ",
      "translation": "ߊ߬ߟߎ߫ ߘߏ߲߬ ߡߊ߫ ߜߏߦߴߊ߬ߟߎ߬ ߦߋ߫ ߞߎ߲߬ ߛߌ߫ ߟߊ߫ ߝߏ߫ ߊ߬ߟߎ߬ ߟߊ ߡߍ߲ ߞߍߣߍ߲߫ ߊߟߊ߫ ߣߌ߬ߣߌ߬ߟߊ߫ ߕߊ߲߬ߘߏ߬ߕߊ ߡߊ߬",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 590,
      "verse_key": "85:9",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِي لَهُۥ مُلۡكُ ٱلسَّمَٰوَٰتِ وَٱلۡأَرۡضِۚ وَٱللَّهُ عَلَىٰ كُلِّ شَيۡءٖ شَهِيدٌ",
      "translation": "ߏ߬ ߡߍ߲ ߕߊ ߦߋ߫ ߛߊ߲ ߠߎ߬ ߣߌ߫ ߘߎ߱ ߡߊ߲߬ߛߊ߬ߦߊ ߘߌ߫ ، ߞߏ ߓߍ߯ ߘߏ߲߬ ߦߋ߫ ߞߍ߫ ߟߴߊߟߊ߫ ߟߋ߬ ߢߣߊ߫",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 590,
      "verse_key": "85:10",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ ٱلَّذِينَ فَتَنُواْ ٱلۡمُؤۡمِنِينَ وَٱلۡمُؤۡمِنَٰتِ ثُمَّ لَمۡ يَتُوبُواْ فَلَهُمۡ عَذَابُ جَهَنَّمَ وَلَهُمۡ عَذَابُ ٱلۡحَرِيقِ",
      "translation": "ߡߍ߲ ߠߎ߫ ߣߊ߬ ߟߊߒߠߦߊߓߊ߮ ߗߍ߬ߡߊ߲ ߣߴߊ߬ ߡߛߏ߬ߡߊ߲ ߠߎ߬ ߞߘߐߓߐ߫ ߟߊ߫ ( ߖߊ߲߰ߞߕߊ ߟߊ߫ )߸ ߊ߬ߟߎ߫ ߘߏ߲߬ ߡߴߊ߬ߟߎ߫ ߖߘߍ߬ߟߊߛߊ߬ߦߌ߲߬ ߜߘߍߞߍ߫ ߸ ߔߊߦߌߔߊߦߌ ߖߊ߲߰ߞߕߊ ߟߴߏ߬ ߟߎ߬ ߦߋ߫ ߊ߬ ߣߌ߫ ߖߋ߬ߣߌ߲߬ߠߌ߲ ߖߊ߲߰ߞߕߊ ߦߴߊ߬ߟߎ߬ ߦߋ߫",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 590,
      "verse_key": "85:11",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ ٱلَّذِينَ ءَامَنُواْ وَعَمِلُواْ ٱلصَّٰلِحَٰتِ لَهُمۡ جَنَّٰتٞ تَجۡرِي مِن تَحۡتِهَا ٱلۡأَنۡهَٰرُۚ ذَٰلِكَ ٱلۡفَوۡزُ ٱلۡكَبِيرُ",
      "translation": "ߡߍ߲ ߠߎ߫ ߞߏ߬ߣߌ߲ ߞߍߣߍ߲߫ ߟߊߒߠߦߊߓߊ߮ ߟߎ߬ ߘߌ߫ ߞߊ߬ ߢߌ߬ߡߊ ߟߎ߬ ߞߍ߫ ߡߍ߲ߘߌߦߊ ߟߎ߫ ߟߴߏ߬ ߟߎ߬ ߦߋ߫ ، ߞߐ߬ߢߊ ߟߎ߬ ߦߋ߫ ߥߦߏ߬ ߟߴߏ߬ ߟߎ߬ ߞߘߐߟߊ ߟߎ߬ ߘߐ߫ ߸ ߏ߬ ߘߏ߲߬ ߦߋ߫ ߣߌ߬ߓߊ߫ ߓߟߋ߬ߓߟߋ߫ ߟߋ߬ ߘߌ߫",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 590,
      "verse_key": "85:12",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ بَطۡشَ رَبِّكَ لَشَدِيدٌ",
      "translation": "ߤߊ߲߫ ߸ ߌ ߡߊ߰ߙߌ ߟߊ߫ ߡߌ߬ߘߊ߬ߟߌ ߜߍ߬ߟߍ߲߬ߡߊ߲߫ ߠߋ߫ ߘߋ߬",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 590,
      "verse_key": "85:13",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُۥ هُوَ يُبۡدِئُ وَيُعِيدُ",
      "translation": "ߊ߬ߟߋ ߞߊ߬ߣߌ߲ ߸ ߊ߬ߟߋ ߟߋ߬ ߟߊ߬ߝߟߐ߬ߟߌ ߞߍ߫ ߟߊ߫ ߞߊ߬ ߣߐ߬ߡߛߊ߬ߦߌ߬ߟߌ ߝߣߊ߫ ߞߍ߫",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 590,
      "verse_key": "85:14",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَهُوَ ٱلۡغَفُورُ ٱلۡوَدُودُ",
      "translation": "ߊ߬ ߣߴߊ߬ߟߋ ߟߋ߫ ߡߊ߬ߞߕߏ߬ߟߌ߬ߟߊ߫ ߞߊ߬ߣߌ߲߬-ߘߌ߬ߦߊ߬ߘߏ߲߬ߓߊ߮ ߘߌ߫",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 590,
      "verse_key": "85:15",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ذُو ٱلۡعَرۡشِ ٱلۡمَجِيدُ",
      "translation": "ߛߍ߬ߡߍ ߕߌ߱ ߟߋ߬ ߸ ߡߌ߬ߙߌ߲߬ߡߊ ߟߋ߬",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 590,
      "verse_key": "85:16",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَعَّالٞ لِّمَا يُرِيدُ",
      "translation": "ߖߘߍ߬ߛߊ߬ߥߏ߬ ߞߍ߬ߟߊ ߟߋ߬",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 590,
      "verse_key": "85:17",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "هَلۡ أَتَىٰكَ حَدِيثُ ٱلۡجُنُودِ",
      "translation": "ߞߏ߬ߞߏ߫ ߞߟߍ߬ߓߏߟߏ߲ ߠߎ߫ ߓߊ߬ߘߏ ߣߊ߬ ߘߴߌ ߡߊ߬ ߞߍ߬؟",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 590,
      "verse_key": "85:18",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِرۡعَوۡنَ وَثَمُودَ",
      "translation": "ߝߙߊߥߎ߲߫ ߠߎ߫ ߣߌ߫ ߛߊ߬ߡߎߘߊ ߟߎ߫ ߞߊ߬ߣߌ߲߫",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 590,
      "verse_key": "85:19",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بَلِ ٱلَّذِينَ كَفَرُواْ فِي تَكۡذِيبٖ",
      "translation": "ߊ߬ߦߌ߫ ߹ ߓߊ߲߬ߓߊ߮ ߟߎ߬ ߦߋ߫ ߡߊ߬ߛߐ߬ߛߐ߬ߟߌ ߟߋ߬ ߘߐ߫",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 590,
      "verse_key": "85:20",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱللَّهُ مِن وَرَآئِهِم مُّحِيطُۢ",
      "translation": "ߊߟߊ߫ ߘߏ߲߬ ߣߵߊ߬ߟߎ߬ ߟߊߝߏ߬ ߟߴߊ߬ߟߎ߬ ߞߕߐ߫ ߟߋ߬",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 590,
      "verse_key": "85:21",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بَلۡ هُوَ قُرۡءَانٞ مَّجِيدٞ",
      "translation": "ߍ߲߬ߍ߲߫ ߸ ( ߌ ߣߊ߬ߣߍ߲߬ ߡߍ߲ ߘߌ߫ ) ߣߌ߲߬ ߞߊ߬ߣߌ߲ ߸ ߘߐ߬ߛߊߙߌ߫ ߡߌ߬ߙߌ߲߬ߡߊ ߟߋ߬",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 590,
      "verse_key": "85:22",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِي لَوۡحٖ مَّحۡفُوظِۭ",
      "translation": "ߊ߬ ߦߋ߫ ߥߟߊ߬ ߟߊߕߊ߲߬ߞߊ߬ߣߍ߲߫ ߠߋ߬ ߝߣߊ߫ ߞߣߐ߫",
      "footnotes": ""
    }
  ]
}