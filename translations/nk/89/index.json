{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 593,
      "verse_key": "89:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡفَجۡرِ",
      "translation": "ߊ߰ ߒ ߣߌ߫ ߖߐ߲߬ߓߏ߬ߙߌ߬ߘߓߌ ߕߍ߫",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 593,
      "verse_key": "89:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَيَالٍ عَشۡرٖ",
      "translation": "ߊ߰ ߒ ߣߌ߫ ߛߎ߫ ߕߊ߲ ߕߍ߫",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 593,
      "verse_key": "89:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلشَّفۡعِ وَٱلۡوَتۡرِ",
      "translation": "ߊ߰ ߒ ߣߌ߫ ߘߊߓߍ߲ ߕߍ߫ ߸ ߊ߰ ߒ ߣߌ߫ ߦߙߋߞߋ ߕߍ߫",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 593,
      "verse_key": "89:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّيۡلِ إِذَا يَسۡرِ",
      "translation": "ߊ߰ ߒ ߣߌ߫ ߛߎ ߕߊ߯ߕߐ ߕߍ߫",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 593,
      "verse_key": "89:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "هَلۡ فِي ذَٰلِكَ قَسَمٞ لِّذِي حِجۡرٍ",
      "translation": "ߊ߬ ߝߐ߫ ߞߊ߬ߟߌ߬ߟߌ߫ ߟߴߏ߬ ߘߐ߫ ߦߟߌߡߊ ߦߋ߫ ߓߊ߬؟",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 593,
      "verse_key": "89:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَمۡ تَرَ كَيۡفَ فَعَلَ رَبُّكَ بِعَادٍ",
      "translation": "ߌ ߡߴߊ߬ ߦߋ߫ ߓߊ߬ ߌ ߡߊ߰ߙߌ ߞߊ߬ ߡߍ߲ ߞߍ߫ ߊ߯ߘߌ ߟߎ߬ ߟߊ߫؟",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 593,
      "verse_key": "89:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِرَمَ ذَاتِ ٱلۡعِمَادِ",
      "translation": "ߌߙߊߡߎ߲߫ ߛߡߊ߬ߛߋ߲߬ ߛߌ߬ߦߊ߬ߡߊ߲߬ߕߌ߮ ߟߎ߫ ߞߊ߬ߣߌ߲߫",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 593,
      "verse_key": "89:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّتِي لَمۡ يُخۡلَقۡ مِثۡلُهَا فِي ٱلۡبِلَٰدِ",
      "translation": "ߏ߬ ߟߎ߬ ߡߍ߲ ߠߎ߫ ߢߐ߲߰ ߡߊ߫ ߘߊ߲߫ ߖߡߊ߬ߣߊ ߟߎ߬ ߞߣߐ߫",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 593,
      "verse_key": "89:9",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَثَمُودَ ٱلَّذِينَ جَابُواْ ٱلصَّخۡرَ بِٱلۡوَادِ",
      "translation": "ߊ߬ ߣߌ߫ ߛߊ߬ߡߎߘߊ ߡߍ߲ ߠߎ߫ ߞߊ߬ ߞߓߊ ߡߊߟߛߍ߫ ߝߏ߲ߝߊ߲ ߞߣߐ߫",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 593,
      "verse_key": "89:10",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَفِرۡعَوۡنَ ذِي ٱلۡأَوۡتَادِ",
      "translation": "ߊ߬ ߣߌ߫ ߝߙߊߥߎ߲߫ ߓߟߐ߬ ߛߌ߬ߦߊ߬ߡߊ߲߬ߕߌ߮",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 593,
      "verse_key": "89:11",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِينَ طَغَوۡاْ فِي ٱلۡبِلَٰدِ",
      "translation": "ߏ߬ ߟߎ߬ ߡߍ߲ ߠߎ߫ ߞߵߊ߬ߟߎ߫ ߘߊ߲߬ߠߊߕߊ߬ߡߌ߲߫ ߖߡߊ߬ߣߊ ߟߎ߬ ߞߣߐ߫",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 593,
      "verse_key": "89:12",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَكۡثَرُواْ فِيهَا ٱلۡفَسَادَ",
      "translation": "ߊ߬ ߣߴߊ߬ߟߎ߫ ߞߊ߬ ߕߌߢߍߟߌ ߟߊߛߌߦߊߦߊ߫ ߦߋ߲߬ ߠߎ߬ ߘߐ߫",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 593,
      "verse_key": "89:13",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَصَبَّ عَلَيۡهِمۡ رَبُّكَ سَوۡطَ عَذَابٍ",
      "translation": "ߌ ߡߊ߰ߙߌ ߞߊ߬ ߖߊ߲߰ߞߕߊ ߦߌߟߡߊ ߟߊߛߏ߰ ߊ߬ߟߎ߫ ߞߊ߲߬ ߏ߬ ߟߋ ߘߐ߫",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 593,
      "verse_key": "89:14",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ رَبَّكَ لَبِٱلۡمِرۡصَادِ",
      "translation": "ߓߊ ߌ ߡߊ߰ߙߌ ߦߋ߫ ߜߟߊ߬ߟߊ߬ߛߌ߰ ߦߙߐ ߘߐ߫ ߟߋ߫ ߘߋ߬",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 593,
      "verse_key": "89:15",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَمَّا ٱلۡإِنسَٰنُ إِذَا مَا ٱبۡتَلَىٰهُ رَبُّهُۥ فَأَكۡرَمَهُۥ وَنَعَّمَهُۥ فَيَقُولُ رَبِّيٓ أَكۡرَمَنِ",
      "translation": "ߒ߬ߓߊ߬ ߡߐ߰ߣߍ߲ ߸ ߣߴߊ߬ ߡߊ߰ߙߌ ߞߵߊ߬ ߓߏ߲߬ߧߊ߫ ߸ ߞߵߊ߬ ߡߊߣߍߡߴߊ߬ ߞߘߐߓߐ ߞߊߡߊ߬ ߸ ߊ߬ ߘߴߊ߬ ߝߐ߫ ߞߏ߫ ߒ ߡߊ߰ߙߌ ߘߏ߲߬ ߓߘߊ߫ ߒ ߓߏ߲߬ߧߊ߬ ( ߢߌ߲߬ ߠߎ߬ ߘߌ߫ ) ߥߍ߫",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 593,
      "verse_key": "89:16",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَمَّآ إِذَا مَا ٱبۡتَلَىٰهُ فَقَدَرَ عَلَيۡهِ رِزۡقَهُۥ فَيَقُولُ رَبِّيٓ أَهَٰنَنِ",
      "translation": "ߒ߬ߓߊ߬ ߣߴߊ߬ ߞߵߊ߬ ߤߊ߬ߙߌ߬ߖߍ߮ ߛߌ߬ߕߌ߫ ߊ߬ ߡߵߊ߬ ߞߘߐߓߐ߫ ߞߊߡߊ߬ ߟߋ߬ ߛߊ߫ ߸ ߊ߬ ߘߴߊ߬ ߝߐ߫ ߞߏ߫ ߒ ߡߊ߰ߙߌ ߘߏ߲߬ ߓߘߊ߫ ߒ ߠߊߝߍߦߊ߫ ( ߣߌ߲߬ ߘߌ߫ ) ߥߍ߫",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 593,
      "verse_key": "89:17",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّاۖ بَل لَّا تُكۡرِمُونَ ٱلۡيَتِيمَ",
      "translation": "ߏ߬ ߕߍ߫ ߞߟߋ߫ ߸ ߊߟߎ߫ ߕߍ߫ ߝߊߙߌߕߊ ߟߊߘߌߦߊ߫ ߟߊ߫ ߟߋ߬",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 593,
      "verse_key": "89:18",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَا تَحَٰٓضُّونَ عَلَىٰ طَعَامِ ٱلۡمِسۡكِينِ",
      "translation": "ߊߟߎ߫ ߘߏ߲߬ ߕߍ߫ ߢߐ߲߮ ߘߐߛߎ߫ ߟߊ߫ ߘߐ߰ߕߐ ߘߡߊߢߌߣߌ߲ ߡߊ߬",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 593,
      "verse_key": "89:19",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَتَأۡكُلُونَ ٱلتُّرَاثَ أَكۡلٗا لَّمّٗا",
      "translation": "ߊ߬ ߣߴߊߟߎ߫ ߖߍ߬ߘߍ ߦߋ߫ ߗߍ ߘߊߥߎ߲߫ ߠߊ߫ ߘߊߥߎ߲ߠߌ߲߫ ߟߊߞߊ߬ߝߏ߬ߣߍ߲ ߠߊ߫ ߞߌߚߌ߫",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 593,
      "verse_key": "89:20",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَتُحِبُّونَ ٱلۡمَالَ حُبّٗا جَمّٗا",
      "translation": "ߊ߬ ߣߴߊߟߎ߫ ߦߋ߫ ߣߊ߲߬ߝߟߏ ߞߊ߬ߣߌ߲߬ ߠߊ߫ ߟߊ߬ߘߌߦߊߢߍ ߘߍ߬ߜߍ ߟߊ߫",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 593,
      "verse_key": "89:21",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّآۖ إِذَا دُكَّتِ ٱلۡأَرۡضُ دَكّٗا دَكّٗا",
      "translation": "ߞߏ߬ߣߌ߲߬ ߤߊ߲߫ ߸ ߣߌ߫ ߘߎ߱ ߘߡߐߢߐ߲ߞߐ߫ ߘߊ߫ ߕߎ߬ߡߊ ߡߍ߲ ߓߘߎߓߘߍ-ߓߘߎߓߘߍ߫",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 593,
      "verse_key": "89:22",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَجَآءَ رَبُّكَ وَٱلۡمَلَكُ صَفّٗا صَفّٗا",
      "translation": "ߊ߬ ߣߌ߫ ߞߵߌ ߡߊ߰ߙߌ ߣߊ߬ ߸ ߊ߬ ߣߌ߫ ߡߟߍ߬ߞߊ ߟߎ߫ ߟߐ߬ߖߟߎ߬ߡߊ߬ߣߍ߲ ߝߙߍߕߍߕߍ߫",
      "footnotes": ""
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 593,
      "verse_key": "89:23",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَجِاْيٓءَ يَوۡمَئِذِۭ بِجَهَنَّمَۚ يَوۡمَئِذٖ يَتَذَكَّرُ ٱلۡإِنسَٰنُ وَأَنَّىٰ لَهُ ٱلذِّكۡرَىٰ",
      "translation": "ߊ߬ ߣߌ߫ ߞߊ߬ ߣߊ߬ ߔߊߦߌߔߊߦߌ ߘߌ߫ ߏ߬ ߟߏ߲ ، ߒ߬ߓߊ߬ ߡߐ߱ ߦߴߊ߬ ߦߟߌߓߌ߬ߟߊ߬ ߟߴߏ߬ ߟߏ߲ ߠߋ߬ ߸ ߏ߬ ߦߟߌߓߌߟߊ ߘߏ߲߬ ߘߌ߫ ߡߎ߲߬ ߓߐ߫ ߊ߬ ߦߋ߫ ߛߊ߫؟",
      "footnotes": ""
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 594,
      "verse_key": "89:24",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَقُولُ يَٰلَيۡتَنِي قَدَّمۡتُ لِحَيَاتِي",
      "translation": "ߊ߬ ߘߴߊ߬ ߞߍ߫ ߊ߬ ߝߐ ߘߌ߫ ( ߣߡߌߛߊ ߡߊ߬ )߸ ߞߏ߫ ߊ߯ߟߊ߲߫ ߥߟߊߞߏ߫ ߒ ߕߎ߲߬ ߦߋ߫ ߢߍߛߌ߯ ߟߊߘߊ߲߫ (ߘߎߢߊ߫ ߘߐ߫ ) ߒ ߠߊ߫ ( ߛߎ߬ߟߊ߬ ) ߢߣߊߡߦߊ ߦߋ߫",
      "footnotes": ""
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 594,
      "verse_key": "89:25",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَيَوۡمَئِذٖ لَّا يُعَذِّبُ عَذَابَهُۥٓ أَحَدٞ",
      "translation": "ߒ߬ߓߊ߬ ߡߐ߰ ߛߌ߫ ߕߍ߫ ߖߊ߲߰ߞߕߴߊ߬ߟߋ߫ ߖߊ߲߰ߞߕߊ߬ߢߊ ߞߊ߲߬ ߏ߬ ߟߏ߲",
      "footnotes": ""
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 594,
      "verse_key": "89:26",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَا يُوثِقُ وَثَاقَهُۥٓ أَحَدٞ",
      "translation": "ߡߐ߰ ߛߌ߫ ߘߏ߲߬ ߕߍ߫ ߛߌ߬ߘߌ߫ ߊ߬ߟߋ߫ ߛߘߌ߬ߢߊ ߞߊ߲߬ ߝߣߊ߫",
      "footnotes": ""
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 594,
      "verse_key": "89:27",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَٰٓأَيَّتُهَا ٱلنَّفۡسُ ٱلۡمُطۡمَئِنَّةُ",
      "translation": "ߤߍ߲߬ ߌߟߋ߬ ߣߌ߫ ߡߝߊߣߍ߲",
      "footnotes": ""
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 594,
      "verse_key": "89:28",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱرۡجِعِيٓ إِلَىٰ رَبِّكِ رَاضِيَةٗ مَّرۡضِيَّةٗ",
      "translation": "ߌ ߘߌ߬ߢߍ߬ߣߍ߲߫ ߟߊߘߌ߬ߢߍ߬ߣߍ߲ ߦߋ߫ ߛߊ߬ߦߌ߲߬ ߌ ߡߊ߰ߙߌ ߡߊ߬ ߟߋ߬ ߞߟߋ߫",
      "footnotes": ""
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 594,
      "verse_key": "89:29",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
