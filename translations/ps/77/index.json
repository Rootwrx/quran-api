{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 580,
      "verse_key": "77:1",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡمُرۡسَلَٰتِ عُرۡفٗا",
      "translation": "په پرله پسې لېږل شوو بادونو باندې قسم.",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 580,
      "verse_key": "77:2",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَٱلۡعَٰصِفَٰتِ عَصۡفٗا",
      "translation": "بيا پر سختو توپاني چلېدونكو باندې.",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 580,
      "verse_key": "77:3",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلنَّٰشِرَٰتِ نَشۡرٗا",
      "translation": "بيا (دورېځو) پرخپروونكو بادونو باندې.",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 580,
      "verse_key": "77:4",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَٱلۡفَٰرِقَٰتِ فَرۡقٗا",
      "translation": "بيا (د حق او باطل) په بېلوونكو.",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 580,
      "verse_key": "77:5",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَٱلۡمُلۡقِيَٰتِ ذِكۡرًا",
      "translation": "بيا د ذكر (قرآن) پر راښكته كوونكو باندې.",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 580,
      "verse_key": "77:6",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عُذۡرًا أَوۡ نُذۡرًا",
      "translation": "چې د عذر يا وېرونې په توګه وي.",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 580,
      "verse_key": "77:7",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّمَا تُوعَدُونَ لَوَٰقِعٞ",
      "translation": "بېشكه څه چې درسره ژمنه كيږي هغه هرومرو پېښېدونكي دي.",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 580,
      "verse_key": "77:8",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِذَا ٱلنُّجُومُ طُمِسَتۡ",
      "translation": "نو كله چې ستوري پيکه شي.",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 580,
      "verse_key": "77:9",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلسَّمَآءُ فُرِجَتۡ",
      "translation": "اوکله چې آسمان څيرې کړل شي.",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 580,
      "verse_key": "77:10",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡجِبَالُ نُسِفَتۡ",
      "translation": "اوغرونه ميده ميده والوزول شي.",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 580,
      "verse_key": "77:11",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلرُّسُلُ أُقِّتَتۡ",
      "translation": "او هر کله چې د رسولانو وخت پوره کړی شي.",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 580,
      "verse_key": "77:12",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لِأَيِّ يَوۡمٍ أُجِّلَتۡ",
      "translation": "د كومې ورځې لپاره (دا كار) ځنډول شوى دى؟",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 580,
      "verse_key": "77:13",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لِيَوۡمِ ٱلۡفَصۡلِ",
      "translation": "د پريكړې ورځې لپاره.",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 580,
      "verse_key": "77:14",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَآ أَدۡرَىٰكَ مَا يَوۡمُ ٱلۡفَصۡلِ",
      "translation": "او ته څه شي خبركړى يې چې د پريكړې ورځ څه ده؟",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 580,
      "verse_key": "77:15",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ",
      "translation": "تباهي ده په هغه ورځ درواغ ګڼونكو لره.",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 580,
      "verse_key": "77:16",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَمۡ نُهۡلِكِ ٱلۡأَوَّلِينَ",
      "translation": "آيا موږ مخکېني نه دي تباه كړي؟",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 580,
      "verse_key": "77:17",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ نُتۡبِعُهُمُ ٱلۡأٓخِرِينَ",
      "translation": "بيا به وروستني هم ورپسې كړو.",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 580,
      "verse_key": "77:18",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَذَٰلِكَ نَفۡعَلُ بِٱلۡمُجۡرِمِينَ",
      "translation": "دغسې کار کوو موږ د مجرمانو سره.",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 580,
      "verse_key": "77:19",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ",
      "translation": "په هغه ورځ درواغ ګڼونكو لپاره تباهي ده.",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 581,
      "verse_key": "77:20",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَمۡ نَخۡلُقكُّم مِّن مَّآءٖ مَّهِينٖ",
      "translation": "آيا تاسې مو له سپكو (خوارو) اوبو څخه نه يئ پيدا كړي؟",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 581,
      "verse_key": "77:21",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَجَعَلۡنَٰهُ فِي قَرَارٖ مَّكِينٍ",
      "translation": "چې هغه مو بيا يو خوندي ځاى كې وساتلې.",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 581,
      "verse_key": "77:22",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَىٰ قَدَرٖ مَّعۡلُومٖ",
      "translation": "تر يوې ټاكلې مودې پورې.",
      "footnotes": ""
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 581,
      "verse_key": "77:23",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَقَدَرۡنَا فَنِعۡمَ ٱلۡقَٰدِرُونَ",
      "translation": "بيا مو اندازه كړ نو (ګوره) موږ ښه اندازه كوونكي يو.",
      "footnotes": ""
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 581,
      "verse_key": "77:24",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ",
      "translation": "تباهي ده په هغه ورځ د درواغ ګڼونكو لپاره.",
      "footnotes": ""
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 581,
      "verse_key": "77:25",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَمۡ نَجۡعَلِ ٱلۡأَرۡضَ كِفَاتًا",
      "translation": "آيا موږ ځمكه راغونډېدونكې نه ده ګرځولې؟",
      "footnotes": ""
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 581,
      "verse_key": "77:26",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَحۡيَآءٗ وَأَمۡوَٰتٗا",
      "translation": "د ژوندويو او مړيو لپاره.",
      "footnotes": ""
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 581,
      "verse_key": "77:27",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَجَعَلۡنَا فِيهَا رَوَٰسِيَ شَٰمِخَٰتٖ وَأَسۡقَيۡنَٰكُم مَّآءٗ فُرَاتٗا",
      "translation": "او په هغې كې مو لوړ لوړ غرونه پيدا كړي او تاسې ته مو خوږې اوبه درڅښلې دي.",
      "footnotes": ""
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 581,
      "verse_key": "77:28",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ",
      "translation": "درواغ ګڼونكو لره په هغه ورځ تباهي ده.",
      "footnotes": ""
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 581,
      "verse_key": "77:29",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱنطَلِقُوٓاْ إِلَىٰ مَا كُنتُم بِهِۦ تُكَذِّبُونَ",
      "translation": "اوس ځئ د هغه شي لور ته چې تاسي درواغ ګڼلو.",
      "footnotes": ""
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 581,
      "verse_key": "77:30",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱنطَلِقُوٓاْ إِلَىٰ ظِلّٖ ذِي ثَلَٰثِ شُعَبٖ",
      "translation": "روان شئ دريو څانګو والا سيوري خوا ته.",
      "footnotes": ""
    },
    {
      "id": 31,
      "verse_number": 31,
      "page_number": 581,
      "verse_key": "77:31",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَّا ظَلِيلٖ وَلَا يُغۡنِي مِنَ ٱللَّهَبِ",
      "translation": "چې هغه نه يخوونكى او نه د لمبو مخنيوونكى دى.",
      "footnotes": ""
    },
    {
      "id": 32,
      "verse_number": 32,
      "page_number": 581,
      "verse_key": "77:32",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهَا تَرۡمِي بِشَرَرٖ كَٱلۡقَصۡرِ",
      "translation": "هغه د ماڼيو غوندې (لوى) بڅركي غورځوي.",
      "footnotes": ""
    },
    {
      "id": 33,
      "verse_number": 33,
      "page_number": 581,
      "verse_key": "77:33",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَأَنَّهُۥ جِمَٰلَتٞ صُفۡرٞ",
      "translation": "لكه چې هغه زېړ توربخن اوښان وي.",
      "footnotes": ""
    },
    {
      "id": 34,
      "verse_number": 34,
      "page_number": 581,
      "verse_key": "77:34",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ",
      "translation": "تباهي ده په هغه ورځ درواغ ګڼونكو لره.",
      "footnotes": ""
    },
    {
      "id": 35,
      "verse_number": 35,
      "page_number": 581,
      "verse_key": "77:35",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "هَٰذَا يَوۡمُ لَا يَنطِقُونَ",
      "translation": "داهسې ورځ ده چې هغوى به څه نه شي ويلاى.",
      "footnotes": ""
    },
    {
      "id": 36,
      "verse_number": 36,
      "page_number": 581,
      "verse_key": "77:36",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَا يُؤۡذَنُ لَهُمۡ فَيَعۡتَذِرُونَ",
      "translation": "او نه به اجازه وركولى شي چې څه عذر وړاندې كړي.",
      "footnotes": ""
    },
    {
      "id": 37,
      "verse_number": 37,
      "page_number": 581,
      "verse_key": "77:37",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ",
      "translation": "تباهي ده په هغه ورځ درواغ ګڼونكو لره.",
      "footnotes": ""
    },
    {
      "id": 38,
      "verse_number": 38,
      "page_number": 581,
      "verse_key": "77:38",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "هَٰذَا يَوۡمُ ٱلۡفَصۡلِۖ جَمَعۡنَٰكُمۡ وَٱلۡأَوَّلِينَ",
      "translation": "همدا د پريكړې ورځ ده، تاسي او پخواني مو (ګرد) راټول كړي ياست.",
      "footnotes": ""
    },
    {
      "id": 39,
      "verse_number": 39,
      "page_number": 581,
      "verse_key": "77:39",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِن كَانَ لَكُمۡ كَيۡدٞ فَكِيدُونِ",
      "translation": "اوس كه څه چل لرئ نو جوړ يې كړئ.",
      "footnotes": ""
    },
    {
      "id": 40,
      "verse_number": 40,
      "page_number": 581,
      "verse_key": "77:40",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ",
      "translation": "تباهى ده په هغه ورځ د درواغ ګڼونكو لپاره.",
      "footnotes": ""
    },
    {
      "id": 41,
      "verse_number": 41,
      "page_number": 581,
      "verse_key": "77:41",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ ٱلۡمُتَّقِينَ فِي ظِلَٰلٖ وَعُيُونٖ",
      "translation": "بېشكه متقيان (ځان ساتونکي) به په سيورو او چينو كې وي.",
      "footnotes": ""
    },
    {
      "id": 42,
      "verse_number": 42,
      "page_number": 581,
      "verse_key": "77:42",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَفَوَٰكِهَ مِمَّا يَشۡتَهُونَ",
      "translation": "او له هغو مېوو څخه چې هغوى يې غواړي.",
      "footnotes": ""
    },
    {
      "id": 43,
      "verse_number": 43,
      "page_number": 581,
      "verse_key": "77:43",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كُلُواْ وَٱشۡرَبُواْ هَنِيٓـَٔۢا بِمَا كُنتُمۡ تَعۡمَلُونَ",
      "translation": "خورئ او څښئ په خوند سره د هغو عملونو په بدله كې چې تاسي به كول.",
      "footnotes": ""
    },
    {
      "id": 44,
      "verse_number": 44,
      "page_number": 581,
      "verse_key": "77:44",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّا كَذَٰلِكَ نَجۡزِي ٱلۡمُحۡسِنِينَ",
      "translation": "بېشكه موږ نېكانو ته همدغه شان بدله وركوو.",
      "footnotes": ""
    },
    {
      "id": 45,
      "verse_number": 45,
      "page_number": 581,
      "verse_key": "77:45",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ",
      "translation": "په هغه ورځ درواغ ګڼونكو لره تباهي ده.",
      "footnotes": ""
    },
    {
      "id": 46,
      "verse_number": 46,
      "page_number": 581,
      "verse_key": "77:46",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كُلُواْ وَتَمَتَّعُواْ قَلِيلًا إِنَّكُم مُّجۡرِمُونَ",
      "translation": "لږه موده خورئ او مزې كوئ په رېښتيا تاسې مجرمان ياست.",
      "footnotes": ""
    },
    {
      "id": 47,
      "verse_number": 47,
      "page_number": 581,
      "verse_key": "77:47",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ",
      "translation": "په هغه ورځ درواغ ګڼونكو لره تباهي ده.",
      "footnotes": ""
    },
    {
      "id": 48,
      "verse_number": 48,
      "page_number": 581,
      "verse_key": "77:48",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا قِيلَ لَهُمُ ٱرۡكَعُواْ لَا يَرۡكَعُونَ",
      "translation": "او كله چې ورته وويل شي (د الله پر وړاندې) ټيټ شئ نو هغوى نه ټيټيږي.",
      "footnotes": ""
    },
    {
      "id": 49,
      "verse_number": 49,
      "page_number": 581,
      "verse_key": "77:49",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ",
      "translation": "تباهي ده په هغه ورځ درواغ ګڼونكو لره.",
      "footnotes": ""
    },
    {
      "id": 50,
      "verse_number": 50,
      "page_number": 581,
      "verse_key": "77:50",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ حَدِيثِۭ بَعۡدَهُۥ يُؤۡمِنُونَ",
      "translation": "نو له دې وروسته په كومه خبره ايمان راوړي؟",
      "footnotes": ""
    }
  ]
}