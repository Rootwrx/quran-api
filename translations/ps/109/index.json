      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قُلۡ يَٰٓأَيُّهَا ٱلۡكَٰفِرُونَ",
      "translation": "ووايه! اې کافرانو!.",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 603,
      "verse_key": "109:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَآ أَعۡبُدُ مَا تَعۡبُدُونَ",
      "translation": "زه هغه نه لمانځم چې تاسي يې لمانځئ.",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 603,
      "verse_key": "109:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَآ أَنتُمۡ عَٰبِدُونَ مَآ أَعۡبُدُ",
      "translation": "او نه تاسي د هغه څه لمانځونکي ياست چې زه يې لمانځم.",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 603,
      "verse_key": "109:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَآ أَنَا۠ عَابِدٞ مَّا عَبَدتُّمۡ",
      "translation": "او نه زه د هغه څه لمانځونکی يم چې تاسو يې لمانځئ.",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 603,
      "verse_key": "109:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَآ أَنتُمۡ عَٰبِدُونَ مَآ أَعۡبُدُ",
      "translation": "او نه تاسي د هغه څه لمانځونکي ياست چې زه يې لمانځم.",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 603,
      "verse_key": "109:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَكُمۡ دِينُكُمۡ وَلِيَ دِينِ",
      "translation": "ستاسو لپاره خپل دين او زما لپاره خپل دين دی.",
      "footnotes": ""
    }
