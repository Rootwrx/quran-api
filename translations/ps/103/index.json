      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡعَصۡرِ",
      "translation": "پر مازديګر قسم.",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
