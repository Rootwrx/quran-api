{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 587,
      "verse_key": "82:1",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذَا ٱلسَّمَآءُ ٱنفَطَرَتۡ",
      "translation": "کله چې آسمان وچوي.",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 587,
      "verse_key": "82:2",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡكَوَاكِبُ ٱنتَثَرَتۡ",
      "translation": "او کله چې ستوري خواره واره شي.",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 587,
      "verse_key": "82:3",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡبِحَارُ فُجِّرَتۡ",
      "translation": "او کله چې دريابونه وبهولی شي.",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 587,
      "verse_key": "82:4",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡقُبُورُ بُعۡثِرَتۡ",
      "translation": "او کله چې قبرونه څېرې کړل شي.",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 587,
      "verse_key": "82:5",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلِمَتۡ نَفۡسٞ مَّا قَدَّمَتۡ وَأَخَّرَتۡ",
      "translation": "بيا به هر څوک پر هغه څه پوه شي چې مخکې يې لېږلي او وروسته يې پرېښې دي.",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 587,
      "verse_key": "82:6",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَٰٓأَيُّهَا ٱلۡإِنسَٰنُ مَا غَرَّكَ بِرَبِّكَ ٱلۡكَرِيمِ",
      "translation": "اې انسانه! ته د خپل عزتمن پالونکي پر وړاندې څه شي په دوکه کې اچولی يې؟",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 587,
      "verse_key": "82:7",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِي خَلَقَكَ فَسَوَّىٰكَ فَعَدَلَكَ",
      "translation": "داسې ذات دی چې پیدا کړی یې ته، نو نیغ قد والا یې کړې.",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 587,
      "verse_key": "82:8",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِيٓ أَيِّ صُورَةٖ مَّا شَآءَ رَكَّبَكَ",
      "translation": "په هره بڼه يې چې غوښتل سره ويې نېښلولې.",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 587,
      "verse_key": "82:9",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّا بَلۡ تُكَذِّبُونَ بِٱلدِّينِ",
      "translation": "هيڅکله نه بلکې تاسي سزا (د قیامت) دروغ ګڼئ.",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 587,
      "verse_key": "82:10",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِنَّ عَلَيۡكُمۡ لَحَٰفِظِينَ",
      "translation": "حال داچې پر تاسو ساتونکې پرېښتې ټاکل شوې دي.",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 587,
      "verse_key": "82:11",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كِرَامٗا كَٰتِبِينَ",
      "translation": "عزتمنې ليکونکې دي.",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 587,
      "verse_key": "82:12",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَعۡلَمُونَ مَا تَفۡعَلُونَ",
      "translation": "ستاسو پر کړو وړو پوهيږي.",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 587,
      "verse_key": "82:13",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ ٱلۡأَبۡرَارَ لَفِي نَعِيمٖ",
      "translation": "پرته له شکه نېکان به په نعمتونو کې وي.",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 587,
      "verse_key": "82:14",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِنَّ ٱلۡفُجَّارَ لَفِي جَحِيمٖ",
      "translation": "او بدان به هرومرو په دوزخ کې وي.",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 587,
      "verse_key": "82:15",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَصۡلَوۡنَهَا يَوۡمَ ٱلدِّينِ",
      "translation": "د بدلې په ورځ به ور ننوځي.",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 587,
      "verse_key": "82:16",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا هُمۡ عَنۡهَا بِغَآئِبِينَ",
      "translation": "او له هغې به پټېدونکي نه وي.",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 587,
      "verse_key": "82:17",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَآ أَدۡرَىٰكَ مَا يَوۡمُ ٱلدِّينِ",
      "translation": "او ته څه خبر يې چې د بدلې ورځ څه ده.",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 587,
      "verse_key": "82:18",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ مَآ أَدۡرَىٰكَ مَا يَوۡمُ ٱلدِّينِ",
      "translation": "بيا ته څه خبر يې چې د بدلې ورځ څه ده.",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 587,
      "verse_key": "82:19",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَوۡمَ لَا تَمۡلِكُ نَفۡسٞ لِّنَفۡسٖ شَيۡـٔٗاۖ وَٱلۡأَمۡرُ يَوۡمَئِذٖ لِّلَّهِ",
      "translation": "هغه ورځ ده چې هيڅوک به د بل چا لپاره څه نه شي کولای او په هغه ورځ به امر يوازې الله لره وي.",
      "footnotes": ""
    }
  ]
}