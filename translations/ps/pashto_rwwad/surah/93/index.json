  "result": [
    {
      "id": "6080",
      "sura": "93",
      "aya": "1",
      "arabic_text": "وَٱلضُّحَىٰ",
      "translation": "قسم دی پر وخت د څاښت باندې.",
      "footnotes": ""
    },
    {
      "id": "6081",
      "sura": "93",
      "aya": "2",
      "arabic_text": "وَٱلَّيۡلِ إِذَا سَجَىٰ",
      "translation": "او پر شپه باندې کله چې وغوړیږي.",
      "footnotes": ""
    },
    {
      "id": "6082",
      "sura": "93",
      "aya": "3",
      "arabic_text": "مَا وَدَّعَكَ رَبُّكَ وَمَا قَلَىٰ",
      "translation": "چې پالونکي دې نه پرېښی یې او نه يې بد راغلی يې.",
      "footnotes": ""
    },
    {
      "id": "6083",
      "sura": "93",
      "aya": "4",
      "arabic_text": "وَلَلۡأٓخِرَةُ خَيۡرٞ لَّكَ مِنَ ٱلۡأُولَىٰ",
      "translation": "او هرو مرو ستا لپاره آخرت تر دنيا غوره دی.",
      "footnotes": ""
    },
    {
      "id": "6084",
      "sura": "93",
      "aya": "5",
      "arabic_text": "وَلَسَوۡفَ يُعۡطِيكَ رَبُّكَ فَتَرۡضَىٰٓ",
      "translation": "او ژر به دې پالونکی دومره څه درکړي چې پرې خوشحاله شې.",
      "footnotes": ""
    },
    {
      "id": "6085",
      "sura": "93",
      "aya": "6",
      "arabic_text": "أَلَمۡ يَجِدۡكَ يَتِيمٗا فَـَٔاوَىٰ",
      "translation": "آيا ته يې يتيم ونه مومندلې چې بيا يې د هستوګنې ځای درکړ.",
      "footnotes": ""
    },
    {
      "id": "6086",
      "sura": "93",
      "aya": "7",
      "arabic_text": "وَوَجَدَكَ ضَآلّٗا فَهَدَىٰ",
      "translation": "او ویې موندلي ته ناخبره نو پوه یې کړې.",
      "footnotes": ""
    },
    {
      "id": "6087",
      "sura": "93",
      "aya": "8",
      "arabic_text": "وَوَجَدَكَ عَآئِلٗا فَأَغۡنَىٰ",
      "translation": "او بې وزله يې وموندلې؛ نو شتمن يې کړې.",
      "footnotes": ""
    },
    {
      "id": "6088",
      "sura": "93",
      "aya": "9",
      "arabic_text": "فَأَمَّا ٱلۡيَتِيمَ فَلَا تَقۡهَرۡ",
      "translation": "نو هر چې یتیم دی غصه پرې مه کوه.",
      "footnotes": ""
    },
    {
      "id": "6089",
      "sura": "93",
      "aya": "10",
      "arabic_text": "وَأَمَّا ٱلسَّآئِلَ فَلَا تَنۡهَرۡ",
      "translation": "او مه سوالګر رټه.",
      "footnotes": ""
    },
    {
      "id": "6090",
      "sura": "93",
      "aya": "11",
      "arabic_text": "وَأَمَّا بِنِعۡمَةِ رَبِّكَ فَحَدِّثۡ",
      "translation": "او د خپل پالونکي د لورېينې ښکارندويي دې کوه.",
      "footnotes": ""
    }
  ]
}