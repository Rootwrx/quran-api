  "result": [
    {
      "id": "6158",
      "sura": "101",
      "aya": "1",
      "arabic_text": "ٱلۡقَارِعَةُ",
      "translation": "(د زړونو) ټکوونکی.",
      "footnotes": ""
    },
    {
      "id": "6159",
      "sura": "101",
      "aya": "2",
      "arabic_text": "مَا ٱلۡقَارِعَةُ",
      "translation": "څه دی (د زړونو) ټکوونکی؟",
      "footnotes": ""
    },
    {
      "id": "6160",
      "sura": "101",
      "aya": "3",
      "arabic_text": "وَمَآ أَدۡرَىٰكَ مَا ٱلۡقَارِعَةُ",
      "translation": "او څه پوهه کړې ته چې څه دی (د زړونو) ټکوونکی؟",
      "footnotes": ""
    },
    {
      "id": "6161",
      "sura": "101",
      "aya": "4",
      "arabic_text": "يَوۡمَ يَكُونُ ٱلنَّاسُ كَٱلۡفَرَاشِ ٱلۡمَبۡثُوثِ",
      "translation": "هغه ورځ ده چې خلک به د خورو ورو پتنګانو غوندې وي.",
      "footnotes": ""
    },
    {
      "id": "6162",
      "sura": "101",
      "aya": "5",
      "arabic_text": "وَتَكُونُ ٱلۡجِبَالُ كَٱلۡعِهۡنِ ٱلۡمَنفُوشِ",
      "translation": "او غرونه به د شيندل شويو رنګه وړيو غوندې وي.",
      "footnotes": ""
    },
    {
      "id": "6163",
      "sura": "101",
      "aya": "6",
      "arabic_text": "فَأَمَّا مَن ثَقُلَتۡ مَوَٰزِينُهُۥ",
      "translation": "نو بيا چې د چا تلې درنې شي.",
      "footnotes": ""
    },
    {
      "id": "6164",
      "sura": "101",
      "aya": "7",
      "arabic_text": "فَهُوَ فِي عِيشَةٖ رَّاضِيَةٖ",
      "translation": "نو هغه به په خوشحاله ژوند کې وي.",
      "footnotes": ""
    },
    {
      "id": "6165",
      "sura": "101",
      "aya": "8",
      "arabic_text": "وَأَمَّا مَنۡ خَفَّتۡ مَوَٰزِينُهُۥ",
      "translation": "او د چا چې تلې سپکې شي.",
      "footnotes": ""
    },
    {
      "id": "6166",
      "sura": "101",
      "aya": "9",
      "arabic_text": "فَأُمُّهُۥ هَاوِيَةٞ",
      "translation": "نو د هغه د ورتګ ځای به ژوره کنده وي.",
      "footnotes": ""
    },
    {
      "id": "6167",
      "sura": "101",
      "aya": "10",
      "arabic_text": "وَمَآ أَدۡرَىٰكَ مَا هِيَهۡ",
      "translation": "او ته څه خبر يې چې هغه څه ده.",
      "footnotes": ""
    },
    {
      "id": "6168",
      "sura": "101",
      "aya": "11",
      "arabic_text": "نَارٌ حَامِيَةُۢ",
      "translation": "سور اور دی.",
      "footnotes": ""
    }
  ]
}