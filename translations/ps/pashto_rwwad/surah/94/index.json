{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 596,
      "verse_key": "94:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَمۡ نَشۡرَحۡ لَكَ صَدۡرَكَ",
      "translation": "آيا سينه مو درته پراخه نه کړه.",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 596,
      "verse_key": "94:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَوَضَعۡنَا عَنكَ وِزۡرَكَ",
      "translation": "او ستا هغه پېټی مو در څخه کوز کړ.",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 596,
      "verse_key": "94:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِيٓ أَنقَضَ ظَهۡرَكَ",
      "translation": "هغه چې ستا ملا يې درنه کړې وه.",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 596,
      "verse_key": "94:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَرَفَعۡنَا لَكَ ذِكۡرَكَ",
      "translation": "او ستا ذکر(ياد) مو درته اوچت کړ.",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 596,
      "verse_key": "94:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِنَّ مَعَ ٱلۡعُسۡرِ يُسۡرًا",
      "translation": "نو بېشکه له سختۍ سره آسانتيا هم شته.",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 596,
      "verse_key": "94:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ مَعَ ٱلۡعُسۡرِ يُسۡرٗا",
      "translation": "بېشکه چې له سختۍ سره آساني هم شته.",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 596,
      "verse_key": "94:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِذَا فَرَغۡتَ فَٱنصَبۡ",
      "translation": "نو کله چې وزګار وې، ځان ستړی کړه.",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 596,
      "verse_key": "94:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِلَىٰ رَبِّكَ فَٱرۡغَب",
      "translation": "او د پالونکي لوري ته دې مخه کړه.",
      "footnotes": ""
    }
  ]
}