{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 534,
      "verse_key": "56:1",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذَا وَقَعَتِ ٱلۡوَاقِعَةُ",
      "translation": "كله چې قيامت راپېښ شي.",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 534,
      "verse_key": "56:2",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَيۡسَ لِوَقۡعَتِهَا كَاذِبَةٌ",
      "translation": "چې پېښېدلو كې يې هيڅ درواغ نشته.",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 534,
      "verse_key": "56:3",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "خَافِضَةٞ رَّافِعَةٌ",
      "translation": "هغه ښكته كوونكى او پورته كوونكى دى.",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 534,
      "verse_key": "56:4",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذَا رُجَّتِ ٱلۡأَرۡضُ رَجّٗا",
      "translation": "كله چې ځمكه په سختو لړزولو ولړزول شي.",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 534,
      "verse_key": "56:5",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَبُسَّتِ ٱلۡجِبَالُ بَسّٗا",
      "translation": "او غرونه يومخ ميده ميده ورژول شي.",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 534,
      "verse_key": "56:6",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَكَانَتۡ هَبَآءٗ مُّنۢبَثّٗا",
      "translation": "نو خوره وره دوړه به شي.",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 534,
      "verse_key": "56:7",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَكُنتُمۡ أَزۡوَٰجٗا ثَلَٰثَةٗ",
      "translation": "او تاسو به درې ډلې شئ.",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 534,
      "verse_key": "56:8",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَصۡحَٰبُ ٱلۡمَيۡمَنَةِ مَآ أَصۡحَٰبُ ٱلۡمَيۡمَنَةِ",
      "translation": "نو د ښي لاس والا خلک، څومره ښه دي د ښي لاس والا خلک!",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 534,
      "verse_key": "56:9",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَصۡحَٰبُ ٱلۡمَشۡـَٔمَةِ مَآ أَصۡحَٰبُ ٱلۡمَشۡـَٔمَةِ",
      "translation": "او د كيڼ (چپ) لاس والا خلک! څومره بد دي د كيڼ (چپ) لاس والا خلک!",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 534,
      "verse_key": "56:10",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلسَّٰبِقُونَ ٱلسَّٰبِقُونَ",
      "translation": "او مخکې کېدونکي خو مخکې کېدونکي دي.",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 534,
      "verse_key": "56:11",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أُوْلَٰٓئِكَ ٱلۡمُقَرَّبُونَ",
      "translation": "همدوى نېږدې كړى شوي دي.",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 534,
      "verse_key": "56:12",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِي جَنَّٰتِ ٱلنَّعِيمِ",
      "translation": "د نعمتونو والا جنتو كې به وي.",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 534,
      "verse_key": "56:13",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُلَّةٞ مِّنَ ٱلۡأَوَّلِينَ",
      "translation": "(د هغوى) لويه ډله به له لومړنيو وي.",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 534,
      "verse_key": "56:14",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَقَلِيلٞ مِّنَ ٱلۡأٓخِرِينَ",
      "translation": "او لږ به له وروستنيو (خلكو) وي.",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 534,
      "verse_key": "56:15",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلَىٰ سُرُرٖ مَّوۡضُونَةٖ",
      "translation": "پر زرينو او بدل شوو تختونو به وي.",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 534,
      "verse_key": "56:16",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مُّتَّكِـِٔينَ عَلَيۡهَا مُتَقَٰبِلِينَ",
      "translation": "پر هغو يو بل ته مخامخ ډډه وهونكي (ناست) وي.",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 535,
      "verse_key": "56:17",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَطُوفُ عَلَيۡهِمۡ وِلۡدَٰنٞ مُّخَلَّدُونَ",
      "translation": "تل (پاتې) هلكان به ورباندې ګرځي راګرځي.",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 535,
      "verse_key": "56:18",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بِأَكۡوَابٖ وَأَبَارِيقَ وَكَأۡسٖ مِّن مَّعِينٖ",
      "translation": "له ګيلاسونو او كوزو سره، او د روانو (بهېدونكو شرابو) له جامونو سره.",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 535,
      "verse_key": "56:19",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَّا يُصَدَّعُونَ عَنۡهَا وَلَا يُنزِفُونَ",
      "translation": "چې نه ترې سر خوږى كيږي او نه بې هوښه كيږي.",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 535,
      "verse_key": "56:20",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَفَٰكِهَةٖ مِّمَّا يَتَخَيَّرُونَ",
      "translation": "او مېوې، له هغو نه چې هغوى يې خوښوي.",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 535,
      "verse_key": "56:21",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَحۡمِ طَيۡرٖ مِّمَّا يَشۡتَهُونَ",
      "translation": "او د مرغانو غوښې، كومې چې زړه يې غواړي.",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 535,
      "verse_key": "56:22",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَحُورٌ عِينٞ",
      "translation": "او د غټو سترګو والا حورې.",
      "footnotes": ""
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 535,
      "verse_key": "56:23",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَأَمۡثَٰلِ ٱللُّؤۡلُوِٕ ٱلۡمَكۡنُونِ",
      "translation": "لكه د پټو ساتل شوو ملغرو غوندې.",
      "footnotes": ""
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 535,
      "verse_key": "56:24",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "جَزَآءَۢ بِمَا كَانُواْ يَعۡمَلُونَ",
      "translation": "د دوى د هغو عملونو په بدله كې چې كول يې.",
      "footnotes": ""
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 535,
      "verse_key": "56:25",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَا يَسۡمَعُونَ فِيهَا لَغۡوٗا وَلَا تَأۡثِيمًا",
      "translation": "نه به چټيات پكې اوري او نه د ګناه څه خبره.",
      "footnotes": ""
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 535,
      "verse_key": "56:26",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَّا قِيلٗا سَلَٰمٗا سَلَٰمٗا",
      "translation": "پرته له سلام، سلام ويلو.",
      "footnotes": ""
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 535,
      "verse_key": "56:27",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَصۡحَٰبُ ٱلۡيَمِينِ مَآ أَصۡحَٰبُ ٱلۡيَمِينِ",
      "translation": "او د ښي لاس والا خلک! څومره ښه دي ښي لاس والا خلک!",
      "footnotes": ""
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 535,
      "verse_key": "56:28",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِي سِدۡرٖ مَّخۡضُودٖ",
      "translation": "په بې اغزيو بېرو كې به وي.",
      "footnotes": ""
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 535,
      "verse_key": "56:29",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَطَلۡحٖ مَّنضُودٖ",
      "translation": "او له بېخه تر سره په ډكو ډكو كيلو كې به وي.",
      "footnotes": ""
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 535,
      "verse_key": "56:30",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَظِلّٖ مَّمۡدُودٖ",
      "translation": "او په اوږده غځېدلي سيوري كې به وي.",
      "footnotes": ""
    },
    {
      "id": 31,
      "verse_number": 31,
      "page_number": 535,
      "verse_key": "56:31",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَآءٖ مَّسۡكُوبٖ",
      "translation": "او را تويېدونكو اوبو كې به وي.",
      "footnotes": ""
    },
    {
      "id": 32,
      "verse_number": 32,
      "page_number": 535,
      "verse_key": "56:32",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَفَٰكِهَةٖ كَثِيرَةٖ",
      "translation": "او ډېرو مېوو كې.",
      "footnotes": ""
    },
    {
      "id": 33,
      "verse_number": 33,
      "page_number": 535,
      "verse_key": "56:33",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَّا مَقۡطُوعَةٖ وَلَا مَمۡنُوعَةٖ",
      "translation": "چې نه ختمېدونكې او نه ترې ايسارېدونكې دي.",
      "footnotes": ""
    },
    {
      "id": 34,
      "verse_number": 34,
      "page_number": 535,
      "verse_key": "56:34",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَفُرُشٖ مَّرۡفُوعَةٍ",
      "translation": "او اوچتو فرشو كې به وي.",
      "footnotes": ""
    },
    {
      "id": 35,
      "verse_number": 35,
      "page_number": 535,
      "verse_key": "56:35",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّآ أَنشَأۡنَٰهُنَّ إِنشَآءٗ",
      "translation": "بېشکه موږ هغه (حورې) په يو ځانګړي پيدايښت سره پيدا كړې دي.",
      "footnotes": ""
    },
    {
      "id": 36,
      "verse_number": 36,
      "page_number": 535,
      "verse_key": "56:36",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَجَعَلۡنَٰهُنَّ أَبۡكَارًا",
      "translation": "او هغوى مو (تل) پېغلې ګرځولې دي.",
      "footnotes": ""
    },
    {
      "id": 37,
      "verse_number": 37,
      "page_number": 535,
      "verse_key": "56:37",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عُرُبًا أَتۡرَابٗا",
      "translation": ".(مېړونو ته) ګرانې همزولې",
      "footnotes": ""
    },
    {
      "id": 38,
      "verse_number": 38,
      "page_number": 535,
      "verse_key": "56:38",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لِّأَصۡحَٰبِ ٱلۡيَمِينِ",
      "translation": "د ښي لاس والا خلكو لپاره.",
      "footnotes": ""
    },
    {
      "id": 39,
      "verse_number": 39,
      "page_number": 535,
      "verse_key": "56:39",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُلَّةٞ مِّنَ ٱلۡأَوَّلِينَ",
      "translation": "یوه لويه ډله له مخکېنيو څخه.",
      "footnotes": ""
    },
    {
      "id": 40,
      "verse_number": 40,
      "page_number": 535,
      "verse_key": "56:40",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَثُلَّةٞ مِّنَ ٱلۡأٓخِرِينَ",
      "translation": "او بله لويه ډله له وروستنيو خلكو څخه.",
      "footnotes": ""
    },
    {
      "id": 41,
      "verse_number": 41,
      "page_number": 535,
      "verse_key": "56:41",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَصۡحَٰبُ ٱلشِّمَالِ مَآ أَصۡحَٰبُ ٱلشِّمَالِ",
      "translation": "او د كيڼ (چپ) لاس والا خلک! څومره بد دي د كيڼ (چپ) لاس والا خلک!",
      "footnotes": ""
    },
    {
      "id": 42,
      "verse_number": 42,
      "page_number": 535,
      "verse_key": "56:42",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِي سَمُومٖ وَحَمِيمٖ",
      "translation": "په توده هوا او خوټېدلو اوبو كې.",
      "footnotes": ""
    },
    {
      "id": 43,
      "verse_number": 43,
      "page_number": 535,
      "verse_key": "56:43",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَظِلّٖ مِّن يَحۡمُومٖ",
      "translation": "او د تور لوګي په سيوري كې به وي.",
      "footnotes": ""
    },
    {
      "id": 44,
      "verse_number": 44,
      "page_number": 535,
      "verse_key": "56:44",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَّا بَارِدٖ وَلَا كَرِيمٍ",
      "translation": "چې نه به سوړ وي او نه عزت راحت والا.",
      "footnotes": ""
    },
    {
      "id": 45,
      "verse_number": 45,
      "page_number": 535,
      "verse_key": "56:45",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُمۡ كَانُواْ قَبۡلَ ذَٰلِكَ مُتۡرَفِينَ",
      "translation": "بېشكه هغوى له دې مخکې نازولي خوشاله وو.",
      "footnotes": ""
    },
    {
      "id": 46,
      "verse_number": 46,
      "page_number": 535,
      "verse_key": "56:46",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَكَانُواْ يُصِرُّونَ عَلَى ٱلۡحِنثِ ٱلۡعَظِيمِ",
      "translation": "او پر لويه ګناه (كفر) به يې ټينګار كولو.",
      "footnotes": ""
    },
    {
      "id": 47,
      "verse_number": 47,
      "page_number": 535,
      "verse_key": "56:47",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَكَانُواْ يَقُولُونَ أَئِذَا مِتۡنَا وَكُنَّا تُرَابٗا وَعِظَٰمًا أَءِنَّا لَمَبۡعُوثُونَ",
      "translation": "او هغوى به ويل: آيا كله چې مړه شو او خاورې هډوكې يو نو آيا بيا به راپورته كېدونكي يو؟",
      "footnotes": ""
    },
    {
      "id": 48,
      "verse_number": 48,
      "page_number": 535,
      "verse_key": "56:48",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَوَءَابَآؤُنَا ٱلۡأَوَّلُونَ",
      "translation": "آيا زموږ مخکني پلرونه هم؟",
      "footnotes": ""
    },
    {
      "id": 49,
      "verse_number": 49,
      "page_number": 535,
      "verse_key": "56:49",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قُلۡ إِنَّ ٱلۡأَوَّلِينَ وَٱلۡأٓخِرِينَ",
      "translation": "ورته ووايه: بېشكه مخکني او وروستني (ټول).",
      "footnotes": ""
    },
    {
      "id": 50,
      "verse_number": 50,
      "page_number": 535,
      "verse_key": "56:50",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَمَجۡمُوعُونَ إِلَىٰ مِيقَٰتِ يَوۡمٖ مَّعۡلُومٖ",
      "translation": "هرومرو د يوې ټاكلې ورځې وخت ته را يو ځاى كېدونكي دي.",
      "footnotes": ""
    },
    {
      "id": 51,
      "verse_number": 51,
      "page_number": 536,
      "verse_key": "56:51",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ إِنَّكُمۡ أَيُّهَا ٱلضَّآلُّونَ ٱلۡمُكَذِّبُونَ",
      "translation": "بيا به هرومرو تاسي اې لار وركو درواغ ګڼونكو!",
      "footnotes": ""
    },
    {
      "id": 52,
      "verse_number": 52,
      "page_number": 536,
      "verse_key": "56:52",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَأٓكِلُونَ مِن شَجَرٖ مِّن زَقُّومٖ",
      "translation": "هرومرو د زقوم له ونې خوراک كوونكي ياست.",
      "footnotes": ""
    },
    {
      "id": 53,
      "verse_number": 53,
      "page_number": 536,
      "verse_key": "56:53",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَمَالِـُٔونَ مِنۡهَا ٱلۡبُطُونَ",
      "translation": "او له هغې به د خپلو ګېډو ډكوونكي ياست.",
      "footnotes": ""
    },
    {
      "id": 54,
      "verse_number": 54,
      "page_number": 536,
      "verse_key": "56:54",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَشَٰرِبُونَ عَلَيۡهِ مِنَ ٱلۡحَمِيمِ",
      "translation": "بيا به پر هغه (خوراک) د خوټېدونكو اوبو څښونكي ياست.",
      "footnotes": ""
    },
    {
      "id": 55,
      "verse_number": 55,
      "page_number": 536,
      "verse_key": "56:55",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَشَٰرِبُونَ شُرۡبَ ٱلۡهِيمِ",
      "translation": "او څښونكي هم د تږو اوښانو غوندي څښاک.",
      "footnotes": ""
    },
    {
      "id": 56,
      "verse_number": 56,
      "page_number": 536,
      "verse_key": "56:56",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "هَٰذَا نُزُلُهُمۡ يَوۡمَ ٱلدِّينِ",
      "translation": "د قيامت په ورځ د هغوى مېلمستيا همدغه ده!",
      "footnotes": ""
    },
    {
      "id": 57,
      "verse_number": 57,
      "page_number": 536,
      "verse_key": "56:57",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "نَحۡنُ خَلَقۡنَٰكُمۡ فَلَوۡلَا تُصَدِّقُونَ",
      "translation": "تاسي (ټول) خو موږ راپيدا كړئ نو ولې باور نه كوئ؟",
      "footnotes": ""
    },
    {
      "id": 58,
      "verse_number": 58,
      "page_number": 536,
      "verse_key": "56:58",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَرَءَيۡتُم مَّا تُمۡنُونَ",
      "translation": "آيا هغه مني ته مو پام كړئ چې تاسي يې څڅوئ؟",
      "footnotes": ""
    },
    {
      "id": 59,
      "verse_number": 59,
      "page_number": 536,
      "verse_key": "56:59",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ءَأَنتُمۡ تَخۡلُقُونَهُۥٓ أَمۡ نَحۡنُ ٱلۡخَٰلِقُونَ",
      "translation": "آيا تاسي يې پيدا كوئ، كه موږ پيدا كوونكي يو؟",
      "footnotes": ""
    },
    {
      "id": 60,
      "verse_number": 60,
      "page_number": 536,
      "verse_key": "56:60",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "نَحۡنُ قَدَّرۡنَا بَيۡنَكُمُ ٱلۡمَوۡتَ وَمَا نَحۡنُ بِمَسۡبُوقِينَ",
      "translation": "(همدا) موږ ستاسو ترمنځ مرګ  ټاكلى او موږ له دې څخه عاجز كېدونكي نه يو.",
      "footnotes": ""
    },
    {
      "id": 61,
      "verse_number": 61,
      "page_number": 536,
      "verse_key": "56:61",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلَىٰٓ أَن نُّبَدِّلَ أَمۡثَٰلَكُمۡ وَنُنشِئَكُمۡ فِي مَا لَا تَعۡلَمُونَ",
      "translation": "چې ستاسو غوندې نور خلک به مو په بدل كې راولو او تاسي به له سره په داسې حالت پيدا كړو چې تاسي نه پرې پوهېږئ.",
      "footnotes": ""
    },
    {
      "id": 62,
      "verse_number": 62,
      "page_number": 536,
      "verse_key": "56:62",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَقَدۡ عَلِمۡتُمُ ٱلنَّشۡأَةَ ٱلۡأُولَىٰ فَلَوۡلَا تَذَكَّرُونَ",
      "translation": "او پرته له شکه تاسو ته خو لومړنی پيدايښت معلوم دى، نو ولې عبرت نه اخلئ؟",
      "footnotes": ""
    },
    {
      "id": 63,
      "verse_number": 63,
      "page_number": 536,
      "verse_key": "56:63",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَرَءَيۡتُم مَّا تَحۡرُثُونَ",
      "translation": "آيا نو د هغه څه خبر به راكړئ چې تاسي يې كرئ؟",
      "footnotes": ""
    },
    {
      "id": 64,
      "verse_number": 64,
      "page_number": 536,
      "verse_key": "56:64",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ءَأَنتُمۡ تَزۡرَعُونَهُۥٓ أَمۡ نَحۡنُ ٱلزَّٰرِعُونَ",
      "translation": "آيا تاسي يې راشنه كوئ كه موږ يې زرغونوونكي يو؟",
      "footnotes": ""
    },
    {
      "id": 65,
      "verse_number": 65,
      "page_number": 536,
      "verse_key": "56:65",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَوۡ نَشَآءُ لَجَعَلۡنَٰهُ حُطَٰمٗا فَظَلۡتُمۡ تَفَكَّهُونَ",
      "translation": "كه موږ وغواړو هرومرو به هغه وچ ټوټه ټوټه كړو بيا به (ټوله ورځ) تعجب سره خبرې كوئ.",
      "footnotes": ""
    },
    {
      "id": 66,
      "verse_number": 66,
      "page_number": 536,
      "verse_key": "56:66",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّا لَمُغۡرَمُونَ",
      "translation": "چې بېشكه موږ تاواني شولو.",
      "footnotes": ""
    },
    {
      "id": 67,
      "verse_number": 67,
      "page_number": 536,
      "verse_key": "56:67",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بَلۡ نَحۡنُ مَحۡرُومُونَ",
      "translation": "بلكې موږ بيخې بې برخې يو.",
      "footnotes": ""
    },
    {
      "id": 68,
      "verse_number": 68,
      "page_number": 536,
      "verse_key": "56:68",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَرَءَيۡتُمُ ٱلۡمَآءَ ٱلَّذِي تَشۡرَبُونَ",
      "translation": "آيا هغه اوبه مو ليدلې چې (تل) يې تاسو څښئ؟",
      "footnotes": ""
    },
    {
      "id": 69,
      "verse_number": 69,
      "page_number": 536,
      "verse_key": "56:69",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ءَأَنتُمۡ أَنزَلۡتُمُوهُ مِنَ ٱلۡمُزۡنِ أَمۡ نَحۡنُ ٱلۡمُنزِلُونَ",
      "translation": "آيا هغه تاسي له ورېځو راتويې كړې دي كه موږ يې را وروونكي يو؟",
      "footnotes": ""
    },
    {
      "id": 70,
      "verse_number": 70,
      "page_number": 536,
      "verse_key": "56:70",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَوۡ نَشَآءُ جَعَلۡنَٰهُ أُجَاجٗا فَلَوۡلَا تَشۡكُرُونَ",
      "translation": "كه موږ وغواړو هغه به تروې ترخې كړو نو ولې شكر نه باسئ؟",
      "footnotes": ""
    },
    {
      "id": 71,
      "verse_number": 71,
      "page_number": 536,
      "verse_key": "56:71",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَرَءَيۡتُمُ ٱلنَّارَ ٱلَّتِي تُورُونَ",
      "translation": "آيا هغه اور ته مو كتلي چې تاسي يې بلوئ؟",
      "footnotes": ""
    },
    {
      "id": 72,
      "verse_number": 72,
      "page_number": 536,
      "verse_key": "56:72",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ءَأَنتُمۡ أَنشَأۡتُمۡ شَجَرَتَهَآ أَمۡ نَحۡنُ ٱلۡمُنشِـُٔونَ",
      "translation": "آيا تاسي يې ونه راپيدا كړې ده كه موږ يې پيدا كوونكي يو؟",
      "footnotes": ""
    },
    {
      "id": 73,
      "verse_number": 73,
      "page_number": 536,
      "verse_key": "56:73",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "نَحۡنُ جَعَلۡنَٰهَا تَذۡكِرَةٗ وَمَتَٰعٗا لِّلۡمُقۡوِينَ",
      "translation": "هغه خو موږ عبرت او د مسافرو لپاره ګټوره ګرځولې ده.",
      "footnotes": ""
    },
    {
      "id": 74,
      "verse_number": 74,
      "page_number": 536,
      "verse_key": "56:74",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَسَبِّحۡ بِٱسۡمِ رَبِّكَ ٱلۡعَظِيمِ",
      "translation": "نو د خپل لوى پالونکي په نوم پاکي بيانوه.",
      "footnotes": ""
    },
    {
      "id": 75,
      "verse_number": 75,
      "page_number": 536,
      "verse_key": "56:75",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "۞ فَلَآ أُقۡسِمُ بِمَوَٰقِعِ ٱلنُّجُومِ",
      "translation": "نو زه د ستورو د پرېوتلو پر ځايونو قسم كوم.",
      "footnotes": ""
    },
    {
      "id": 76,
      "verse_number": 76,
      "page_number": 536,
      "verse_key": "56:76",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِنَّهُۥ لَقَسَمٞ لَّوۡ تَعۡلَمُونَ عَظِيمٌ",
      "translation": "او كه پوهېږئ دا په رېښتيا لوى قسم دى.",
      "footnotes": ""
    },
    {
      "id": 77,
      "verse_number": 77,
      "page_number": 537,
      "verse_key": "56:77",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُۥ لَقُرۡءَانٞ كَرِيمٞ",
      "translation": "چې پرته له شکه دا يو عزتمن قرآن دى.",
      "footnotes": ""
    },
    {
      "id": 78,
      "verse_number": 78,
      "page_number": 537,
      "verse_key": "56:78",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِي كِتَٰبٖ مَّكۡنُونٖ",
      "translation": "چې په يو پټ ساتل شوي كتاب كې دى.",
      "footnotes": ""
    },
    {
      "id": 79,
      "verse_number": 79,
      "page_number": 537,
      "verse_key": "56:79",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَّا يَمَسُّهُۥٓ إِلَّا ٱلۡمُطَهَّرُونَ",
      "translation": "هغه ته پرته له ډېرو پاکو بل څوك لاس نه وروړي.",
      "footnotes": ""
    },
    {
      "id": 80,
      "verse_number": 80,
      "page_number": 537,
      "verse_key": "56:80",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تَنزِيلٞ مِّن رَّبِّ ٱلۡعَٰلَمِينَ",
      "translation": "د رب العالمين له خوا نازل شوى دى.",
      "footnotes": ""
    },
    {
      "id": 81,
      "verse_number": 81,
      "page_number": 537,
      "verse_key": "56:81",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَبِهَٰذَا ٱلۡحَدِيثِ أَنتُم مُّدۡهِنُونَ",
      "translation": "آيا نو تاسي د دې خبرى (قرآن) سپک ګڼونكي ياست؟",
      "footnotes": ""
    },
    {
      "id": 82,
      "verse_number": 82,
      "page_number": 537,
      "verse_key": "56:82",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَتَجۡعَلُونَ رِزۡقَكُمۡ أَنَّكُمۡ تُكَذِّبُونَ",
      "translation": "او (يوازې دا خبره) خپله برخه ګرځوئ چې درواغ يې ګڼئ.",
      "footnotes": ""
    },
    {
      "id": 83,
      "verse_number": 83,
      "page_number": 537,
      "verse_key": "56:83",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلَوۡلَآ إِذَا بَلَغَتِ ٱلۡحُلۡقُومَ",
      "translation": "نو ولې هغه وخت چې (روح) مرۍ ته ورسيږي.",
      "footnotes": ""
    },
    {
      "id": 84,
      "verse_number": 84,
      "page_number": 537,
      "verse_key": "56:84",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنتُمۡ حِينَئِذٖ تَنظُرُونَ",
      "translation": "او تاسي هغه وخت (په سترګو) ګورئ.",
      "footnotes": ""
    },
    {
      "id": 85,
      "verse_number": 85,
      "page_number": 537,
      "verse_key": "56:85",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَنَحۡنُ أَقۡرَبُ إِلَيۡهِ مِنكُمۡ وَلَٰكِن لَّا تُبۡصِرُونَ",
      "translation": "او موږ له تاسي زيات ورنېږدې يو خو تاسي نه وينئ.",
      "footnotes": ""
    },
    {
      "id": 86,
      "verse_number": 86,
      "page_number": 537,
      "verse_key": "56:86",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلَوۡلَآ إِن كُنتُمۡ غَيۡرَ مَدِينِينَ",
      "translation": "نو كه تاسي ته جزا نه دركول كيږي.",
      "footnotes": ""
    },
    {
      "id": 87,
      "verse_number": 87,
      "page_number": 537,
      "verse_key": "56:87",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تَرۡجِعُونَهَآ إِن كُنتُمۡ صَٰدِقِينَ",
      "translation": "ولې (هغه روح) نه راستنوئ كه تاسي رښتيني ياست.",
      "footnotes": ""
    },
    {
      "id": 88,
      "verse_number": 88,
      "page_number": 537,
      "verse_key": "56:88",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَمَّآ إِن كَانَ مِنَ ٱلۡمُقَرَّبِينَ",
      "translation": "نو بيا كه چېرې هغه (الله ته) له نېږدې كړى شوو څخه وي.",
      "footnotes": ""
    },
    {
      "id": 89,
      "verse_number": 89,
      "page_number": 537,
      "verse_key": "56:89",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَرَوۡحٞ وَرَيۡحَانٞ وَجَنَّتُ نَعِيمٖ",
      "translation": "نو هوسايي، ښه روزي او نعمتونو والا جنت دى.",
      "footnotes": ""
    },
    {
      "id": 90,
      "verse_number": 90,
      "page_number": 537,
      "verse_key": "56:90",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَمَّآ إِن كَانَ مِنۡ أَصۡحَٰبِ ٱلۡيَمِينِ",
      "translation": "او كه هغه له ښي لاس والا كسانو څخه وي.",
      "footnotes": ""
    },
    {
      "id": 91,
      "verse_number": 91,
      "page_number": 537,
      "verse_key": "56:91",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَسَلَٰمٞ لَّكَ مِنۡ أَصۡحَٰبِ ٱلۡيَمِينِ",
      "translation": "نو (بيا هم) تاته سلامتي ده چې د ښي لاس والا خلكو څخه يې.",
      "footnotes": ""
    },
    {
      "id": 92,
      "verse_number": 92,
      "page_number": 537,
      "verse_key": "56:92",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَمَّآ إِن كَانَ مِنَ ٱلۡمُكَذِّبِينَ ٱلضَّآلِّينَ",
      "translation": "خو كه چېرې له درواغ ګڼونكو لار وركو څخه وي.",
      "footnotes": ""
    },
    {
      "id": 93,
      "verse_number": 93,
      "page_number": 537,
      "verse_key": "56:93",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَنُزُلٞ مِّنۡ حَمِيمٖ",
      "translation": "نو (هغه ته) د خوټېدونكو اوبو مېلمستيا ده.",
      "footnotes": ""
    },
    {
      "id": 94,
      "verse_number": 94,
      "page_number": 537,
      "verse_key": "56:94",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَتَصۡلِيَةُ جَحِيمٍ",
      "translation": "او دوزخ ته ورننه اېستل دي.",
      "footnotes": ""
    },
    {
      "id": 95,
      "verse_number": 95,
      "page_number": 537,
      "verse_key": "56:95",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ هَٰذَا لَهُوَ حَقُّ ٱلۡيَقِينِ",
      "translation": "يقيناً همدا حق او يقيني ده.",
      "footnotes": ""
    },
    {
      "id": 96,
      "verse_number": 96,
      "page_number": 537,
      "verse_key": "56:96",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَسَبِّحۡ بِٱسۡمِ رَبِّكَ ٱلۡعَظِيمِ",
      "translation": "د خپل لوى پالونکي په نوم پاکي بيانوه.",
      "footnotes": ""
    }
  ]
}