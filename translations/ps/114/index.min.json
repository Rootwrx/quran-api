{
  "verses": [
    {
      "number": "1",
      "arabic_text": "قُلۡ أَعُوذُ بِرَبِّ ٱلنَّاسِ",
      "translation": "ووايه د (ټولو) خلکو پر پالونکي پناه غواړم."
    },
    {
      "number": "2",
      "arabic_text": "مَلِكِ ٱلنَّاسِ",
      "translation": "چې د خلکو پاچا دی."
    },
    {
      "number": "3",
      "arabic_text": "إِلَٰهِ ٱلنَّاسِ",
      "translation": "په حقه د عبادت لایق دی."
    },
    {
      "number": "4",
      "arabic_text": "مِن شَرِّ ٱلۡوَسۡوَاسِ ٱلۡخَنَّاسِ",
      "translation": "د بيا بيا تلونکي راتلونکي وسوسه اچوونکي له شره."
    },
    {
      "number": "5",
      "arabic_text": "ٱلَّذِي يُوَسۡوِسُ فِي صُدُورِ ٱلنَّاسِ",
      "translation": "هغه چې د خلکو په زړونو کې وسوسې اچوي."
    },
    {
      "number": "6",
      "arabic_text": "مِنَ ٱلۡجِنَّةِ وَٱلنَّاسِ",
      "translation": "هم له پېريانو او هم له انسانانو."
    }
  ]
}