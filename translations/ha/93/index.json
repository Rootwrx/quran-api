{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 596,
      "verse_key": "93:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلضُّحَىٰ",
      "translation": "Inã rantsuwa da hantsi.",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 596,
      "verse_key": "93:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّيۡلِ إِذَا سَجَىٰ",
      "translation": "Da dare a lõkacin da ya rufe (da duhunsa).",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 596,
      "verse_key": "93:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَا وَدَّعَكَ رَبُّكَ وَمَا قَلَىٰ",
      "translation": "Ubangijinka bai yi maka bankwana ba, kuma bai ƙĩ kaba.",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 596,
      "verse_key": "93:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَلۡأٓخِرَةُ خَيۡرٞ لَّكَ مِنَ ٱلۡأُولَىٰ",
      "translation": "Kuma lalle ta ƙarshe* ce mafi alheri a gare ka daga ta farko.",
      "footnotes": "* Ta ƙarshe, wãto, Lãhira; kuma ta farko, wato, dũniya."
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 596,
      "verse_key": "93:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَسَوۡفَ يُعۡطِيكَ رَبُّكَ فَتَرۡضَىٰٓ",
      "translation": "Kuma lalle ne, Ubangijinka zai yi ta bã ka kyauta sai ka yarda.",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 596,
      "verse_key": "93:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَمۡ يَجِدۡكَ يَتِيمٗا فَـَٔاوَىٰ",
      "translation": "Ashe, bai sãme ka marãya ba, sa'an nan Ya yi maka makõma?",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 596,
      "verse_key": "93:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَوَجَدَكَ ضَآلّٗا فَهَدَىٰ",
      "translation": "Kuma Ya sãme ka bã ka da shari'ã, sai Ya shiryar da kai?",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 596,
      "verse_key": "93:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَوَجَدَكَ عَآئِلٗا فَأَغۡنَىٰ",
      "translation": "Kuma Ya sãme ka matalauci, sai Ya wadãtã ka?",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 596,
      "verse_key": "93:9",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَمَّا ٱلۡيَتِيمَ فَلَا تَقۡهَرۡ",
      "translation": "Sabõda haka, amma marãya, to, kada ka rinjãye shi.",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 596,
      "verse_key": "93:10",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَمَّا ٱلسَّآئِلَ فَلَا تَنۡهَرۡ",
      "translation": "Kuma amma mai tambaya, kada ka yi masa tsãwa.",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 596,
      "verse_key": "93:11",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَمَّا بِنِعۡمَةِ رَبِّكَ فَحَدِّثۡ",
      "translation": "Kuma amma ni'imar Ubangijinka, sai ka faɗa (dõmin gõdiya).",
      "footnotes": ""
    }
  ]
}