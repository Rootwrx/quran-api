{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 593,
      "verse_key": "89:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡفَجۡرِ",
      "translation": "Inã rantsuwa da alfijiri.",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 593,
      "verse_key": "89:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَيَالٍ عَشۡرٖ",
      "translation": "Da darũruwa gõma.",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 593,
      "verse_key": "89:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلشَّفۡعِ وَٱلۡوَتۡرِ",
      "translation": "Da (adadi na) cikã da (na) mãrã.",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 593,
      "verse_key": "89:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّيۡلِ إِذَا يَسۡرِ",
      "translation": "Da dare idan yana shũɗewa.",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 593,
      "verse_key": "89:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "هَلۡ فِي ذَٰلِكَ قَسَمٞ لِّذِي حِجۡرٍ",
      "translation": "Ko a cikin waɗannan akwai abin rantsuwa ga mai hankali (da yake kange shi daga zunubi)?",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 593,
      "verse_key": "89:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَمۡ تَرَ كَيۡفَ فَعَلَ رَبُّكَ بِعَادٍ",
      "translation": "Ba ka ga yadda Ubangijinka Ya aikata da Ãdãwa ba?",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 593,
      "verse_key": "89:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِرَمَ ذَاتِ ٱلۡعِمَادِ",
      "translation": "Iramawa mãsu sakon* ƙĩrar jiki.",
      "footnotes": "* Tsawo mai ma'ana."
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 593,
      "verse_key": "89:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّتِي لَمۡ يُخۡلَقۡ مِثۡلُهَا فِي ٱلۡبِلَٰدِ",
      "translation": "Waɗanda ba a halitta kwatankwacinsu ba a cikin garũruwa (na dũniya).",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 593,
      "verse_key": "89:9",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَثَمُودَ ٱلَّذِينَ جَابُواْ ٱلصَّخۡرَ بِٱلۡوَادِ",
      "translation": "Da samũdãwa waɗanda suka fasa duwãtsu a cikin Wadi* suka yi gidãje)?",
      "footnotes": "* Wãdil-ƙura, sunan wuri ne kusa da Madina wajen Syria, wato Sham."
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 593,
      "verse_key": "89:10",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَفِرۡعَوۡنَ ذِي ٱلۡأَوۡتَادِ",
      "translation": "Da Fir'auna mai turãku.",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 593,
      "verse_key": "89:11",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِينَ طَغَوۡاْ فِي ٱلۡبِلَٰدِ",
      "translation": "Waɗanda suka ƙẽtare iyãkarsu, a cikin garũruwa?",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 593,
      "verse_key": "89:12",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَكۡثَرُواْ فِيهَا ٱلۡفَسَادَ",
      "translation": "Sabõda haka, suka yawaita yin ɓarna a cikinsu.",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 593,
      "verse_key": "89:13",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَصَبَّ عَلَيۡهِمۡ رَبُّكَ سَوۡطَ عَذَابٍ",
      "translation": "Sabõda haka Ubangijinka Ya zuba musu bũlãlar azãba.",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 593,
      "verse_key": "89:14",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ رَبَّكَ لَبِٱلۡمِرۡصَادِ",
      "translation": "Lalle ne, Ubangijinka Yana nan a mafaka.",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 593,
      "verse_key": "89:15",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَمَّا ٱلۡإِنسَٰنُ إِذَا مَا ٱبۡتَلَىٰهُ رَبُّهُۥ فَأَكۡرَمَهُۥ وَنَعَّمَهُۥ فَيَقُولُ رَبِّيٓ أَكۡرَمَنِ",
      "translation": "To, amma fa mutum idan Ubangijibsa Ya jarraba shi, wãto Yã girmama shi kuma Ya yi masa ni'ima, sai ya ce: \"Ubangijina Ya girmama ni.\"",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 593,
      "verse_key": "89:16",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَمَّآ إِذَا مَا ٱبۡتَلَىٰهُ فَقَدَرَ عَلَيۡهِ رِزۡقَهُۥ فَيَقُولُ رَبِّيٓ أَهَٰنَنِ",
      "translation": "Kuma idan Ya jarraba shi, wãto Ya ƙuntata masa abincinsa, sai ya ce: \"Ubangijina Ya walãkanta ni.\"",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 593,
      "verse_key": "89:17",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّاۖ بَل لَّا تُكۡرِمُونَ ٱلۡيَتِيمَ",
      "translation": "A'aha! Bari wannan, ai bã ku girmama marãya!",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 593,
      "verse_key": "89:18",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَا تَحَٰٓضُّونَ عَلَىٰ طَعَامِ ٱلۡمِسۡكِينِ",
      "translation": "Bã ku kwaɗaitã wa jũnanku ga (tattalin) abincin matalauci!",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 593,
      "verse_key": "89:19",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَتَأۡكُلُونَ ٱلتُّرَاثَ أَكۡلٗا لَّمّٗا",
      "translation": "Kuma kuna cin dũkiyar gãdo, ci na tãrãwa.",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 593,
      "verse_key": "89:20",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَتُحِبُّونَ ٱلۡمَالَ حُبّٗا جَمّٗا",
      "translation": "Kuma kuna son dũkiya, so mai yawa.",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 593,
      "verse_key": "89:21",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّآۖ إِذَا دُكَّتِ ٱلۡأَرۡضُ دَكّٗا دَكّٗا",
      "translation": "A'aha! Idan aka niƙa ƙasa niƙewa sosai.",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 593,
      "verse_key": "89:22",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَجَآءَ رَبُّكَ وَٱلۡمَلَكُ صَفّٗا صَفّٗا",
      "translation": "Kuma Ubangijinka Ya zo, alhãli malã'iku na jẽre, sahu- sahu.",
      "footnotes": ""
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 593,
      "verse_key": "89:23",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَجِاْيٓءَ يَوۡمَئِذِۭ بِجَهَنَّمَۚ يَوۡمَئِذٖ يَتَذَكَّرُ ٱلۡإِنسَٰنُ وَأَنَّىٰ لَهُ ٱلذِّكۡرَىٰ",
      "translation": "Kuma a ranar nan aka zo da Jahannama. A ranar nan mutum zai yi tunãni. To, inã fa tunãni yake a gare shi!",
      "footnotes": ""
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 594,
      "verse_key": "89:24",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَقُولُ يَٰلَيۡتَنِي قَدَّمۡتُ لِحَيَاتِي",
      "translation": "Yana dinga cẽwa, \"Kaitona, dã na gabatar (da aikin ƙwarai) domin rãyuwata!\"",
      "footnotes": ""
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 594,
      "verse_key": "89:25",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَيَوۡمَئِذٖ لَّا يُعَذِّبُ عَذَابَهُۥٓ أَحَدٞ",
      "translation": "To, a rãnar nan bãbu wani mai yin azãba irin azãbar Allah.",
      "footnotes": ""
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 594,
      "verse_key": "89:26",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَا يُوثِقُ وَثَاقَهُۥٓ أَحَدٞ",
      "translation": "Kuma bãbu wani mai ɗauri irin ɗaurin Sa.",
      "footnotes": ""
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 594,
      "verse_key": "89:27",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَٰٓأَيَّتُهَا ٱلنَّفۡسُ ٱلۡمُطۡمَئِنَّةُ",
      "translation": "Yã kai rai mai natsuwa!",
      "footnotes": ""
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 594,
      "verse_key": "89:28",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱرۡجِعِيٓ إِلَىٰ رَبِّكِ رَاضِيَةٗ مَّرۡضِيَّةٗ",
      "translation": "Ka koma zuwa ga Ubangijinka, alhãli kana mai yarda (da abin da Ya ƙaddara maka a dũniya) abar yardarwa (da sakamakon da zã a ba ka a Lãhira).",
      "footnotes": ""
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 594,
      "verse_key": "89:29",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَٱدۡخُلِي فِي عِبَٰدِي",
      "translation": "Sabõbda haka, ka shiga cikin bãyi Na (mãsu bin umurui a dũniya).",
      "footnotes": ""
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 594,
      "verse_key": "89:30",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱدۡخُلِي جَنَّتِي",
      "translation": "Kuma ka shiga AljannaTa (a Lãhira).",
      "footnotes": ""
    }
  ]
}