{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 591,
      "verse_key": "87:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "سَبِّحِ ٱسۡمَ رَبِّكَ ٱلۡأَعۡلَى",
      "translation": "Ka tsarkake sũnan Ubangijinka Mafi ɗaukaka.",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 591,
      "verse_key": "87:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِي خَلَقَ فَسَوَّىٰ",
      "translation": "Wanda Yã yi halitta sa'an nan Ya daidaita abin halittar.",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 591,
      "verse_key": "87:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّذِي قَدَّرَ فَهَدَىٰ",
      "translation": "Kuma Wanda Ya ƙaddara (abin da Ya so) sannan Ya shiryar, (da mutum ga hanyar alhẽri da ta sharri).",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 591,
      "verse_key": "87:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّذِيٓ أَخۡرَجَ ٱلۡمَرۡعَىٰ",
      "translation": "Kuma Wanda Ya fitar da makiyãyã.",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 591,
      "verse_key": "87:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَجَعَلَهُۥ غُثَآءً أَحۡوَىٰ",
      "translation": "Sa'an nan Ya mayar da ita ƙeƙasassa, baƙa.",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 591,
      "verse_key": "87:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "سَنُقۡرِئُكَ فَلَا تَنسَىٰٓ",
      "translation": "Za mu karantar da kai (Alƙur'ãni) sabõda haka bã zã ka mantã (shi) ba.",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 591,
      "verse_key": "87:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَّا مَا شَآءَ ٱللَّهُۚ إِنَّهُۥ يَعۡلَمُ ٱلۡجَهۡرَ وَمَا يَخۡفَىٰ",
      "translation": "Fãce abin da Allah Ya so, lalle ne Shi (Allah) Ya san bayyane da abin da yake bõye.",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 591,
      "verse_key": "87:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَنُيَسِّرُكَ لِلۡيُسۡرَىٰ",
      "translation": "Kuma za Mu sauƙaƙe maka (al'amari) zuwa ga (Shari'a) mai sauƙi.",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 591,
      "verse_key": "87:9",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَذَكِّرۡ إِن نَّفَعَتِ ٱلذِّكۡرَىٰ",
      "translation": "Sabõda baka, ka tunãtar, idan tunatarwa zã ta yi amfãni.",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 591,
      "verse_key": "87:10",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "سَيَذَّكَّرُ مَن يَخۡشَىٰ",
      "translation": "Wanda yake tsõron (Allah) Zai tuna.",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 591,
      "verse_key": "87:11",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيَتَجَنَّبُهَا ٱلۡأَشۡقَى",
      "translation": "Kuma shaƙiyyi, zai nisanceta,",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 591,
      "verse_key": "87:12",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِي يَصۡلَى ٱلنَّارَ ٱلۡكُبۡرَىٰ",
      "translation": "Wanda zai shiga wutar da tã fi girma.",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 591,
      "verse_key": "87:13",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ لَا يَمُوتُ فِيهَا وَلَا يَحۡيَىٰ",
      "translation": "Sa'an nan bã zai mutu ba a cikinta, kuma bã zai rãyu ba.",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 591,
      "verse_key": "87:14",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قَدۡ أَفۡلَحَ مَن تَزَكَّىٰ",
      "translation": "Lalle ne wanda ya tsarkaka (da ĩmãni) yã sãmu babban rabo.",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 591,
      "verse_key": "87:15",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَذَكَرَ ٱسۡمَ رَبِّهِۦ فَصَلَّىٰ",
      "translation": "Kuma ya ambaci sũnan* Ubangijinsa, sa'an nan yã yi salla.",
      "footnotes": "* Ya ambaci sũnan Allah ga ayyukansa duka, ya yi sallolin nan biyar da sauran."
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 592,
      "verse_key": "87:16",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بَلۡ تُؤۡثِرُونَ ٱلۡحَيَوٰةَ ٱلدُّنۡيَا",
      "translation": "Ba haka ba! Kunã zãɓin rãyuwa ta kusa dũniya.*",
      "footnotes": "* Rayuwa iri biyu ce, ta dũniya da ta Lãhira."
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 592,
      "verse_key": "87:17",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡأٓخِرَةُ خَيۡرٞ وَأَبۡقَىٰٓ",
      "translation": "Alhãli Lãhira ita ce mafi alheri kuma mafi wanzuwa.",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 592,
      "verse_key": "87:18",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ هَٰذَا لَفِي ٱلصُّحُفِ ٱلۡأُولَىٰ",
      "translation": "Lalle ne, wannan* yanã a cikin littafan farko.",
      "footnotes": "* Kamanta rãyuwar ciyãwa da rãyar da ummiyyi da ilmi bãbu mantuwa, da rãyar da mãtattu cikin sa'ãda ko cikin shaƙãwa yana cikin litaffan farko."
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 592,
      "verse_key": "87:19",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "صُحُفِ إِبۡرَٰهِيمَ وَمُوسَىٰ",
      "translation": "Littaffan Ibrãhĩm da Mũsã.",
      "footnotes": ""
    }
  ]
}