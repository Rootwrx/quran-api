{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 603,
      "verse_key": "109:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قُلۡ يَٰٓأَيُّهَا ٱلۡكَٰفِرُونَ",
      "translation": "Ka ce: \"Ya kũ kãfirai!\"",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 603,
      "verse_key": "109:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَآ أَعۡبُدُ مَا تَعۡبُدُونَ",
      "translation": "\"Bã zan bautã wa abin da kuke bautã wa ba.\"",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 603,
      "verse_key": "109:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَآ أَنتُمۡ عَٰبِدُونَ مَآ أَعۡبُدُ",
      "translation": "\"Kuma kũ, ba ku zama masu bauta wa abin da nake bautã wa ba.\"",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 603,
      "verse_key": "109:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَآ أَنَا۠ عَابِدٞ مَّا عَبَدتُّمۡ",
      "translation": "\"Kuma nĩ ban zama mai bautã wa abin da kuka bautã wa ba.\"",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 603,
      "verse_key": "109:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَآ أَنتُمۡ عَٰبِدُونَ مَآ أَعۡبُدُ",
      "translation": "\"Kuma kũ, ba ku zama mãsu bautã wa abin da nake* bautã wa ba.\"",
      "footnotes": "* Ãya ta 4 da ta 5 sun nũna addinin ƙarya yakan canza amma na gaskiya bã ya canzawa."
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 603,
      "verse_key": "109:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَكُمۡ دِينُكُمۡ وَلِيَ دِينِ",
      "translation": "\"Addininku na garẽ ku, kuma addinina yanã gare ni.\"",
      "footnotes": ""
    }
  ]
}