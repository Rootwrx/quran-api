{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 600,
      "verse_key": "101:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلۡقَارِعَةُ",
      "translation": "[Он] Фурукӯбанда",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 600,
      "verse_key": "101:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَا ٱلۡقَارِعَةُ",
      "translation": "[Он] Фурукӯбанда чист?",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 600,
      "verse_key": "101:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَآ أَدۡرَىٰكَ مَا ٱلۡقَارِعَةُ",
      "translation": "Ва ту чи донӣ, ки [он] фурукӯбанда чист?",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 600,
      "verse_key": "101:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَوۡمَ يَكُونُ ٱلنَّاسُ كَٱلۡفَرَاشِ ٱلۡمَبۡثُوثِ",
      "translation": "Рӯзе, ки мардум монанди парвонаҳои пароканда [ҳайрону саргардон] хоҳанд буд",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 600,
      "verse_key": "101:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَتَكُونُ ٱلۡجِبَالُ كَٱلۡعِهۡنِ ٱلۡمَنفُوشِ",
      "translation": "Ва кӯҳҳо монанди пашми рангини ҳаллоҷишуда (задашуда) хоҳанд шуд",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 600,
      "verse_key": "101:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَمَّا مَن ثَقُلَتۡ مَوَٰزِينُهُۥ",
      "translation": "Ва аммо ҳар касе, ки [дар он рӯз] паллаи мизонаш сангин бошад",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 600,
      "verse_key": "101:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَهُوَ فِي عِيشَةٖ رَّاضِيَةٖ",
      "translation": "[Дар биҳишт] Дар зиндагии роҳатбахш хоҳад буд",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 600,
      "verse_key": "101:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَمَّا مَنۡ خَفَّتۡ مَوَٰزِينُهُۥ",
      "translation": "Ва аммо ҳар кас, ки паллаи мизонаш сабук бошад",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 600,
      "verse_key": "101:9",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأُمُّهُۥ هَاوِيَةٞ",
      "translation": "Пас, [маскану] паноҳгоҳаш «ҳовия» аст",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 600,
      "verse_key": "101:10",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَآ أَدۡرَىٰكَ مَا هِيَهۡ",
      "translation": "Ва ту чи донӣ, ки он [ҳовия] чист?",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 600,
      "verse_key": "101:11",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "نَارٌ حَامِيَةُۢ",
      "translation": "Оташе аст сӯзон [ва шуълавар]",
      "footnotes": ""
    }
  ]
}