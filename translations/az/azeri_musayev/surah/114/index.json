{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 604,
      "verse_key": "114:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قُلۡ أَعُوذُ بِرَبِّ ٱلنَّاسِ",
      "translation": "(Ey Peyğəmbər!) De: “Sığınıram insanların Rəbbinə,",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 604,
      "verse_key": "114:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَلِكِ ٱلنَّاسِ",
      "translation": "insanların Sahibinə,",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 604,
      "verse_key": "114:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَٰهِ ٱلنَّاسِ",
      "translation": "insanların İlahına!",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 604,
      "verse_key": "114:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مِن شَرِّ ٱلۡوَسۡوَاسِ ٱلۡخَنَّاسِ",
      "translation": "Vəsvəsə verən, (Allahın adı çəkiləndə isə qaçıb) gizlənən şeytanın şərindən",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 604,
      "verse_key": "114:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِي يُوَسۡوِسُ فِي صُدُورِ ٱلنَّاسِ",
      "translation": "O şeytan ki, insanların sinələrinə (qəlblərinə) vəsvəsə salır,",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 604,
      "verse_key": "114:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مِنَ ٱلۡجِنَّةِ وَٱلنَّاسِ",
      "translation": "(O şeytan) cinlərdən də olur, insanlardan da!”",
      "footnotes": ""
    }
  ]
}