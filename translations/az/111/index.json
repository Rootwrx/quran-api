{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 603,
      "verse_key": "111:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تَبَّتۡ يَدَآ أَبِي لَهَبٖ وَتَبَّ",
      "translation": "Əbu Ləhəbin əlləri qurusun, qurudu da!",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 603,
      "verse_key": "111:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَآ أَغۡنَىٰ عَنۡهُ مَالُهُۥ وَمَا كَسَبَ",
      "translation": "Ona nə malı fayda verdi, nə də qazandığı.",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 603,
      "verse_key": "111:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "سَيَصۡلَىٰ نَارٗا ذَاتَ لَهَبٖ",
      "translation": "O, alovlanan bir oda girəcəkdir.",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 603,
      "verse_key": "111:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱمۡرَأَتُهُۥ حَمَّالَةَ ٱلۡحَطَبِ",
      "translation": "Onun odun daşıyan arvadı da.",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 603,
      "verse_key": "111:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِي جِيدِهَا حَبۡلٞ مِّن مَّسَدِۭ",
      "translation": "Onun (arvadının) boynunda xurma liflərindən kəndir olacaqdır.",
      "footnotes": ""
    }
  ]
}