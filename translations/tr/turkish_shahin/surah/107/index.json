{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 602,
      "verse_key": "107:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَرَءَيۡتَ ٱلَّذِي يُكَذِّبُ بِٱلدِّينِ",
      "translation": "Dini yalanlayanı gördün mü?",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 602,
      "verse_key": "107:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَذَٰلِكَ ٱلَّذِي يَدُعُّ ٱلۡيَتِيمَ",
      "translation": "İşte o, yetimi itip kakar,",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 602,
      "verse_key": "107:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَا يَحُضُّ عَلَىٰ طَعَامِ ٱلۡمِسۡكِينِ",
      "translation": "Yoksulu doyurmaya teşvik etmez;",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 602,
      "verse_key": "107:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَوَيۡلٞ لِّلۡمُصَلِّينَ",
      "translation": "Yazıklar olsun o namaz kılanlara ki,",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 602,
      "verse_key": "107:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِينَ هُمۡ عَن صَلَاتِهِمۡ سَاهُونَ",
      "translation": "onlar namazlarını ciddiye almazlar.",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 602,
      "verse_key": "107:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِينَ هُمۡ يُرَآءُونَ",
      "translation": "Onlar gösteriş yapanlardır, ",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 602,
      "verse_key": "107:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيَمۡنَعُونَ ٱلۡمَاعُونَ",
      "translation": "hayra da mâni olurlar.",
      "footnotes": ""
    }
  ]
}