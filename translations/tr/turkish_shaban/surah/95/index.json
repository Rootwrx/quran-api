{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 597,
      "verse_key": "95:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلتِّينِ وَٱلزَّيۡتُونِ",
      "translation": " Andolsun, İncir ve zeytine,",
      "footnotes": null
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 597,
      "verse_key": "95:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَطُورِ سِينِينَ",
      "translation": " Sina Dağı’na,",
      "footnotes": null
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 597,
      "verse_key": "95:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَهَٰذَا ٱلۡبَلَدِ ٱلۡأَمِينِ",
      "translation": "Ve bu güvenli beldeye,",
      "footnotes": null
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 597,
      "verse_key": "95:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَقَدۡ خَلَقۡنَا ٱلۡإِنسَٰنَ فِيٓ أَحۡسَنِ تَقۡوِيمٖ",
      "translation": " Biz, insanı en güzel şekilde yarattık.",
      "footnotes": null
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 597,
      "verse_key": "95:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ رَدَدۡنَٰهُ أَسۡفَلَ سَٰفِلِينَ",
      "translation": " Daha sonra, onu aşağıların en aşağısına indirdik.",
      "footnotes": null
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 597,
      "verse_key": "95:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَّا ٱلَّذِينَ ءَامَنُواْ وَعَمِلُواْ ٱلصَّٰلِحَٰتِ فَلَهُمۡ أَجۡرٌ غَيۡرُ مَمۡنُونٖ",
      "translation": " Fakat iman edip salih amel işleyenler için eksilmeyen devamlı bir ecir vardır.",
      "footnotes": null
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 597,
      "verse_key": "95:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَمَا يُكَذِّبُكَ بَعۡدُ بِٱلدِّينِ",
      "translation": " O halde, sana hesap gününü yalan saydıran nedir?",
      "footnotes": null
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 597,
      "verse_key": "95:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَيۡسَ ٱللَّهُ بِأَحۡكَمِ ٱلۡحَٰكِمِينَ",
      "translation": "Allah, hüküm verenlerin en üstünü değil midir?",
      "footnotes": null
    }
  ]
}