{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 587,
      "verse_key": "82:1",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذَا ٱلسَّمَآءُ ٱنفَطَرَتۡ",
      "translation": " Gök yarıldığı,",
      "footnotes": null
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 587,
      "verse_key": "82:2",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡكَوَاكِبُ ٱنتَثَرَتۡ",
      "translation": " Yıldızlar/gezegenler döküldüğü,",
      "footnotes": null
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 587,
      "verse_key": "82:3",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡبِحَارُ فُجِّرَتۡ",
      "translation": "Denizler kaynayıp birbirine karıştırıldığı,",
      "footnotes": null
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 587,
      "verse_key": "82:4",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡقُبُورُ بُعۡثِرَتۡ",
      "translation": " Kabirlerin içi dışına çıkarıldığı zaman,",
      "footnotes": null
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 587,
      "verse_key": "82:5",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلِمَتۡ نَفۡسٞ مَّا قَدَّمَتۡ وَأَخَّرَتۡ",
      "translation": " Kişi ne hazırlamış olduğunu ne ertelemiş olduğunu bilecektir.",
      "footnotes": null
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 587,
      "verse_key": "82:6",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَٰٓأَيُّهَا ٱلۡإِنسَٰنُ مَا غَرَّكَ بِرَبِّكَ ٱلۡكَرِيمِ",
      "translation": "Ey insan! Kerim olan Rabbine karşı seni aldatan nedir?",
      "footnotes": null
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 587,
      "verse_key": "82:7",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِي خَلَقَكَ فَسَوَّىٰكَ فَعَدَلَكَ",
      "translation": "Seni yaratan ve seni düzenleyip sana biçim veren.",
      "footnotes": null
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 587,
      "verse_key": "82:8",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِيٓ أَيِّ صُورَةٖ مَّا شَآءَ رَكَّبَكَ",
      "translation": "O, seni dilediği sûrette yarattı. ",
      "footnotes": null
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 587,
      "verse_key": "82:9",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّا بَلۡ تُكَذِّبُونَ بِٱلدِّينِ",
      "translation": " Hayır! Hesabı yalanlıyorsunuz.",
      "footnotes": null
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 587,
      "verse_key": "82:10",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِنَّ عَلَيۡكُمۡ لَحَٰفِظِينَ",
      "translation": " Oysa üzerinizde kaydediciler var.",
      "footnotes": null
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 587,
      "verse_key": "82:11",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كِرَامٗا كَٰتِبِينَ",
      "translation": " Şerefli yazıcılar/Kiramen Katibin.",
      "footnotes": null
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 587,
      "verse_key": "82:12",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَعۡلَمُونَ مَا تَفۡعَلُونَ",
      "translation": " Ne yaptığınızı biliyorlar.",
      "footnotes": null
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 587,
      "verse_key": "82:13",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ ٱلۡأَبۡرَارَ لَفِي نَعِيمٖ",
      "translation": " İyiler, elbette nimetler içinde olacaktır.",
      "footnotes": null
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 587,
      "verse_key": "82:14",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِنَّ ٱلۡفُجَّارَ لَفِي جَحِيمٖ",
      "translation": " Kötüler/günahkârlar ise kesinlikle Cehennem'dedir.",
      "footnotes": null
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 587,
      "verse_key": "82:15",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَصۡلَوۡنَهَا يَوۡمَ ٱلدِّينِ",
      "translation": " Hesap günü oraya atılacaklardır.",
      "footnotes": null
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 587,
      "verse_key": "82:16",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا هُمۡ عَنۡهَا بِغَآئِبِينَ",
      "translation": " Onlar, oradan kaybolacak değillerdir.",
      "footnotes": null
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 587,
      "verse_key": "82:17",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَآ أَدۡرَىٰكَ مَا يَوۡمُ ٱلدِّينِ",
      "translation": " Sana bildiren nedir? Hesap gününün ne olduğunu…",
      "footnotes": null
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 587,
      "verse_key": "82:18",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ مَآ أَدۡرَىٰكَ مَا يَوۡمُ ٱلدِّينِ",
      "translation": " Yine, hesap gününün ne olduğunu sana bildiren nedir?",
      "footnotes": null
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 587,
      "verse_key": "82:19",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَوۡمَ لَا تَمۡلِكُ نَفۡسٞ لِّنَفۡسٖ شَيۡـٔٗاۖ وَٱلۡأَمۡرُ يَوۡمَئِذٖ لِّلَّهِ",
      "translation": "O gün hiçbir kimse başkası için bir şeye sahip değildir. O gün, emir sadece Allah’ındır. ",
      "footnotes": null
    }
  ]
}