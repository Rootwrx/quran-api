{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 593,
      "verse_key": "89:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡفَجۡرِ",
      "translation": " Andolsun fecre,",
      "footnotes": null
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 593,
      "verse_key": "89:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَيَالٍ عَشۡرٖ",
      "translation": " On geceye,",
      "footnotes": null
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 593,
      "verse_key": "89:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلشَّفۡعِ وَٱلۡوَتۡرِ",
      "translation": " Çifte ve teke,",
      "footnotes": null
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 593,
      "verse_key": "89:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّيۡلِ إِذَا يَسۡرِ",
      "translation": " Geçip giden geceye,",
      "footnotes": null
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 593,
      "verse_key": "89:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "هَلۡ فِي ذَٰلِكَ قَسَمٞ لِّذِي حِجۡرٍ",
      "translation": " Bunlarda akıl sahibi için elbette birer yemin vardır. ",
      "footnotes": null
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 593,
      "verse_key": "89:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَمۡ تَرَ كَيۡفَ فَعَلَ رَبُّكَ بِعَادٍ",
      "translation": " Ad kavmine Rabbin neler yaptı, görmedin mi?",
      "footnotes": null
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 593,
      "verse_key": "89:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِرَمَ ذَاتِ ٱلۡعِمَادِ",
      "translation": " Sütunlar sahibi İrem’e…",
      "footnotes": null
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 593,
      "verse_key": "89:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّتِي لَمۡ يُخۡلَقۡ مِثۡلُهَا فِي ٱلۡبِلَٰدِ",
      "translation": " Ve şehirler arasında bir benzeri yaratılmamıştı.",
      "footnotes": null
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 593,
      "verse_key": "89:9",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَثَمُودَ ٱلَّذِينَ جَابُواْ ٱلصَّخۡرَ بِٱلۡوَادِ",
      "translation": " Ki vadide kayalar oyan Semud’a,",
      "footnotes": null
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 593,
      "verse_key": "89:10",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَفِرۡعَوۡنَ ذِي ٱلۡأَوۡتَادِ",
      "translation": " Kazıklar/ordular sahibi Firavun’a,",
      "footnotes": null
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 593,
      "verse_key": "89:11",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِينَ طَغَوۡاْ فِي ٱلۡبِلَٰدِ",
      "translation": " Onlar, ülkelerde azmışlardı.",
      "footnotes": null
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 593,
      "verse_key": "89:12",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَكۡثَرُواْ فِيهَا ٱلۡفَسَادَ",
      "translation": " Oralarda fesadı çoğaltmışlardı.",
      "footnotes": null
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 593,
      "verse_key": "89:13",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَصَبَّ عَلَيۡهِمۡ رَبُّكَ سَوۡطَ عَذَابٍ",
      "translation": " Rabbin de onlara azap kırbacı yağdırdı.",
      "footnotes": null
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 593,
      "verse_key": "89:14",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ رَبَّكَ لَبِٱلۡمِرۡصَادِ",
      "translation": " Çünkü Rabbin gözetlemektedir. ",
      "footnotes": null
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 593,
      "verse_key": "89:15",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَمَّا ٱلۡإِنسَٰنُ إِذَا مَا ٱبۡتَلَىٰهُ رَبُّهُۥ فَأَكۡرَمَهُۥ وَنَعَّمَهُۥ فَيَقُولُ رَبِّيٓ أَكۡرَمَنِ",
      "translation": " İşte insan, onu Rabbi imtihan ettiği zaman, ona ikramda bulunup, nimetlendirince: \"Rabbim bana ikramda bulundu\" der.",
      "footnotes": null
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 593,
      "verse_key": "89:16",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَمَّآ إِذَا مَا ٱبۡتَلَىٰهُ فَقَدَرَ عَلَيۡهِ رِزۡقَهُۥ فَيَقُولُ رَبِّيٓ أَهَٰنَنِ",
      "translation": "Ama onu, imtihan edip rızkını daralttığında: \"Rabbim beni zelil etti\" der.",
      "footnotes": null
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 593,
      "verse_key": "89:17",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّاۖ بَل لَّا تُكۡرِمُونَ ٱلۡيَتِيمَ",
      "translation": " Hayır! Yetime ikramda bulunmuyorsunuz.",
      "footnotes": null
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 593,
      "verse_key": "89:18",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَا تَحَٰٓضُّونَ عَلَىٰ طَعَامِ ٱلۡمِسۡكِينِ",
      "translation": " Düşkünü doyurmaya teşvik etmiyorsunuz",
      "footnotes": null
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 593,
      "verse_key": "89:19",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَتَأۡكُلُونَ ٱلتُّرَاثَ أَكۡلٗا لَّمّٗا",
      "translation": " Mirası da şiddetle (hak gözetmeden) yiyorsunuz.",
      "footnotes": null
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 593,
      "verse_key": "89:20",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَتُحِبُّونَ ٱلۡمَالَ حُبّٗا جَمّٗا",
      "translation": "Malı da pek çok seviyorsunuz.",
      "footnotes": null
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 593,
      "verse_key": "89:21",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّآۖ إِذَا دُكَّتِ ٱلۡأَرۡضُ دَكّٗا دَكّٗا",
      "translation": " Hayır! Yer paramparça olduğu zaman, ",
      "footnotes": null
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 593,
      "verse_key": "89:22",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَجَآءَ رَبُّكَ وَٱلۡمَلَكُ صَفّٗا صَفّٗا",
      "translation": "Rabbin ve saf saf melekler geldiği zaman, ",
      "footnotes": null
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 593,
      "verse_key": "89:23",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَجِاْيٓءَ يَوۡمَئِذِۭ بِجَهَنَّمَۚ يَوۡمَئِذٖ يَتَذَكَّرُ ٱلۡإِنسَٰنُ وَأَنَّىٰ لَهُ ٱلذِّكۡرَىٰ",
      "translation": " İşte cehennem o gün getirilir. Ve insan o gün (yaptıklarını) hatırlar. Fakat bu hatırlamanın ne faydası var! ",
      "footnotes": null
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 594,
      "verse_key": "89:24",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَقُولُ يَٰلَيۡتَنِي قَدَّمۡتُ لِحَيَاتِي",
      "translation": "Keşke hayatım için (ameller) yapıp sunsaydım.",
      "footnotes": null
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 594,
      "verse_key": "89:25",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَيَوۡمَئِذٖ لَّا يُعَذِّبُ عَذَابَهُۥٓ أَحَدٞ",
      "translation": " İşte, hiç kimse o gün Allah’ın cezalandırdığı gibi cezalandıramaz.",
      "footnotes": null
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 594,
      "verse_key": "89:26",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَا يُوثِقُ وَثَاقَهُۥٓ أَحَدٞ",
      "translation": " Hiç kimse onun bağladığı gibi bağlayamaz.",
      "footnotes": null
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 594,
      "verse_key": "89:27",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَٰٓأَيَّتُهَا ٱلنَّفۡسُ ٱلۡمُطۡمَئِنَّةُ",
      "translation": " Ey huzura ermiş nefis! ",
      "footnotes": null
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 594,
      "verse_key": "89:28",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱرۡجِعِيٓ إِلَىٰ رَبِّكِ رَاضِيَةٗ مَّرۡضِيَّةٗ",
      "translation": " Sen O'ndan hoşnut, O da senden hoşnut olarak Rabbine dön!",
      "footnotes": null
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 594,
      "verse_key": "89:29",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَٱدۡخُلِي فِي عِبَٰدِي",
      "translation": "Kullarımın arasına katıl!",
      "footnotes": null
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 594,
      "verse_key": "89:30",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱدۡخُلِي جَنَّتِي",
      "translation": " Ve gir Cennet'ime!",
      "footnotes": null
    }
  ]
}