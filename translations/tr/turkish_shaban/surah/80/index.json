{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 585,
      "verse_key": "80:1",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَبَسَ وَتَوَلَّىٰٓ",
      "translation": "(Peygamber) Yüzünü ekşitti ve döndü.",
      "footnotes": null
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 585,
      "verse_key": "80:2",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَن جَآءَهُ ٱلۡأَعۡمَىٰ",
      "translation": " Ona gözleri görmeyen kimse geldi diye…",
      "footnotes": null
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 585,
      "verse_key": "80:3",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا يُدۡرِيكَ لَعَلَّهُۥ يَزَّكَّىٰٓ",
      "translation": " Ne bilirsin belki o, arınacaktır.",
      "footnotes": null
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 585,
      "verse_key": "80:4",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَوۡ يَذَّكَّرُ فَتَنفَعَهُ ٱلذِّكۡرَىٰٓ",
      "translation": " Veya öğüt alacak da öğüt ona fayda verecektir.",
      "footnotes": null
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 585,
      "verse_key": "80:5",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَمَّا مَنِ ٱسۡتَغۡنَىٰ",
      "translation": " Ama, kendisini ihtiyaçsız görene.",
      "footnotes": null
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 585,
      "verse_key": "80:6",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَنتَ لَهُۥ تَصَدَّىٰ",
      "translation": " Sen, yöneliyorsun ona.",
      "footnotes": null
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 585,
      "verse_key": "80:7",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا عَلَيۡكَ أَلَّا يَزَّكَّىٰ",
      "translation": " Arınmamasından sen sorumlu değilsin!",
      "footnotes": null
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 585,
      "verse_key": "80:8",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَمَّا مَن جَآءَكَ يَسۡعَىٰ",
      "translation": " Ama, sana koşarak gelen,",
      "footnotes": null
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 585,
      "verse_key": "80:9",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَهُوَ يَخۡشَىٰ",
      "translation": " Ve korkarak.",
      "footnotes": null
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 585,
      "verse_key": "80:10",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَنتَ عَنۡهُ تَلَهَّىٰ",
      "translation": " Sen ise ilgilenmiyorsun. ",
      "footnotes": null
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 585,
      "verse_key": "80:11",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّآ إِنَّهَا تَذۡكِرَةٞ",
      "translation": "Hayır! (Şunu iyi bil ki) şüphesiz bu bir öğüttür.",
      "footnotes": null
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 585,
      "verse_key": "80:12",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَمَن شَآءَ ذَكَرَهُۥ",
      "translation": " Dileyen kimse düşünüp, öğüt alır.",
      "footnotes": null
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 585,
      "verse_key": "80:13",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِي صُحُفٖ مُّكَرَّمَةٖ",
      "translation": " Şerefli sahifelerde.",
      "footnotes": null
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 585,
      "verse_key": "80:14",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَّرۡفُوعَةٖ مُّطَهَّرَةِۭ",
      "translation": " Yükseltilmiş ve tertemiz.",
      "footnotes": null
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 585,
      "verse_key": "80:15",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بِأَيۡدِي سَفَرَةٖ",
      "translation": " Elçilerin ellerinde.",
      "footnotes": null
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 585,
      "verse_key": "80:16",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كِرَامِۭ بَرَرَةٖ",
      "translation": "Şerefli ve tertemiz/itaatkar.",
      "footnotes": null
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 585,
      "verse_key": "80:17",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قُتِلَ ٱلۡإِنسَٰنُ مَآ أَكۡفَرَهُۥ",
      "translation": " Kahrolası (kâfir) insan ne de çok kâfirdir! ",
      "footnotes": null
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 585,
      "verse_key": "80:18",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مِنۡ أَيِّ شَيۡءٍ خَلَقَهُۥ",
      "translation": "Allah, onu hangi şeyden yarattı?",
      "footnotes": null
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 585,
      "verse_key": "80:19",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مِن نُّطۡفَةٍ خَلَقَهُۥ فَقَدَّرَهُۥ",
      "translation": " Bir sperm damlasından onu yaratıp, (aşamalardan geçirerek) biçime koydu. ",
      "footnotes": null
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 585,
      "verse_key": "80:20",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ ٱلسَّبِيلَ يَسَّرَهُۥ",
      "translation": " Sonra da ona yolu kolaylaştırdı.",
      "footnotes": null
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 585,
      "verse_key": "80:21",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ أَمَاتَهُۥ فَأَقۡبَرَهُۥ",
      "translation": " Sonra onu öldürüp kabre koydu. ",
      "footnotes": null
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 585,
      "verse_key": "80:22",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ إِذَا شَآءَ أَنشَرَهُۥ",
      "translation": " Sonra, onu dilediği zaman yeniden diriltecek.",
      "footnotes": null
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 585,
      "verse_key": "80:23",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّا لَمَّا يَقۡضِ مَآ أَمَرَهُۥ",
      "translation": " Hayır! Buna rağmen henüz onun emrini yerine getirmedi.",
      "footnotes": null
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 585,
      "verse_key": "80:24",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلۡيَنظُرِ ٱلۡإِنسَٰنُ إِلَىٰ طَعَامِهِۦٓ",
      "translation": " İnsan yemeğine bir baksın. ",
      "footnotes": null
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 585,
      "verse_key": "80:25",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَنَّا صَبَبۡنَا ٱلۡمَآءَ صَبّٗا",
      "translation": " Ki, biz suyu döktükçe döktük.",
      "footnotes": null
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 585,
      "verse_key": "80:26",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ شَقَقۡنَا ٱلۡأَرۡضَ شَقّٗا",
      "translation": " Sonra yeri yardıkça yardık.",
      "footnotes": null
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 585,
      "verse_key": "80:27",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَنۢبَتۡنَا فِيهَا حَبّٗا",
      "translation": " Ve orada taneler bitirdik.",
      "footnotes": null
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 585,
      "verse_key": "80:28",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَعِنَبٗا وَقَضۡبٗا",
      "translation": " Üzümler, yoncalar.",
      "footnotes": null
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 585,
      "verse_key": "80:29",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَزَيۡتُونٗا وَنَخۡلٗا",
      "translation": " Zeytinler, hurmalar.",
      "footnotes": null
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 585,
      "verse_key": "80:30",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَحَدَآئِقَ غُلۡبٗا",
      "translation": " İri ağaçlı bahçeler.",
      "footnotes": null
    },
    {
      "id": 31,
      "verse_number": 31,
      "page_number": 585,
      "verse_key": "80:31",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَفَٰكِهَةٗ وَأَبّٗا",
      "translation": " Meyveler ve otlaklar.",
      "footnotes": null
    },
    {
      "id": 32,
      "verse_number": 32,
      "page_number": 585,
      "verse_key": "80:32",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَّتَٰعٗا لَّكُمۡ وَلِأَنۡعَٰمِكُمۡ",
      "translation": " Sizin ve hayvanlarınız için bir meta olarak.",
      "footnotes": null
    },
    {
      "id": 33,
      "verse_number": 33,
      "page_number": 585,
      "verse_key": "80:33",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِذَا جَآءَتِ ٱلصَّآخَّةُ",
      "translation": " O büyük gürültü geldiği zaman,",
      "footnotes": null
    },
    {
      "id": 34,
      "verse_number": 34,
      "page_number": 585,
      "verse_key": "80:34",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَوۡمَ يَفِرُّ ٱلۡمَرۡءُ مِنۡ أَخِيهِ",
      "translation": " O gün kişi kardeşinden kaçar.",
      "footnotes": null
    },
    {
      "id": 35,
      "verse_number": 35,
      "page_number": 585,
      "verse_key": "80:35",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأُمِّهِۦ وَأَبِيهِ",
      "translation": " Anasından, babasından.",
      "footnotes": null
    },
    {
      "id": 36,
      "verse_number": 36,
      "page_number": 585,
      "verse_key": "80:36",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَصَٰحِبَتِهِۦ وَبَنِيهِ",
      "translation": " Eşinden ve evladından.",
      "footnotes": null
    },
    {
      "id": 37,
      "verse_number": 37,
      "page_number": 585,
      "verse_key": "80:37",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لِكُلِّ ٱمۡرِيٕٖ مِّنۡهُمۡ يَوۡمَئِذٖ شَأۡنٞ يُغۡنِيهِ",
      "translation": " O gün herkes için kendine yetecek bir işi vardır.",
      "footnotes": null
    },
    {
      "id": 38,
      "verse_number": 38,
      "page_number": 585,
      "verse_key": "80:38",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وُجُوهٞ يَوۡمَئِذٖ مُّسۡفِرَةٞ",
      "translation": " Yüzler vardır o gün apaydınlık.",
      "footnotes": null
    },
    {
      "id": 39,
      "verse_number": 39,
      "page_number": 585,
      "verse_key": "80:39",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ضَاحِكَةٞ مُّسۡتَبۡشِرَةٞ",
      "translation": " Güleç ve neşeli.",
      "footnotes": null
    },
    {
      "id": 40,
      "verse_number": 40,
      "page_number": 585,
      "verse_key": "80:40",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَوُجُوهٞ يَوۡمَئِذٍ عَلَيۡهَا غَبَرَةٞ",
      "translation": " Yüzler vardır o gün, üzeri tozlu.",
      "footnotes": null
    },
    {
      "id": 41,
      "verse_number": 41,
      "page_number": 585,
      "verse_key": "80:41",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تَرۡهَقُهَا قَتَرَةٌ",
      "translation": " Karartı bürümüş.",
      "footnotes": null
    },
    {
      "id": 42,
      "verse_number": 42,
      "page_number": 585,
      "verse_key": "80:42",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أُوْلَٰٓئِكَ هُمُ ٱلۡكَفَرَةُ ٱلۡفَجَرَةُ",
      "translation": " İşte onlar, kâfirler ve facirler onlardır.",
      "footnotes": null
    }
  ]
}