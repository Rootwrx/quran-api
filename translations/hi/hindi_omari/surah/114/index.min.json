{
  "verses": [
    {
      "number": "1",
      "arabic_text": "قُلۡ أَعُوذُ بِرَبِّ ٱلنَّاسِ",
      "translation": "(ऐ नबी!) कह दीजिए : मैं शरण लेता हूँ लोगों के पालनहार की।"
    },
    {
      "number": "2",
      "arabic_text": "مَلِكِ ٱلنَّاسِ",
      "translation": "लोगों के बादशाह की।"
    },
    {
      "number": "3",
      "arabic_text": "إِلَٰهِ ٱلنَّاسِ",
      "translation": "लोगों के सत्य पूज्य की।[1]"
    },
    {
      "number": "4",
      "arabic_text": "مِن شَرِّ ٱلۡوَسۡوَاسِ ٱلۡخَنَّاسِ",
      "translation": "वसवसा डालने वाले, पीछे हट जाने वाले की बुराई से।"
    },
    {
      "number": "5",
      "arabic_text": "ٱلَّذِي يُوَسۡوِسُ فِي صُدُورِ ٱلنَّاسِ",
      "translation": "जो लोगों के दिलों में वसवसे डालता है।"
    },
    {
      "number": "6",
      "arabic_text": "مِنَ ٱلۡجِنَّةِ وَٱلنَّاسِ",
      "translation": "जिन्नों और इनसानों में से।[2]"
    }
  ]
}