{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 590,
      "verse_key": "85:1",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلسَّمَآءِ ذَاتِ ٱلۡبُرُوجِ",
      "translation": "អល់ឡោះស្បថនឹងមេឃដែលមានផ្កាយជាច្រើន។",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 590,
      "verse_key": "85:2",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡيَوۡمِ ٱلۡمَوۡعُودِ",
      "translation": "និងថ្ងៃដែលបានសន្យា(ថ្ងៃបរលោក)។",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 590,
      "verse_key": "85:3",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَشَاهِدٖ وَمَشۡهُودٖ",
      "translation": "និងអ្នកដែលធ្វើសាក្សី ហើយនិងភស្ដុតាងទាំងឡាយដែល គេយកមកបង្ហាញ។",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 590,
      "verse_key": "85:4",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قُتِلَ أَصۡحَٰبُ ٱلۡأُخۡدُودِ",
      "translation": "គេបានដាក់បណ្ដាសាចំពោះក្រុមអូខទូត(ជាពួកដែលជីក រណ្ដៅសម្លាប់អ្នកមានជំនឿ)។",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 590,
      "verse_key": "85:5",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلنَّارِ ذَاتِ ٱلۡوَقُودِ",
      "translation": "ជារណ្ដៅភ្លើងដែលមានកំណាត់អូស",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 590,
      "verse_key": "85:6",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذۡ هُمۡ عَلَيۡهَا قُعُودٞ",
      "translation": "ខណៈដែលពួកគេអង្គុយនៅជុំវិញរណ្ដៅភ្លើងនោះ",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 590,
      "verse_key": "85:7",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَهُمۡ عَلَىٰ مَا يَفۡعَلُونَ بِٱلۡمُؤۡمِنِينَ شُهُودٞ",
      "translation": "ដោយពួកគេសម្លឹងមើលទៅលើអ្វីដែលពួកគេកំពុងប្រព្រឹត្ដ ទៅលើបណ្ដាអ្នកដែលមានជំនឿ។",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 590,
      "verse_key": "85:8",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا نَقَمُواْ مِنۡهُمۡ إِلَّآ أَن يُؤۡمِنُواْ بِٱللَّهِ ٱلۡعَزِيزِ ٱلۡحَمِيدِ",
      "translation": "ហើយពួកគេបានធ្វើបាបបណ្ដាអ្នកដែលមានជំនឿ គឺដោយ សារតែពួកគេមានជំនឿលើអល់ឡោះដ៏មហាខ្លាំងពូកែនិងជាទីកោតសរសើរ។",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 590,
      "verse_key": "85:9",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِي لَهُۥ مُلۡكُ ٱلسَّمَٰوَٰتِ وَٱلۡأَرۡضِۚ وَٱللَّهُ عَلَىٰ كُلِّ شَيۡءٖ شَهِيدٌ",
      "translation": "គឺជាអ្នកដែលមានការគ្រប់គ្រងមេឃជាច្រើនជាន់និងផែនដី។ ហើយអល់ឡោះ គឺជាសាក្សីទៅលើអ្វីៗទាំងអស់។",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 590,
      "verse_key": "85:10",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ ٱلَّذِينَ فَتَنُواْ ٱلۡمُؤۡمِنِينَ وَٱلۡمُؤۡمِنَٰتِ ثُمَّ لَمۡ يَتُوبُواْ فَلَهُمۡ عَذَابُ جَهَنَّمَ وَلَهُمۡ عَذَابُ ٱلۡحَرِيقِ",
      "translation": "ពិតប្រាកដណាស់ ពួកដែលដុតបណ្ដាអ្នកដែលមានជំនឿប្រុសស្រី បន្ទាប់មកពួកគេមិនបានសារភាពទោសកំហុស (ចំពោះអល់ឡោះ)ទេនោះ ពួកគេនឹងត្រូវទទួលទារុណកម្មនៅក្នុង នរកជើហាន់ណាំ និងទទួលទារុណកម្មដែលឆាបឆេះយ៉ាងឈឺចាប់ បំផុត។",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 590,
      "verse_key": "85:11",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ ٱلَّذِينَ ءَامَنُواْ وَعَمِلُواْ ٱلصَّٰلِحَٰتِ لَهُمۡ جَنَّٰتٞ تَجۡرِي مِن تَحۡتِهَا ٱلۡأَنۡهَٰرُۚ ذَٰلِكَ ٱلۡفَوۡزُ ٱلۡكَبِيرُ",
      "translation": "ពិតប្រាកដណាស់ បណ្ដាអ្នកដែលមានជំនឿ និងបានសាងអំពើល្អត្រឹមត្រូវនោះ ពួកគេនឹងទទួលបានឋានសួគ៌ដែលមានទនេ្ល ជាច្រើនហូរពីក្រោមវា។ នោះគឺជាជោគជ័យដ៏ធំធេង។",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 590,
      "verse_key": "85:12",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ بَطۡشَ رَبِّكَ لَشَدِيدٌ",
      "translation": "ពិតប្រាកដណាស់ ទារុណកម្មនៃម្ចាស់របស់អ្នកគឺខ្លាំងក្លា បំផុត។",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 590,
      "verse_key": "85:13",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُۥ هُوَ يُبۡدِئُ وَيُعِيدُ",
      "translation": "ពិតប្រាកដណាស់ ទ្រង់គឺជាអ្នកដែលបង្កើត និងជាអ្នកធ្វើឱ្យរស់ឡើងវិញ។",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 590,
      "verse_key": "85:14",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَهُوَ ٱلۡغَفُورُ ٱلۡوَدُودُ",
      "translation": "ហើយទ្រង់មហាអភ័យទោស មហាអាណិតស្រឡាញ់។",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 590,
      "verse_key": "85:15",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ذُو ٱلۡعَرۡشِ ٱلۡمَجِيدُ",
      "translation": "ម្ចាស់នៃអារ៉ស្ហដ៏ថ្កុំថ្កើង។",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 590,
      "verse_key": "85:16",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَعَّالٞ لِّمَا يُرِيدُ",
      "translation": "ជាម្ចាស់ដែលធ្វើតាមអ្វីដែលទ្រង់មានចេតនា។",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 590,
      "verse_key": "85:17",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "هَلۡ أَتَىٰكَ حَدِيثُ ٱلۡجُنُودِ",
      "translation": "តើរឿងរបស់កងទ័ពបានមកដល់អ្នក(មូហាំម៉ាត់)ឬទេ?",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 590,
      "verse_key": "85:18",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِرۡعَوۡنَ وَثَمُودَ",
      "translation": "(ដូចជា)ហ្វៀរអោន និងសាមូដ។",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 590,
      "verse_key": "85:19",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بَلِ ٱلَّذِينَ كَفَرُواْ فِي تَكۡذِيبٖ",
      "translation": "ផ្ទុយទៅវិញ ពួកដែលគ្មានជំនឿនៅតែព្យាយាម បដិសេធ។",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 590,
      "verse_key": "85:20",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱللَّهُ مِن وَرَآئِهِم مُّحِيطُۢ",
      "translation": "ហើយអល់ឡោះគឺជាអ្នកឡោមព័ទ្ធពីខាងក្រោយពួកគេ (ដឹងរាល់់សកម្មភាពរបស់ពួកគេ)។",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 590,
      "verse_key": "85:21",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بَلۡ هُوَ قُرۡءَانٞ مَّجِيدٞ",
      "translation": "ផ្ទុយទៅវិញ វាគឺជាគម្ពីរគួរអានដ៏មហាថ្កុំថ្កើង។",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 590,
      "verse_key": "85:22",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِي لَوۡحٖ مَّحۡفُوظِۭ",
      "translation": "ដែលមាននៅក្នុងសៀវភៅកំណត់ត្រា(ឡាវហុលម៉ះ- ហ៊្វូស)។",
      "footnotes": ""
    }
  ]
}