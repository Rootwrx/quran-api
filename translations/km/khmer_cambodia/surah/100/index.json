{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 599,
      "verse_key": "100:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡعَٰدِيَٰتِ ضَبۡحٗا",
      "translation": "អល់ឡោះស្បថនឹងសត្វសេះទាំងឡាយដែលរត់ទាំងដង្ហក់ នៅក្នុងសមរភូមិ។",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 599,
      "verse_key": "100:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَٱلۡمُورِيَٰتِ قَدۡحٗا",
      "translation": "និងសត្វសេះដែលរត់យ៉ាងលឿនចេញផ្កាភ្លើងតាមក្រចក របស់វា។",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 599,
      "verse_key": "100:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَٱلۡمُغِيرَٰتِ صُبۡحٗا",
      "translation": "និងសត្វសេះដែលវាបោលសម្រុកទៅខ្មាំងសត្រូវទាំងព្រឹក ព្រលឹម។",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 599,
      "verse_key": "100:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَثَرۡنَ بِهِۦ نَقۡعٗا",
      "translation": "ហើយពួកវាធ្វើឱ្យធូលីហុយទ្រលោមនៅចំពោះមុខពួក សត្រូវ ។",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 599,
      "verse_key": "100:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَوَسَطۡنَ بِهِۦ جَمۡعًا",
      "translation": "ហើយពួកវាសម្រុកចូលទៅកណ្ដាលហ្វូងសត្រូវ ។",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 599,
      "verse_key": "100:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ ٱلۡإِنسَٰنَ لِرَبِّهِۦ لَكَنُودٞ",
      "translation": "ពិតប្រាកដណាស់ មនុស្សលោកគឺរមិលគុណចំពោះម្ចាស់ របស់គេ។",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 599,
      "verse_key": "100:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِنَّهُۥ عَلَىٰ ذَٰلِكَ لَشَهِيدٞ",
      "translation": "ហើយពិតប្រាកដណាស់ គេគឺជាសាក្សីលើរឿងនោះ",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 599,
      "verse_key": "100:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِنَّهُۥ لِحُبِّ ٱلۡخَيۡرِ لَشَدِيدٌ",
      "translation": "ពីព្រោះគេ គឺជាមនុស្សដែលស្រឡាញ់ទ្រព្យសម្បត្ដិយ៉ាង ខ្លាំង។",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 599,
      "verse_key": "100:9",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "۞ أَفَلَا يَعۡلَمُ إِذَا بُعۡثِرَ مَا فِي ٱلۡقُبُورِ",
      "translation": "ដូចេ្នះ តើគេមិនដឹងទេឬថា នៅពេលដែលអ្វីៗនៅក្នុងផ្នូរ ត្រូវគេបពោ្ចញមក(ដើម្បីជំនុំជំរះ)?",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 600,
      "verse_key": "100:10",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَحُصِّلَ مَا فِي ٱلصُّدُورِ",
      "translation": "និងអ្វីដែលមាននៅក្នុងចិត្ដ(របស់មនុស្ស)នឹងត្រូវគេ លាតត្រដាងឱ្យឃើញយ៉ាងច្បាស់នោះ ?",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 600,
      "verse_key": "100:11",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ رَبَّهُم بِهِمۡ يَوۡمَئِذٖ لَّخَبِيرُۢ",
      "translation": "ពិតប្រាកដណាស់ នៅថ្ងៃនោះម្ចាស់របស់ពួកគេដឹងបំផុត ចំពោះរឿងរ៉ាវរបស់ពួកគេ។",
      "footnotes": ""
    }
  ]
}