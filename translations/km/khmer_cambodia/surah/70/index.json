{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 568,
      "verse_key": "70:1",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "سَأَلَ سَآئِلُۢ بِعَذَابٖ وَاقِعٖ",
      "translation": "អ្នកសុំម្នាក់(អ្នកគ្មានជំនឿ)បានបួងសួងសុំឱ្យទណ្ឌណកម្មកើតឡើង",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 568,
      "verse_key": "70:2",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لِّلۡكَٰفِرِينَ لَيۡسَ لَهُۥ دَافِعٞ",
      "translation": "ចំពោះពួកដែលគ្មានជំនឿ។ គ្មានអ្នកណាម្នាក់អាចការពារគេ",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 568,
      "verse_key": "70:3",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مِّنَ ٱللَّهِ ذِي ٱلۡمَعَارِجِ",
      "translation": "ពី(ទារុណកម្ម)អល់ឡោះដែលជាម្ចាស់នៃផ្លូវឡើងទៅលើ មេឃ",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 568,
      "verse_key": "70:4",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تَعۡرُجُ ٱلۡمَلَٰٓئِكَةُ وَٱلرُّوحُ إِلَيۡهِ فِي يَوۡمٖ كَانَ مِقۡدَارُهُۥ خَمۡسِينَ أَلۡفَ سَنَةٖ",
      "translation": "ដែលម៉ាឡាអ៊ីកាត់ និងជីព្រីលឡើងទៅកាន់ទ្រង់ក្នុងមួយ ថ្ងៃដែលរយៈពេលរបស់វាចំនួនហាសិបពាន់ឆ្នាំឡើយ។",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 568,
      "verse_key": "70:5",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَٱصۡبِرۡ صَبۡرٗا جَمِيلًا",
      "translation": "ដូចេ្នះចូរអ្នក(មូហាំម៉ាត់)អត់ធ្មត់នូវការអត់ធ្មត់មួយ ដ៏ល្អចុះ។",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 568,
      "verse_key": "70:6",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُمۡ يَرَوۡنَهُۥ بَعِيدٗا",
      "translation": "ពិតប្រាកដណាស់ ពួកគេ(ពួកគ្មានជំនឿ)ឃើញទារុណកម្មនៅឆ្ងាយបំផុត។",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 568,
      "verse_key": "70:7",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَنَرَىٰهُ قَرِيبٗا",
      "translation": "តែយើងឃើញវានៅជិតបង្កើយ។",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 568,
      "verse_key": "70:8",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَوۡمَ تَكُونُ ٱلسَّمَآءُ كَٱلۡمُهۡلِ",
      "translation": "នៅថ្ងៃដែលមេឃនឹងក្លាយដូចជាលោហធាតុកំពុងរលាយ។",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 568,
      "verse_key": "70:9",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَتَكُونُ ٱلۡجِبَالُ كَٱلۡعِهۡنِ",
      "translation": "ហើយភ្នំក៏ក្លាយដូចជារោមសត្វប៉ើងព្រោងព្រាត។",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 568,
      "verse_key": "70:10",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَا يَسۡـَٔلُ حَمِيمٌ حَمِيمٗا",
      "translation": "(នៅថ្ងៃនោះ)គ្មានមិត្ដភកិ្ដណាសួរសុខទុក្ខមិត្ដភកិ្ដណា ឡើយ",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 569,
      "verse_key": "70:11",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يُبَصَّرُونَهُمۡۚ يَوَدُّ ٱلۡمُجۡرِمُ لَوۡ يَفۡتَدِي مِنۡ عَذَابِ يَوۡمِئِذِۭ بِبَنِيهِ",
      "translation": "ទោះបីជាពួកគេត្រូវគេឱ្យឃើញគ្នាក៏ដោយ។ អ្នកសាង អំពើបាបមានបំណងលោះខ្លួនឱ្យរួចផុតពីទារុណកម្មនៅថ្ងៃនោះ ដោយកូនចៅរបស់គេ",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 569,
      "verse_key": "70:12",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَصَٰحِبَتِهِۦ وَأَخِيهِ",
      "translation": "និងភរិយា និងបងប្អូនរបស់គេ",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 569,
      "verse_key": "70:13",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَفَصِيلَتِهِ ٱلَّتِي تُـٔۡوِيهِ",
      "translation": "និងក្រុមគ្រួសារ សាច់សារលោហិតរបស់គេដែលផ្ដល់ទីជំរកដល់គេ",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 569,
      "verse_key": "70:14",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَن فِي ٱلۡأَرۡضِ جَمِيعٗا ثُمَّ يُنجِيهِ",
      "translation": "និងអ្នកដែលនៅលើផែនដីទាំងអស់ បន្ទាប់មកសង្គ្រោះ គេឱ្យផុតពីទារុណកម្ម។",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 569,
      "verse_key": "70:15",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّآۖ إِنَّهَا لَظَىٰ",
      "translation": "មិនដូច្នោះទេ. ពិតប្រាកដណាស់វាគឺជាភ្លើងនរកដែលឆេះ សន្ធោសន្ធៅ",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 569,
      "verse_key": "70:16",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "نَزَّاعَةٗ لِّلشَّوَىٰ",
      "translation": "ដែលវាបកស្បែកក្បាល (និងខ្លួនប្រាណ)។",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 569,
      "verse_key": "70:17",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تَدۡعُواْ مَنۡ أَدۡبَرَ وَتَوَلَّىٰ",
      "translation": "វា(នរក)ស្រែកហៅអ្នកដែលបានបែរខ្នង និងគេចចេញ (ពីមាគ៌ាត្រឹមត្រូវ)",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 569,
      "verse_key": "70:18",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَجَمَعَ فَأَوۡعَىٰٓ",
      "translation": "និងបានប្រមូល(ទ្រព្យសម្បត្ដិ) រួចលាក់ទុក(មិនបរិច្ចាគ ទាន)។",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 569,
      "verse_key": "70:19",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "۞ إِنَّ ٱلۡإِنسَٰنَ خُلِقَ هَلُوعًا",
      "translation": "ពិតប្រាកដណាស់ មនុស្សលោកត្រូវបានគេបង្កើតឡើង ដោយគ្មានការអត់ធ្មត់ឡើយ។",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 569,
      "verse_key": "70:20",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذَا مَسَّهُ ٱلشَّرُّ جَزُوعٗا",
      "translation": "ដោយគេមានក្ដីព្រួយបារម្ភនៅពេលដែលគ្រោះអាក្រក់ បានកើតឡើងចំពោះគេ។",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 569,
      "verse_key": "70:21",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا مَسَّهُ ٱلۡخَيۡرُ مَنُوعًا",
      "translation": "ហើយគេបែរជាកំណាញ់ នៅពេលដែលគេបានទទួលបាន ផលល្អ(មានទ្រព្យសម្បត្ដិ)។",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 569,
      "verse_key": "70:22",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَّا ٱلۡمُصَلِّينَ",
      "translation": "លើកលែងតែបណ្ដាអ្នកដែលប្រតិបត្ដិសឡាតប៉ុណ្ណោះ។",
      "footnotes": ""
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 569,
      "verse_key": "70:23",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِينَ هُمۡ عَلَىٰ صَلَاتِهِمۡ دَآئِمُونَ",
      "translation": "ដែលពួកគេប្រកាន់ខ្ជាប់នឹងការប្រតិបត្ដិសឡាតរបស់គេ ជានិច្ច។",
      "footnotes": ""
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 569,
      "verse_key": "70:24",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّذِينَ فِيٓ أَمۡوَٰلِهِمۡ حَقّٞ مَّعۡلُومٞ",
      "translation": "និងបណ្ដាអ្នកដែលទ្រព្យសម្បត្ដិរបស់ពួកគេគឺមានសិទ្ធិ (មានចំណែក)ដែលត្រូវបានគេកំណត់(ឱ្យបរិច្ចាគភាគទាន)",
      "footnotes": ""
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 569,
      "verse_key": "70:25",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لِّلسَّآئِلِ وَٱلۡمَحۡرُومِ",
      "translation": "សម្រាប់អ្នកក្រដែលហ៊ានសុំ និងអ្នកក្រដែលមិនហ៊ានសុំ។",
      "footnotes": ""
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 569,
      "verse_key": "70:26",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّذِينَ يُصَدِّقُونَ بِيَوۡمِ ٱلدِّينِ",
      "translation": "និងបណ្ដាអ្នកដែលជឿជាក់លើថ្ងៃជំនុំជំរះ(ថ្ងៃបរលោក)។",
      "footnotes": ""
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 569,
      "verse_key": "70:27",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّذِينَ هُم مِّنۡ عَذَابِ رَبِّهِم مُّشۡفِقُونَ",
      "translation": "និងបណ្ដាអ្នកដែលកោតខ្លាចទារុណកម្មនៃម្ចាស់របស់ ពួកគេ។",
      "footnotes": ""
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 569,
      "verse_key": "70:28",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ عَذَابَ رَبِّهِمۡ غَيۡرُ مَأۡمُونٖ",
      "translation": "ពិតប្រាកដណាស់ ទារុណកម្មនៃម្ចាស់របស់ពួកគេគឺគ្មាន អ្នកណាម្នាក់មានសុវត្ថិភាពឡើយ។",
      "footnotes": ""
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 569,
      "verse_key": "70:29",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّذِينَ هُمۡ لِفُرُوجِهِمۡ حَٰفِظُونَ",
      "translation": "និងបណ្ដាអ្នកដែលបានថែរក្សាប្រដាប់ភេទរបស់ពួកគេ (ពីអំពើហ្ស៊ីណា)។",
      "footnotes": ""
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 569,
      "verse_key": "70:30",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَّا عَلَىٰٓ أَزۡوَٰجِهِمۡ أَوۡ مَا مَلَكَتۡ أَيۡمَٰنُهُمۡ فَإِنَّهُمۡ غَيۡرُ مَلُومِينَ",
      "translation": "លើកលែងតែចំពោះភរិយារបស់ពួកគេ និងខ្ញុំបម្រើស្រី ដាច់ថ្លៃដែលនៅក្រោមការគ្រប់គ្រងរបស់ពួកគេប៉ុណ្ណោះ។ ហើយ ពិតប្រាកដណាស់ ពួកគេមិនត្រូវបានគេយកទោសពៃរ៍ឡើយ។",
      "footnotes": ""
    },
    {
      "id": 31,
      "verse_number": 31,
      "page_number": 569,
      "verse_key": "70:31",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَمَنِ ٱبۡتَغَىٰ وَرَآءَ ذَٰلِكَ فَأُوْلَٰٓئِكَ هُمُ ٱلۡعَادُونَ",
      "translation": "អ្នកណាហើយដែលបានស្វែងរកការតេ្រកត្រអាលក្រៅពី (ប្រពន្ធ និងខ្ញុំបម្រើដាច់ថ្លៃ)នោះ ពួកទាំងនោះហើយគឺជាពួកដែល បានល្មើស(នឹងបទបញ្ញត្ដិរបស់អល់ឡោះ)។",
      "footnotes": ""
    },
    {
      "id": 32,
      "verse_number": 32,
      "page_number": 569,
      "verse_key": "70:32",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّذِينَ هُمۡ لِأَمَٰنَٰتِهِمۡ وَعَهۡدِهِمۡ رَٰعُونَ",
      "translation": "ហើយបណ្ដាអ្នកដែលថែរក្សានូវភាពស្មោះត្រង់ និងការសន្យារបស់ពួកគេ។",
      "footnotes": ""
    },
    {
      "id": 33,
      "verse_number": 33,
      "page_number": 569,
      "verse_key": "70:33",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّذِينَ هُم بِشَهَٰدَٰتِهِمۡ قَآئِمُونَ",
      "translation": "និងបណ្ដាអ្នកដែលប្រកាន់ខ្ជាប់(ដោយយុត្ដិធម៌)ក្នុងការ ធ្វើសាក្សីរបស់ពួកគេ។",
      "footnotes": ""
    },
    {
      "id": 34,
      "verse_number": 34,
      "page_number": 569,
      "verse_key": "70:34",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّذِينَ هُمۡ عَلَىٰ صَلَاتِهِمۡ يُحَافِظُونَ",
      "translation": "ហើយនិងបណ្ដាអ្នកដែលថែរក្សានូវការប្រតិបត្ដិសឡាត របស់ពួកគេ។",
      "footnotes": ""
    },
    {
      "id": 35,
      "verse_number": 35,
      "page_number": 569,
      "verse_key": "70:35",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أُوْلَٰٓئِكَ فِي جَنَّٰتٖ مُّكۡرَمُونَ",
      "translation": "អ្នកទាំងនោះហើយ គឺជាអ្នកដែលត្រូវគេលើកតម្កើងនៅ ក្នុងឋានសួគ៌។",
      "footnotes": ""
    },
    {
      "id": 36,
      "verse_number": 36,
      "page_number": 569,
      "verse_key": "70:36",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَمَالِ ٱلَّذِينَ كَفَرُواْ قِبَلَكَ مُهۡطِعِينَ",
      "translation": "តើហេតុអ្វីបានជាពួកគ្មានជំនឿប្រញាប់ប្រញាល់ឆ្ពោះទៅ កាន់អ្នក",
      "footnotes": ""
    },
    {
      "id": 37,
      "verse_number": 37,
      "page_number": 569,
      "verse_key": "70:37",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَنِ ٱلۡيَمِينِ وَعَنِ ٱلشِّمَالِ عِزِينَ",
      "translation": "ទាំងខាងស្ដាំ និងខាងឆេ្វងជាក្រុមៗដូចេ្នះ?",
      "footnotes": ""
    },
    {
      "id": 38,
      "verse_number": 38,
      "page_number": 569,
      "verse_key": "70:38",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَيَطۡمَعُ كُلُّ ٱمۡرِيٕٖ مِّنۡهُمۡ أَن يُدۡخَلَ جَنَّةَ نَعِيمٖ",
      "translation": "តើរាល់ៗបុគ្គលក្នុងចំណោមពួកគេមានបំណងចូលឋាន សួគ៌ណាអ៊ីមឬ?",
      "footnotes": ""
    },
    {
      "id": 39,
      "verse_number": 39,
      "page_number": 569,
      "verse_key": "70:39",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّآۖ إِنَّا خَلَقۡنَٰهُم مِّمَّا يَعۡلَمُونَ",
      "translation": "មិនដូច្នោះទេ. ពិតប្រាកដណាស់ យើងបានបង្កើតពួកគេ អំពីអ្វីដែលពួកគេបានដឹងស្រាប់(ទឹកកាម)។",
      "footnotes": ""
    },
    {
      "id": 40,
      "verse_number": 40,
      "page_number": 570,
      "verse_key": "70:40",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلَآ أُقۡسِمُ بِرَبِّ ٱلۡمَشَٰرِقِ وَٱلۡمَغَٰرِبِ إِنَّا لَقَٰدِرُونَ",
      "translation": "យើងស្បថនឹងម្ចាស់គ្រប់គ្រង(អល់ឡោះស្បថនឹងខ្លួនឯង) កន្លែងដែលព្រះអាទិត្យនិងព្រះច័ន្ទរះ និងកន្លែងដែលព្រះអាទិត្យនិង ព្រះច័ន្ទលិចថា ពិតប្រាកដណាស់យើងគឺជាអ្នកដែលមានអានុភាព",
      "footnotes": ""
    },
    {
      "id": 41,
      "verse_number": 41,
      "page_number": 570,
      "verse_key": "70:41",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلَىٰٓ أَن نُّبَدِّلَ خَيۡرٗا مِّنۡهُمۡ وَمَا نَحۡنُ بِمَسۡبُوقِينَ",
      "translation": "លើការជំនួសក្រុមផេ្សងដែលល្អជាងពួកគេទៅទៀត ហើយគ្មានអ្នកណាម្នាក់អាចយកឈ្នះយើងបានឡើយ។",
      "footnotes": ""
    },
    {
      "id": 42,
      "verse_number": 42,
      "page_number": 570,
      "verse_key": "70:42",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَذَرۡهُمۡ يَخُوضُواْ وَيَلۡعَبُواْ حَتَّىٰ يُلَٰقُواْ يَوۡمَهُمُ ٱلَّذِي يُوعَدُونَ",
      "translation": "ដូចេ្នះ ចូរអ្នក(មូហាំម៉ាត់)បណ្ដោយឱ្យពួកគេលិចលង់ (ក្នុងភាពវងេ្វង) និងលេងសើចរហូតដល់ពួកគេជួបនឹងថ្ងៃរបស់ ពួកគេដែលត្រូវគេសន្យាធ្វើទារុណកម្ម(នៅថ្ងៃបរលោក)ចុះ។",
      "footnotes": ""
    },
    {
      "id": 43,
      "verse_number": 43,
      "page_number": 570,
      "verse_key": "70:43",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَوۡمَ يَخۡرُجُونَ مِنَ ٱلۡأَجۡدَاثِ سِرَاعٗا كَأَنَّهُمۡ إِلَىٰ نُصُبٖ يُوفِضُونَ",
      "translation": "គឺនៅថ្ងៃដែលពួកគេចេញពីផ្នូរយ៉ាងលឿនប្រៀបដូចជា ពួកគេឆ្ពោះទៅកាន់រូបសំណាកដែលពួកគេបានបង្កើតដើម្បីគោរព សក្ការៈ(នៅក្នុងលោកិយ)។",
      "footnotes": ""
    },
    {
      "id": 44,
      "verse_number": 44,
      "page_number": 570,
      "verse_key": "70:44",
      "juz_number": 29,
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "خَٰشِعَةً أَبۡصَٰرُهُمۡ تَرۡهَقُهُمۡ ذِلَّةٞۚ ذَٰلِكَ ٱلۡيَوۡمُ ٱلَّذِي كَانُواْ يُوعَدُونَ",
      "translation": "ភ្នែករបស់ពួកគេទម្លាក់ចុះ ខណៈដែលការលំបាកគ្របដណ្ដប់ពួកគេ។ នោះគឺជាថ្ងៃដែលពួកគេត្រូវបានគេសន្យា។",
      "footnotes": ""
    }
  ]
}