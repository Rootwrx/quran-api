{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 526,
      "verse_key": "53:1",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلنَّجۡمِ إِذَا هَوَىٰ",
      "translation": "អល់ឡោះស្បថនឹងផ្កាយនៅពេលវាលិចបាត់។",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 526,
      "verse_key": "53:2",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَا ضَلَّ صَاحِبُكُمۡ وَمَا غَوَىٰ",
      "translation": "(មូហាំម៉ាត់)មិត្ដភក្ដិរបស់ពួកអ្នកមិនបានវងេ្វង និងមិនបាន ភាន់ច្រឡំឡើយ។",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 526,
      "verse_key": "53:3",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا يَنطِقُ عَنِ ٱلۡهَوَىٰٓ",
      "translation": "ហើយគេក៏មិននិយាយតាមទំនើងចិត្ដដែរ។",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 526,
      "verse_key": "53:4",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنۡ هُوَ إِلَّا وَحۡيٞ يُوحَىٰ",
      "translation": "វា(ពាក្យសំដីដែលគេនិយាយ)គ្មានអ្វីក្រៅពីវ៉ាហ៊ីដែលត្រូវ គេវ៉ាហ៊ីមកនោះឡើយ។",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 526,
      "verse_key": "53:5",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلَّمَهُۥ شَدِيدُ ٱلۡقُوَىٰ",
      "translation": "អ្នកដែលមានកម្លាំងខ្លាំងក្លា(ជីព្រីល)បានបង្រៀនគេ។",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 526,
      "verse_key": "53:6",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ذُو مِرَّةٖ فَٱسۡتَوَىٰ",
      "translation": "ដែលមានលក្ខណៈល្អឥតខ្ចោះ និងបានបង្ហាញរូបរាងពិត។",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 526,
      "verse_key": "53:7",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَهُوَ بِٱلۡأُفُقِ ٱلۡأَعۡلَىٰ",
      "translation": "ដោយគេ(ជីព្រីល)ស្ថិតនៅឯជើងមេឃដ៏ខ្ពស់បំផុត។",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 526,
      "verse_key": "53:8",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ دَنَا فَتَدَلَّىٰ",
      "translation": "បន្ទាប់មកគេបានមកជិត(មូហាំម៉ាត់) និងកាន់តែជិតថែម ទៀត។",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 526,
      "verse_key": "53:9",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَكَانَ قَابَ قَوۡسَيۡنِ أَوۡ أَدۡنَىٰ",
      "translation": "ហើយ(ការជិតនេះ)គឺមានប្រវែងរវាងចុងទាំងពីរនៃធ្នូ ឬក៏ ជិតជាងនេះទៅទៀត។",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 526,
      "verse_key": "53:10",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَوۡحَىٰٓ إِلَىٰ عَبۡدِهِۦ مَآ أَوۡحَىٰ",
      "translation": "រួចទ្រង់បានផ្ដល់វ៉ាហ៊ីឱ្យខ្ញុំរបស់ទ្រង់(មូហាំម៉ាត់)នូវអ្វី ដែលទ្រង់បានវ៉ាហ៊ី។",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 526,
      "verse_key": "53:11",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَا كَذَبَ ٱلۡفُؤَادُ مَا رَأَىٰٓ",
      "translation": "ចិត្ដរបស់គេ(មូហាំម៉ាត់)មិនបានភូតកុហកនូវអ្វីដែលគេ បានឃើញឡើយ។",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 526,
      "verse_key": "53:12",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَتُمَٰرُونَهُۥ عَلَىٰ مَا يَرَىٰ",
      "translation": "ដូចេ្នះ តើពួកអ្នកបដិសេធនឹងគេចំពោះអ្វីដែលគេឃើញ ឬ?",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 526,
      "verse_key": "53:13",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَقَدۡ رَءَاهُ نَزۡلَةً أُخۡرَىٰ",
      "translation": "ហើយជាការពិតណាស់ គេបានឃើញជីព្រីលជាថ្មីម្ដងទៀត។",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 526,
      "verse_key": "53:14",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عِندَ سِدۡرَةِ ٱلۡمُنتَهَىٰ",
      "translation": "នៅឯសិតរ៉តុលមុនតាហា(នៅចុងបពា្ចប់នៃមេឃជាន់ ទីប្រាំពីរ)។",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 526,
      "verse_key": "53:15",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عِندَهَا جَنَّةُ ٱلۡمَأۡوَىٰٓ",
      "translation": "នៅកន្លែងនោះមានឋានសួគ៌ម៉ាកវ៉ា។",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 526,
      "verse_key": "53:16",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذۡ يَغۡشَى ٱلسِّدۡرَةَ مَا يَغۡشَىٰ",
      "translation": "នៅពេលដែល(ពន្លឺរបស់អល់ឡោះ និងបណ្ដាម៉ាឡាអ៊ី-កាត់)គ្របដណ្ដប់លើសិតរ៉តុលមុនតាហានូវអ្វីដែលគ្របដណ្ដប់។",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 526,
      "verse_key": "53:17",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَا زَاغَ ٱلۡبَصَرُ وَمَا طَغَىٰ",
      "translation": "ខណៈនោះគេ(មូហាំម៉ាត់)មិនបានងាកមើល(ឆេ្វងស្ដាំ) និងមិនបានបំពានឡើយ។",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 526,
      "verse_key": "53:18",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَقَدۡ رَأَىٰ مِنۡ ءَايَٰتِ رَبِّهِ ٱلۡكُبۡرَىٰٓ",
      "translation": "ជាការពិតណាស់ គេបានឃើញភស្ដុតាងដ៏ធំមួយចំនួននៃ ម្ចាស់របស់គេ។",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 526,
      "verse_key": "53:19",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَرَءَيۡتُمُ ٱللَّٰتَ وَٱلۡعُزَّىٰ",
      "translation": "តើពួកអ្នក(ពួកមូស្ហរីគីន)យល់យ៉ាងណាចំពោះរូប សំណាកឈ្មោះឡាត និងអ៊ុសហ្សា",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 526,
      "verse_key": "53:20",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَنَوٰةَ ٱلثَّالِثَةَ ٱلۡأُخۡرَىٰٓ",
      "translation": "ហើយនិងម៉ាណាត ដែលជារូបសំណាកទីបីផេ្សងទៀត នោះ (តើវាផ្ដល់ផលប្រយោជន៍ ឬបង្កគ្រោះថ្នាក់ដល់ពួកអ្នកឬទេ)?",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 526,
      "verse_key": "53:21",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَكُمُ ٱلذَّكَرُ وَلَهُ ٱلۡأُنثَىٰ",
      "translation": "តើពួកអ្នកមានកូនប្រុស ហើយទ្រង់មានកូនស្រីឬ?",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 526,
      "verse_key": "53:22",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تِلۡكَ إِذٗا قِسۡمَةٞ ضِيزَىٰٓ",
      "translation": "បើដូច្នោះមែន នោះគឺជាការបែងចែកមួយដ៏អយុត្ដិធម៌ បំផុត។",
      "footnotes": ""
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 526,
      "verse_key": "53:23",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنۡ هِيَ إِلَّآ أَسۡمَآءٞ سَمَّيۡتُمُوهَآ أَنتُمۡ وَءَابَآؤُكُم مَّآ أَنزَلَ ٱللَّهُ بِهَا مِن سُلۡطَٰنٍۚ إِن يَتَّبِعُونَ إِلَّا ٱلظَّنَّ وَمَا تَهۡوَى ٱلۡأَنفُسُۖ وَلَقَدۡ جَآءَهُم مِّن رَّبِّهِمُ ٱلۡهُدَىٰٓ",
      "translation": "វាទាំងនោះគ្មានអី្វក្រៅពីឈ្មោះដែលពួកអ្នកនិងជីដូន ជីតារបស់ពួកអ្នកបានដាក់ឈ្មោះឱ្យពួកវានោះឡើយ។ អល់ឡោះ មិនបានបញ្ចុះភស្ដុតាងណាមួយសម្រាប់(ឱ្យពួកអ្នកគោរពសក្ការៈ) វាឡើយ។ ពួកគេមិនធ្វើតាមអ្វីក្រៅពីការស្មាន និងតាមទំនើងចិត្ដ ឡើយ។ ហើយជាពិតណាស់ ការចង្អុលបង្ហាញពីម្ចាស់របស់ពួកគេ ពិតជាបានមកដល់ពួកគេ។",
      "footnotes": ""
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 526,
      "verse_key": "53:24",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَمۡ لِلۡإِنسَٰنِ مَا تَمَنَّىٰ",
      "translation": "តើមនុស្សលោកទទួលបានអ្វីដែលគេប៉ងប្រាថ្នាឬ?",
      "footnotes": ""
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 526,
      "verse_key": "53:25",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلِلَّهِ ٱلۡأٓخِرَةُ وَٱلۡأُولَىٰ",
      "translation": "(មិនដូចេ្នះឡើយ) ព្រោះកិច្ចការថ្ងៃបរលោក និង លោកិយគឺជាកម្មសិទ្ធិរបស់អល់ឡោះ។",
      "footnotes": ""
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 526,
      "verse_key": "53:26",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "۞ وَكَم مِّن مَّلَكٖ فِي ٱلسَّمَٰوَٰتِ لَا تُغۡنِي شَفَٰعَتُهُمۡ شَيۡـًٔا إِلَّا مِنۢ بَعۡدِ أَن يَأۡذَنَ ٱللَّهُ لِمَن يَشَآءُ وَيَرۡضَىٰٓ",
      "translation": "ហើយមានម៉ាឡាអ៊ីកាត់ជាច្រើននៅលើមេឃជាច្រើនជាន់ ដែលការអន្ដរាគមន៍របស់ពួកគេមិនអាចផ្ដល់ប្រយោជន៍អ្វីឡើយ លើកលែងតែបន្ទាប់ពីអល់ឡោះអនុញ្ញាតឱ្យអ្នកណាដែលទ្រង់មាន ចេតនានិងយល់ព្រមប៉ុណ្ណោះ ។",
      "footnotes": ""
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 527,
      "verse_key": "53:27",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ ٱلَّذِينَ لَا يُؤۡمِنُونَ بِٱلۡأٓخِرَةِ لَيُسَمُّونَ ٱلۡمَلَٰٓئِكَةَ تَسۡمِيَةَ ٱلۡأُنثَىٰ",
      "translation": "ពិតប្រាកដណាស់ ពួកដែលគ្មានជំនឿនឹងថ្ងៃបរលោក ពួក គេនឹងដាក់ឈ្មោះឱ្យម៉ាឡាអ៊ីកាត់ជាឈ្មោះស្រី។",
      "footnotes": ""
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 527,
      "verse_key": "53:28",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا لَهُم بِهِۦ مِنۡ عِلۡمٍۖ إِن يَتَّبِعُونَ إِلَّا ٱلظَّنَّۖ وَإِنَّ ٱلظَّنَّ لَا يُغۡنِي مِنَ ٱلۡحَقِّ شَيۡـٔٗا",
      "translation": "ដោយពួកគេពុំមានចំណេះដឹងពីវាឡើយ។ ពួកគេមិនធ្វើ តាមអ្វីក្រៅពីការស្មាននោះឡើយ។ ហើយពិតប្រាកដណាស់ ការ ស្មានមិនអាចផ្ដល់ផលប្រយោជន៍អ្វីដល់ការពិតឡើយ។",
      "footnotes": ""
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 527,
      "verse_key": "53:29",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَعۡرِضۡ عَن مَّن تَوَلَّىٰ عَن ذِكۡرِنَا وَلَمۡ يُرِدۡ إِلَّا ٱلۡحَيَوٰةَ ٱلدُّنۡيَا",
      "translation": "ដូចេ្នះ ចូរអ្នក(មូហាំម៉ាត់)ចៀសឱ្យឆ្ងាយពីអ្នកដែលបែរ ចេញពីការរំលឹក(គម្ពីរគួរអាន)របស់យើង ហើយគេមិនចង់បានអ្វី ក្រៅពីការរស់នៅក្នុងលោកិយនេះឡើយ។",
      "footnotes": ""
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 527,
      "verse_key": "53:30",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ذَٰلِكَ مَبۡلَغُهُم مِّنَ ٱلۡعِلۡمِۚ إِنَّ رَبَّكَ هُوَ أَعۡلَمُ بِمَن ضَلَّ عَن سَبِيلِهِۦ وَهُوَ أَعۡلَمُ بِمَنِ ٱهۡتَدَىٰ",
      "translation": "នោះហើយគឺជាកំរិតយល់ដឹងរបស់ពួកគេ។ ពិតប្រាកដ ណាស់ ម្ចាស់របស់អ្នក(មូហាំម៉ាត់) ទ្រង់ដឹងបំផុតចំពោះអ្នកណា ដែលបានវងេ្វងពីមាគ៌ារបស់ទ្រង់។ ហើយទ្រង់ក៏ដឹងបំផុតដែរ ចំពោះអ្នកណាដែលទទួលបានការចង្អុលបង្ហាញ។",
      "footnotes": ""
    },
    {
      "id": 31,
      "verse_number": 31,
      "page_number": 527,
      "verse_key": "53:31",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلِلَّهِ مَا فِي ٱلسَّمَٰوَٰتِ وَمَا فِي ٱلۡأَرۡضِ لِيَجۡزِيَ ٱلَّذِينَ أَسَٰٓـُٔواْ بِمَا عَمِلُواْ وَيَجۡزِيَ ٱلَّذِينَ أَحۡسَنُواْ بِٱلۡحُسۡنَى",
      "translation": "ហើយអ្វីៗដែលនៅលើមេឃជាច្រើនជាន់និងអ្វីៗដែលនៅ លើផែនដី គឺជាកម្មសិទ្ធិរបស់អល់ឡោះ ដែលទ្រង់នឹងតបស្នងចំពោះ ពួកដែលបានប្រព្រឹត្ដបាបកម្មឱ្យសមទៅនឹងអ្វីដែលពួកគេបាន ប្រព្រឹត្ដ។ ហើយទ្រង់នឹងតបស្នងចំពោះបណ្ដាអ្នកដែលបានសាង អំពើល្អនូវផលបុណ្យដ៏ល្អប្រពៃ(ឋានសួគ៌)។",
      "footnotes": ""
    },
    {
      "id": 32,
      "verse_number": 32,
      "page_number": 527,
      "verse_key": "53:32",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِينَ يَجۡتَنِبُونَ كَبَٰٓئِرَ ٱلۡإِثۡمِ وَٱلۡفَوَٰحِشَ إِلَّا ٱللَّمَمَۚ إِنَّ رَبَّكَ وَٰسِعُ ٱلۡمَغۡفِرَةِۚ هُوَ أَعۡلَمُ بِكُمۡ إِذۡ أَنشَأَكُم مِّنَ ٱلۡأَرۡضِ وَإِذۡ أَنتُمۡ أَجِنَّةٞ فِي بُطُونِ أُمَّهَٰتِكُمۡۖ فَلَا تُزَكُّوٓاْ أَنفُسَكُمۡۖ هُوَ أَعۡلَمُ بِمَنِ ٱتَّقَىٰٓ",
      "translation": "បណ្ដាអ្នកដែលចៀសវាងពីបាបកម្មធំៗ និងអំពើអសីល-ធម៌ផេ្សងៗ ក្រៅពីបាបកម្មតូចតាចនោះ ពិតប្រាកដណាស់ ម្ចាស់ របស់អ្នកគឺជាអ្នកទូលំទូលាយក្នុងការអភ័យទោស។ ទ្រង់ដឹងបំផុត ពីលក្ខណៈរបស់ពួកអ្នក ខណៈដែលទ្រង់បានបង្កើតពួកអ្នកពីដី និង នៅពេលពួកអ្នកជាទារកនៅក្នុងពោះម្ដាយរបស់ពួកអ្នក។ ដូចេ្នះ ចូរ ពួកអ្នកកុំមានមោទនភាពចំពោះខ្លួនឯងថាជាអ្នកស្អាតស្អំឱ្យសោះ ព្រោះទ្រង់ដឹងបំផុតចំពោះអ្នកដែលកោតខ្លាចទ្រង់។",
      "footnotes": ""
    },
    {
      "id": 33,
      "verse_number": 33,
      "page_number": 527,
      "verse_key": "53:33",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَرَءَيۡتَ ٱلَّذِي تَوَلَّىٰ",
      "translation": "ដូចេ្នះ តើអ្នក(មូហាំម៉ាត់)មិនបានឃើញអ្នកដែលបាន បែរចេញ(ពីគម្ពីរគួរអាន)",
      "footnotes": ""
    },
    {
      "id": 34,
      "verse_number": 34,
      "page_number": 527,
      "verse_key": "53:34",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَعۡطَىٰ قَلِيلٗا وَأَكۡدَىٰٓ",
      "translation": "និងបានបរិច្ចាគទ្រព្យសម្បត្ដិខ្លះៗ រួចមកក៏បានផ្ដាច់វិញ ទេឬ?",
      "footnotes": ""
    },
    {
      "id": 35,
      "verse_number": 35,
      "page_number": 527,
      "verse_key": "53:35",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَعِندَهُۥ عِلۡمُ ٱلۡغَيۡبِ فَهُوَ يَرَىٰٓ",
      "translation": "តើគេមានចំណេះដឹងនៃប្រការអាថ៌កំបាំង ហើយគេអាច មើលឃើញផ្ទាល់នឹងភ្នែកឬ?",
      "footnotes": ""
    },
    {
      "id": 36,
      "verse_number": 36,
      "page_number": 527,
      "verse_key": "53:36",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَمۡ لَمۡ يُنَبَّأۡ بِمَا فِي صُحُفِ مُوسَىٰ",
      "translation": "ឬមួយគេមិនត្រូវបានគេប្រាប់នូវអ្វីដែលមាននៅក្នុងគម្ពីរ របស់មូសា",
      "footnotes": ""
    },
    {
      "id": 37,
      "verse_number": 37,
      "page_number": 527,
      "verse_key": "53:37",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِبۡرَٰهِيمَ ٱلَّذِي وَفَّىٰٓ",
      "translation": "និងគម្ពីររបស់អ៊ីព្រហ៊ីមដែលបានបំពេញករណីយកិច្ចទេ ឬ?",
      "footnotes": ""
    },
    {
      "id": 38,
      "verse_number": 38,
      "page_number": 527,
      "verse_key": "53:38",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَّا تَزِرُ وَازِرَةٞ وِزۡرَ أُخۡرَىٰ",
      "translation": "ដើម្បីកុំឱ្យអ្នកដែលសាងបាបកម្មទទួលបន្ទុកចំពោះ បាបកម្មរបស់អ្នកដទៃ។",
      "footnotes": ""
    },
    {
      "id": 39,
      "verse_number": 39,
      "page_number": 527,
      "verse_key": "53:39",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَن لَّيۡسَ لِلۡإِنسَٰنِ إِلَّا مَا سَعَىٰ",
      "translation": "ហើយសម្រាប់មនុស្សលោកមិនទទួលបានក្រៅពីអ្វីដែល ខ្លួនគេបានប្រឹងប្រែងធ្វើនោះឡើយ។",
      "footnotes": ""
    },
    {
      "id": 40,
      "verse_number": 40,
      "page_number": 527,
      "verse_key": "53:40",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّ سَعۡيَهُۥ سَوۡفَ يُرَىٰ",
      "translation": "ហើយពិតប្រាកដណាស់ ការប្រឹងប្រែងរបស់គេនឹងត្រូវ គេបង្ហាញឱ្យឃើញ(នៅថ្ងៃបរលោក)។",
      "footnotes": ""
    },
    {
      "id": 41,
      "verse_number": 41,
      "page_number": 527,
      "verse_key": "53:41",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ يُجۡزَىٰهُ ٱلۡجَزَآءَ ٱلۡأَوۡفَىٰ",
      "translation": "បន្ទាប់មកទ្រង់នឹងតបស្នងឱ្យគេវិញនូវការតបស្នងមួយ យ៉ាងពេញលេញ។",
      "footnotes": ""
    },
    {
      "id": 42,
      "verse_number": 42,
      "page_number": 527,
      "verse_key": "53:42",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّ إِلَىٰ رَبِّكَ ٱلۡمُنتَهَىٰ",
      "translation": "ហើយពិតប្រាកដណាស់ ទីបពា្ចប់នៃអ្វីៗទាំងអស់គឺវិល ត្រឡប់ទៅរកម្ចាស់របស់អ្នក(មូហាំម៉ាត់)វិញ។",
      "footnotes": ""
    },
    {
      "id": 43,
      "verse_number": 43,
      "page_number": 527,
      "verse_key": "53:43",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّهُۥ هُوَ أَضۡحَكَ وَأَبۡكَىٰ",
      "translation": "ហើយពិតប្រាកដណាស់ ទ្រង់ជាអ្នកដែលធ្វើឱ្យ(គ្រប់គ្នា) សើច និងធ្វើឱ្យយំ។",
      "footnotes": ""
    },
    {
      "id": 44,
      "verse_number": 44,
