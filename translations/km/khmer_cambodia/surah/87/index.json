{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 591,
      "verse_key": "87:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "سَبِّحِ ٱسۡمَ رَبِّكَ ٱلۡأَعۡلَى",
      "translation": "ចូរអ្នក(មូហាំម៉ាត់)លើកតម្កើងនាមនៃម្ចាស់របស់ អ្នកដ៏ឧត្ដុង្គឧត្ដមចុះ។",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 591,
      "verse_key": "87:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِي خَلَقَ فَسَوَّىٰ",
      "translation": "ទ្រង់ជាអ្នកដែលបានបង្កើត(អ្វីៗទាំងអស់) រួចទ្រង់បាន រៀបចំឱ្យមានលក្ខណៈសមរម្យ។",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 591,
      "verse_key": "87:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّذِي قَدَّرَ فَهَدَىٰ",
      "translation": "និងជាអ្នកដែលបានកំណត់វាសនា និងផ្ដល់ការចង្អុលបង្ហាញ។",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 591,
      "verse_key": "87:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّذِيٓ أَخۡرَجَ ٱلۡمَرۡعَىٰ",
      "translation": "និងជាអ្នកដែលបណ្ដុះរុក្ខជាតិបៃតងគ្រប់បែបយ៉ាង។",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 591,
      "verse_key": "87:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَجَعَلَهُۥ غُثَآءً أَحۡوَىٰ",
      "translation": "ក្រោយមកទ្រង់បានធ្វើឱ្យវាក្លាយជាចំបើងស្ងួតពណ៌ខ្មៅ។",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 591,
      "verse_key": "87:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "سَنُقۡرِئُكَ فَلَا تَنسَىٰٓ",
      "translation": "យើងនឹងសូត្រ(គម្ពីរគួរអាន)ឱ្យអ្នក(មូហាំម៉ាត់)ស្ដាប់ ហើយអ្នកនឹងមិនភេ្លចវាឡើយ។",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 591,
      "verse_key": "87:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَّا مَا شَآءَ ٱللَّهُۚ إِنَّهُۥ يَعۡلَمُ ٱلۡجَهۡرَ وَمَا يَخۡفَىٰ",
      "translation": "លើកលែងតែអ្វីដែលអល់ឡោះមានចេតនា(ឱ្យអ្នកភេ្លច) ប៉ុណ្ណោះ។ ពិតប្រាកដណាស់ ទ្រង់ដឹងនូវអ្វីដែលលាតត្រដាង និង លាក់កំបាំង។",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 591,
      "verse_key": "87:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَنُيَسِّرُكَ لِلۡيُسۡرَىٰ",
      "translation": "ហើយយើងនឹងសម្រួលឱ្យអ្នកនូវមាគ៌ាដ៏ងាយស្រួល(គ្រប់ កិច្ចការ)។",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 591,
      "verse_key": "87:9",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَذَكِّرۡ إِن نَّفَعَتِ ٱلذِّكۡرَىٰ",
      "translation": "ដូចេ្នះ ចូរអ្នកក្រើនរំលឹកចុះ ប្រសិនបើការក្រើនរំលឹកនោះ មានផលប្រយោជន៍",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 591,
      "verse_key": "87:10",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "سَيَذَّكَّرُ مَن يَخۡشَىٰ",
      "translation": "អ្នកដែលកោតខ្លាចអល់ឡោះ នឹងទទួលយកការក្រើន រំលឹក។",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 591,
      "verse_key": "87:11",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيَتَجَنَّبُهَا ٱلۡأَشۡقَى",
      "translation": "ប៉ុន្ដែអ្នកដែលមិនកោតខ្លាចអល់ឡោះនឹងគេចចេញពីវា",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 591,
      "verse_key": "87:12",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِي يَصۡلَى ٱلنَّارَ ٱلۡكُبۡرَىٰ",
      "translation": "គឺជាអ្នកដែលនឹងត្រូវចូលទៅក្នុងភើ្លងនរកយ៉ាងធំធេង។",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 591,
      "verse_key": "87:13",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ لَا يَمُوتُ فِيهَا وَلَا يَحۡيَىٰ",
      "translation": "បន្ទាប់មកគេមិនអាចស្លាប់ ហើយក៏មិនអាចរស់ដោយ ងាយស្រួលដែរនៅក្នុងនរកនោះ។",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 591,
      "verse_key": "87:14",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قَدۡ أَفۡلَحَ مَن تَزَكَّىٰ",
      "translation": "អ្នកដែលសំអាតខ្លួន(ពីប្រការស្ហ៊ីរិក) ពិតជាទទួលបាន ជោគជ័យ។",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 591,
      "verse_key": "87:15",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَذَكَرَ ٱسۡمَ رَبِّهِۦ فَصَلَّىٰ",
      "translation": "និងអ្នកដែលបានរំលឹកនាមនៃម្ចាស់របស់គេ ហើយនិង ប្រតិបត្ដិសឡាត។",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 592,
      "verse_key": "87:16",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بَلۡ تُؤۡثِرُونَ ٱلۡحَيَوٰةَ ٱلدُّنۡيَا",
      "translation": "ផ្ទុយទៅវិញ ពួកអ្នកបែរជាស្រឡាញ់ការរស់នៅក្នុង លោកិយទៅវិញ",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 592,
      "verse_key": "87:17",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡأٓخِرَةُ خَيۡرٞ وَأَبۡقَىٰٓ",
      "translation": "ថ្វីត្បិតតែថ្ងៃបរលោកវាប្រសើរជាងនិងអមតៈក៏ដោយ។",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 592,
      "verse_key": "87:18",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ هَٰذَا لَفِي ٱلصُّحُفِ ٱلۡأُولَىٰ",
      "translation": "ពិតប្រាកដណាស់ រឿងនេះគឺមានចែងនៅក្នុងគម្ពីរមុនៗ។",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 592,
      "verse_key": "87:19",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "صُحُفِ إِبۡرَٰهِيمَ وَمُوسَىٰ",
      "translation": "ដូចជាគម្ពីររបស់អ៊ីព្រហ៊ីម និងមូសាជាដើម។",
      "footnotes": ""
    }
  ]
}