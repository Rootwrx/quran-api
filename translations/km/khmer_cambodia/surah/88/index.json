{
  "result": [
    {
      "id": "5968",
      "sura": "88",
      "aya": "1",
      "arabic_text": "هَلۡ أَتَىٰكَ حَدِيثُ ٱلۡغَٰشِيَةِ",
      "translation": "តើដំណឹងនៃថ្ងៃបរលោកបានមកដល់អ្នក(មូហាំម៉ាត់) ហើយឬនៅ?",
      "footnotes": ""
    },
    {
      "id": "5969",
      "sura": "88",
      "aya": "2",
      "arabic_text": "وُجُوهٞ يَوۡمَئِذٍ خَٰشِعَةٌ",
      "translation": "នៅថ្ងៃនោះទឹកមុខ(របស់ពួកគ្មានជំនឿ) គឺមានភាពអាម៉ាស់។",
      "footnotes": ""
    },
    {
      "id": "5970",
      "sura": "88",
      "aya": "3",
      "arabic_text": "عَامِلَةٞ نَّاصِبَةٞ",
      "translation": "រវល់យ៉ាងខ្លាំង និងនឿយហត់បំផុត។",
      "footnotes": ""
    },
    {
      "id": "5971",
      "sura": "88",
      "aya": "4",
      "arabic_text": "تَصۡلَىٰ نَارًا حَامِيَةٗ",
      "translation": "ពួកគេនឹងចូលក្នុងភើ្លងនរកដ៏សែនក្ដៅ។",
      "footnotes": ""
    },
    {
      "id": "5972",
      "sura": "88",
      "aya": "5",
      "arabic_text": "تُسۡقَىٰ مِنۡ عَيۡنٍ ءَانِيَةٖ",
      "translation": "ពួកគេនឹងត្រូវគេឱ្យផឹកទឹកដ៏ក្ដៅបំផុត។",
      "footnotes": ""
    },
    {
      "id": "5973",
      "sura": "88",
      "aya": "6",
      "arabic_text": "لَّيۡسَ لَهُمۡ طَعَامٌ إِلَّا مِن ضَرِيعٖ",
      "translation": "គ្មានចំណីអាហារណាសម្រាប់ពួកគេក្រៅពីបន្លានិងជាតិពុល",
      "footnotes": ""
    },
    {
      "id": "5974",
      "sura": "88",
      "aya": "7",
      "arabic_text": "لَّا يُسۡمِنُ وَلَا يُغۡنِي مِن جُوعٖ",
      "translation": "ដែលវានឹងមិនធ្វើឱ្យធំធាត់ និងមិនធ្វើឱ្យបាត់ការសេ្រក ឃ្លាននោះឡើយ ។",
      "footnotes": ""
    },
    {
      "id": "5975",
      "sura": "88",
      "aya": "8",
      "arabic_text": "وُجُوهٞ يَوۡمَئِذٖ نَّاعِمَةٞ",
      "translation": "នៅថ្ងៃនោះដែរ ទឹកមុខ(របស់បណ្ដាអ្នកមានជំនឿ)ប្រកប ដោយក្ដីរីករាយ។",
      "footnotes": ""
    },
    {
      "id": "5976",
      "sura": "88",
      "aya": "9",
      "arabic_text": "لِّسَعۡيِهَا رَاضِيَةٞ",
      "translation": "ដោយសារតែពួកគេពេញចិត្ដចំពោះការប្រឹងប្រែងរបស់ខ្លួន (នៅក្នុងលោកិយ) ។",
      "footnotes": ""
    },
    {
      "id": "5977",
      "sura": "88",
      "aya": "10",
      "arabic_text": "فِي جَنَّةٍ عَالِيَةٖ",
      "translation": "នៅក្នុងឋានសួគ៌ដ៏ខ្ពស់ត្រដែត",
      "footnotes": ""
    },
    {
      "id": "5978",
      "sura": "88",
      "aya": "11",
      "arabic_text": "لَّا تَسۡمَعُ فِيهَا لَٰغِيَةٗ",
      "translation": "ដែលនៅក្នុងវានោះ ពួកគេមិនឮពាក្យសំដីឥតប្រយោជន៍ ឡើយ។",
      "footnotes": ""
    },
    {
      "id": "5979",
      "sura": "88",
      "aya": "12",
      "arabic_text": "فِيهَا عَيۡنٞ جَارِيَةٞ",
      "translation": "នៅទីនោះ មានប្រភពទឹកដែលហូរ។",
      "footnotes": ""
    },
    {
      "id": "5980",
      "sura": "88",
      "aya": "13",
      "arabic_text": "فِيهَا سُرُرٞ مَّرۡفُوعَةٞ",
      "translation": "នៅទីនោះដែរ មានគ្រែខ្ពស់ៗ។",
      "footnotes": ""
    },
    {
      "id": "5981",
      "sura": "88",
      "aya": "14",
      "arabic_text": "وَأَكۡوَابٞ مَّوۡضُوعَةٞ",
      "translation": "និងកែវដែលបានរៀបចំទុកដាក់ជាសេ្រច។",
      "footnotes": ""
    },
    {
      "id": "5982",
      "sura": "88",
      "aya": "15",
      "arabic_text": "وَنَمَارِقُ مَصۡفُوفَةٞ",
      "translation": "និងខ្នើយកើយរៀបដាក់ជាជួរៗ។",
      "footnotes": ""
    },
    {
      "id": "5983",
      "sura": "88",
      "aya": "16",
      "arabic_text": "وَزَرَابِيُّ مَبۡثُوثَةٌ",
      "translation": "និងកម្រាលព្រំគ្រប់បែបយ៉ាងដែលបានក្រាលសន្ធឹងរួច ជាសេ្រច។",
      "footnotes": ""
    },
    {
      "id": "5984",
      "sura": "88",
      "aya": "17",
      "arabic_text": "أَفَلَا يَنظُرُونَ إِلَى ٱلۡإِبِلِ كَيۡفَ خُلِقَتۡ",
      "translation": "ហើយតើពួកគេមិនបានពិចារណាចំពោះសត្វអូដ្ឋទេឬថា វាត្រូវបានគេបង្កើតយ៉ាងដូចមេ្ដច?",
      "footnotes": ""
    },
    {
      "id": "5985",
      "sura": "88",
      "aya": "18",
      "arabic_text": "وَإِلَى ٱلسَّمَآءِ كَيۡفَ رُفِعَتۡ",
      "translation": "និងមើលទៅមេឃ តើវាត្រូវបានគេលើកឱ្យខ្ពស់ឡើង យ៉ាងដូចមេ្ដច?",
      "footnotes": ""
    },
    {
      "id": "5986",
      "sura": "88",
      "aya": "19",
      "arabic_text": "وَإِلَى ٱلۡجِبَالِ كَيۡفَ نُصِبَتۡ",
      "translation": "និងមើលទៅភ្នំទាំងឡាយ តើវាត្រូវបានគេបោះភ្ជាប់ យ៉ាងរឹងមាំដូចមេ្ដច ?",
      "footnotes": ""
    },
    {
      "id": "5987",
      "sura": "88",
      "aya": "20",
      "arabic_text": "وَإِلَى ٱلۡأَرۡضِ كَيۡفَ سُطِحَتۡ",
      "translation": "និងមើលទៅផែនដី តើវាត្រូវបានគេពន្លាតសន្ធឹងយ៉ាង ដូចមេ្ដចដែរ ?",
      "footnotes": ""
    },
    {
      "id": "5988",
      "sura": "88",
      "aya": "21",
      "arabic_text": "فَذَكِّرۡ إِنَّمَآ أَنتَ مُذَكِّرٞ",
      "translation": "ដូចេ្នះ ចូរអ្នក(មូហាំម៉ាត់)ក្រើនរំលឹកចុះ ការពិតអ្នក គ្រាន់តែជាអ្នកដែលក្រើនរំលឹកប៉ុណ្ណោះ។",
      "footnotes": ""
    },
    {
      "id": "5989",
      "sura": "88",
      "aya": "22",
      "arabic_text": "لَّسۡتَ عَلَيۡهِم بِمُصَيۡطِرٍ",
      "translation": "អ្នកពុំមែនជាអ្នកប្រើអំណាចផ្ដាច់ការទៅលើពួកគេ ឡើយ។",
      "footnotes": ""
    },
    {
      "id": "5990",
      "sura": "88",
      "aya": "23",
      "arabic_text": "إِلَّا مَن تَوَلَّىٰ وَكَفَرَ",
      "translation": "លើកលែងតែអ្នកណាដែលងាកចេញ និងគ្មានជំនឿ ប៉ុណ្ណោះ។",
      "footnotes": ""
    },
    {
      "id": "5991",
      "sura": "88",
      "aya": "24",
      "arabic_text": "فَيُعَذِّبُهُ ٱللَّهُ ٱلۡعَذَابَ ٱلۡأَكۡبَرَ",
      "translation": "អល់ឡោះនឹងធ្វើទារុណកម្មគេនូវទារុណកម្មមួយយ៉ាង ធ្ងន់ធ្ងរបំផុត។",
      "footnotes": ""
    },
    {
      "id": "5992",
      "sura": "88",
      "aya": "25",
      "arabic_text": "إِنَّ إِلَيۡنَآ إِيَابَهُمۡ",
      "translation": "ពិតប្រាកដណាស់ ការវិលត្រឡប់របស់ពួកគេគឺមកកាន់ យើងតែម្នាក់គត់។",
      "footnotes": ""
    },
    {
      "id": "5993",
      "sura": "88",
      "aya": "26",
      "arabic_text": "ثُمَّ إِنَّ عَلَيۡنَا حِسَابَهُم",
      "translation": "បន្ទាប់មក ពិតប្រាកដណាស់ ការជំនុំជំរះរបស់ពួកគេ គឺជាតួនាទីរបស់យើង។",
      "footnotes": ""
    }
  ]
}