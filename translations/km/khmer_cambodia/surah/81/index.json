{
  "result": [
    {
      "id": "5801",
      "sura": "81",
      "aya": "1",
      "arabic_text": "إِذَا ٱلشَّمۡسُ كُوِّرَتۡ",
      "translation": "នៅពេលដែលព្រះអាទិត្យត្រូវបានគេធ្វើឱ្យបាត់រស្មី។",
      "footnotes": ""
    },
    {
      "id": "5802",
      "sura": "81",
      "aya": "2",
      "arabic_text": "وَإِذَا ٱلنُّجُومُ ٱنكَدَرَتۡ",
      "translation": "និងនៅពេលដែលហ្វូងផ្កាយបានធ្លាក់បាក់បែក។",
      "footnotes": ""
    },
    {
      "id": "5803",
      "sura": "81",
      "aya": "3",
      "arabic_text": "وَإِذَا ٱلۡجِبَالُ سُيِّرَتۡ",
      "translation": "និងនៅពេលដែលភ្នំត្រូវបានគេធ្វើឱ្យរសាត់ដូចធូលី។",
      "footnotes": ""
    },
    {
      "id": "5804",
      "sura": "81",
      "aya": "4",
      "arabic_text": "وَإِذَا ٱلۡعِشَارُ عُطِّلَتۡ",
      "translation": "និងនៅពេលដែលសត្វអូដ្ឋមានផ្ទៃពោះត្រូវបានគេបោះបង់ ចោល។",
      "footnotes": ""
    },
    {
      "id": "5805",
      "sura": "81",
      "aya": "5",
      "arabic_text": "وَإِذَا ٱلۡوُحُوشُ حُشِرَتۡ",
      "translation": "និងនៅពេលដែលសត្វសាហាវត្រូវបានគេប្រមូលផ្ដុំ។",
      "footnotes": ""
    },
    {
      "id": "5806",
      "sura": "81",
      "aya": "6",
      "arabic_text": "وَإِذَا ٱلۡبِحَارُ سُجِّرَتۡ",
      "translation": "និងនៅពេលដែលទឹកសមុទ្រត្រូវបានគេធ្វើឱ្យឆេះជា អណ្ដាតភ្លើង។",
      "footnotes": ""
    },
    {
      "id": "5807",
      "sura": "81",
      "aya": "7",
      "arabic_text": "وَإِذَا ٱلنُّفُوسُ زُوِّجَتۡ",
      "translation": "និងនៅពេលដែលព្រលឹងត្រូវបានគេបញ្ចូលទៅក្នុងខ្លួន របស់គេវិញ។",
      "footnotes": ""
    },
    {
      "id": "5808",
      "sura": "81",
      "aya": "8",
      "arabic_text": "وَإِذَا ٱلۡمَوۡءُۥدَةُ سُئِلَتۡ",
      "translation": "និងនៅពេលទារិកាដែលគេបានកប់ទាំងរស់ត្រូវបានគេ សួរថាៈ",
      "footnotes": ""
    },
    {
      "id": "5809",
      "sura": "81",
      "aya": "9",
      "arabic_text": "بِأَيِّ ذَنۢبٖ قُتِلَتۡ",
      "translation": "ដោយសារបាបកម្មអី្វបានជានាងត្រូវបានគេសម្លាប់?",
      "footnotes": ""
    },
    {
      "id": "5810",
      "sura": "81",
      "aya": "10",
      "arabic_text": "وَإِذَا ٱلصُّحُفُ نُشِرَتۡ",
      "translation": "និងនៅពេលដែលសៀវភៅកំណត់ហេតុ(អំពើល្អ និង អំពើអាក្រក់)ត្រូវបានគេលាតត្រដាង។",
      "footnotes": ""
    },
    {
      "id": "5811",
      "sura": "81",
      "aya": "11",
      "arabic_text": "وَإِذَا ٱلسَّمَآءُ كُشِطَتۡ",
      "translation": "និងនៅពេលដែលមេឃត្រូវបានគេយកចេញពីកន្លែង របស់វា។",
      "footnotes": ""
    },
    {
      "id": "5812",
      "sura": "81",
      "aya": "12",
      "arabic_text": "وَإِذَا ٱلۡجَحِيمُ سُعِّرَتۡ",
      "translation": "និងនៅពេលដែលភ្លើងនរកជើហ៊ីមត្រូវបានគេធ្វើឱ្យឆេះ យ៉ាងសន្ធោសន្ធៅ។",
      "footnotes": ""
    },
    {
      "id": "5813",
      "sura": "81",
      "aya": "13",
      "arabic_text": "وَإِذَا ٱلۡجَنَّةُ أُزۡلِفَتۡ",
      "translation": "និងនៅពេលដែលឋានសួគ៌ត្រូវបានគេដាក់ឱ្យនៅជិត (អ្នកដែលកោតខ្លាចអល់ឡោះ) ។",
      "footnotes": ""
    },
    {
      "id": "5814",
      "sura": "81",
      "aya": "14",
      "arabic_text": "عَلِمَتۡ نَفۡسٞ مَّآ أَحۡضَرَتۡ",
      "translation": "(ពេលនោះ)បុគ្គលគ្រប់រូបនឹងដឹងអំពីអ្វីដែលគេបាន ប្រព្រឹត្ដកន្លងមក។",
      "footnotes": ""
    },
    {
      "id": "5815",
      "sura": "81",
      "aya": "15",
      "arabic_text": "فَلَآ أُقۡسِمُ بِٱلۡخُنَّسِ",
      "translation": "ជាការពិតណាស់ យើងស្បថនឹងហ្វូងផ្កាយដែលលិចបាត់។",
      "footnotes": ""
    },
    {
      "id": "5816",
      "sura": "81",
      "aya": "16",
      "arabic_text": "ٱلۡجَوَارِ ٱلۡكُنَّسِ",
      "translation": "និងហូ្វងផ្កាយដែលដើរតាមគន្លងរបស់វាយ៉ាងលឿន រួច ក៏លិចបាត់។",
      "footnotes": ""
    },
    {
      "id": "5817",
      "sura": "81",
      "aya": "17",
      "arabic_text": "وَٱلَّيۡلِ إِذَا عَسۡعَسَ",
      "translation": "និងពេលយប់ នៅពេលដែលវាមកដល់ទីបពា្ចប់។",
      "footnotes": ""
    },
    {
      "id": "5818",
      "sura": "81",
      "aya": "18",
      "arabic_text": "وَٱلصُّبۡحِ إِذَا تَنَفَّسَ",
      "translation": "និងពេលព្រលឹម នៅពេលដែលវាបពោ្ចញពន្លឺ។",
      "footnotes": ""
    },
    {
      "id": "5819",
      "sura": "81",
      "aya": "19",
      "arabic_text": "إِنَّهُۥ لَقَوۡلُ رَسُولٖ كَرِيمٖ",
      "translation": "ពិតប្រាកដណាស់ គម្ពីរគួរអានគឺនាំមកតាមរយៈពាក្យ សំដីរបស់អ្នកនាំសារ(ជីព្រីល)ដ៏ថ្កុំថ្កើង",
      "footnotes": ""
    },
    {
      "id": "5820",
      "sura": "81",
      "aya": "20",
      "arabic_text": "ذِي قُوَّةٍ عِندَ ذِي ٱلۡعَرۡشِ مَكِينٖ",
      "translation": "ដែលមានអានុភាពខ្លាំងក្លា(ក្នុងការទទួលខុសត្រូវលើ ការងារ) និងមានឋានៈខ្ពង់ខ្ពស់ចំពោះម្ចាស់នៃអារ៉ស្ហ",
      "footnotes": ""
    },
    {
      "id": "5821",
      "sura": "81",
      "aya": "21",
      "arabic_text": "مُّطَاعٖ ثَمَّ أَمِينٖ",
      "translation": "ដែលជាអ្នកត្រូវគេគោរពនៅទីនោះ និងជាអ្នកស្មោះត្រង់។",
      "footnotes": ""
    },
    {
      "id": "5822",
      "sura": "81",
      "aya": "22",
      "arabic_text": "وَمَا صَاحِبُكُم بِمَجۡنُونٖ",
      "translation": "ហើយមិត្ដរបស់ពួកអ្នក(មូហាំម៉ាត់)មិនមែនជា មនុស្សវិកលចរិតឡើយ។",
      "footnotes": ""
    },
    {
      "id": "5823",
      "sura": "81",
      "aya": "23",
      "arabic_text": "وَلَقَدۡ رَءَاهُ بِٱلۡأُفُقِ ٱلۡمُبِينِ",
      "translation": "ហើយជាការពិតណាស់ គាត់(មូហាំម៉ាត់)បានឃើញគេ (ជីព្រីល)យ៉ាងច្បាស់នៅឯជើងមេឃ។",
      "footnotes": ""
    },
    {
      "id": "5824",
      "sura": "81",
      "aya": "24",
      "arabic_text": "وَمَا هُوَ عَلَى ٱلۡغَيۡبِ بِضَنِينٖ",
      "translation": "ហើយគេ(មូហាំម៉ាត់)មិនកំណាញ់លើការផ្សព្វផ្សាយ ប្រការអាថ៌កំបាំង(សាររបស់អល់ឡោះ)ឡើយ។",
      "footnotes": ""
    },
    {
      "id": "5825",
      "sura": "81",
      "aya": "25",
      "arabic_text": "وَمَا هُوَ بِقَوۡلِ شَيۡطَٰنٖ رَّجِيمٖ",
      "translation": "ហើយគម្ពីរគួរអានក៏មិនមែនជាពាក្យសំដីរបស់ស្ហៃតន ដែលត្រូវគេដាក់បណ្ដាសាដែរ។",
      "footnotes": ""
    },
    {
      "id": "5826",
      "sura": "81",
      "aya": "26",
      "arabic_text": "فَأَيۡنَ تَذۡهَبُونَ",
      "translation": "ហេតុនេះ តើសតិរបស់ពួកដែលបដិសេធនឹងគម្ពីរគួរអាន ទៅណាអស់ហើយ?",
      "footnotes": ""
    },
    {
      "id": "5827",
      "sura": "81",
      "aya": "27",
      "arabic_text": "إِنۡ هُوَ إِلَّا ذِكۡرٞ لِّلۡعَٰلَمِينَ",
      "translation": "វា(គម្ពីរគួរអាន) គ្មានអ្វីក្រៅពីការរំលឹកដល់ពិភពទាំង អស់នោះឡើយ។",
      "footnotes": ""
    },
    {
      "id": "5828",
      "sura": "81",
      "aya": "28",
      "arabic_text": "لِمَن شَآءَ مِنكُمۡ أَن يَسۡتَقِيمَ",
      "translation": "សម្រាប់អ្នកណាដែលមានបំណងចង់ដើរលើមាគ៌ាដ៏ត្រឹម ត្រូវក្នុងចំណោមពួកអ្នក។",
      "footnotes": ""
    },
    {
      "id": "5829",
      "sura": "81",
      "aya": "29",
      "arabic_text": "وَمَا تَشَآءُونَ إِلَّآ أَن يَشَآءَ ٱللَّهُ رَبُّ ٱلۡعَٰلَمِينَ",
      "translation": "ហើយពួកអ្នកមិនអាចមានបំណងធ្វើអ្វីមួយបានឡើយ លើកលែងតែអល់ឡោះដែលជាម្ចាស់គ្រប់គ្រងពិភពទាំងអស់ មានចេតនាប៉ុណ្ណោះ។",
      "footnotes": ""
    }
  ]
}