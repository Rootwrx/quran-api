{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 586,
      "verse_key": "81:1",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذَا ٱلشَّمۡسُ كُوِّرَتۡ",
      "translation": "នៅពេលដែលព្រះអាទិត្យត្រូវបានគេធ្វើឱ្យបាត់រស្មី។",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 586,
      "verse_key": "81:2",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلنُّجُومُ ٱنكَدَرَتۡ",
      "translation": "និងនៅពេលដែលហ្វូងផ្កាយបានធ្លាក់បាក់បែក។",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 586,
      "verse_key": "81:3",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡجِبَالُ سُيِّرَتۡ",
      "translation": "និងនៅពេលដែលភ្នំត្រូវបានគេធ្វើឱ្យរសាត់ដូចធូលី។",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 586,
      "verse_key": "81:4",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡعِشَارُ عُطِّلَتۡ",
      "translation": "និងនៅពេលដែលសត្វអូដ្ឋមានផ្ទៃពោះត្រូវបានគេបោះបង់ ចោល។",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 586,
      "verse_key": "81:5",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡوُحُوشُ حُشِرَتۡ",
      "translation": "និងនៅពេលដែលសត្វសាហាវត្រូវបានគេប្រមូលផ្ដុំ។",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 586,
      "verse_key": "81:6",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡبِحَارُ سُجِّرَتۡ",
      "translation": "និងនៅពេលដែលទឹកសមុទ្រត្រូវបានគេធ្វើឱ្យឆេះជា អណ្ដាតភ្លើង។",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 586,
      "verse_key": "81:7",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلنُّفُوسُ زُوِّجَتۡ",
      "translation": "និងនៅពេលដែលព្រលឹងត្រូវបានគេបញ្ចូលទៅក្នុងខ្លួន របស់គេវិញ។",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 586,
      "verse_key": "81:8",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡمَوۡءُۥدَةُ سُئِلَتۡ",
      "translation": "និងនៅពេលទារិកាដែលគេបានកប់ទាំងរស់ត្រូវបានគេ សួរថាៈ",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 586,
      "verse_key": "81:9",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بِأَيِّ ذَنۢبٖ قُتِلَتۡ",
      "translation": "ដោយសារបាបកម្មអី្វបានជានាងត្រូវបានគេសម្លាប់?",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 586,
      "verse_key": "81:10",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلصُّحُفُ نُشِرَتۡ",
      "translation": "និងនៅពេលដែលសៀវភៅកំណត់ហេតុ(អំពើល្អ និង អំពើអាក្រក់)ត្រូវបានគេលាតត្រដាង។",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 586,
      "verse_key": "81:11",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلسَّمَآءُ كُشِطَتۡ",
      "translation": "និងនៅពេលដែលមេឃត្រូវបានគេយកចេញពីកន្លែង របស់វា។",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 586,
      "verse_key": "81:12",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡجَحِيمُ سُعِّرَتۡ",
      "translation": "និងនៅពេលដែលភ្លើងនរកជើហ៊ីមត្រូវបានគេធ្វើឱ្យឆេះ យ៉ាងសន្ធោសន្ធៅ។",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 586,
      "verse_key": "81:13",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡجَنَّةُ أُزۡلِفَتۡ",
      "translation": "និងនៅពេលដែលឋានសួគ៌ត្រូវបានគេដាក់ឱ្យនៅជិត (អ្នកដែលកោតខ្លាចអល់ឡោះ) ។",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 586,
      "verse_key": "81:14",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلِمَتۡ نَفۡسٞ مَّآ أَحۡضَرَتۡ",
      "translation": "(ពេលនោះ)បុគ្គលគ្រប់រូបនឹងដឹងអំពីអ្វីដែលគេបាន ប្រព្រឹត្ដកន្លងមក។",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 586,
      "verse_key": "81:15",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلَآ أُقۡسِمُ بِٱلۡخُنَّسِ",
      "translation": "ជាការពិតណាស់ យើងស្បថនឹងហ្វូងផ្កាយដែលលិចបាត់។",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 586,
      "verse_key": "81:16",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلۡجَوَارِ ٱلۡكُنَّسِ",
      "translation": "និងហូ្វងផ្កាយដែលដើរតាមគន្លងរបស់វាយ៉ាងលឿន រួច ក៏លិចបាត់។",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 586,
      "verse_key": "81:17",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّيۡلِ إِذَا عَسۡعَسَ",
      "translation": "និងពេលយប់ នៅពេលដែលវាមកដល់ទីបពា្ចប់។",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 586,
      "verse_key": "81:18",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلصُّبۡحِ إِذَا تَنَفَّسَ",
      "translation": "និងពេលព្រលឹម នៅពេលដែលវាបពោ្ចញពន្លឺ។",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 586,
      "verse_key": "81:19",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُۥ لَقَوۡلُ رَسُولٖ كَرِيمٖ",
      "translation": "ពិតប្រាកដណាស់ គម្ពីរគួរអានគឺនាំមកតាមរយៈពាក្យ សំដីរបស់អ្នកនាំសារ(ជីព្រីល)ដ៏ថ្កុំថ្កើង",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 586,
      "verse_key": "81:20",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ذِي قُوَّةٍ عِندَ ذِي ٱلۡعَرۡشِ مَكِينٖ",
      "translation": "ដែលមានអានុភាពខ្លាំងក្លា(ក្នុងការទទួលខុសត្រូវលើ ការងារ) និងមានឋានៈខ្ពង់ខ្ពស់ចំពោះម្ចាស់នៃអារ៉ស្ហ",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 586,
      "verse_key": "81:21",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مُّطَاعٖ ثَمَّ أَمِينٖ",
      "translation": "ដែលជាអ្នកត្រូវគេគោរពនៅទីនោះ និងជាអ្នកស្មោះត្រង់។",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 586,
      "verse_key": "81:22",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا صَاحِبُكُم بِمَجۡنُونٖ",
      "translation": "ហើយមិត្ដរបស់ពួកអ្នក(មូហាំម៉ាត់)មិនមែនជា មនុស្សវិកលចរិតឡើយ។",
      "footnotes": ""
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 586,
      "verse_key": "81:23",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَقَدۡ رَءَاهُ بِٱلۡأُفُقِ ٱلۡمُبِينِ",
      "translation": "ហើយជាការពិតណាស់ គាត់(មូហាំម៉ាត់)បានឃើញគេ (ជីព្រីល)យ៉ាងច្បាស់នៅឯជើងមេឃ។",
      "footnotes": ""
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 586,
      "verse_key": "81:24",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا هُوَ عَلَى ٱلۡغَيۡبِ بِضَنِينٖ",
      "translation": "ហើយគេ(មូហាំម៉ាត់)មិនកំណាញ់លើការផ្សព្វផ្សាយ ប្រការអាថ៌កំបាំង(សាររបស់អល់ឡោះ)ឡើយ។",
      "footnotes": ""
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 586,
      "verse_key": "81:25",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا هُوَ بِقَوۡلِ شَيۡطَٰنٖ رَّجِيمٖ",
      "translation": "ហើយគម្ពីរគួរអានក៏មិនមែនជាពាក្យសំដីរបស់ស្ហៃតន ដែលត្រូវគេដាក់បណ្ដាសាដែរ។",
      "footnotes": ""
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 586,
      "verse_key": "81:26",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَيۡنَ تَذۡهَبُونَ",
      "translation": "ហេតុនេះ តើសតិរបស់ពួកដែលបដិសេធនឹងគម្ពីរគួរអាន ទៅណាអស់ហើយ?",
      "footnotes": ""
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 586,
      "verse_key": "81:27",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنۡ هُوَ إِلَّا ذِكۡرٞ لِّلۡعَٰلَمِينَ",
      "translation": "វា(គម្ពីរគួរអាន) គ្មានអ្វីក្រៅពីការរំលឹកដល់ពិភពទាំង អស់នោះឡើយ។",
      "footnotes": ""
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 586,
      "verse_key": "81:28",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لِمَن شَآءَ مِنكُمۡ أَن يَسۡتَقِيمَ",
      "translation": "សម្រាប់អ្នកណាដែលមានបំណងចង់ដើរលើមាគ៌ាដ៏ត្រឹម ត្រូវក្នុងចំណោមពួកអ្នក។",
      "footnotes": ""
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 586,
      "verse_key": "81:29",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
