{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 598,
      "verse_key": "97:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّآ أَنزَلۡنَٰهُ فِي لَيۡلَةِ ٱلۡقَدۡرِ",
      "translation": "ពិតប្រាកដណាស់ យើងបានបញ្ចុះគម្ពីរគួរអាននៅក្នុង យប់អាល់កដរ៍។",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 598,
      "verse_key": "97:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَآ أَدۡرَىٰكَ مَا لَيۡلَةُ ٱلۡقَدۡرِ",
      "translation": "ហើយតើអ្វីដែលធ្វើឱ្យអ្នកដឹងថា យប់អាល់កដរ៍ជាអ្វីនោះ?",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 598,
      "verse_key": "97:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَيۡلَةُ ٱلۡقَدۡرِ خَيۡرٞ مِّنۡ أَلۡفِ شَهۡرٖ",
      "translation": "យប់អាល់កដរ៍នោះគឺប្រសើរជាងមួយពាន់ខែ ។",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 598,
      "verse_key": "97:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تَنَزَّلُ ٱلۡمَلَٰٓئِكَةُ وَٱلرُّوحُ فِيهَا بِإِذۡنِ رَبِّهِم مِّن كُلِّ أَمۡرٖ",
      "translation": "ម៉ាឡាអ៊ីកាត់ជាច្រើន និងជីព្រីលចុះមកក្នុងយប់នោះដោយ មានការអនុញ្ញាតពីម្ចាស់របស់ពួកគេ ដើម្បីអនុវត្ដរាល់បទបញ្ជា។",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 598,
      "verse_key": "97:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "سَلَٰمٌ هِيَ حَتَّىٰ مَطۡلَعِ ٱلۡفَجۡرِ",
      "translation": "នៅក្នុងយប់នោះដែរ ពោរពេញទៅដោយសេចក្ដីសុខសន្ដិភាពរហូតដល់ហ្វាជើរលេចឡើង ។",
      "footnotes": ""
    }
  ]
}