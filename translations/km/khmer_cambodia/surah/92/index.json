{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 595,
      "verse_key": "92:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّيۡلِ إِذَا يَغۡشَىٰ",
      "translation": "អល់ឡោះស្បថនឹងពេលយប់នៅពេលដែលវាគ្របដណ្ដប់ ដោយភាពងងឹត។",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 595,
      "verse_key": "92:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلنَّهَارِ إِذَا تَجَلَّىٰ",
      "translation": "និងពេលថ្ងៃ ពេលដែលវាបានបពោ្ចញពន្លឺដ៏ត្រចះត្រចង់។",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 595,
      "verse_key": "92:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا خَلَقَ ٱلذَّكَرَ وَٱلۡأُنثَىٰٓ",
      "translation": "និងអ្នកដែលបានបង្កើតឱ្យមានប្រុស និងស្រី។",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 595,
      "verse_key": "92:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ سَعۡيَكُمۡ لَشَتَّىٰ",
      "translation": "ពិតប្រាកដណាស់ ការប្រឹងប្រែងរបស់ពួកអ្នក(នៅក្នុង លោកិយ) គឺខុសៗគ្នា។",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 595,
      "verse_key": "92:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَمَّا مَنۡ أَعۡطَىٰ وَٱتَّقَىٰ",
      "translation": "ចំណែកឯអ្នកណាដែលបានបរិច្ចាគទាន និងកោតខ្លាច អល់ឡោះ",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 595,
      "verse_key": "92:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَصَدَّقَ بِٱلۡحُسۡنَىٰ",
      "translation": "ហើយនិងមានជំនឿលើការតបស្នងល្អនោះ",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 595,
      "verse_key": "92:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَسَنُيَسِّرُهُۥ لِلۡيُسۡرَىٰ",
      "translation": "យើងនឹងរៀបចំឱ្យគេនូវមាគ៌ាដែលងាយស្រួល",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 595,
      "verse_key": "92:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَمَّا مَنۢ بَخِلَ وَٱسۡتَغۡنَىٰ",
      "translation": "រីឯអ្នកណាហើយដែលកំណាញ់ និងមិនត្រូវការជំនួយពី អល់ឡោះ",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 595,
      "verse_key": "92:9",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَكَذَّبَ بِٱلۡحُسۡنَىٰ",
      "translation": "ហើយនិងបានបដិសេធនឹងការតបស្នងល្អនោះ",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 595,
      "verse_key": "92:10",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَسَنُيَسِّرُهُۥ لِلۡعُسۡرَىٰ",
      "translation": "យើងនឹងរៀបចំឱ្យគេនូវមាគ៌ាដែលលំបាក។",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 595,
      "verse_key": "92:11",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا يُغۡنِي عَنۡهُ مَالُهُۥٓ إِذَا تَرَدَّىٰٓ",
      "translation": "ហើយទ្រព្យសម្បត្ដិរបស់គេមិនផ្ដល់ផលប្រយោជន៍ដល់ គេឡើយ នៅពេលដែលគេបានធ្លាក់ចូលទៅក្នុងនរកហើយនោះ។",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 595,
      "verse_key": "92:12",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ عَلَيۡنَا لَلۡهُدَىٰ",
      "translation": "ពិតប្រាកដណាស់ ភារកិច្ចរបស់យើងគឺផ្ដល់ការចង្អុលបង្ហាញ។",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 595,
      "verse_key": "92:13",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِنَّ لَنَا لَلۡأٓخِرَةَ وَٱلۡأُولَىٰ",
      "translation": "ហើយពិតប្រាកដណាស់ ថ្ងៃអវសាននៃលោកិយ និងថ្ងៃដំបូងនៃលោកិយ គឺជាកម្មសិទ្ធិរបស់យើង។",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 595,
      "verse_key": "92:14",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَنذَرۡتُكُمۡ نَارٗا تَلَظَّىٰ",
      "translation": "ដូចេ្នះ យើងបានដាស់តឿនព្រមានពួកអ្នកអំពីភ្លើងនរក ដែលឆេះសន្ធោសន្ធៅ",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 596,
      "verse_key": "92:15",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَا يَصۡلَىٰهَآ إِلَّا ٱلۡأَشۡقَى",
      "translation": "ដែលគ្មានអ្នកណាចូលទៅក្នុងនោះ ក្រៅពីជនទុច្ចរិត",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 596,
      "verse_key": "92:16",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِي كَذَّبَ وَتَوَلَّىٰ",
      "translation": "ដែលគេបានបដិសេធ(នឹងអ្នកនាំសារ) និងបានបែរចេញ (ពីជំនឿលើអល់ឡោះ)នោះឡើយ។",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 596,
      "verse_key": "92:17",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَسَيُجَنَّبُهَا ٱلۡأَتۡقَى",
      "translation": "ហើយគេនឹងបញ្ចៀសអ្នកដែលកោតខ្លាចអល់ឡោះអំពីវា(នរក)",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 596,
      "verse_key": "92:18",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِي يُؤۡتِي مَالَهُۥ يَتَزَكَّىٰ",
      "translation": "ដែលបរិច្ចាគទ្រព្យសម្បត្ដិរបស់គេដើម្បីសំអាតខ្លួន។",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 596,
      "verse_key": "92:19",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا لِأَحَدٍ عِندَهُۥ مِن نِّعۡمَةٖ تُجۡزَىٰٓ",
      "translation": "ហើយគេមិនត្រូវការការតបស្នងនៀកម៉ាត់ពីអ្នកណាម្នាក់ ឡើយ។",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 596,
      "verse_key": "92:20",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَّا ٱبۡتِغَآءَ وَجۡهِ رَبِّهِ ٱلۡأَعۡلَىٰ",
      "translation": "លើកលែងតែដើម្បីស្វែងរកការយល់ព្រមពីម្ចាស់របស់ គេដ៏មហាឧត្ដុង្គឧត្ដមប៉ុណ្ណោះ។",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 596,
      "verse_key": "92:21",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَسَوۡفَ يَرۡضَىٰ",
      "translation": "ហើយគេពិតជានឹងពេញចិត្ដជាមិនខាន(អ្វីដែលយើង តបស្នងដល់គេ)។",
      "footnotes": ""
    }
  ]
}