{
  "verses": [
    {
      "number": "1",
      "arabic_text": "قُلۡ أَعُوذُ بِرَبِّ ٱلنَّاسِ",
      "translation": "ចូរអ្នក(មូហាំម៉ាត់)ពោលថាៈ ខ្ញុំសុំពីម្ចាស់នៃមនុស្សលោក"
    },
    {
      "number": "2",
      "arabic_text": "مَلِكِ ٱلنَّاسِ",
      "translation": "ដែលជាសេ្ដចគ្រប់គ្រងមនុស្សលោក"
    },
    {
      "number": "3",
      "arabic_text": "إِلَٰهِ ٱلنَّاسِ",
      "translation": "ជាម្ចាស់នៃមនុស្សលោក"
    },
    {
      "number": "4",
      "arabic_text": "مِن شَرِّ ٱلۡوَسۡوَاسِ ٱلۡخَنَّاسِ",
      "translation": "ឱ្យបញ្ចៀសពីប្រការអាក្រក់នៃការញុះញង់របស់ ពួកស្ហៃតន"
    },
    {
      "number": "5",
      "arabic_text": "ٱلَّذِي يُوَسۡوِسُ فِي صُدُورِ ٱلنَّاسِ",
      "translation": "ដែលវាញុះញង់នៅក្នុងចិត្ដមនុស្សលោក។"
    },
    {
      "number": "6",
      "arabic_text": "مِنَ ٱلۡجِنَّةِ وَٱلنَّاسِ",
      "translation": "-(ស្ហៃតននោះ)មកអំពីជិន និងមនុស្សលោក។"
    }
  ]
}