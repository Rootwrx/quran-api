{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 601,
      "verse_key": "104:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيۡلٞ لِّكُلِّ هُمَزَةٖ لُّمَزَةٍ",
      "translation": "វិនាសអន្ដរាយហើយចំពោះអ្នកនិយាយមួលបង្កាច់ និង អ្នកនិយាយដើមគេ។",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 601,
      "verse_key": "104:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِي جَمَعَ مَالٗا وَعَدَّدَهُۥ",
      "translation": "គឺជាអ្នកដែលបានប្រមូលទ្រព្យសម្បត្ដិនិងរាប់ចំនួនរបស់វា",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 601,
      "verse_key": "104:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَحۡسَبُ أَنَّ مَالَهُۥٓ أَخۡلَدَهُۥ",
      "translation": "ដោយគេគិតថា ទ្រព្យសម្បត្ដិរបស់គេពិតជាធ្វើឱ្យគេនៅ គង់វង្សជារៀងហូត។",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 601,
      "verse_key": "104:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّاۖ لَيُنۢبَذَنَّ فِي ٱلۡحُطَمَةِ",
      "translation": "មិនដូច្នោះទេ. ការពិតគេនឹងត្រូវគេបោះបញ្ចូលទៅក្នុង ហ៊ូ ត ម៉ះ។",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 601,
      "verse_key": "104:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَآ أَدۡرَىٰكَ مَا ٱلۡحُطَمَةُ",
      "translation": "តើអ្វីដែលធ្វើឱ្យអ្នកដឹងថា ហ៊ូ ត ម៉ះជាអ្វីនោះ?",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 601,
      "verse_key": "104:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "نَارُ ٱللَّهِ ٱلۡمُوقَدَةُ",
      "translation": "គឺជាភ្លើងរបស់អល់ឡោះដែលបានបង្កាត់ទុក(នៅក្នុង នរក)",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 601,
      "verse_key": "104:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّتِي تَطَّلِعُ عَلَى ٱلۡأَفۡـِٔدَةِ",
      "translation": "ដែលវាឆាបឆេះរាលដល់បេះដូង។",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 601,
      "verse_key": "104:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهَا عَلَيۡهِم مُّؤۡصَدَةٞ",
      "translation": "ពិតប្រាកដណាស់ នរកនោះត្រូវបានបិទជិត ដើម្បីកុំឱ្យ ពួកគេចេញ។",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 601,
      "verse_key": "104:9",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِي عَمَدٖ مُّمَدَّدَةِۭ",
      "translation": "ត្រូវបានចងភ្ជាប់នឹងសសរដ៏វែង។",
      "footnotes": ""
    }
  ]
}