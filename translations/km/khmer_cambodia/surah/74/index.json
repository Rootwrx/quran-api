{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 575,
      "verse_key": "74:1",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَٰٓأَيُّهَا ٱلۡمُدَّثِّرُ",
      "translation": "ឱអ្នកដណ្ដប់ភួយ(មូហាំម៉ាត់).",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 575,
      "verse_key": "74:2",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قُمۡ فَأَنذِرۡ",
      "translation": "ចូរអ្នកក្រោកឡើង ហើយចូរអ្នកដាស់តឿនព្រមាន(មនុស្ស លោកអំពីទារុណកម្មរបស់អល់ឡោះ)។",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 575,
      "verse_key": "74:3",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَرَبَّكَ فَكَبِّرۡ",
      "translation": "ហើយចូរអ្នកលើកតម្កើងម្ចាស់របស់អ្នកចុះ។",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 575,
      "verse_key": "74:4",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَثِيَابَكَ فَطَهِّرۡ",
      "translation": "ហើយចូរអ្នកសំអាតសំលៀកបំពាក់របស់អ្នក។",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 575,
      "verse_key": "74:5",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلرُّجۡزَ فَٱهۡجُرۡ",
      "translation": "ហើយចូរអ្នកចៀសឱ្យឆ្ងាយពីអំពើស្ហ៊ីរិក។",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 575,
      "verse_key": "74:6",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَا تَمۡنُن تَسۡتَكۡثِرُ",
      "translation": "ហើយចូរអ្នកកុំឱ្យអ្វីដល់គេ ដោយអ្នកចង់បានការតបស្នង ច្រើនមកវិញឱ្យសោះ។",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 575,
      "verse_key": "74:7",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلِرَبِّكَ فَٱصۡبِرۡ",
      "translation": "ហើយដើម្បីបំពេញបទបញ្ជានៃម្ចាស់របស់អ្នកចូរអ្នក អត់ធ្មត់។",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 575,
      "verse_key": "74:8",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِذَا نُقِرَ فِي ٱلنَّاقُورِ",
      "translation": "ព្រោះនៅពេលដែលគេផ្លុំត្រែ។",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 575,
      "verse_key": "74:9",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَذَٰلِكَ يَوۡمَئِذٖ يَوۡمٌ عَسِيرٌ",
      "translation": "គឺថ្ងៃនោះហើយ ជាថ្ងៃដែលលំបាកបំផុត។",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 575,
      "verse_key": "74:10",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلَى ٱلۡكَٰفِرِينَ غَيۡرُ يَسِيرٖ",
      "translation": "ចំពោះពួកប្រឆាំង គឺគ្មានការងាយស្រួលឡើយ។",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 575,
      "verse_key": "74:11",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ذَرۡنِي وَمَنۡ خَلَقۡتُ وَحِيدٗا",
      "translation": "ចូរអ្នក(មូហាំម៉ាត់)ទុកឱ្យយើងជាមួយនឹងអ្នកដែលយើង បានបង្កើតគេតែម្នាក់ឯងចុះ។",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 575,
      "verse_key": "74:12",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَجَعَلۡتُ لَهُۥ مَالٗا مَّمۡدُودٗا",
      "translation": "ហើយយើងបានធ្វើឱ្យគេមានទ្រព្យសម្បត្ដិច្រើនស្ដុកស្ដម្ភ។",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 575,
      "verse_key": "74:13",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَبَنِينَ شُهُودٗا",
      "translation": "និងមានកូនចៅដែលនៅជាមួយគេ។",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 575,
      "verse_key": "74:14",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَهَّدتُّ لَهُۥ تَمۡهِيدٗا",
      "translation": "ហើយយើងបានធ្វើឱ្យគេរស់នៅយ៉ាងសុខស្រួល។",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 575,
      "verse_key": "74:15",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ يَطۡمَعُ أَنۡ أَزِيدَ",
      "translation": "បន្ទាប់មក គេមានភាពលោភលន់ចង់ឱ្យយើងបន្ថែមឱ្យ គេទៀត។",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 575,
      "verse_key": "74:16",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّآۖ إِنَّهُۥ كَانَ لِأٓيَٰتِنَا عَنِيدٗا",
      "translation": "មិនដូច្នោះទេ. ពិតប្រាកដណាស់ គេគឺជាអ្នកដែលប្រឆាំង រឹងរូសចំពោះអាយ៉ាត់របស់យើង។",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 575,
      "verse_key": "74:17",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "سَأُرۡهِقُهُۥ صَعُودًا",
      "translation": "យើងនឹងធ្វើឱ្យគេមានទុក្ខលំបាកយ៉ាងធ្ងន់ធ្ងរ។",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 576,
      "verse_key": "74:18",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُۥ فَكَّرَ وَقَدَّرَ",
      "translation": "ព្រោះពិតប្រាកដណាស់ គេបានសញ្ចឹងគិត(ចំពោះពាក្យ សំដីរបស់មូហាំម៉ាត់) និងបានរៀបគំរោងធ្វើអាក្រក់។",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 576,
      "verse_key": "74:19",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَقُتِلَ كَيۡفَ قَدَّرَ",
      "translation": "ដូចេ្នះ គេត្រូវបានដាក់បណ្ដាសា។ តើគេរៀបគំរោងធ្វើ អាក្រក់បានយ៉ាងដូចមេ្ដច?",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 576,
      "verse_key": "74:20",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ قُتِلَ كَيۡفَ قَدَّرَ",
      "translation": "បន្ទាប់មក គេត្រូវបានដាក់បណ្ដាសា។ តើគេរៀបគំរោង ធ្វើអាក្រក់បានយ៉ាងដូចមេ្ដច?",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 576,
      "verse_key": "74:21",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ نَظَرَ",
      "translation": "បន្ទាប់មក គេបានពិចារណា។",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 576,
      "verse_key": "74:22",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ عَبَسَ وَبَسَرَ",
      "translation": "បន្ទាប់មកទៀត គេបានធ្វើមុខក្រញូវ និងខឹងមួម៉ៅ។",
      "footnotes": ""
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 576,
      "verse_key": "74:23",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ أَدۡبَرَ وَٱسۡتَكۡبَرَ",
      "translation": "ក្រោយមក គេបានបែរចេញ(ពីការពិត)និងបានក្រអឺត ក្រទមថែមទៀត។",
      "footnotes": ""
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 576,
      "verse_key": "74:24",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَقَالَ إِنۡ هَٰذَآ إِلَّا سِحۡرٞ يُؤۡثَرُ",
      "translation": "រួចគេក៏បាននិយាយថាៈ នេះគ្មានអ្វីក្រៅពីមន្ដអាគមដែល គេទទួលបានពីអ្នកជំនាន់មុននោះឡើយ។",
      "footnotes": ""
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 576,
      "verse_key": "74:25",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنۡ هَٰذَآ إِلَّا قَوۡلُ ٱلۡبَشَرِ",
      "translation": "នេះគ្មានអ្វីក្រៅពីពាក្យសំដីរបស់មនុស្សលោកឡើយ។",
      "footnotes": ""
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 576,
      "verse_key": "74:26",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "سَأُصۡلِيهِ سَقَرَ",
      "translation": "ហើយយើងនឹងបញ្ចូលគេទៅក្នុងនរកសាក័រ។",
      "footnotes": ""
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 576,
      "verse_key": "74:27",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَآ أَدۡرَىٰكَ مَا سَقَرُ",
      "translation": "តើអ្វីដែលធ្វើឱ្យអ្នកដឹងថា នរកសាក័រជាអ្វីនោះ?",
      "footnotes": ""
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 576,
      "verse_key": "74:28",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَا تُبۡقِي وَلَا تَذَرُ",
      "translation": "វាមិនបន្សល់ទុកសាច់ឡើយ ហើយក៏មិនចោលឆ្អឹងដែរ។",
      "footnotes": ""
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 576,
      "verse_key": "74:29",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَوَّاحَةٞ لِّلۡبَشَرِ",
      "translation": "(នរកសាក័រនោះ)វាដុតស្បែកមនុស្សលោក។",
      "footnotes": ""
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 576,
      "verse_key": "74:30",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلَيۡهَا تِسۡعَةَ عَشَرَ",
      "translation": "នៅក្នុងនរកសាក័រនោះ មានម៉ាឡាអ៊ីកាត់ដប់ប្រាំបួនរូប (ចាំធ្វើទារុណកម្ម)។",
      "footnotes": ""
    },
    {
      "id": 31,
      "verse_number": 31,
      "page_number": 576,
      "verse_key": "74:31",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا جَعَلۡنَآ أَصۡحَٰبَ ٱلنَّارِ إِلَّا مَلَٰٓئِكَةٗۖ وَمَا جَعَلۡنَا عِدَّتَهُمۡ إِلَّا فِتۡنَةٗ لِّلَّذِينَ كَفَرُواْ لِيَسۡتَيۡقِنَ ٱلَّذِينَ أُوتُواْ ٱلۡكِتَٰبَ وَيَزۡدَادَ ٱلَّذِينَ ءَامَنُوٓاْ إِيمَٰنٗا وَلَا يَرۡتَابَ ٱلَّذِينَ أُوتُواْ ٱلۡكِتَٰبَ وَٱلۡمُؤۡمِنُونَ وَلِيَقُولَ ٱلَّذِينَ فِي قُلُوبِهِم مَّرَضٞ وَٱلۡكَٰفِرُونَ مَاذَآ أَرَادَ ٱللَّهُ بِهَٰذَا مَثَلٗاۚ كَذَٰلِكَ يُضِلُّ ٱللَّهُ مَن يَشَآءُ وَيَهۡدِي مَن يَشَآءُۚ وَمَا يَعۡلَمُ جُنُودَ رَبِّكَ إِلَّا هُوَۚ وَمَا هِيَ إِلَّا ذِكۡرَىٰ لِلۡبَشَرِ",
      "translation": "ហើយយើងមិនបានបង្កើតអ្នកការពារនរកក្រៅពីម៉ាឡា-អ៊ីកាត់ឡើយ។ ហើយយើងមិនបានបង្កើតចំនួនរបស់ពួកគេឡើយ លើកលែងតែជាការសាកល្បងចំពោះពួកដែលគ្មានជំនឿប៉ុណ្ណោះ ដើម្បីឱ្យបណ្ដាអ្នកដែលបានទទួលគម្ពីរ(ពួកយ៉ាហ៊ូទី និងណាសរ៉នី) ជឿជាក់យ៉ាងច្បាស់ និងដើម្បីឱ្យបណ្ដាអ្នកដែលមានជំនឿមានការ ជឿជាក់ថែមទៀត និងដើម្បីកុំឱ្យអ្នកដែលបានទទួលគម្ពីរ និង បណ្ដាអ្នកដែលមានជំនឿមានការសង្ស័យ។ និងដើម្បីឱ្យបណ្ដាអ្នក ដែលមានជំងឺនៅក្នុងចិត្ដរបស់ពួកគេ និងពួកដែលប្រឆាំងនិយាយ ថាៈ តើអល់ឡោះមានបំណងអ្វីចំពោះឧទាហរណ៍ទាំងនេះ។ ដូចេ្នះ ដែរ អល់ឡោះធ្វើឱ្យវងេ្វងចំពោះអ្នកណាដែលទ្រង់មានចេតនា ហើយក៏ចង្អុលបង្ហាញចំពោះអ្នកណាដែលទ្រង់មានចេតនាដែរ។ ហើយគ្មាននរណាម្នាក់ដឹងពីចំនួនកងទ័ពនៃម្ចាស់របស់អ្នកក្រៅពី ទ្រង់ឡើយ ហើយវា(នរកសាក័រ)ក៏គ្មានអ្វីក្រៅពីជាការរំលឹកដល់ មនុស្សលោកដែរ។",
      "footnotes": ""
    },
    {
      "id": 32,
      "verse_number": 32,
      "page_number": 576,
      "verse_key": "74:32",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّا وَٱلۡقَمَرِ",
      "translation": "មិនដូច្នោះទេ. អល់ឡោះស្បថនឹងព្រះច័ន្ទ។",
      "footnotes": ""
    },
    {
      "id": 33,
      "verse_number": 33,
      "page_number": 576,
      "verse_key": "74:33",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّيۡلِ إِذۡ أَدۡبَرَ",
      "translation": "និងពេលយប់នៅពេលវាបានកន្លងផុតទៅ។",
      "footnotes": ""
    },
    {
      "id": 34,
      "verse_number": 34,
      "page_number": 576,
      "verse_key": "74:34",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلصُّبۡحِ إِذَآ أَسۡفَرَ",
      "translation": "និងពេលព្រឹកនៅពេលដែលវាបពោ្ចញពន្លឺ។",
      "footnotes": ""
    },
    {
      "id": 35,
      "verse_number": 35,
      "page_number": 576,
      "verse_key": "74:35",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهَا لَإِحۡدَى ٱلۡكُبَرِ",
      "translation": "ពិតប្រាកដណាស់វា(នរកសាក័រ)ជាប្រការមួយ ដ៏ធំធេង។",
      "footnotes": ""
    },
    {
      "id": 36,
      "verse_number": 36,
      "page_number": 576,
      "verse_key": "74:36",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "نَذِيرٗا لِّلۡبَشَرِ",
      "translation": "ដើម្បីជាការព្រមានដល់មនុស្សលោក។",
      "footnotes": ""
    },
    {
      "id": 37,
      "verse_number": 37,
      "page_number": 576,
      "verse_key": "74:37",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لِمَن شَآءَ مِنكُمۡ أَن يَتَقَدَّمَ أَوۡ يَتَأَخَّرَ",
      "translation": "សម្រាប់អ្នកណាដែលចង់ជ្រើសរើសមាគ៌ាឆ្ពោះទៅមុខ (ដោយដើរតាមមាគ៌ាត្រឹមត្រូវ) ឬស្ថិតនៅខាងក្រោយ(ដោយ ប្រព្រឹត្ដបាបកម្ម)។",
      "footnotes": ""
    },
    {
      "id": 38,
      "verse_number": 38,
      "page_number": 576,
      "verse_key": "74:38",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كُلُّ نَفۡسِۭ بِمَا كَسَبَتۡ رَهِينَةٌ",
      "translation": "បុគ្គលគ្រប់រូបគឺត្រូវទទួលខុសត្រូវចំពោះអ្វីដែលខ្លួនបាន ប្រព្រឹត្ដកន្លងមក។",
      "footnotes": ""
    },
    {
      "id": 39,
      "verse_number": 39,
      "page_number": 576,
      "verse_key": "74:39",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَّآ أَصۡحَٰبَ ٱلۡيَمِينِ",
      "translation": "លើកលែងតែបណ្ដាអ្នកដែលទទួលសៀវភៅខាងដៃស្ដាំ ប៉ុណ្ណោះ។",
      "footnotes": ""
    },
    {
      "id": 40,
      "verse_number": 40,
      "page_number": 576,
      "verse_key": "74:40",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِي جَنَّٰتٖ يَتَسَآءَلُونَ",
      "translation": "ពួកគេសួរគ្នានៅក្នុងឋានសួគ៌",
      "footnotes": ""
    },
    {
      "id": 41,
      "verse_number": 41,
      "page_number": 576,
      "verse_key": "74:41",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَنِ ٱلۡمُجۡرِمِينَ",
      "translation": "អំពីពួកប្រព្រឹត្ដបាបកម្មថាៈ",
      "footnotes": ""
    },
    {
      "id": 42,
      "verse_number": 42,
      "page_number": 576,
      "verse_key": "74:42",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَا سَلَكَكُمۡ فِي سَقَرَ",
      "translation": "ហេតុអ្វីបានជាទ្រង់បញ្ចូលពួកអ្នកទៅក្នុងនរកសាក័រ ?",
      "footnotes": ""
    },
    {
      "id": 43,
      "verse_number": 43,
      "page_number": 576,
      "verse_key": "74:43",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قَالُواْ لَمۡ نَكُ مِنَ ٱلۡمُصَلِّينَ",
      "translation": "ពួកគេបានឆ្លើយថាៈ ពួកយើងកាលពីមុនមិនបានស្ថិតក្នុង ចំណោមអ្នកដែលប្រតិបត្ដិសឡាតឡើយ។",
      "footnotes": ""
    },
    {
      "id": 44,
      "verse_number": 44,
      "page_number": 576,
      "verse_key": "74:44",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَمۡ نَكُ نُطۡعِمُ ٱلۡمِسۡكِينَ",
      "translation": "ហើយយើងមិនធ្លាប់បានផ្ដល់ចំណីអាហារដល់អ្នកក្រីក្រ ឡើយ។",
      "footnotes": ""
    },
    {
      "id": 45,
      "verse_number": 45,
      "page_number": 576,
      "verse_key": "74:45",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَكُنَّا نَخُوضُ مَعَ ٱلۡخَآئِضِينَ",
      "translation": "ហើយពួកយើងធ្លាប់បាននិយាយភូតកុហកជាមួយនឹងពួក ដែលនិយាយកុហក។",
      "footnotes": ""
    },
    {
      "id": 46,
      "verse_number": 46,
      "page_number": 576,
      "verse_key": "74:46",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَكُنَّا نُكَذِّبُ بِيَوۡمِ ٱلدِّينِ",
      "translation": "ហើយពួកយើងក៏ធ្លាប់បានបដិសេធនឹងថ្ងៃបរលោក",
      "footnotes": ""
    },
    {
      "id": 47,
      "verse_number": 47,
      "page_number": 576,
      "verse_key": "74:47",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "حَتَّىٰٓ أَتَىٰنَا ٱلۡيَقِينُ",
      "translation": "រហូតទាល់តែសេចក្ដីស្លាប់បានមកដល់ពួកយើង។",
      "footnotes": ""
    },
    {
      "id": 48,
      "verse_number": 48,
      "page_number": 577,
      "verse_key": "74:48",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَمَا تَنفَعُهُمۡ شَفَٰعَةُ ٱلشَّٰفِعِينَ",
      "translation": "ហើយការធ្វើអន្ដរាគមន៍របស់បណ្ដាអ្នកអន្ដរាគមន៍គឺគ្មាន ផលប្រយោជន៍អ្វីដល់ពួកគេឡើយ។",
      "footnotes": ""
    },
    {
      "id": 49,
      "verse_number": 49,
      "page_number": 577,
      "verse_key": "74:49",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَمَا لَهُمۡ عَنِ ٱلتَّذۡكِرَةِ مُعۡرِضِينَ",
      "translation": "ដូចេ្នះ ហេតុអ្វីបានជាពួកគេងាកចេញពីការរំលឹក(គម្ពីរ គួរអាន)ទៅវិញ?",
      "footnotes": ""
    },
    {
      "id": 50,
      "verse_number": 50,
      "page_number": 577,
      "verse_key": "74:50",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَأَنَّهُمۡ حُمُرٞ مُّسۡتَنفِرَةٞ",
      "translation": "ពួកទាំងនោះប្រៀបដូចជាសត្វលាដែលផ្អើល",
      "footnotes": ""
    },
    {
      "id": 51,
      "verse_number": 51,
      "page_number": 577,
      "verse_key": "74:51",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَرَّتۡ مِن قَسۡوَرَةِۭ",
      "translation": "ដែលវារត់គេចពីសត្វតោ។",
      "footnotes": ""
    },
    {
      "id": 52,
      "verse_number": 52,
      "page_number": 577,
      "verse_key": "74:52",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بَلۡ يُرِيدُ كُلُّ ٱمۡرِيٕٖ مِّنۡهُمۡ أَن يُؤۡتَىٰ صُحُفٗا مُّنَشَّرَةٗ",
      "translation": "ផ្ទុយទៅវិញ បុគ្គលគ្រប់រូបមានបំណងចង់ឱ្យគេផ្ដល់គម្ពីរ ដែលរីកសុសសាយ(ដូចគម្ពីរគួរអាន)ដែរ។",
      "footnotes": ""
    },
    {
      "id": 53,
      "verse_number": 53,
      "page_number": 577,
      "verse_key": "74:53",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّاۖ بَل لَّا يَخَافُونَ ٱلۡأٓخِرَةَ",
      "translation": "មិនដូច្នោះទេ ផ្ទុយទៅវិញពួកគេមិនខ្លាចថ្ងៃបរលោក ឡើយ។",
      "footnotes": ""
    },
    {
      "id": 54,
      "verse_number": 54,
      "page_number": 577,
      "verse_key": "74:54",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
