{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 589,
      "verse_key": "84:1",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذَا ٱلسَّمَآءُ ٱنشَقَّتۡ",
      "translation": "នៅពេលដែលមេឃត្រូវបានបែកបាក់។",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 589,
      "verse_key": "84:2",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَذِنَتۡ لِرَبِّهَا وَحُقَّتۡ",
      "translation": "ហើយវាបានស្ដាប់បង្គាប់ និងប្រតិបត្ដិតាមម្ចាស់របស់វា។",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 589,
      "verse_key": "84:3",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡأَرۡضُ مُدَّتۡ",
      "translation": "ហើយនៅពេលដែលផែនដីត្រូវបានលាតសន្ធឹង។",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 589,
      "verse_key": "84:4",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَلۡقَتۡ مَا فِيهَا وَتَخَلَّتۡ",
      "translation": "ហើយវាបានបពោ្ចញនូវអ្វីដែលមាននៅក្នុងខ្លួនរបស់វា ហើយធ្វើឱ្យទទេស្អាត។",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 589,
      "verse_key": "84:5",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَذِنَتۡ لِرَبِّهَا وَحُقَّتۡ",
      "translation": "ហើយវាបានស្ដាប់បង្គាប់ និងប្រតិបត្ដិតាមម្ចាស់របស់វា។",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 589,
      "verse_key": "84:6",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَٰٓأَيُّهَا ٱلۡإِنسَٰنُ إِنَّكَ كَادِحٌ إِلَىٰ رَبِّكَ كَدۡحٗا فَمُلَٰقِيهِ",
      "translation": "ឱមនុស្សលោក. ពិតប្រាកដណាស់ អ្នកគឺជាអ្នកដែលខំ ប្រឹងប្រែងសាងចំពោះម្ចាស់របស់អ្នក(ទោះជាអំពើល្អ ឬអាក្រក់) អ្នកនឹងជួបទ្រង់ជាមិនខាន(នៅថ្ងៃបរលោក)។",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 589,
      "verse_key": "84:7",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَمَّا مَنۡ أُوتِيَ كِتَٰبَهُۥ بِيَمِينِهِۦ",
      "translation": "ហើយចំណែកឯអ្នកដែលត្រូវបានគេប្រគល់សៀវភៅ កំណត់ហេតុរបស់គេនៅដៃស្ដាំ",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 589,
      "verse_key": "84:8",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَسَوۡفَ يُحَاسَبُ حِسَابٗا يَسِيرٗا",
      "translation": "គេនឹងត្រូវទទួលនូវការកាត់សេចក្ដីយ៉ាងងាយស្រួលបំផុត។",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 589,
      "verse_key": "84:9",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيَنقَلِبُ إِلَىٰٓ أَهۡلِهِۦ مَسۡرُورٗا",
      "translation": "ហើយគេនឹងត្រូវត្រឡប់ទៅកាន់គ្រួសាររបស់គេវិញដោយ សេចក្ដីរីករាយក្រៃលែង។",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 589,
      "verse_key": "84:10",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَمَّا مَنۡ أُوتِيَ كِتَٰبَهُۥ وَرَآءَ ظَهۡرِهِۦ",
      "translation": "រីឯអ្នកដែលត្រូវបានគេប្រគល់នូវសៀវភៅរបស់គេនៅ ដៃឆេ្វងពីក្រោយខ្នងរបស់គេ",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 589,
      "verse_key": "84:11",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَسَوۡفَ يَدۡعُواْ ثُبُورٗا",
      "translation": "គេនឹងស្រែកដង្ហោយថាៈ វិនាសអន្ដរាយហើយខ្ញុំ។",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 589,
      "verse_key": "84:12",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيَصۡلَىٰ سَعِيرًا",
      "translation": "ហើយគេនឹងចូលក្នុងភ្លើងនរកដែលឆេះយ៉ាងសន្ធោ សន្ធៅ។",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 589,
      "verse_key": "84:13",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُۥ كَانَ فِيٓ أَهۡلِهِۦ مَسۡرُورًا",
      "translation": "ពិតប្រាកដណាស់ គេធ្លាប់រស់នៅក្នុងលោកិយជាមួយនឹង ក្រុមគ្រួសាររបស់គេយ៉ាងសប្បាយរីករាយ។",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 589,
      "verse_key": "84:14",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُۥ ظَنَّ أَن لَّن يَحُورَ",
      "translation": "ពិតប្រាកដណាស់ គេបានគិតថា គេនឹងមិនត្រូវវិលត្រឡប់ (ទៅកាន់អល់ឡោះ)វិញឡើយ។",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 589,
      "verse_key": "84:15",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بَلَىٰٓۚ إِنَّ رَبَّهُۥ كَانَ بِهِۦ بَصِيرٗا",
      "translation": "មិនដូច្នោះទេ. ពិតប្រាកដណាស់ ម្ចាស់របស់គេដឹងបំផុត ចំពោះរូបគេ។",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 589,
      "verse_key": "84:16",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلَآ أُقۡسِمُ بِٱلشَّفَقِ",
      "translation": "ដូចេ្នះ យើងស្បថនឹងពេលថ្ងៃរៀបលិច។",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 589,
      "verse_key": "84:17",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّيۡلِ وَمَا وَسَقَ",
      "translation": "និងពេលយប់ និងអ្វីដែលបានប្រមូលផ្ដុំ។",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 589,
      "verse_key": "84:18",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡقَمَرِ إِذَا ٱتَّسَقَ",
      "translation": "និងព្រះច័ន្ទនៅពេលដែលវាបានរះពេញវង់។",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 589,
      "verse_key": "84:19",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَتَرۡكَبُنَّ طَبَقًا عَن طَبَقٖ",
      "translation": "ពួកអ្នកនឹងវិវដ្ដពីដំណាក់កាលមួយទៅដំណាក់កាលមួយ (នៅក្នុងលោកិយ)។",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 589,
      "verse_key": "84:20",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَمَا لَهُمۡ لَا يُؤۡمِنُونَ",
      "translation": "ដូចេ្នះ តើមានរឿងអី្វទៅបានជាពួកគេគ្មានជំនឿ?",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 589,
      "verse_key": "84:21",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": "obligatory",
      "sajdah_number": 13,
      "text": "وَإِذَا قُرِئَ عَلَيۡهِمُ ٱلۡقُرۡءَانُ لَا يَسۡجُدُونَۤ۩",
      "translation": "ហើយនៅពេលដែលមានគេសូត្រគម្ពីរគួរអានឱ្យពួកគេស្ដាប់ពួកគេមិនស៊ូជូតឡើយ។",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 589,
      "verse_key": "84:22",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بَلِ ٱلَّذِينَ كَفَرُواْ يُكَذِّبُونَ",
      "translation": "មិនតែប៉ុណ្ណោះ ពួកដែលគ្មានជំនឿថែមទាំងបដិសេធ ទៀតផង។",
      "footnotes": ""
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 589,
      "verse_key": "84:23",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱللَّهُ أَعۡلَمُ بِمَا يُوعُونَ",
      "translation": "ហើយអល់ឡោះដឹងបំផុតនូវអ្វីដែលពួកគេលាក់ទុកនៅ ក្នុងចិត្ដ។",
      "footnotes": ""
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 589,
      "verse_key": "84:24",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبَشِّرۡهُم بِعَذَابٍ أَلِيمٍ",
      "translation": "ដូចេ្នះ ចូរអ្នក(មូហាំម៉ាត់)ប្រាប់ពួកគេអំពីទារុណកម្ម ដ៏សែនឈឺចាប់ចុះ។",
      "footnotes": ""
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 589,
      "verse_key": "84:25",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَّا ٱلَّذِينَ ءَامَنُواْ وَعَمِلُواْ ٱلصَّٰلِحَٰتِ لَهُمۡ أَجۡرٌ غَيۡرُ مَمۡنُونِۭ",
      "translation": "លើកលែងតែបណ្ដាអ្នកដែលមានជំនឿនិងបានសាងអំពើល្អត្រឹមត្រូវប៉ុណ្ណោះ។ ពួកគេនឹងទទួលបានផលបុណ្យដោយឥត គណនា។",
      "footnotes": ""
    }
  ]
}