{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 587,
      "verse_key": "82:1",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذَا ٱلسَّمَآءُ ٱنفَطَرَتۡ",
      "translation": "នៅពេលដែលមេឃបានបេ្រះបែក។",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 587,
      "verse_key": "82:2",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡكَوَاكِبُ ٱنتَثَرَتۡ",
      "translation": "និងនៅពេលដែលភពទាំងឡាយបានបាក់ធ្លាក់ខេ្ទចខ្ទី ។",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 587,
      "verse_key": "82:3",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡبِحَارُ فُجِّرَتۡ",
      "translation": "និងនៅពេលដែលទឹកសមុទ្រត្រូវបានគេបំផ្ទុះឡើង ។",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 587,
      "verse_key": "82:4",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡقُبُورُ بُعۡثِرَتۡ",
      "translation": "និងនៅពេលដែលផ្នូរទាំងឡាយត្រូវបានគេបំផុសឡើង។",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 587,
      "verse_key": "82:5",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلِمَتۡ نَفۡسٞ مَّا قَدَّمَتۡ وَأَخَّرَتۡ",
      "translation": "មនុស្សម្នាក់ៗនឹងបានដឹងនូវអ្វីដែលខ្លួនបានសាងកន្លងមក និងអ្វីដែលខ្លួនមិនទាន់សាង។",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 587,
      "verse_key": "82:6",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَٰٓأَيُّهَا ٱلۡإِنسَٰنُ مَا غَرَّكَ بِرَبِّكَ ٱلۡكَرِيمِ",
      "translation": "ឱមនុស្សលោក. តើអ្វីទៅដែលបានបោកប្រាស់អ្នកឱ្យធ្វើ ស្ហ៊ីរិកចំពោះម្ចាស់របស់អ្នកដ៏ថ្កុំថ្កើង",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 587,
      "verse_key": "82:7",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِي خَلَقَكَ فَسَوَّىٰكَ فَعَدَلَكَ",
      "translation": "ដែលបានបង្កើតអ្នក រួចធ្វើឱ្យអ្នកមានរូបរាងពេញលេញ ព្រមទាំងផ្ដល់ឱ្យអ្នកនូវលក្ខណៈល្អសមរម្យ",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 587,
      "verse_key": "82:8",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِيٓ أَيِّ صُورَةٖ مَّا شَآءَ رَكَّبَكَ",
      "translation": "ក្នុងរូបរាងណាមួយដែលទ្រង់មានចេតនា ទ្រង់នឹង បង្កើតអ្នកនោះ?",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 587,
      "verse_key": "82:9",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّا بَلۡ تُكَذِّبُونَ بِٱلدِّينِ",
      "translation": "មិនដូច្នោះទេ. ផ្ទុយទៅវិញពួកអ្នកបែរជាបដិសេធនឹងថ្ងៃ ជំនុំជំរះ(ថ្ងៃបរលោក)ទៅវិញ។",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 587,
      "verse_key": "82:10",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِنَّ عَلَيۡكُمۡ لَحَٰفِظِينَ",
      "translation": "ហើយពិតប្រាកដណាស់ ពួកអ្នកគឺមានអ្នកតាមឃ្លាំមើល (ម៉ាឡាអ៊ីកាត់)។",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 587,
      "verse_key": "82:11",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كِرَامٗا كَٰتِبِينَ",
      "translation": "ជាអ្នកថ្កុំថ្កើង និងជាអ្នកកត់ត្រា(រាល់អំពើរបស់អ្នក)។",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 587,
      "verse_key": "82:12",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَعۡلَمُونَ مَا تَفۡعَلُونَ",
      "translation": "ពួកគេ(ម៉ាឡាអ៊ីកាត់)ដឹងនូវអ្វីដែលពួកអ្នកប្រព្រឹត្ដ។",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 587,
      "verse_key": "82:13",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ ٱلۡأَبۡرَارَ لَفِي نَعِيمٖ",
      "translation": "ពិតប្រាកដណាស់ បណ្ដាអ្នកដែលបានសាងអំពើល្អគឺ ស្ថិតក្នុងឋានសួគ៌ណាអ៊ីម។",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 587,
      "verse_key": "82:14",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِنَّ ٱلۡفُجَّارَ لَفِي جَحِيمٖ",
      "translation": "ហើយពិតប្រាកដណាស់ ពួកដែលប្រព្រឹត្ដអំពើអាក្រក់ គឺស្ថិតក្នុងនរកជើហ៊ីម។",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 587,
      "verse_key": "82:15",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَصۡلَوۡنَهَا يَوۡمَ ٱلدِّينِ",
      "translation": "ពួកគេនឹងចូលទៅក្នុងឋាននរកនោះនៅថ្ងៃជំនុំជំរះ(ថ្ងៃ បរលោក)។",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 587,
      "verse_key": "82:16",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا هُمۡ عَنۡهَا بِغَآئِبِينَ",
      "translation": "ហើយពួកគេមិនអាចគេចវេះពីវាបានឡើយ។",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 587,
      "verse_key": "82:17",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَآ أَدۡرَىٰكَ مَا يَوۡمُ ٱلدِّينِ",
      "translation": "តើអ្វីដែលធ្វើឱ្យអ្នកដឹងថា ថ្ងៃជំនុំជំរះជាអ្វីនោះ?",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 587,
      "verse_key": "82:18",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ مَآ أَدۡرَىٰكَ مَا يَوۡمُ ٱلدِّينِ",
      "translation": "បន្ទាប់មក តើអ្វីដែលធ្វើឱ្យអ្នកដឹងថា ថ្ងៃជំនុំជំរះជាអ្វី នោះ ?",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 587,
      "verse_key": "82:19",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَوۡمَ لَا تَمۡلِكُ نَفۡسٞ لِّنَفۡسٖ شَيۡـٔٗاۖ وَٱلۡأَمۡرُ يَوۡمَئِذٖ لِّلَّهِ",
      "translation": "គឺជាថ្ងៃដែលគ្មានបុគ្គលណាម្នាក់អាចមានអំណាចអ្វីជួយ ដល់បុគ្គលម្នាក់ទៀតបានឡើយ។ ហើយសេចកី្ដសមេ្រចនៅថ្ងៃ នោះគឺជាកម្មសិទ្ធិរបស់អល់ឡោះតែមួយគត់។",
      "footnotes": ""
    }
  ]
}