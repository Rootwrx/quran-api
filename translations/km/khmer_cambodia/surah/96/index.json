{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 597,
      "verse_key": "96:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱقۡرَأۡ بِٱسۡمِ رَبِّكَ ٱلَّذِي خَلَقَ",
      "translation": "ចូរអ្នក(មូហាំម៉ាត់)សូត្រក្នុងនាមម្ចាស់របស់អ្នកដែលបាន បង្កើត(នូវអ្វីៗទាំងអស់)ចុះ។",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 597,
      "verse_key": "96:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "خَلَقَ ٱلۡإِنسَٰنَ مِنۡ عَلَقٍ",
      "translation": "ទ្រង់បានបង្កើតមនុស្សលោកពីកំំណកឈាមមួយដុំ។",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 597,
      "verse_key": "96:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱقۡرَأۡ وَرَبُّكَ ٱلۡأَكۡرَمُ",
      "translation": "ចូរអ្នកសូត្រចុះ ហើយម្ចាស់របស់អ្នកមហាសប្បុរស",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 597,
      "verse_key": "96:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِي عَلَّمَ بِٱلۡقَلَمِ",
      "translation": "ដែលទ្រង់បានបង្រៀន(ការសរសេរ)ដោយស្លាបប៉ាកា។",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 597,
      "verse_key": "96:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلَّمَ ٱلۡإِنسَٰنَ مَا لَمۡ يَعۡلَمۡ",
      "translation": "ទ្រង់បានបង្រៀនមនុស្សលោកនូវអ្វីដែលគេមិនបានដឹង។",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 597,
      "verse_key": "96:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّآ إِنَّ ٱلۡإِنسَٰنَ لَيَطۡغَىٰٓ",
      "translation": "ទេ. ការពិតមនុស្សលោកគឺពិតជាបំពាន(លើច្បាប់របស់ អល់ឡោះ)",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 597,
      "verse_key": "96:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَن رَّءَاهُ ٱسۡتَغۡنَىٰٓ",
      "translation": "ដោយសារគេយល់ឃើញថា ខួ្លនគេមានគ្រប់គ្រាន់ហើយ។",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 597,
      "verse_key": "96:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ إِلَىٰ رَبِّكَ ٱلرُّجۡعَىٰٓ",
      "translation": "ពិតប្រាកដណាស់ កន្លែងវិលត្រឡប់ គឺឆ្ពោះទៅកាន់ម្ចាស់ របស់អ្នក។",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 597,
      "verse_key": "96:9",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَرَءَيۡتَ ٱلَّذِي يَنۡهَىٰ",
      "translation": "តើអ្នក(មូហាំម៉ាត់)បានឃើញអ្នកដែល(អាពូចាហាល់) ហាមឃាត់",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 597,
      "verse_key": "96:10",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَبۡدًا إِذَا صَلَّىٰٓ",
      "translation": "ខ្ញុំរបស់អល់ឡោះ(មូហាំម៉ាត់)នៅពេលដែលគេសឡាត ទេឬ?",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 597,
      "verse_key": "96:11",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَرَءَيۡتَ إِن كَانَ عَلَى ٱلۡهُدَىٰٓ",
      "translation": "តើអ្នកយល់ឃើញយ៉ាងដូចមេ្ដច ប្រសិនបើគេ(ខ្ញុំរបស់ អល់ឡោះ)ស្ថិតនៅលើមាគ៌ាដ៏ត្រឹមត្រូវ",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 597,
      "verse_key": "96:12",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَوۡ أَمَرَ بِٱلتَّقۡوَىٰٓ",
      "translation": "ឬគេបានប្រើគ្នាឱ្យកោតខ្លាចអល់ឡោះនោះ?",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 597,
      "verse_key": "96:13",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَرَءَيۡتَ إِن كَذَّبَ وَتَوَلَّىٰٓ",
      "translation": "តើអ្នក(មូហាំម៉ាត់)យល់ឃើញយ៉ាងដូចមេ្ដចដែរ ប្រសិន បើគេ(អាពូចាហាល់)បានបដិសេធនិងបែរចេញ(ពីជំនឿ)នោះ ?",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 597,
      "verse_key": "96:14",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَمۡ يَعۡلَم بِأَنَّ ٱللَّهَ يَرَىٰ",
      "translation": "តើគេ(អាពូចាហាល់)មិនដឹងទេឬថា អល់ឡោះពិតជាឃើញ(ទង្វើរបស់គេ)នោះ?",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 597,
      "verse_key": "96:15",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّا لَئِن لَّمۡ يَنتَهِ لَنَسۡفَعَۢا بِٱلنَّاصِيَةِ",
      "translation": "ផ្ទុយទៅវិញ. ប្រសិនបើគេមិនឈប់បំពានទេនោះ យើង នឹងចាប់ក្បាលរបស់គេរុញទម្លាក់ទៅក្នុងនរកជាមិនខាន ។",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 597,
      "verse_key": "96:16",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "نَاصِيَةٖ كَٰذِبَةٍ خَاطِئَةٖ",
      "translation": "គឺក្បាលអ្នកដែលភូតកុហក និងសាងបាបកម្ម។",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 597,
      "verse_key": "96:17",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلۡيَدۡعُ نَادِيَهُۥ",
      "translation": "ដូចេ្នះ ចូរអ្នកឱ្យគេស្រែកហៅក្រុមរបស់គេមកជួយចុះ",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 597,
      "verse_key": "96:18",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "سَنَدۡعُ ٱلزَّبَانِيَةَ",
      "translation": "យើងនឹងហៅអ្នកយាមនរក(ដើម្បីចាប់គេបោះទម្លាក់ ទៅក្នុងនរក)។",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 597,
      "verse_key": "96:19",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": "obligatory",
      "sajdah_number": 14,
      "text": "كَلَّا لَا تُطِعۡهُ وَٱسۡجُدۡۤ وَٱقۡتَرِب۩",
      "translation": "ផុ្ទយទៅវិញ ចូរអ្នក(មូហាំម៉ាត់)កុំធ្វើតាមគេឱ្យសោះ ហើយចូរអ្នកស៊ូជូត និងបញ្ជិតខ្លួនទៅចំពោះទ្រង់ចុះ។",
      "footnotes": ""
    }
  ]
}