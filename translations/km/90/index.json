{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 594,
      "verse_key": "90:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَآ أُقۡسِمُ بِهَٰذَا ٱلۡبَلَدِ",
      "translation": "យើង(អល់ឡោះ)ស្បថនឹងទីក្រុងនេះ(ទីក្រុងម៉ាក្កះ)។",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 594,
      "verse_key": "90:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنتَ حِلُّۢ بِهَٰذَا ٱلۡبَلَدِ",
      "translation": "ខណៈដែលអ្នក(មូហាំម៉ាត់)ជាអ្នកដែលរស់នៅក្នុងទីក្រុងនេះ។",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 594,
      "verse_key": "90:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَوَالِدٖ وَمَا وَلَدَ",
      "translation": "ហើយទ្រង់បានស្បថនឹងឪពុកមនុស្សលោក(អាដាំ) និង អ្នកដែលបានកើត(កូនចៅរបស់គាត់)។",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 594,
      "verse_key": "90:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَقَدۡ خَلَقۡنَا ٱلۡإِنسَٰنَ فِي كَبَدٍ",
      "translation": "ជាការពិតណាស់ យើងបានបង្កើតមនុស្សលោកឱ្យស្ថិតនៅ ក្នុងភាពលំបាក។",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 594,
      "verse_key": "90:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَيَحۡسَبُ أَن لَّن يَقۡدِرَ عَلَيۡهِ أَحَدٞ",
      "translation": "តើគេគិតថា គ្មានអ្នកណាអាចមានអានុភាពលើខ្លួនគេទេឬ ?",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 594,
      "verse_key": "90:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَقُولُ أَهۡلَكۡتُ مَالٗا لُّبَدًا",
      "translation": "ដោយគេនិយាយថាៈ ខ្ញុំបានចំណាយទ្រព្យសម្បត្ដិយ៉ាង ច្រើន។",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 594,
      "verse_key": "90:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَيَحۡسَبُ أَن لَّمۡ يَرَهُۥٓ أَحَدٌ",
      "translation": "តើគេគិតថា គ្មាននរណាម្នាក់បានឃើញគេទេឬ ?",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 594,
      "verse_key": "90:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَمۡ نَجۡعَل لَّهُۥ عَيۡنَيۡنِ",
      "translation": "តើយើងមិនបានបង្កើតគេឱ្យមានភ្នែកមួយគូ",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 594,
      "verse_key": "90:9",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلِسَانٗا وَشَفَتَيۡنِ",
      "translation": "និងអណ្ដាតមួយ ហើយនិងបបូរមាត់ទេឬ ?",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 594,
      "verse_key": "90:10",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَهَدَيۡنَٰهُ ٱلنَّجۡدَيۡنِ",
      "translation": "ហើយយើងថែមទាំងបានចង្អុលបង្ហាញមាគ៌ាពីរ(ល្អ និង អាក្រក់)ដល់គេទៀតផង។",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 594,
      "verse_key": "90:11",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلَا ٱقۡتَحَمَ ٱلۡعَقَبَةَ",
      "translation": "តែគេមិនបានដើរនៅលើមាគ៌ាដែលលំបាក(មាគ៌ាទៅ កាន់ភាពជោគជ័យ)ឡើយ។",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 594,
      "verse_key": "90:12",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَآ أَدۡرَىٰكَ مَا ٱلۡعَقَبَةُ",
      "translation": "តើអ្វីដែលធ្វើឱ្យអ្នកដឹងថា មាគ៌ាដែលលំបាកជាអ្វីនោះ?",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 594,
      "verse_key": "90:13",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَكُّ رَقَبَةٍ",
      "translation": "គឺការដោះលែងខ្ញុំបម្រើដាច់ថ្លៃឱ្យមានសិទ្ធិសេរីភាព។",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 594,
      "verse_key": "90:14",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَوۡ إِطۡعَٰمٞ فِي يَوۡمٖ ذِي مَسۡغَبَةٖ",
      "translation": "ឬផ្ដល់ចំណីអាហារនៅក្នុងថ្ងៃមួយដែលមានគ្រោះទុរភិក្ស។",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 594,
      "verse_key": "90:15",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَتِيمٗا ذَا مَقۡرَبَةٍ",
      "translation": "(ឬផ្ដល់ចំណីអាហារដល់)កូនកំព្រាដែលជាញាតិសន្ដាន។",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 594,
      "verse_key": "90:16",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَوۡ مِسۡكِينٗا ذَا مَتۡرَبَةٖ",
      "translation": "ឬអ្នកក្រីក្រដែលគ្មានអ្វីសោះ។",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 594,
      "verse_key": "90:17",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ كَانَ مِنَ ٱلَّذِينَ ءَامَنُواْ وَتَوَاصَوۡاْ بِٱلصَّبۡرِ وَتَوَاصَوۡاْ بِٱلۡمَرۡحَمَةِ",
      "translation": "បន្ទាប់មក គេបានក្លាយជាមនុស្សម្នាក់ក្នុងចំណោមអ្នក ដែលមានជំនឿ និងបានផ្ដែផ្ដាំគ្នាឱ្យមានការអត់ធ្មត់ ហើយនិងបាន ផ្ដែផ្ដាំគ្នាឱ្យមានក្ដីមេត្ដាករុណា។",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 594,
      "verse_key": "90:18",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أُوْلَٰٓئِكَ أَصۡحَٰبُ ٱلۡمَيۡمَنَةِ",
      "translation": "អ្នកទាំងនោះគឺជាក្រុមដែលទទួលបញ្ជីដោយដៃស្ដាំ។",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 594,
      "verse_key": "90:19",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّذِينَ كَفَرُواْ بِـَٔايَٰتِنَا هُمۡ أَصۡحَٰبُ ٱلۡمَشۡـَٔمَةِ",
      "translation": "ហើយពួកដែលគ្មានជំនឿនឹងអាយ៉ាត់ៗរបស់យើង ពួក គេគឺជាពួកដែលទទួលបញ្ជីដោយដៃឆេ្វង។",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 594,
      "verse_key": "90:20",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلَيۡهِمۡ نَارٞ مُّؤۡصَدَةُۢ",
      "translation": "ភ្លើងនរកគ្របដណ្ដប់លើពួកគេយ៉ាងជិត។",
      "footnotes": ""
    }
  ]
}