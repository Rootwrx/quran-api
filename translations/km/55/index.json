{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 531,
      "verse_key": "55:1",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلرَّحۡمَٰنُ",
      "translation": "អល់ឡោះមហាសប្បុរស។",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 531,
      "verse_key": "55:2",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلَّمَ ٱلۡقُرۡءَانَ",
      "translation": "ទ្រង់បានបង្រៀនគម្ពីរគួរអាន(ដល់មនុស្សលោក)។",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 531,
      "verse_key": "55:3",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "خَلَقَ ٱلۡإِنسَٰنَ",
      "translation": "ទ្រង់បានបង្កើតមនុស្សលោក។",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 531,
      "verse_key": "55:4",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلَّمَهُ ٱلۡبَيَانَ",
      "translation": "ទ្រង់បានបង្រៀនគេឱ្យចេះបរិយាយសព្វបែបយ៉ាង។",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 531,
      "verse_key": "55:5",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلشَّمۡسُ وَٱلۡقَمَرُ بِحُسۡبَانٖ",
      "translation": "ព្រះអាទិត្យ និងព្រះច័ន្ទធ្វើដំណើរតាមការកំណត់ដ៏ជាក់ លាក់។",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 531,
      "verse_key": "55:6",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلنَّجۡمُ وَٱلشَّجَرُ يَسۡجُدَانِ",
      "translation": "ហើយរុក្ខជាតិនិងដើមឈើនានាស៊ូជូត(ចំពោះអល់ឡោះ)។",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 531,
      "verse_key": "55:7",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلسَّمَآءَ رَفَعَهَا وَوَضَعَ ٱلۡمِيزَانَ",
      "translation": "ហើយមេឃ ទ្រង់បានលើកវាឱ្យខ្ពស់ ហើយទ្រង់បានបង្កើត ឱ្យមានភាពយុត្ដិធម៌(នៅលើផែនដី)",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 531,
      "verse_key": "55:8",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَّا تَطۡغَوۡاْ فِي ٱلۡمِيزَانِ",
      "translation": "ដើម្បីកុំឱ្យពួកអ្នកបំពានលើភាពយុត្ដិធម៌នោះ។",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 531,
      "verse_key": "55:9",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَقِيمُواْ ٱلۡوَزۡنَ بِٱلۡقِسۡطِ وَلَا تُخۡسِرُواْ ٱلۡمِيزَانَ",
      "translation": "ហើយចូរពួកអ្នកប្រតិបត្ដិក្នុងការថ្លឹងឱ្យបានត្រឹមត្រូវ ហើយ កុំធ្វើឱ្យខ្វះភ្នែកជញ្ជីងឱ្យសោះ។",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 531,
      "verse_key": "55:10",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡأَرۡضَ وَضَعَهَا لِلۡأَنَامِ",
      "translation": "ហើយផែនដី ទ្រង់បានបង្កើតវា(រាបស្មើ)សម្រាប់សត្វ លោក។",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 531,
      "verse_key": "55:11",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِيهَا فَٰكِهَةٞ وَٱلنَّخۡلُ ذَاتُ ٱلۡأَكۡمَامِ",
      "translation": "ដោយនៅលើវា មានផ្លែឈើ និងដើមល្មើដែលមានចង្កោម ផ្លែជាច្រើន។",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 531,
      "verse_key": "55:12",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡحَبُّ ذُو ٱلۡعَصۡفِ وَٱلرَّيۡحَانُ",
      "translation": "និងគ្រាប់ធញ្ញជាតិដែលមានសំបក ព្រមទាំងរុក្ខជាតិដែល មានក្លិនក្រអូប។",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 531,
      "verse_key": "55:13",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "ដូចេ្នះ តើនៀកម៉ាត់ណាមួយនៃម្ចាស់របស់អ្នកទាំងពីរ (មនុស្សលោកនិងជិន)ដែលអ្នកទាំងពីរបដិសេធនោះ?",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 531,
      "verse_key": "55:14",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "خَلَقَ ٱلۡإِنسَٰنَ مِن صَلۡصَٰلٖ كَٱلۡفَخَّارِ",
      "translation": "ទ្រង់បានបង្កើតមនុស្សលោក(អាដាំ)ពីដីឥដ្ឋស្ងួត ដូចជា ដីដែលគេសូនជាសំភារៈ។",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 531,
      "verse_key": "55:15",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَخَلَقَ ٱلۡجَآنَّ مِن مَّارِجٖ مِّن نَّارٖ",
      "translation": "ហើយទ្រង់បានបង្កើតជិនអំពីអណ្ដាតភ្លើង។",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 531,
      "verse_key": "55:16",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "ដូចេ្នះ តើនៀកម៉ាត់ណាមួយនៃម្ចាស់របស់អ្នកទាំងពីរ (មនុស្សលោកនិងជិន)ដែលអ្នកទាំងពីរបដិសេធនោះ?",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 532,
      "verse_key": "55:17",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "رَبُّ ٱلۡمَشۡرِقَيۡنِ وَرَبُّ ٱلۡمَغۡرِبَيۡنِ",
      "translation": "(អល់ឡោះ)ជាម្ចាស់កន្លែងទាំងពីរនៃព្រះអាទិត្យរះ (ពេលរដូវក្ដៅ និងរដូវរងា) និងជាម្ចាស់កន្លែងទាំងពីរនៃព្រះ អាទិត្យលិច(ពេលរដូវក្ដៅ និងរដូវរងា)។",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 532,
      "verse_key": "55:18",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "ដូចេ្នះ តើនៀកម៉ាត់ណាមួយនៃម្ចាស់របស់អ្នកទាំងពីរ (មនុស្សលោកនិងជិន)ដែលអ្នកទាំងពីរបដិសេធនោះ?",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 532,
      "verse_key": "55:19",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَرَجَ ٱلۡبَحۡرَيۡنِ يَلۡتَقِيَانِ",
      "translation": "ទ្រង់បានធ្វើឱ្យសមុទ្រពីរ(ទឹកសាបនិងទឹកប្រៃ)ជួបគ្នា។",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 532,
      "verse_key": "55:20",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بَيۡنَهُمَا بَرۡزَخٞ لَّا يَبۡغِيَانِ",
      "translation": "នៅរវាងវាទាំងពីរមានរនាំងខណ្ឌមិនឱ្យជ្រាបចូលគ្នា។",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 532,
      "verse_key": "55:21",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "ដូចេ្នះ តើនៀកម៉ាត់ណាមួយនៃម្ចាស់របស់អ្នកទាំងពីរ (មនុស្សលោកនិងជិន)ដែលអ្នកទាំងពីរបដិសេធនោះ?",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 532,
      "verse_key": "55:22",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَخۡرُجُ مِنۡهُمَا ٱللُّؤۡلُؤُ وَٱلۡمَرۡجَانُ",
      "translation": "គជ់ខ្យង និងផ្កាថ្មចេញពីវាទាំងពីរ(សមុទ្រទឹកសាបនិងទឹកប្រៃ)។",
      "footnotes": ""
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 532,
      "verse_key": "55:23",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "ដូចេ្នះ តើនៀកម៉ាត់ណាមួយនៃម្ចាស់របស់អ្នកទាំងពីរ (មនុស្សលោកនិងជិន)ដែលអ្នកទាំងពីរបដិសេធនោះ?",
      "footnotes": ""
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 532,
      "verse_key": "55:24",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَهُ ٱلۡجَوَارِ ٱلۡمُنشَـَٔاتُ فِي ٱلۡبَحۡرِ كَٱلۡأَعۡلَٰمِ",
      "translation": "ហើយនាវាធំៗដែលធ្វើដំណើរនៅលើផ្ទៃសមុទ្រហាក់ ដូចជាភ្នំនោះ គឺជាកម្មសិទ្ធិរបស់ទ្រង់។",
      "footnotes": ""
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 532,
      "verse_key": "55:25",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "ដូចេ្នះ តើនៀកម៉ាត់ណាមួយនៃម្ចាស់របស់អ្នកទាំងពីរ (មនុស្សលោកនិងជិន)ដែលអ្នកទាំងពីរបដិសេធនោះ?",
      "footnotes": ""
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 532,
      "verse_key": "55:26",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كُلُّ مَنۡ عَلَيۡهَا فَانٖ",
      "translation": "វីៗទាំងអស់នៅលើផែនដីនឹងវិនាសអន្ដរាយ។",
      "footnotes": ""
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 532,
      "verse_key": "55:27",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيَبۡقَىٰ وَجۡهُ رَبِّكَ ذُو ٱلۡجَلَٰلِ وَٱلۡإِكۡرَامِ",
