{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 577,
      "verse_key": "75:1",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَآ أُقۡسِمُ بِيَوۡمِ ٱلۡقِيَٰمَةِ",
      "translation": "យើង(អល់ឡោះ)ស្បថនឹងថ្ងៃបរលោក។",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 577,
      "verse_key": "75:2",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَآ أُقۡسِمُ بِٱلنَّفۡسِ ٱللَّوَّامَةِ",
      "translation": "ហើយយើងស្បថនឹងជីវិត(អ្នកមានជំនឿ)ដែលមានការ សោកស្ដាយចំពោះខ្លួនឯង។",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 577,
      "verse_key": "75:3",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَيَحۡسَبُ ٱلۡإِنسَٰنُ أَلَّن نَّجۡمَعَ عِظَامَهُۥ",
      "translation": "តើមនុស្សលោក(អ្នកដែលគ្មានជំនឿ)នឹកស្មានថា យើង គ្មានលទ្ធភាពប្រមូលឆ្អឹងរបស់គេឬ?",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 577,
      "verse_key": "75:4",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بَلَىٰ قَٰدِرِينَ عَلَىٰٓ أَن نُّسَوِّيَ بَنَانَهُۥ",
      "translation": "មិនដូច្នោះទេ. តាមពិតយើងជាអ្នកមានលទ្ធភាពគ្រប់គ្រាន់ ក្នុងការរៀបសន្លាក់ឆ្អឹង ម្រាមដៃ ម្រាមជើងរបស់គេឱ្យមានលក្ខណៈ ពេញលេញដូចដើមវិញ។",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 577,
      "verse_key": "75:5",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بَلۡ يُرِيدُ ٱلۡإِنسَٰنُ لِيَفۡجُرَ أَمَامَهُۥ",
      "translation": "ផ្ទុយទៅវិញ មនុស្សលោកនៅតែមានបំណងចង់ប្រព្រឹត្ដ បាបកម្មដដែល",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 577,
      "verse_key": "75:6",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَسۡـَٔلُ أَيَّانَ يَوۡمُ ٱلۡقِيَٰمَةِ",
      "translation": "ដោយគេសួរថាៈ តើថ្ងៃបរលោកនឹងកើតឡើងនៅ ពេលណា?",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 577,
      "verse_key": "75:7",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِذَا بَرِقَ ٱلۡبَصَرُ",
      "translation": "នៅពេលដែលភ្នែកស្រឡាំងកាំង(ដោយសារស្ថាន ភាពថ្ងៃបរលោក)",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 577,
      "verse_key": "75:8",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَخَسَفَ ٱلۡقَمَرُ",
      "translation": "និងព្រះច័ន្ទបាត់ពន្លឺ",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 577,
      "verse_key": "75:9",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَجُمِعَ ٱلشَّمۡسُ وَٱلۡقَمَرُ",
      "translation": "ហើយព្រះអាទិត្យនិងព្រះច័ន្ទត្រូវបានគេប្រមូល",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 577,
      "verse_key": "75:10",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَقُولُ ٱلۡإِنسَٰنُ يَوۡمَئِذٍ أَيۡنَ ٱلۡمَفَرُّ",
      "translation": "ថ្ងៃនោះមនុស្សលោកនឹងនិយាយថាៈ តើពួកយើងត្រូវ រត់ទៅទីណា ?",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 577,
      "verse_key": "75:11",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّا لَا وَزَرَ",
      "translation": "ទេ. គ្មានកន្លែងរត់ឡើយ។",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 577,
      "verse_key": "75:12",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَىٰ رَبِّكَ يَوۡمَئِذٍ ٱلۡمُسۡتَقَرُّ",
      "translation": "កន្លែងវិលត្រឡប់នៅថ្ងៃនោះមានតែឆ្ពោះទៅកាន់ម្ចាស់ របស់អ្នកប៉ុណ្ណោះ។",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 577,
      "verse_key": "75:13",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يُنَبَّؤُاْ ٱلۡإِنسَٰنُ يَوۡمَئِذِۭ بِمَا قَدَّمَ وَأَخَّرَ",
      "translation": "នៅថ្ងៃនោះមនុស្សលោកត្រូវគេប្រាប់នូវអ្វីដែលគេបាន ប្រព្រឹត្ដកន្លងមក និងអ្វីដែលគេមិនបានប្រព្រឹត្ដ។",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 577,
      "verse_key": "75:14",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بَلِ ٱلۡإِنسَٰنُ عَلَىٰ نَفۡسِهِۦ بَصِيرَةٞ",
      "translation": "ផ្ទុយទៅវិញ មនុស្សលោកគឺជាអ្នកធ្វើសាក្សីលើខួ្លនឯង។",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 577,
      "verse_key": "75:15",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَوۡ أَلۡقَىٰ مَعَاذِيرَهُۥ",
      "translation": "ហើយទោះបីជាគេបាននាំមកនូវការដោះសារបស់គេយ៉ាង ណាក៏ដោយ(ក៏គ្មានប្រយោជន៍អ្វីដល់គេដែរ)។",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 577,
      "verse_key": "75:16",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَا تُحَرِّكۡ بِهِۦ لِسَانَكَ لِتَعۡجَلَ بِهِۦٓ",
      "translation": "ចូរអ្នក(មូហាំម៉ាត់)កុំកម្រើកអណ្ដាតរបស់អ្នកចំពោះ ការសូត្រវា ព្រោះតែប្រញាប់ទនេ្ទញវាឱ្យសោះ។",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 577,
      "verse_key": "75:17",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ عَلَيۡنَا جَمۡعَهُۥ وَقُرۡءَانَهُۥ",
      "translation": "ពិតប្រាកដណាស់ យើងជាអ្នកទទួលខុសត្រូវថែរក្សាវា (នៅក្នុងចិត្ដរបស់អ្នក) ហើយឱ្យអ្នកស្ទាត់ជំនាញក្នុងការសូត្រវា។",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 577,
      "verse_key": "75:18",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِذَا قَرَأۡنَٰهُ فَٱتَّبِعۡ قُرۡءَانَهُۥ",
      "translation": "ដូចេ្នះ នៅពេលដែលយើងបានសូត្រវា(តាមរយៈជីព្រីល) ចូរអ្នកត្រាប់តាមការសូត្ររបស់វាចុះ។",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 577,
      "verse_key": "75:19",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ إِنَّ عَلَيۡنَا بَيَانَهُۥ",
      "translation": "បន្ទាប់មក ពិតប្រាកដណាស់ យើងជាអ្នកទទួលខុសត្រូវ ក្នុងការបំភ្លឺវា។",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 578,
      "verse_key": "75:20",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّا بَلۡ تُحِبُّونَ ٱلۡعَاجِلَةَ",
      "translation": "ទេ. ផ្ទុយទៅវិញ ពួកអ្នកស្រឡាញ់ចូលចិត្ដការរស់នៅក្នុង លោកិយ",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 578,
      "verse_key": "75:21",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَتَذَرُونَ ٱلۡأٓخِرَةَ",
      "translation": "ហើយពួកអ្នកបោះបង់ថ្ងៃបរលោក។",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 578,
      "verse_key": "75:22",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وُجُوهٞ يَوۡمَئِذٖ نَّاضِرَةٌ",
      "translation": "វង់ភ័ក្ដ្រ(អ្នកដែលមានជំនឿ)នៅថ្ងៃនោះគឺមានពន្លឺចែង ចាំង",
      "footnotes": ""
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 578,
      "verse_key": "75:23",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَىٰ رَبِّهَا نَاظِرَةٞ",
      "translation": "ដោយបានមើលទៅកាន់ម្ចាស់របស់ពួកគេ ។",
      "footnotes": ""
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 578,
      "verse_key": "75:24",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَوُجُوهٞ يَوۡمَئِذِۭ بَاسِرَةٞ",
      "translation": "រីឯមុខ(អ្នកដែលគ្មានជំនឿ)នៅថ្ងៃនោះគឺក្រញូវ",
      "footnotes": ""
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 578,
      "verse_key": "75:25",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تَظُنُّ أَن يُفۡعَلَ بِهَا فَاقِرَةٞ",
      "translation": "ដោយពួកគេគិតយ៉ាងច្បាស់ថា គេនឹងធ្វើទារុណកម្មពួកគេ យ៉ាងធ្ងន់ធ្ងរ។",
      "footnotes": ""
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 578,
      "verse_key": "75:26",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّآ إِذَا بَلَغَتِ ٱلتَّرَاقِيَ",
      "translation": "មិនដូច្នោះទេ. នៅពេលដែលវា(ដង្ហើម)បានមកដល់ បំពង់ក។",
      "footnotes": ""
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 578,
      "verse_key": "75:27",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَقِيلَ مَنۡۜ رَاقٖ",
      "translation": "ហើយមានគេនិយាយថាៈ តើអ្នកណាជាអ្នកព្យាបាល (ឱ្យរួចផុតពីសេចក្ដីស្លាប់បាន)?",
      "footnotes": ""
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 578,
      "verse_key": "75:28",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَظَنَّ أَنَّهُ ٱلۡفِرَاقُ",
      "translation": "ហើយគេបានជឿយ៉ាងច្បាស់ថាៈ គេពិតជាលាចាកពី លោកិយជាមិនខាន។",
