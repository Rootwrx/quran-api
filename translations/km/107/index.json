{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 602,
      "verse_key": "107:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَرَءَيۡتَ ٱلَّذِي يُكَذِّبُ بِٱلدِّينِ",
      "translation": "តើអ្នក(មូហាំម៉ាត់)បានឃើញបុគ្គលដែលបដិសេធនឹងថ្ងៃ ជំនុំជំរះដែរឬទេ ?",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 602,
      "verse_key": "107:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَذَٰلِكَ ٱلَّذِي يَدُعُّ ٱلۡيَتِيمَ",
      "translation": "នោះហើយគឺជាបុគ្គលដែលបោះបង់កូនកំព្រា។",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 602,
      "verse_key": "107:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَا يَحُضُّ عَلَىٰ طَعَامِ ٱلۡمِسۡكِينِ",
      "translation": "ហើយមិនជម្រុញគ្នាឱ្យផ្ដល់ចំណីអាហារដល់អ្នកក្រីក្រ។",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 602,
      "verse_key": "107:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَوَيۡلٞ لِّلۡمُصَلِّينَ",
      "translation": "ដូចេ្នះ ភាពវិនាសអន្ដរាយគឺសម្រាប់ពួកដែលសឡាត",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 602,
      "verse_key": "107:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِينَ هُمۡ عَن صَلَاتِهِمۡ سَاهُونَ",
      "translation": "ដែលពួកគេព្រងើយកន្ដើយចំពោះការសឡាតរបស់ពួកគេ។",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 602,
      "verse_key": "107:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِينَ هُمۡ يُرَآءُونَ",
      "translation": "ពួកគេគឺជាពួកដែលសាងអំពើល្អដើម្បីចង់បង្ហាញឱ្យគេ ឃើញតែប៉ុណ្ណោះ។",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 602,
      "verse_key": "107:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيَمۡنَعُونَ ٱلۡمَاعُونَ",
      "translation": "ហើយពួកគេមិនឱ្យទ្រព្យគេខ្ចីឡើយ សូម្បីតែបន្ដិចបន្ដួច ក៏ដោយ។",
      "footnotes": ""
    }
  ]
}