{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 582,
      "verse_key": "78:1",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَمَّ يَتَسَآءَلُونَ",
      "translation": "តើពួកគេដេញដោលគ្នាអំពីរឿងអ្វី?",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 582,
      "verse_key": "78:2",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَنِ ٱلنَّبَإِ ٱلۡعَظِيمِ",
      "translation": "គឺអំពីដំណឹងដ៏សំខាន់",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 582,
      "verse_key": "78:3",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِي هُمۡ فِيهِ مُخۡتَلِفُونَ",
      "translation": "ដែលពួកគេកំពុងតែខ្វែងគំនិតគ្នាអំពីវា។",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 582,
      "verse_key": "78:4",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّا سَيَعۡلَمُونَ",
      "translation": "ទេ. ពួកគេគង់តែនឹងដឹង។",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 582,
      "verse_key": "78:5",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ كَلَّا سَيَعۡلَمُونَ",
      "translation": "ទេ. បន្ទាប់មកពួកគេគង់តែនឹងដឹងយ៉ាងច្បាស់។",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 582,
      "verse_key": "78:6",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَمۡ نَجۡعَلِ ٱلۡأَرۡضَ مِهَٰدٗا",
      "translation": "តើយើងមិនបានបង្កើតផែនដីឱ្យមានសណ្ឋានរាបស្មើ",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 582,
      "verse_key": "78:7",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡجِبَالَ أَوۡتَادٗا",
      "translation": "និងភ្នំទាំងឡាយជាបង្គោលស្នឹងទេឬ?",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 582,
      "verse_key": "78:8",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَخَلَقۡنَٰكُمۡ أَزۡوَٰجٗا",
      "translation": "ហើយយើងបានបង្កើតពួកអ្នកជាគូៗ(ប្រុស និងស្រី)។",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 582,
      "verse_key": "78:9",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَجَعَلۡنَا نَوۡمَكُمۡ سُبَاتٗا",
      "translation": "ហើយយើងបានបង្កើតការដេករបស់ពួកអ្នកទុកជាការឈប់ សម្រាក។",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 582,
      "verse_key": "78:10",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَجَعَلۡنَا ٱلَّيۡلَ لِبَاسٗا",
      "translation": "ហើយយើងបានបង្កើតពេលយប់ជាការបិទបាំង។",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 582,
      "verse_key": "78:11",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَجَعَلۡنَا ٱلنَّهَارَ مَعَاشٗا",
      "translation": "ហើយយើងបានបង្កើតពេលថ្ងៃជាការរស់នៅ។",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 582,
      "verse_key": "78:12",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَبَنَيۡنَا فَوۡقَكُمۡ سَبۡعٗا شِدَادٗا",
      "translation": "ហើយយើងបានបង្កើតមេឃប្រាំពីរជាន់ដ៏រឹងមាំនៅពីលើ ពួកអ្នក។",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 582,
      "verse_key": "78:13",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَجَعَلۡنَا سِرَاجٗا وَهَّاجٗا",
      "translation": "ហើយយើងបានបង្កើតព្រះអាទិត្យដែលមានពន្លឺចែង ចាំង។",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 582,
      "verse_key": "78:14",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنزَلۡنَا مِنَ ٱلۡمُعۡصِرَٰتِ مَآءٗ ثَجَّاجٗا",
      "translation": "ហើយយើងបានបញ្ចុះពីដុំពពកនូវទឹកភ្លៀងស្រោចស្រព គ្រប់ទីកន្លែង។",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 582,
      "verse_key": "78:15",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لِّنُخۡرِجَ بِهِۦ حَبّٗا وَنَبَاتٗا",
      "translation": "ដើម្បីយើងនឹងបពោ្ចញតាមរយៈវានូវគ្រាប់ធញ្ញជាតិ និង រុក្ខជាតិគ្រប់ប្រភេទ។",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 582,
      "verse_key": "78:16",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَجَنَّٰتٍ أَلۡفَافًا",
      "translation": "និងចំការដំណាំដែលមានផលានុផលគ្រប់ប្រភេទ។",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 582,
      "verse_key": "78:17",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ يَوۡمَ ٱلۡفَصۡلِ كَانَ مِيقَٰتٗا",
      "translation": "ពិតប្រាកដណាស់ ថ្ងៃកាត់សេចក្ដីគឺមានពេលកំណត់ ច្បាស់លាស់។",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 582,
      "verse_key": "78:18",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَوۡمَ يُنفَخُ فِي ٱلصُّورِ فَتَأۡتُونَ أَفۡوَاجٗا",
      "translation": "នៅថ្ងៃដែលគេផ្លុំត្រែ ពេលនោះពួកអ្នកនឹងមក(កាន់ កន្លែងជំនុំជំរះ)ជាក្រុមៗ។",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 582,
      "verse_key": "78:19",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَفُتِحَتِ ٱلسَّمَآءُ فَكَانَتۡ أَبۡوَٰبٗا",
      "translation": "ហើយមេឃនឹងត្រូវគេបើកចំហ ពេលនោះវាមានទ្វារ ជាច្រើន។",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 582,
      "verse_key": "78:20",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَسُيِّرَتِ ٱلۡجِبَالُ فَكَانَتۡ سَرَابًا",
      "translation": "ហើយភ្នំទាំងឡាយត្រូវបានគេធ្វើឱ្យរបើកចេញពីកន្លែង របស់វា ហើយវារសាត់ដូចធូលី។",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 582,
      "verse_key": "78:21",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ جَهَنَّمَ كَانَتۡ مِرۡصَادٗا",
      "translation": "ពិតប្រាកដណាស់ នរកជើហាន់ណាំគឺជាកន្លែងត្រៀម។",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 582,
      "verse_key": "78:22",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لِّلطَّٰغِينَ مَـَٔابٗا",
      "translation": "ជាកន្លែងរស់នៅសម្រាប់ពួកដែលបំពាន។",
      "footnotes": ""
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 582,
      "verse_key": "78:23",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَّٰبِثِينَ فِيهَآ أَحۡقَابٗا",
      "translation": "ដោយពួកគេរស់នៅទីនោះជារៀងរហូត។",
      "footnotes": ""
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 582,
      "verse_key": "78:24",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَّا يَذُوقُونَ فِيهَا بَرۡدٗا وَلَا شَرَابًا",
      "translation": "ពួកគេមិនដែលស្គាល់រសជាតិត្រជាក់នៅទីនោះឡើយ ហើយក៏គ្មានទឹកផឹកដែរ។",
      "footnotes": ""
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 582,
      "verse_key": "78:25",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَّا حَمِيمٗا وَغَسَّاقٗا",
      "translation": "មានតែទឹកក្ដៅ និងខ្ទុះប៉ុណ្ណោះ",
      "footnotes": ""
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 582,
      "verse_key": "78:26",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "جَزَآءٗ وِفَاقًا",
      "translation": "ជាការតបស្នងដ៏សមហេតុផលបំផុត។",
      "footnotes": ""
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 582,
      "verse_key": "78:27",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُمۡ كَانُواْ لَا يَرۡجُونَ حِسَابٗا",
      "translation": "ពិតប្រាកដណាស់ ពួកគេគឺជាពួកដែលមិនសង្ឃឹមថានឹង ជួបការជំនុំជំរះឡើយ។",
      "footnotes": ""
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 582,
      "verse_key": "78:28",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَكَذَّبُواْ بِـَٔايَٰتِنَا كِذَّابٗا",
      "translation": "ថែមទាំងពួកគេបានបដិសេធនឹងអាយ៉ាត់ៗរបស់យើង ទៀតផង។",
      "footnotes": ""
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 582,
      "verse_key": "78:29",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَكُلَّ شَيۡءٍ أَحۡصَيۡنَٰهُ كِتَٰبٗا",
      "translation": "ហើយអ្វីៗទាំងអស់យើងបានកត់ត្រាវាទុកនៅក្នុង សៀវភៅកំណត់ត្រា(ឡាវហុលម៉ះហ្វ៊ូស)រួចជាសេ្រច។",
      "footnotes": ""
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 582,
      "verse_key": "78:30",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَذُوقُواْ فَلَن نَّزِيدَكُمۡ إِلَّا عَذَابًا",
      "translation": "ដូចេ្នះ ចូរពួកអ្នកភ្លក្សចុះ ហើយយើងមិនបន្ថែមអ្វីឱ្យពួក អ្នកក្រៅពីទារុណកម្មឡើយ។",
      "footnotes": ""
    },
    {
      "id": 31,
      "verse_number": 31,
      "page_number": 583,
      "verse_key": "78:31",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ لِلۡمُتَّقِينَ مَفَازًا",
      "translation": "ពិតប្រាកដណាស់ ភាពជោគជ័យគឺសម្រាប់បណ្ដាអ្នក ដែលកោតខ្លាចអល់ឡោះ។",
      "footnotes": ""
    },
    {
      "id": 32,
      "verse_number": 32,
      "page_number": 583,
      "verse_key": "78:32",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "حَدَآئِقَ وَأَعۡنَٰبٗا",
      "translation": "គឺ(ពួកគេ)មានចំការដំណាំ និងចំការទំពាំងបាយជូរ។",
      "footnotes": ""
    },
    {
      "id": 33,
      "verse_number": 33,
      "page_number": 583,
      "verse_key": "78:33",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَكَوَاعِبَ أَتۡرَابٗا",
      "translation": "និងមានស្ដ្រីព្រហ្មចារីជាច្រើនដែលមានវ័យស្មើៗគ្នា។",
      "footnotes": ""
    },
    {
      "id": 34,
      "verse_number": 34,
      "page_number": 583,
      "verse_key": "78:34",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَكَأۡسٗا دِهَاقٗا",
      "translation": "ព្រមទាំងកែវដែលពេញទៅដោយស្រារួចជាសេ្រច។",
      "footnotes": ""
    },
    {
      "id": 35,
      "verse_number": 35,
      "page_number": 583,
      "verse_key": "78:35",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَّا يَسۡمَعُونَ فِيهَا لَغۡوٗا وَلَا كِذَّٰبٗا",
      "translation": "នៅក្នុងវា(ឋានសួគ៌) ពួកគេមិនឮពាក្យសំដីដែលឥត ប្រយោជន៍ឡើយ ហើយក៏មិនឮពាក្យភូតកុហកដែរ។",
      "footnotes": ""
    },
    {
      "id": 36,
      "verse_number": 36,
      "page_number": 583,
      "verse_key": "78:36",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "جَزَآءٗ مِّن رَّبِّكَ عَطَآءً حِسَابٗا",
      "translation": "គឺជាការតបស្នងពីម្ចាស់របស់អ្នកដែលជាអំណោយមួយ ដ៏ពេញលេញ។",
      "footnotes": ""
    },
    {
      "id": 37,
      "verse_number": 37,
      "page_number": 583,
      "verse_key": "78:37",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "رَّبِّ ٱلسَّمَٰوَٰتِ وَٱلۡأَرۡضِ وَمَا بَيۡنَهُمَا ٱلرَّحۡمَٰنِۖ لَا يَمۡلِكُونَ مِنۡهُ خِطَابٗا",
      "translation": "គឺមកពីម្ចាស់គ្រប់គ្រងមេឃជាច្រើនជាន់ និងផែនដី និង អ្វីៗដែលនៅរវាងវាទាំងពីរ។ នេះគឺមកពីម្ចាស់មហាសប្បុរស។ ពួកគេ(មនុស្សលោក)មិនអាចនិយាយអ្វីបានឡើយ លុះត្រាតែ មានការអនុញ្ញាតពីទ្រង់។",
      "footnotes": ""
    },
    {
      "id": 38,
      "verse_number": 38,
      "page_number": 583,
      "verse_key": "78:38",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَوۡمَ يَقُومُ ٱلرُّوحُ وَٱلۡمَلَٰٓئِكَةُ صَفّٗاۖ لَّا يَتَكَلَّمُونَ إِلَّا مَنۡ أَذِنَ لَهُ ٱلرَّحۡمَٰنُ وَقَالَ صَوَابٗا",
      "translation": "នៅថ្ងៃដែលជីព្រីល និងម៉ាឡាអ៊ីកាត់ទាំងឡាយឈរ តំរៀបជាជួរៗ ពួកគេមិនអាចនិយាយអ្វីបានឡើយ លើកលែងតែ អ្នកណាដែលម្ចាស់មហាសប្បុរសអនុញ្ញាតឱ្យគេ ហើយគេនិយាយ នូវពាក្យសំដីណាដែលត្រឹមត្រូវប៉ុណ្ណោះ។",
      "footnotes": ""
    },
    {
      "id": 39,
      "verse_number": 39,
      "page_number": 583,
      "verse_key": "78:39",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ذَٰلِكَ ٱلۡيَوۡمُ ٱلۡحَقُّۖ فَمَن شَآءَ ٱتَّخَذَ إِلَىٰ رَبِّهِۦ مَـَٔابًا",
      "translation": "នោះគឺជាថ្ងៃដែលនឹងកើតឡើងយ៉ាងពិតប្រាកដ។ ដូចេ្នះ អ្នកណាហើយចង់(គេចផុតពីភាពលំបាកនៅថ្ងៃបរលោក)នោះ ចូរឱ្យគេជ្រើសរើសកន្លែងវិលត្រឡប់ទៅកាន់ម្ចាស់របស់គេចុះ។",
      "footnotes": ""
    },
    {
      "id": 40,
      "verse_number": 40,
      "page_number": 583,
      "verse_key": "78:40",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّآ أَنذَرۡنَٰكُمۡ عَذَابٗا قَرِيبٗا يَوۡمَ يَنظُرُ ٱلۡمَرۡءُ مَا قَدَّمَتۡ يَدَاهُ وَيَقُولُ ٱلۡكَافِرُ يَٰلَيۡتَنِي كُنتُ تُرَٰبَۢا",
      "translation": "ពិតប្រាកដណាស់ យើងបានដាស់តឿនព្រមានពួកអ្នក នូវទារុណកម្មដែលនៅជិតបំផុតនាថ្ងៃដែលបុគ្គលម្នាក់ៗមើលទៅ ចំពោះអ្វីដែលដៃទាំងពីររបស់គេបានប្រព្រឹត្ដកន្លងមក។ ហើយអ្នក គ្មានជំនឿនឹងនិយាយថាៈ ឱអនិច្ចា. ធ្វើមេ្ដចទើបខ្ញុំក្លាយជាដីវិញ។",
      "footnotes": ""
    }
  ]
}