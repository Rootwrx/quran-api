{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 597,
      "verse_key": "95:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلتِّينِ وَٱلزَّيۡتُونِ",
      "translation": "អល់ឡោះស្បថនឹងផ្លែទីន(ផ្លែល្វា)និងហ្សៃទូន (ផ្លែអូលីវ)។",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 597,
      "verse_key": "95:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَطُورِ سِينِينَ",
      "translation": "និងភ្នំទូរស៊ីណា។",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 597,
      "verse_key": "95:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَهَٰذَا ٱلۡبَلَدِ ٱلۡأَمِينِ",
      "translation": "និងទីក្រុងនេះ(ម៉ាក្កះ)ដែលប្រកបដោយសុខសន្ដិភាព។",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 597,
      "verse_key": "95:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَقَدۡ خَلَقۡنَا ٱلۡإِنسَٰنَ فِيٓ أَحۡسَنِ تَقۡوِيمٖ",
      "translation": "ជាការពិតណាស់ យើងបានបង្កើតមនុស្សលោកស្ថិតក្នុង ទ្រង់ទ្រាយមួយដ៏ល្អប្រសើរបំផុត។",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 597,
      "verse_key": "95:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ رَدَدۡنَٰهُ أَسۡفَلَ سَٰفِلِينَ",
      "translation": "បន្ទាប់មកយើងបានបញ្ជូនពួកគេត្រឡប់ទៅកាន់កន្លែង ដែលទាបជាងគេបំផុត(បាតនរក)។",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 597,
      "verse_key": "95:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَّا ٱلَّذِينَ ءَامَنُواْ وَعَمِلُواْ ٱلصَّٰلِحَٰتِ فَلَهُمۡ أَجۡرٌ غَيۡرُ مَمۡنُونٖ",
      "translation": "លើកលែងតែបណ្ដាអ្នកដែលមានជំនឿនិងបានសាងអំពើ ល្អត្រឹមត្រូវ គឺពួកគេនឹងទទួលបានផលបុណ្យជានិរន្ដរ៍។",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 597,
      "verse_key": "95:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَمَا يُكَذِّبُكَ بَعۡدُ بِٱلدِّينِ",
      "translation": "ដូចេ្នះ បន្ទាប់ពីនេះទៅ តើអី្វដែលធ្វើឱ្យអ្នក(មនុស្សលោក) បដិសេធនឹងថ្ងៃជំនុំជំរះ(ថ្ងៃបរលោក)នោះ?",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 597,
      "verse_key": "95:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَيۡسَ ٱللَّهُ بِأَحۡكَمِ ٱلۡحَٰكِمِينَ",
      "translation": "តើអល់ឡោះមិនមែនជាអ្នកកាត់សេចក្ដីដ៏ប្រសើរបំផុត ជាងអ្នកកាត់សេចក្ដីទាំងឡាយទេឬ?",
      "footnotes": ""
    }
  ]
}