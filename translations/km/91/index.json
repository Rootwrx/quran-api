{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 595,
      "verse_key": "91:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلشَّمۡسِ وَضُحَىٰهَا",
      "translation": "អល់ឡោះស្បថនឹងព្រះអាទិត្យ និងរស្មីដ៏ត្រចះត្រចង់ របស់វានៅពេលព្រឹក។",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 595,
      "verse_key": "91:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡقَمَرِ إِذَا تَلَىٰهَا",
      "translation": "និងព្រះច័ន្ទនៅពេលដែលវាបានរះឡើងបន្ទាប់ពីព្រះអាទិត្យលិច ។",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 595,
      "verse_key": "91:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلنَّهَارِ إِذَا جَلَّىٰهَا",
      "translation": "និងពេលថ្ងៃនៅពេលដែលព្រះអាទិត្យបានបពោ្ចញពន្លឺ។",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 595,
      "verse_key": "91:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّيۡلِ إِذَا يَغۡشَىٰهَا",
      "translation": "និងពេលយប់នៅពេលដែលវាបានគ្របដណ្ដប់ព្រះអាទិត្យ។",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 595,
      "verse_key": "91:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلسَّمَآءِ وَمَا بَنَىٰهَا",
      "translation": "និងមេឃ និងអ្នកដែលបានបង្កើតវា។",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 595,
      "verse_key": "91:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡأَرۡضِ وَمَا طَحَىٰهَا",
      "translation": "និងផែនដី និងអ្នកដែលបានលាតសនឹ្ធងវា។",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 595,
      "verse_key": "91:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَنَفۡسٖ وَمَا سَوَّىٰهَا",
      "translation": "និងរូបរាង(អាដាំ) និងអ្នកដែលបានធ្វើឱ្យគេមានលក្ខណៈ សមរម្យ។",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 595,
      "verse_key": "91:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَلۡهَمَهَا فُجُورَهَا وَتَقۡوَىٰهَا",
      "translation": "រួចមកទ្រង់បានចង្អុលបង្ហាញដល់គេឱ្យស្គាល់នូវអំពើទុច្ចរិត និងអំពើសុច្ចរិតរបស់គេ។",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 595,
      "verse_key": "91:9",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قَدۡ أَفۡلَحَ مَن زَكَّىٰهَا",
      "translation": "អ្នកដែលសំអាតខ្លួនគេ(ដោយសាងអំពើល្អ) ពិតជាទទួល បានជោគជ័យ។",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 595,
      "verse_key": "91:10",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَقَدۡ خَابَ مَن دَسَّىٰهَا",
      "translation": "ហើយអ្នកដែលបានធ្វើឱ្យខ្លួនគេកខ្វក់(ដោយសាងអំពើ អាក្រក់)ពិតជាបានខាតបង់។",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 595,
      "verse_key": "91:11",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَذَّبَتۡ ثَمُودُ بِطَغۡوَىٰهَآ",
      "translation": "ក្រុមសាមូដបានបដិសេធ(អ្នកនាំសាររបស់ពួកគេ)ដោយ សារការបំពានរបស់ពួកគេ។",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 595,
      "verse_key": "91:12",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذِ ٱنۢبَعَثَ أَشۡقَىٰهَا",
      "translation": "នៅពេលដែលជនកំណាចបំផុតក្នុងចំណោមពួកគេបាន ចេញមក(សម្លាប់សត្វអូដ្ឋញី)។",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 595,
      "verse_key": "91:13",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَقَالَ لَهُمۡ رَسُولُ ٱللَّهِ نَاقَةَ ٱللَّهِ وَسُقۡيَٰهَا",
      "translation": "ពេលនោះអ្នកនាំសាររបស់អល់ឡោះ(សឡេះ)បាន ពោលទៅកាន់ពួកគេថាៈ (ចូរប្រុងប្រយ័ត្នចំពោះ)សត្វអូដ្ឋញី របស់អល់ឡោះ ហើយកុំរារាំងដល់ការផឹករបស់វាឱ្យសោះ។",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 595,
      "verse_key": "91:14",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَكَذَّبُوهُ فَعَقَرُوهَا فَدَمۡدَمَ عَلَيۡهِمۡ رَبُّهُم بِذَنۢبِهِمۡ فَسَوَّىٰهَا",
      "translation": "តែពួកគេបានបដិសេធនឹងគាត់ និងបានសម្លាប់វា។ ពេល នោះម្ចាស់របស់ពួកគេបានបំផ្លាញពួកគេដោយសារតែបាបកម្មរបស់ពួកគេ។ ហើយទ្រង់បានធើ្វឱ្យពួកគេវិនាសអន្ដរាយទាំងអស់គ្នា។",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 595,
      "verse_key": "91:15",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَا يَخَافُ عُقۡبَٰهَا",
      "translation": "ហើយទ្រង់មិនខ្លាចលទ្ធផលចុងក្រោយរបស់ពួកគេ (ដែលទ្រង់បានដាក់ទណ្ឌកម្មលើពួកគេ)នោះឡើយ។",
      "footnotes": ""
    }
  ]
}