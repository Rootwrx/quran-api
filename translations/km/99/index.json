{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 599,
      "verse_key": "99:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذَا زُلۡزِلَتِ ٱلۡأَرۡضُ زِلۡزَالَهَا",
      "translation": "នៅពេលដែលផែនដីត្រូវបានគេធ្វើឱ្យរញ្ជួយយ៉ាងខ្លាំងក្លា។",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 599,
      "verse_key": "99:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَخۡرَجَتِ ٱلۡأَرۡضُ أَثۡقَالَهَا",
      "translation": "ហើយផែនដីបានបពោ្ចញនូវសាកសពទាំងអស់ដែលមាន នៅក្នុងខ្លួនរបស់វា ។",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 599,
      "verse_key": "99:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَقَالَ ٱلۡإِنسَٰنُ مَا لَهَا",
      "translation": "ហើយមនុស្សលោកក៏និយាយថាៈ តើមានរឿងអ្វីកើត ឡើងចំពោះផែនដីនេះ?",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 599,
      "verse_key": "99:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَوۡمَئِذٖ تُحَدِّثُ أَخۡبَارَهَا",
      "translation": "នៅថ្ងៃនោះ វានឹងប្រកាសប្រាប់នូវរឿងរ៉ាវរបស់វា",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 599,
      "verse_key": "99:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بِأَنَّ رَبَّكَ أَوۡحَىٰ لَهَا",
      "translation": "ពីព្រោះម្ចាស់របស់អ្នកបានបញ្ជាឱ្យវាធ្វើដូច្នោះ។",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 599,
      "verse_key": "99:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَوۡمَئِذٖ يَصۡدُرُ ٱلنَّاسُ أَشۡتَاتٗا لِّيُرَوۡاْ أَعۡمَٰلَهُمۡ",
      "translation": "នៅថ្ងៃនោះដែរ មនុស្សលោកនឹងចេញពីផ្នូរមកទាំងក្រុមៗខុសៗគ្នាដើម្បីទទួលការបង្ហាញឱ្យឃើញនូវអំពើទាំងឡាយរបស់ ពួកគេ។",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 599,
      "verse_key": "99:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَمَن يَعۡمَلۡ مِثۡقَالَ ذَرَّةٍ خَيۡرٗا يَرَهُۥ",
      "translation": "ដូចេ្នះ អ្នកដែលធ្វើអំពើល្អសូម្បីមានទម្ងន់ប៉ុនលំអងធូលី ក៏គេនឹងឃើញវាដែរ។",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 599,
      "verse_key": "99:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَن يَعۡمَلۡ مِثۡقَالَ ذَرَّةٖ شَرّٗا يَرَهُۥ",
      "translation": "ចំណែកឯអ្នកដែលធ្វើអំពើអាក្រក់ សូម្បីមានទម្ងន់ប៉ុន លំអងធូលីក៏គេនឹងមើលឃើញវាដែរ។",
      "footnotes": ""
    }
  ]
}