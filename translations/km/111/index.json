{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 603,
      "verse_key": "111:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تَبَّتۡ يَدَآ أَبِي لَهَبٖ وَتَبَّ",
      "translation": "ដៃទាំងពីរ(ទង្វើ)របស់អាពូឡាហាប់បានវិនាសអន្ដរាយ ហើយគេក៏វិនាសអន្ដរាយ។",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 603,
      "verse_key": "111:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَآ أَغۡنَىٰ عَنۡهُ مَالُهُۥ وَمَا كَسَبَ",
      "translation": "ទ្រព្យសម្បត្ដិរបស់គេនិងអ្វីៗដែលគេបានសាងកន្លងមក មិនបានផ្ដល់ផលប្រយោជន៍ដល់គេឡើយ។",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 603,
      "verse_key": "111:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "سَيَصۡلَىٰ نَارٗا ذَاتَ لَهَبٖ",
      "translation": "គេនឹងចូលទៅក្នុងនរកដែលមានអណ្ដាតភ្លើងដ៏សន្ធោ សន្ធៅ។",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 603,
      "verse_key": "111:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱمۡرَأَتُهُۥ حَمَّالَةَ ٱلۡحَطَبِ",
      "translation": "ហើយភរិយារបស់គេដែលជាអ្នកយកបន្លា(រាយលើផ្លូវ ដើម្បីធ្វើបាបមូហាំម៉ាត់)ក៏ដូច្នោះដែរ។",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 603,
      "verse_key": "111:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِي جِيدِهَا حَبۡلٞ مِّن مَّسَدِۭ",
      "translation": "នៅនឹងករបស់នាងមានខ្សែច្រវ៉ាក់ភ្លើង។",
      "footnotes": ""
    }
  ]
}