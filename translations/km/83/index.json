{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 587,
      "verse_key": "83:1",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيۡلٞ لِّلۡمُطَفِّفِينَ",
      "translation": "ភាពវិនាសអន្ដរាយគឺសម្រាប់ពួកដែលគៃបន្លំ។",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 587,
      "verse_key": "83:2",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِينَ إِذَا ٱكۡتَالُواْ عَلَى ٱلنَّاسِ يَسۡتَوۡفُونَ",
      "translation": "ជាពួកដែលទាមទារចង់បានគ្រប់ចំនួននៅពេលដែលពួកគេ ថ្លឹងវាស់ ឬវាល់ពីអ្នកដទៃ។",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 587,
      "verse_key": "83:3",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا كَالُوهُمۡ أَو وَّزَنُوهُمۡ يُخۡسِرُونَ",
      "translation": "តែនៅពេលដែលពួកគេវាស់ឬវាល់ ឬថ្លឹងឱ្យអ្នកដទៃ គឺ ពួកគេធ្វើឱ្យខ្វះ។",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 587,
      "verse_key": "83:4",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَا يَظُنُّ أُوْلَٰٓئِكَ أَنَّهُم مَّبۡعُوثُونَ",
      "translation": "តើពួកទាំងនោះមិនគិតថាៈ ពិតប្រាកដណាស់ ពួកគេនឹង ត្រូវគេធ្វើឱ្យរស់ឡើងវិញ",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 587,
      "verse_key": "83:5",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لِيَوۡمٍ عَظِيمٖ",
      "translation": "នៅថ្ងៃមួយដ៏ធំធេង(ថ្ងៃបរលោក)ទេឬ ?",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 587,
      "verse_key": "83:6",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَوۡمَ يَقُومُ ٱلنَّاسُ لِرَبِّ ٱلۡعَٰلَمِينَ",
      "translation": "គឺនៅថ្ងៃដែលមនុស្សលោកនឹងឈរនៅចំពោះមុខម្ចាស់ គ្រប់គ្រងពិភពទាំងអស់។",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 588,
      "verse_key": "83:7",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّآ إِنَّ كِتَٰبَ ٱلۡفُجَّارِ لَفِي سِجِّينٖ",
      "translation": "មិនដូច្នោះទេ. ការពិតបញ្ជីរបស់ពួកប្រព្រឹត្ដអំពើអាក្រក់ គឺស្ថិតក្នុងស៊ិជ្ជីន។",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 588,
      "verse_key": "83:8",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَآ أَدۡرَىٰكَ مَا سِجِّينٞ",
      "translation": "តើអ្វីដែលធ្វើឱ្យអ្នកដឹងថា ស៊ិជ្ជីនជាអ្វីនោះ?",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 588,
      "verse_key": "83:9",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كِتَٰبٞ مَّرۡقُومٞ",
      "translation": "គឺជាបញ្ជីមួយដែលត្រូវបានគេកត់ត្រាទុក។",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 588,
      "verse_key": "83:10",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ",
      "translation": "ភាពវិនាសអន្ដរាយនៅថ្ងៃនោះគឺសម្រាប់ពួកដែល បដិសេធ។",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 588,
      "verse_key": "83:11",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِينَ يُكَذِّبُونَ بِيَوۡمِ ٱلدِّينِ",
      "translation": "គឺជាពួកដែលបដិសេធនឹងថ្ងៃជំនុំជំរះ។",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 588,
      "verse_key": "83:12",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا يُكَذِّبُ بِهِۦٓ إِلَّا كُلُّ مُعۡتَدٍ أَثِيمٍ",
      "translation": "ហើយគ្មានអ្នកណាម្នាក់អាចបដិសេធនឹងវាក្រៅពីពួក ដែលបំពានប្រព្រឹត្ដបាបកម្មនោះឡើយ។",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 588,
      "verse_key": "83:13",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذَا تُتۡلَىٰ عَلَيۡهِ ءَايَٰتُنَا قَالَ أَسَٰطِيرُ ٱلۡأَوَّلِينَ",
      "translation": "នៅពេលមានគេសូត្រអាយ៉ាត់ៗរបស់យើងឱ្យគេស្ដាប់ គេបែរជាបាននិយាយថាៈ នេះគឺជារឿងពេ្រងនិទានទៅវិញ។",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 588,
      "verse_key": "83:14",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّاۖ بَلۡۜ رَانَ عَلَىٰ قُلُوبِهِم مَّا كَانُواْ يَكۡسِبُونَ",
      "translation": "ទេ. ផ្ទុយទៅវិញ អ្វី(បាបកម្ម)ដែលពួកគេប្រព្រឹត្ដនោះ បានគ្របដណ្ដប់លើចិត្ដរបស់ពួកគេ។",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 588,
      "verse_key": "83:15",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّآ إِنَّهُمۡ عَن رَّبِّهِمۡ يَوۡمَئِذٖ لَّمَحۡجُوبُونَ",
      "translation": "ទេ. ពិតប្រាកដណាស់ ពួកគេនឹងត្រូវគេបិទបាំងពីការ មើលឃើញម្ចាស់របស់ពួកគេនៅថ្ងៃនោះ(ថ្ងៃបរលោក)។",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 588,
      "verse_key": "83:16",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ إِنَّهُمۡ لَصَالُواْ ٱلۡجَحِيمِ",
      "translation": "ក្រោយមក ពួកគេពិតជានឹងត្រូវចូលនរកជើហ៊ីម។",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 588,
      "verse_key": "83:17",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ يُقَالُ هَٰذَا ٱلَّذِي كُنتُم بِهِۦ تُكَذِّبُونَ",
      "translation": "ក្រោយមកទៀត មានគេនិយាយថាៈ នេះហើយគឺជាអ្វី ដែលពួកអ្នកធ្លាប់បដិសេធនឹងវានោះ។",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 588,
      "verse_key": "83:18",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّآ إِنَّ كِتَٰبَ ٱلۡأَبۡرَارِ لَفِي عِلِّيِّينَ",
      "translation": "ទេ. ពិតប្រាកដណាស់ បញ្ជីរបស់បណ្ដាអ្នកដែលសាង អំពើល្អ គឺស្ថិតក្នុងអ៊ីល្លីយូន។",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 588,
      "verse_key": "83:19",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَآ أَدۡرَىٰكَ مَا عِلِّيُّونَ",
      "translation": "តើអ្វីដែលធ្វើឱ្យអ្នកដឹងថា អ៊ីល្លីយូនជាអ្វីនោះ?",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 588,
      "verse_key": "83:20",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كِتَٰبٞ مَّرۡقُومٞ",
      "translation": "គឺជាបញ្ជីមួយដែលត្រូវបានគេកត់ត្រាទុក។",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 588,
      "verse_key": "83:21",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَشۡهَدُهُ ٱلۡمُقَرَّبُونَ",
      "translation": "ដែលបណ្ដាម៉ាឡាអ៊ីកាត់កំពុងតែធ្វើសាក្សីចំពោះបញ្ជីនោះ។",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 588,
      "verse_key": "83:22",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ ٱلۡأَبۡرَارَ لَفِي نَعِيمٍ",
      "translation": "ពិតប្រាកដណាស់ បណ្ដាអ្នកដែលសាងអំពើល្អ គឺស្ថិតនៅ ក្នុងឋានសួគ៌ណាអ៊ីម។",
      "footnotes": ""
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 588,
      "verse_key": "83:23",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلَى ٱلۡأَرَآئِكِ يَنظُرُونَ",
      "translation": "ដោយពួកគេអង្គុយនៅលើគ្រែគយគន់(វត្ថុនានា)។",
      "footnotes": ""
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 588,
      "verse_key": "83:24",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تَعۡرِفُ فِي وُجُوهِهِمۡ نَضۡرَةَ ٱلنَّعِيمِ",
      "translation": "អ្នកនឹងមើលឃើញភាពសប្បាយរីករាយនៅលើផ្ទៃមុខ របស់ពួកគេ។",
      "footnotes": ""
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 588,
      "verse_key": "83:25",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يُسۡقَوۡنَ مِن رَّحِيقٖ مَّخۡتُومٍ",
      "translation": "ពួកគេនឹងត្រូវគេឱ្យទទួលទានសុរាដ៏បរិសុទ្ធដែលត្រូវបាន បិទជិត។",
      "footnotes": ""
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 588,
      "verse_key": "83:26",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "خِتَٰمُهُۥ مِسۡكٞۚ وَفِي ذَٰلِكَ فَلۡيَتَنَافَسِ ٱلۡمُتَنَٰفِسُونَ",
      "translation": "ក្លិនរបស់វាក្រអូបបំផុត។ ហើយដើម្បីបានដូចនេះ ចូរឱ្យ បណ្ដាអ្នកដែលប្រកួតប្រជែង ប្រកួតប្រជែងគ្នាចុះ។",
      "footnotes": ""
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 588,
      "verse_key": "83:27",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمِزَاجُهُۥ مِن تَسۡنِيمٍ",
      "translation": "ហើយការលាយរបស់វាគឺអំពីទឹកនៅក្នុងឋានសួគ៌។",
      "footnotes": ""
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 588,
      "verse_key": "83:28",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَيۡنٗا يَشۡرَبُ بِهَا ٱلۡمُقَرَّبُونَ",
      "translation": "គឺជាប្រភពទឹកដែលបណ្ដាអ្នកមានឋានៈខ្ពង់ខ្ពស់ទទួល ទានវា។",
      "footnotes": ""
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 588,
      "verse_key": "83:29",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ ٱلَّذِينَ أَجۡرَمُواْ كَانُواْ مِنَ ٱلَّذِينَ ءَامَنُواْ يَضۡحَكُونَ",
      "translation": "ពិតប្រាកដណាស់ ពួកដែលបានប្រព្រឹត្ដអំពើអាក្រក់(នៅ ក្នុងលោកិយ) ពួកគេធ្លាប់សើចចំអកឱ្យបណ្ដាអ្នកដែលមានជំនឿ។",
      "footnotes": ""
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 588,
      "verse_key": "83:30",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا مَرُّواْ بِهِمۡ يَتَغَامَزُونَ",
      "translation": "ហើយនៅពេលដែលពួកគេបានដើរកាត់បណ្ដាអ្នកដែល មានជំនឿ ពួកគេតែងតែសម្ដែងការចំអកឡកឡឺយ។",
      "footnotes": ""
    },
    {
      "id": 31,
      "verse_number": 31,
      "page_number": 588,
      "verse_key": "83:31",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱنقَلَبُوٓاْ إِلَىٰٓ أَهۡلِهِمُ ٱنقَلَبُواْ فَكِهِينَ",
      "translation": "ហើយនៅពេលដែលពួកគេត្រឡប់ទៅកាន់បក្សពួករបស់ គេវិញ គឺពួកគេត្រឡប់ដោយការចំអកដាក់អ្នកមានជំនឿ។",
      "footnotes": ""
    },
    {
      "id": 32,
      "verse_number": 32,
      "page_number": 588,
      "verse_key": "83:32",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا رَأَوۡهُمۡ قَالُوٓاْ إِنَّ هَٰٓؤُلَآءِ لَضَآلُّونَ",
      "translation": "ហើយនៅពេលដែលពួកគេបានឃើញបណ្ដាអ្នកដែល មានជំនឿ ពួកគេនិយាយថាៈ ពិតប្រាកដណាស់ ពួកទាំងនេះជាពួក ដែលវងេ្វង។",
      "footnotes": ""
    },
    {
      "id": 33,
      "verse_number": 33,
      "page_number": 588,
      "verse_key": "83:33",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَآ أُرۡسِلُواْ عَلَيۡهِمۡ حَٰفِظِينَ",
      "translation": "ហើយពួកគេ(ពួកគ្មានជំនឿ)មិនត្រូវបានគេចាត់តាំង ជាអ្នកឃ្លាំមើលបណ្ដាអ្នកដែលមានជំនឿឡើយ។",
      "footnotes": ""
    },
    {
      "id": 34,
      "verse_number": 34,
      "page_number": 588,
      "verse_key": "83:34",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَٱلۡيَوۡمَ ٱلَّذِينَ ءَامَنُواْ مِنَ ٱلۡكُفَّارِ يَضۡحَكُونَ",
      "translation": "ដូចេ្នះ នៅថ្ងៃនេះ(ថ្ងៃបរលោក) បណ្ដាអ្នកដែលមាន ជំនឿសើចចំអកចំពោះពួកដែលគ្មានជំនឿវិញ។",
      "footnotes": ""
    },
    {
      "id": 35,
      "verse_number": 35,
      "page_number": 589,
      "verse_key": "83:35",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلَى ٱلۡأَرَآئِكِ يَنظُرُونَ",
      "translation": "ពួកគេអង្គុយនៅលើគ្រែគយគន់(វត្ថុនានា)។",
      "footnotes": ""
    },
    {
      "id": 36,
      "verse_number": 36,
      "page_number": 589,
      "verse_key": "83:36",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "هَلۡ ثُوِّبَ ٱلۡكُفَّارُ مَا كَانُواْ يَفۡعَلُونَ",
      "translation": "តើពួកគ្មានជំនឿត្រូវបានគេតបស្នងនូវអ្វីដែលពួកគេ ធ្លាប់បានប្រព្រឹត្ដដែរឬទេ ?",
      "footnotes": ""
    }
  ]
}