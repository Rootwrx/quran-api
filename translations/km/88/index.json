{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 592,
      "verse_key": "88:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "هَلۡ أَتَىٰكَ حَدِيثُ ٱلۡغَٰشِيَةِ",
      "translation": "តើដំណឹងនៃថ្ងៃបរលោកបានមកដល់អ្នក(មូហាំម៉ាត់) ហើយឬនៅ?",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 592,
      "verse_key": "88:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وُجُوهٞ يَوۡمَئِذٍ خَٰشِعَةٌ",
      "translation": "នៅថ្ងៃនោះទឹកមុខ(របស់ពួកគ្មានជំនឿ) គឺមានភាពអាម៉ាស់។",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 592,
      "verse_key": "88:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَامِلَةٞ نَّاصِبَةٞ",
      "translation": "រវល់យ៉ាងខ្លាំង និងនឿយហត់បំផុត។",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 592,
      "verse_key": "88:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تَصۡلَىٰ نَارًا حَامِيَةٗ",
      "translation": "ពួកគេនឹងចូលក្នុងភើ្លងនរកដ៏សែនក្ដៅ។",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 592,
      "verse_key": "88:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تُسۡقَىٰ مِنۡ عَيۡنٍ ءَانِيَةٖ",
      "translation": "ពួកគេនឹងត្រូវគេឱ្យផឹកទឹកដ៏ក្ដៅបំផុត។",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 592,
      "verse_key": "88:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَّيۡسَ لَهُمۡ طَعَامٌ إِلَّا مِن ضَرِيعٖ",
      "translation": "គ្មានចំណីអាហារណាសម្រាប់ពួកគេក្រៅពីបន្លានិងជាតិពុល",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 592,
      "verse_key": "88:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَّا يُسۡمِنُ وَلَا يُغۡنِي مِن جُوعٖ",
      "translation": "ដែលវានឹងមិនធ្វើឱ្យធំធាត់ និងមិនធ្វើឱ្យបាត់ការសេ្រក ឃ្លាននោះឡើយ ។",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 592,
      "verse_key": "88:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وُجُوهٞ يَوۡمَئِذٖ نَّاعِمَةٞ",
      "translation": "នៅថ្ងៃនោះដែរ ទឹកមុខ(របស់បណ្ដាអ្នកមានជំនឿ)ប្រកប ដោយក្ដីរីករាយ។",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 592,
      "verse_key": "88:9",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لِّسَعۡيِهَا رَاضِيَةٞ",
      "translation": "ដោយសារតែពួកគេពេញចិត្ដចំពោះការប្រឹងប្រែងរបស់ខ្លួន (នៅក្នុងលោកិយ) ។",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 592,
      "verse_key": "88:10",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِي جَنَّةٍ عَالِيَةٖ",
      "translation": "នៅក្នុងឋានសួគ៌ដ៏ខ្ពស់ត្រដែត",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 592,
      "verse_key": "88:11",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَّا تَسۡمَعُ فِيهَا لَٰغِيَةٗ",
      "translation": "ដែលនៅក្នុងវានោះ ពួកគេមិនឮពាក្យសំដីឥតប្រយោជន៍ ឡើយ។",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 592,
      "verse_key": "88:12",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِيهَا عَيۡنٞ جَارِيَةٞ",
      "translation": "នៅទីនោះ មានប្រភពទឹកដែលហូរ។",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 592,
      "verse_key": "88:13",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِيهَا سُرُرٞ مَّرۡفُوعَةٞ",
      "translation": "នៅទីនោះដែរ មានគ្រែខ្ពស់ៗ។",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 592,
      "verse_key": "88:14",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَكۡوَابٞ مَّوۡضُوعَةٞ",
      "translation": "និងកែវដែលបានរៀបចំទុកដាក់ជាសេ្រច។",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 592,
      "verse_key": "88:15",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَنَمَارِقُ مَصۡفُوفَةٞ",
      "translation": "និងខ្នើយកើយរៀបដាក់ជាជួរៗ។",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 592,
      "verse_key": "88:16",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَزَرَابِيُّ مَبۡثُوثَةٌ",
      "translation": "និងកម្រាលព្រំគ្រប់បែបយ៉ាងដែលបានក្រាលសន្ធឹងរួច ជាសេ្រច។",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 592,
      "verse_key": "88:17",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَلَا يَنظُرُونَ إِلَى ٱلۡإِبِلِ كَيۡفَ خُلِقَتۡ",
      "translation": "ហើយតើពួកគេមិនបានពិចារណាចំពោះសត្វអូដ្ឋទេឬថា វាត្រូវបានគេបង្កើតយ៉ាងដូចមេ្ដច?",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 592,
      "verse_key": "88:18",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِلَى ٱلسَّمَآءِ كَيۡفَ رُفِعَتۡ",
      "translation": "និងមើលទៅមេឃ តើវាត្រូវបានគេលើកឱ្យខ្ពស់ឡើង យ៉ាងដូចមេ្ដច?",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 592,
      "verse_key": "88:19",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِلَى ٱلۡجِبَالِ كَيۡفَ نُصِبَتۡ",
      "translation": "និងមើលទៅភ្នំទាំងឡាយ តើវាត្រូវបានគេបោះភ្ជាប់ យ៉ាងរឹងមាំដូចមេ្ដច ?",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 592,
      "verse_key": "88:20",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِلَى ٱلۡأَرۡضِ كَيۡفَ سُطِحَتۡ",
      "translation": "និងមើលទៅផែនដី តើវាត្រូវបានគេពន្លាតសន្ធឹងយ៉ាង ដូចមេ្ដចដែរ ?",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 592,
      "verse_key": "88:21",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَذَكِّرۡ إِنَّمَآ أَنتَ مُذَكِّرٞ",
      "translation": "ដូចេ្នះ ចូរអ្នក(មូហាំម៉ាត់)ក្រើនរំលឹកចុះ ការពិតអ្នក គ្រាន់តែជាអ្នកដែលក្រើនរំលឹកប៉ុណ្ណោះ។",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 592,
      "verse_key": "88:22",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَّسۡتَ عَلَيۡهِم بِمُصَيۡطِرٍ",
      "translation": "អ្នកពុំមែនជាអ្នកប្រើអំណាចផ្ដាច់ការទៅលើពួកគេ ឡើយ។",
      "footnotes": ""
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 592,
      "verse_key": "88:23",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَّا مَن تَوَلَّىٰ وَكَفَرَ",
      "translation": "លើកលែងតែអ្នកណាដែលងាកចេញ និងគ្មានជំនឿ ប៉ុណ្ណោះ។",
      "footnotes": ""
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 592,
      "verse_key": "88:24",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَيُعَذِّبُهُ ٱللَّهُ ٱلۡعَذَابَ ٱلۡأَكۡبَرَ",
      "translation": "អល់ឡោះនឹងធ្វើទារុណកម្មគេនូវទារុណកម្មមួយយ៉ាង ធ្ងន់ធ្ងរបំផុត។",
      "footnotes": ""
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 592,
      "verse_key": "88:25",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ إِلَيۡنَآ إِيَابَهُمۡ",
      "translation": "ពិតប្រាកដណាស់ ការវិលត្រឡប់របស់ពួកគេគឺមកកាន់ យើងតែម្នាក់គត់។",
      "footnotes": ""
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 592,
      "verse_key": "88:26",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ إِنَّ عَلَيۡنَا حِسَابَهُم",
      "translation": "បន្ទាប់មក ពិតប្រាកដណាស់ ការជំនុំជំរះរបស់ពួកគេ គឺជាតួនាទីរបស់យើង។",
      "footnotes": ""
    }
  ]
}