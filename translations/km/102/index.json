{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 600,
      "verse_key": "102:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلۡهَىٰكُمُ ٱلتَّكَاثُرُ",
      "translation": "ភាពក្រអឺតក្រទមនឹងភាពមានទ្រព្យសម្បត្ដិ និងកូនចៅ ជាច្រើនបានធ្វើឱ្យពួកអ្នកភេ្លចខ្លួន(ពីការគោរពប្រតិបត្ដិចំពោះ អល់ឡោះ)។",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 600,
      "verse_key": "102:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "حَتَّىٰ زُرۡتُمُ ٱلۡمَقَابِرَ",
      "translation": "រហូតពួកអ្នកចូលទៅក្នុងផ្នូរ(រហូតអ្នកស្លាប់)។",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 600,
      "verse_key": "102:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّا سَوۡفَ تَعۡلَمُونَ",
      "translation": "ទេ. ពួកអ្នកគង់តែនឹងដឹងជាមិនខាន ។",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 600,
      "verse_key": "102:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ كَلَّا سَوۡفَ تَعۡلَمُونَ",
      "translation": "ទេ. បន្ទាប់មកពួកអ្នកគង់តែនឹងដឹង។",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 600,
      "verse_key": "102:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّا لَوۡ تَعۡلَمُونَ عِلۡمَ ٱلۡيَقِينِ",
      "translation": "ទេ. ប្រសិនបើពួកអ្នកដឹង(ពីលទ្ធផលនៃការភេ្លចខ្លួនរបស់ ពួកអ្នក)ដោយការដឹងយ៉ាងច្បាស់លាស់នោះ(ពួកអ្នកពិតជាមិនធ្វើ ដូច្នោះឡើយ)។",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 600,
      "verse_key": "102:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَتَرَوُنَّ ٱلۡجَحِيمَ",
      "translation": "ជាការពិតណាស់ ពួកអ្នកនឹងឃើញនរកជើហ៊ីមជាមិនខាន។",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 600,
      "verse_key": "102:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ لَتَرَوُنَّهَا عَيۡنَ ٱلۡيَقِينِ",
      "translation": "បន្ទាប់មក ពួកអ្នកពិតជានឹងឃើញវាយ៉ាងច្បាស់នឹងភ្នែក។",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 600,
      "verse_key": "102:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ لَتُسۡـَٔلُنَّ يَوۡمَئِذٍ عَنِ ٱلنَّعِيمِ",
      "translation": "ក្រោយមក ពួកអ្នកនឹងត្រូវគេសួរនៅថ្ងៃនោះអំពីនៀកម៉ាត់ ដែលទ្រង់បានប្រទានឱ្យពួកអ្នក។",
      "footnotes": ""
    }
  ]
}