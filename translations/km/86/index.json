{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 591,
      "verse_key": "86:1",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلسَّمَآءِ وَٱلطَّارِقِ",
      "translation": "អល់ឡោះស្បថនឹងមេឃ និងអាត់តរិក។",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 591,
      "verse_key": "86:2",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَآ أَدۡرَىٰكَ مَا ٱلطَّارِقُ",
      "translation": "ហើយអ្វីដែលធ្វើឱ្យអ្នកដឹងថា អាត់តរិកជាអ្វីនោះ?",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 591,
      "verse_key": "86:3",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلنَّجۡمُ ٱلثَّاقِبُ",
      "translation": "គឺជាផ្កាយដែលមានរស្មីភ្លឺចិញ្ចាច។",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 591,
      "verse_key": "86:4",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِن كُلُّ نَفۡسٖ لَّمَّا عَلَيۡهَا حَافِظٞ",
      "translation": "បុគ្គលគ្រប់ៗរូបគឺសុទ្ធតែមានអ្នកថែរក្សាគេ។",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 591,
      "verse_key": "86:5",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلۡيَنظُرِ ٱلۡإِنسَٰنُ مِمَّ خُلِقَ",
      "translation": "ដូចេ្នះ ចូរឱ្យមនុស្សលោកមើលចុះថា តើរូបគេត្រូវបាន បង្កើតមកពីអ្វី ?",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 591,
      "verse_key": "86:6",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "خُلِقَ مِن مَّآءٖ دَافِقٖ",
      "translation": "រូបគេត្រូវបានបង្កើតមកពីទឹកដែលបាញ់ចេញ(ពីប្រដាប់ ភេទបុរសចូលទៅក្នុងប្រដាប់ភេទស្ដ្រី)",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 591,
      "verse_key": "86:7",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَخۡرُجُ مِنۢ بَيۡنِ ٱلصُّلۡبِ وَٱلتَّرَآئِبِ",
      "translation": "ដែលវាចេញពីឆ្អឹងខ្នងបុរស និងឆ្អឹងជំនីស្ដ្រី។",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 591,
      "verse_key": "86:8",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُۥ عَلَىٰ رَجۡعِهِۦ لَقَادِرٞ",
      "translation": "ពិតប្រាកដណាស់ ទ្រង់ជាអ្នកមានអានុភាពក្នុងការធ្វើឱ្យ គេរស់ឡើងវិញ។",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 591,
      "verse_key": "86:9",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَوۡمَ تُبۡلَى ٱلسَّرَآئِرُ",
      "translation": "-នៅថ្ងៃដែលអាថ៌កំបាំងទាំងអស់ត្រូវបានលាតត្រដាង។",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 591,
      "verse_key": "86:10",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَمَا لَهُۥ مِن قُوَّةٖ وَلَا نَاصِرٖ",
      "translation": "ពេលនោះគេគ្មានអំណាច ហើយក៏គ្មានអ្នកជួយដែរ។",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 591,
      "verse_key": "86:11",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلسَّمَآءِ ذَاتِ ٱلرَّجۡعِ",
      "translation": "អល់ឡោះស្បថនឹងមេឃដែលមានភ្លៀងធ្លាក់ម្ដងហើយ ម្ដងទៀត។",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 591,
      "verse_key": "86:12",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡأَرۡضِ ذَاتِ ٱلصَّدۡعِ",
      "translation": "និងផែនដីដែលមានការបេ្រះបែក(ដោយសាររុក្ខជាតិ លូតលាស់)។",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 591,
      "verse_key": "86:13",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُۥ لَقَوۡلٞ فَصۡلٞ",
      "translation": "ពិតប្រាកដណាស់ វា(គម្ពីរគួរអាន)គឺជាពាក្យពេចន៍ ដែលមានការបែងចែក(រវាងខុសនិងត្រូវ)។",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 591,
      "verse_key": "86:14",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا هُوَ بِٱلۡهَزۡلِ",
      "translation": "ហើយគម្ពីរគួរអាន ក៏មិនមែនសម្រាប់លេងសើចដែរ។",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 591,
      "verse_key": "86:15",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُمۡ يَكِيدُونَ كَيۡدٗا",
      "translation": "ពិតប្រាកដណាស់ ពួកគេរៀបចំល្បិចកលអាក្រក់(ដើម្បី ប្រឆាំងនឹងមូហាំម៉ាត់)។",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 591,
      "verse_key": "86:16",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَكِيدُ كَيۡدٗا",
      "translation": "ហើយយើងក៏មានផែនការ(ដើម្បីតបតនឹងពួកគេ)វិញ ដែរ។",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 591,
      "verse_key": "86:17",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَمَهِّلِ ٱلۡكَٰفِرِينَ أَمۡهِلۡهُمۡ رُوَيۡدَۢا",
      "translation": "ដូចេ្នះ ចូរអ្នក(មូហាំម៉ាត់)ពន្យារពេលឱ្យពួកគ្មានជំនឿ និងរង់ចាំមើលពួកគេមួយរយៈសិនចុះ។",
      "footnotes": ""
    }
  ]
}