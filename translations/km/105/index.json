{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 601,
      "verse_key": "105:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَمۡ تَرَ كَيۡفَ فَعَلَ رَبُّكَ بِأَصۡحَٰبِ ٱلۡفِيلِ",
      "translation": "តើអ្នក(មូហាំម៉ាត់)មិនបានឃើញទេឬថា ម្ចាស់របស់អ្នក បានធ្វើទៅលើក្រុមទ័ពដំរីយ៉ាងដូចមេ្ដច ?",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 601,
      "verse_key": "105:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَمۡ يَجۡعَلۡ كَيۡدَهُمۡ فِي تَضۡلِيلٖ",
      "translation": "តើទ្រង់មិនបានធ្វើឱ្យឧបាយកលអាក្រក់របស់ពួកគេវងេ្វង (បរាជ័យ)ទេឬ ?",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 601,
      "verse_key": "105:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَرۡسَلَ عَلَيۡهِمۡ طَيۡرًا أَبَابِيلَ",
      "translation": "ហើយទ្រង់បានបញ្ជូនបក្សីទាំងហ្វូងៗទៅប្រយុទ្ធតទល់ នឹងពួកគេ។",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 601,
      "verse_key": "105:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تَرۡمِيهِم بِحِجَارَةٖ مِّن سِجِّيلٖ",
      "translation": "ពួកវាទម្លាក់ទៅលើពួកគេដោយដុំថ្មស៊ីចជ្ជីល(ដែលមកពី ភ្លើងនរក)។",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 601,
      "verse_key": "105:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَجَعَلَهُمۡ كَعَصۡفٖ مَّأۡكُولِۭ",
      "translation": "រួចមកទ្រង់បានធ្វើឱ្យពួកគេប្រៀបដូចជាស្លឹកឈើដែលត្រូវ ដង្កូវស៊ី។",
      "footnotes": ""
    }
  ]
}