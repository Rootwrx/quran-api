{
  "verses": [
    {
      "number": "1",
      "arabic_text": "قُلۡ أَعُوذُ بِرَبِّ ٱلنَّاسِ",
      "translation": "(ئەی موحەممەد ﷺ) بڵێ: پەنا دەگرم به پەروەردگاری خەڵکی\n"
    },
    {
      "number": "2",
      "arabic_text": "مَلِكِ ٱلنَّاسِ",
      "translation": "بە پادشای خەڵکی\n"
    },
    {
      "number": "3",
      "arabic_text": "إِلَٰهِ ٱلنَّاسِ",
      "translation": "بە پەرستراوی خەڵکی\n"
    },
    {
      "number": "4",
      "arabic_text": "مِن شَرِّ ٱلۡوَسۡوَاسِ ٱلۡخَنَّاسِ",
      "translation": "لە زیان وشەڕی وەسوەسە پەیداکەر(کە کاتێک ناوی خودا ببرێت) خۆی پەنھان دەکات\n"
    },
    {
      "number": "5",
      "arabic_text": "ٱلَّذِي يُوَسۡوِسُ فِي صُدُورِ ٱلنَّاسِ",
      "translation": "ئەوەی کە وەسوەسە ئەخاتە دڵی خەڵکیەوە\n"
    },
    {
      "number": "6",
      "arabic_text": "مِنَ ٱلۡجِنَّةِ وَٱلنَّاسِ",
      "translation": "لە جنۆکە و لە ئادەمیزاد\n"
    }
  ]
}