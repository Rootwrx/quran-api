{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 601,
      "verse_key": "104:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيۡلٞ لِّكُلِّ هُمَزَةٖ لُّمَزَةٍ",
      "translation": "سزاو لەناوچوون بۆ ھەموو نەنگ گرێکی بەدگۆ (عەیب گرێکی تانە دەر)\n",
      "footnotes": null
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 601,
      "verse_key": "104:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِي جَمَعَ مَالٗا وَعَدَّدَهُۥ",
      "translation": "ئەوەی کە ھەرماڵ و سامان کۆ دەکاتەوە ودەیژمێرێ\n",
      "footnotes": null
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 601,
      "verse_key": "104:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَحۡسَبُ أَنَّ مَالَهُۥٓ أَخۡلَدَهُۥ",
      "translation": "وا دەزانێ کە ماڵەکەی ھەر دەیھێڵێتەوە\n",
      "footnotes": null
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 601,
      "verse_key": "104:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّاۖ لَيُنۢبَذَنَّ فِي ٱلۡحُطَمَةِ",
      "translation": "نەخێر وانیە، بێگومان فڕێ دەدرێتە ناو ئاگرێکی تێکشکێنەرەوە\n",
      "footnotes": null
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 601,
      "verse_key": "104:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَآ أَدۡرَىٰكَ مَا ٱلۡحُطَمَةُ",
      "translation": "تۆ چووزانیت ئەو ئاگرە چیە\n",
      "footnotes": null
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 601,
      "verse_key": "104:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "نَارُ ٱللَّهِ ٱلۡمُوقَدَةُ",
      "translation": "ئاگرێکی تاودراوی خواییە\n",
      "footnotes": null
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 601,
      "verse_key": "104:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّتِي تَطَّلِعُ عَلَى ٱلۡأَفۡـِٔدَةِ",
      "translation": "کە دێ بەسەر دڵاندا (ھوروژم دەباتە سەر دڵان)\n",
      "footnotes": null
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 601,
      "verse_key": "104:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهَا عَلَيۡهِم مُّؤۡصَدَةٞ",
      "translation": "بێگومان ئەو ئاگرە داخراوە لەسەریان (و دەرچوونیان نییە)\n",
      "footnotes": null
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 601,
      "verse_key": "104:9",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِي عَمَدٖ مُّمَدَّدَةِۭ",
      "translation": "(زنجیر کراون) لەناو چەند کۆتێکی ڕاکشاودا (بەستراونەتەوە بەچەند ستونێکی بەرزو بڵندەوە)\n",
      "footnotes": null
    }
  ]
}