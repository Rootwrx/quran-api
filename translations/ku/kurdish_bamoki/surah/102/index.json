{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 600,
      "verse_key": "102:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلۡهَىٰكُمُ ٱلتَّكَاثُرُ",
      "translation": " ئێوەی بێ ئاگا کردووە شانازیتان بە زۆریی سەروەت و سامانەوە\n",
      "footnotes": null
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 600,
      "verse_key": "102:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "حَتَّىٰ زُرۡتُمُ ٱلۡمَقَابِرَ",
      "translation": "تا دەمرن ودەچن بۆ گۆڕستان\n",
      "footnotes": null
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 600,
      "verse_key": "102:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّا سَوۡفَ تَعۡلَمُونَ",
      "translation": "وامەکەن ! لەمەولا دەزانن\n",
      "footnotes": null
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 600,
      "verse_key": "102:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ كَلَّا سَوۡفَ تَعۡلَمُونَ",
      "translation": "دووبارە وامەکەن لەمەولا دەزانن\n",
      "footnotes": null
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 600,
      "verse_key": "102:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّا لَوۡ تَعۡلَمُونَ عِلۡمَ ٱلۡيَقِينِ",
      "translation": "وانیە (وەکو خۆتان دەیزانن) ئەگەر(ئەنجامی خۆتان) بزانیایە بەدڵنیاییەوە (وا بێ ئاگا نەئەبوون)\n",
      "footnotes": null
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 600,
      "verse_key": "102:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَتَرَوُنَّ ٱلۡجَحِيمَ",
      "translation": "سوێند بەخوا بێگومان دۆزەخ دەبینن\n",
      "footnotes": null
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 600,
      "verse_key": "102:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ لَتَرَوُنَّهَا عَيۡنَ ٱلۡيَقِينِ",
      "translation": "دووبارە سوێند بەخوا دڵنیا بن بە چاوی ڕاستەقینە دەی بینن\n",
      "footnotes": null
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 600,
      "verse_key": "102:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ لَتُسۡـَٔلُنَّ يَوۡمَئِذٍ عَنِ ٱلنَّعِيمِ",
      "translation": "لە پاشان سوێند بەخوا لەو ڕۆژەدا (لەسەر ھەموو ناز ونیعمەتێک کە خوا پێی دابوون لەدونیادا) لێپرسینەوەتان لەگەڵدا دەکرێت\n",
      "footnotes": null
    }
  ]
}