{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 597,
      "verse_key": "96:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱقۡرَأۡ بِٱسۡمِ رَبِّكَ ٱلَّذِي خَلَقَ",
      "translation": "(ئەی موحەممەد ﷺ) بخوێنە بە ناوی پەروەردگارتەوە کە ھەموو شتێکی دروست کردووە\n",
      "footnotes": null
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 597,
      "verse_key": "96:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "خَلَقَ ٱلۡإِنسَٰنَ مِنۡ عَلَقٍ",
      "translation": " مرۆڤی دروست کردوە لە خوێنێکی بەستوو\n",
      "footnotes": null
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 597,
      "verse_key": "96:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱقۡرَأۡ وَرَبُّكَ ٱلۡأَكۡرَمُ",
      "translation": "بخوێنە، پەروەردگاری تۆ(لە ھەموو کەس) بەخشندە ترە\n",
      "footnotes": null
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 597,
      "verse_key": "96:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِي عَلَّمَ بِٱلۡقَلَمِ",
      "translation": "ئەوەی کە(مرۆڤی) فێر(ی نووسین) کردووە بەقەڵەم\n",
      "footnotes": null
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 597,
      "verse_key": "96:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلَّمَ ٱلۡإِنسَٰنَ مَا لَمۡ يَعۡلَمۡ",
      "translation": "مرۆڤی شارەزا کردووە بەو شتانەی کە نەی دەزانی\n",
      "footnotes": null
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 597,
      "verse_key": "96:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّآ إِنَّ ٱلۡإِنسَٰنَ لَيَطۡغَىٰٓ",
      "translation": "نا (وانیە ) بەڕاستی مرۆڤ یاخی دەبێت\n",
      "footnotes": null
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 597,
      "verse_key": "96:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَن رَّءَاهُ ٱسۡتَغۡنَىٰٓ",
      "translation": "خۆی ببینێت كە دەوڵەمەند بووە\n",
      "footnotes": null
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 597,
      "verse_key": "96:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ إِلَىٰ رَبِّكَ ٱلرُّجۡعَىٰٓ",
      "translation": "بێگومان ھەر بۆ لای پەروەردگاری تۆیە گەڕانەوە\n",
      "footnotes": null
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 597,
      "verse_key": "96:9",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَرَءَيۡتَ ٱلَّذِي يَنۡهَىٰ",
      "translation": "ئایا بینیت ئەو کەسەی کە جڵەوگیریی ئەکات\n",
      "footnotes": null
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 597,
      "verse_key": "96:10",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَبۡدًا إِذَا صَلَّىٰٓ",
      "translation": "لە بەندەیەک کەنوێژ بکات ؟\n",
      "footnotes": null
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 597,
      "verse_key": "96:11",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَرَءَيۡتَ إِن كَانَ عَلَى ٱلۡهُدَىٰٓ",
      "translation": "ئایا پێم بڵێ ئەگەر(ئەو کەسە) لەسەر ڕێی ڕاست بووایە\n",
      "footnotes": null
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 597,
      "verse_key": "96:12",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَوۡ أَمَرَ بِٱلتَّقۡوَىٰٓ",
      "translation": " یان فەرمانی بدایە بەپارێزکاریی ولەخوا ترسان (باشتر نەبوو)\n",
      "footnotes": null
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 597,
      "verse_key": "96:13",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَرَءَيۡتَ إِن كَذَّبَ وَتَوَلَّىٰٓ",
      "translation": "ئایا بینیت ئەگەر بڕوای(بە ئاینی ڕاست) نەبێت وپشتی لێ ھەڵکات (ڕزگاری دەبێ)\n",
      "footnotes": null
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 597,
      "verse_key": "96:14",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَمۡ يَعۡلَم بِأَنَّ ٱللَّهَ يَرَىٰ",
      "translation": "ئایا نەی زانیوە کەخوا دەی بینێت\n",
      "footnotes": null
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 597,
      "verse_key": "96:15",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّا لَئِن لَّمۡ يَنتَهِ لَنَسۡفَعَۢا بِٱلنَّاصِيَةِ",
      "translation": "وا نیە (کە بیری لێ دەکاتەوە) بە ڕاستی ئەگەر واز نەھێنێت ڕای دەکێشین (بە قژی) پێشەسەری\n",
      "footnotes": null
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 597,
      "verse_key": "96:16",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "نَاصِيَةٖ كَٰذِبَةٍ خَاطِئَةٖ",
      "translation": "پێشەسەرێکی درۆزنی بە ھەڵەدا چوو\n",
      "footnotes": null
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 597,
      "verse_key": "96:17",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلۡيَدۡعُ نَادِيَهُۥ",
      "translation": "ئەمجا با ئەو بانگی ھاوڕێکانی خۆی بکات\n",
      "footnotes": null
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 597,
      "verse_key": "96:18",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "سَنَدۡعُ ٱلزَّبَانِيَةَ",
      "translation": " ئێمەش بانگی فریشتەی زەبر بە دەست دەکەین\n",
      "footnotes": null
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 597,
      "verse_key": "96:19",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": "obligatory",
      "sajdah_number": 14,
      "text": "كَلَّا لَا تُطِعۡهُ وَٱسۡجُدۡۤ وَٱقۡتَرِب۩",
      "translation": "نەخێر، بە گوێی(ئەو کەسانە) مەکە، وە کوڕنوش بەرەو (لەخوا) نزیک بکەوەرەوە\n",
      "footnotes": null
    }
  ]
}