{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 582,
      "verse_key": "78:1",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَمَّ يَتَسَآءَلُونَ",
      "translation": "ئەوە لەچی پرسیار لەیەکتری دەکەن (ئەو بێ بڕوایانە)\n",
      "footnotes": null
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 582,
      "verse_key": "78:2",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَنِ ٱلنَّبَإِ ٱلۡعَظِيمِ",
      "translation": "لەو دەنگ وباسە گەورەیە پرسیار دەکەن؟!\n",
      "footnotes": null
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 582,
      "verse_key": "78:3",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِي هُمۡ فِيهِ مُخۡتَلِفُونَ",
      "translation": "کە ئەوان تێیدا جیاوازن\n",
      "footnotes": null
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 582,
      "verse_key": "78:4",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّا سَيَعۡلَمُونَ",
      "translation": "نا، با پەشیمان بنەوە، لەمەولا تێدەگەن\n",
      "footnotes": null
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 582,
      "verse_key": "78:5",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ كَلَّا سَيَعۡلَمُونَ",
      "translation": "دووبارە(دەڵیم) نا، لەمەولا تێدەگەن\n",
      "footnotes": null
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 582,
      "verse_key": "78:6",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَمۡ نَجۡعَلِ ٱلۡأَرۡضَ مِهَٰدٗا",
      "translation": "ئاخۆ زەویمان بۆ ڕانەخستوون ؟\n",
      "footnotes": null
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 582,
      "verse_key": "78:7",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡجِبَالَ أَوۡتَادٗا",
      "translation": "وە کێوەکانمان نەگێڕاوە بە مێخ ؟!\n",
      "footnotes": null
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 582,
      "verse_key": "78:8",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَخَلَقۡنَٰكُمۡ أَزۡوَٰجٗا",
      "translation": "وە ئێوەمان دروست نەکردوە بەجووت\n",
      "footnotes": null
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 582,
      "verse_key": "78:9",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَجَعَلۡنَا نَوۡمَكُمۡ سُبَاتٗا",
      "translation": "وە نوستنمان نەکردووە بەھۆی حەسانەوەتان\n",
      "footnotes": null
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 582,
      "verse_key": "78:10",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَجَعَلۡنَا ٱلَّيۡلَ لِبَاسٗا",
      "translation": "وە شەومان نەگێڕاوە بەکاڵا (داپۆشەر)\n",
      "footnotes": null
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 582,
      "verse_key": "78:11",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَجَعَلۡنَا ٱلنَّهَارَ مَعَاشٗا",
      "translation": "وە ڕۆژمان نەگێڕاوە بەکاتی ژیان (و ھەڵسوڕان وپەیداکردنی ژیوار)\n",
      "footnotes": null
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 582,
      "verse_key": "78:12",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَبَنَيۡنَا فَوۡقَكُمۡ سَبۡعٗا شِدَادٗا",
      "translation": "وە حەوت ئاسمانی بەھێزمان دروست نەکردوە لە سەرتانەوە ؟\n",
      "footnotes": null
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 582,
      "verse_key": "78:13",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَجَعَلۡنَا سِرَاجٗا وَهَّاجٗا",
      "translation": "وە دروستمان نەکردوە چرایەکی درەوشاوە (کە ڕۆژە)\n",
      "footnotes": null
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 582,
      "verse_key": "78:14",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنزَلۡنَا مِنَ ٱلۡمُعۡصِرَٰتِ مَآءٗ ثَجَّاجٗا",
      "translation": "وە ئاوی لێزمە وبەخوڕمان نەناردۆتە خوارەوە لە ھەورە گوشراوەکانەوە\n",
      "footnotes": null
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 582,
      "verse_key": "78:15",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لِّنُخۡرِجَ بِهِۦ حَبّٗا وَنَبَاتٗا",
      "translation": "تا پێی بڕوێنین دانەوێڵە وگژو گیاو\n",
      "footnotes": null
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 582,
      "verse_key": "78:16",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَجَنَّٰتٍ أَلۡفَافًا",
      "translation": "باخاتی پڕ وچڕ\n",
      "footnotes": null
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 582,
      "verse_key": "78:17",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ يَوۡمَ ٱلۡفَصۡلِ كَانَ مِيقَٰتٗا",
      "translation": "بەڕاستی ڕۆژی جیاکردنەوەی (چاك وخراپ) کاتێکی بۆ دیاری کراوە\n",
      "footnotes": null
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 582,
      "verse_key": "78:18",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَوۡمَ يُنفَخُ فِي ٱلصُّورِ فَتَأۡتُونَ أَفۡوَاجٗا",
      "translation": "ئەو ڕۆژەی کە فوو دەکرێت بەکەڕەنادا ئەمجا پۆل پۆل دێن\n",
      "footnotes": null
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 582,
      "verse_key": "78:19",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَفُتِحَتِ ٱلسَّمَآءُ فَكَانَتۡ أَبۡوَٰبٗا",
      "translation": "وە ئاسمان (لە ھەموو لایەکەوە) کرایەوە وبوو بەدەرگا\n",
      "footnotes": null
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 582,
      "verse_key": "78:20",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَسُيِّرَتِ ٱلۡجِبَالُ فَكَانَتۡ سَرَابًا",
      "translation": "وە کێوەکان خرانە ڕێ وبوون بە (تراویلکە=سەراب)\n",
      "footnotes": null
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 582,
      "verse_key": "78:21",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ جَهَنَّمَ كَانَتۡ مِرۡصَادٗا",
      "translation": "بەڕاستی دۆزەخ پەرێزگای بێ بڕوایانە (ودەمێکە چاوەڕوانیانە)\n",
      "footnotes": null
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 582,
      "verse_key": "78:22",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لِّلطَّٰغِينَ مَـَٔابٗا",
      "translation": "وەجێ گەڕانەوەی یاخییانە\n",
      "footnotes": null
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 582,
      "verse_key": "78:23",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَّٰبِثِينَ فِيهَآ أَحۡقَابٗا",
      "translation": "تێیدا دەمێننەوە ھەزاران ساڵ (ماوەیەکی زۆر)\n",
      "footnotes": null
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 582,
      "verse_key": "78:24",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَّا يَذُوقُونَ فِيهَا بَرۡدٗا وَلَا شَرَابًا",
      "translation": "تێیدا نا چەژن نەساردی وفێنکی ونەخواردنەوە\n",
      "footnotes": null
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 582,
      "verse_key": "78:25",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَّا حَمِيمٗا وَغَسَّاقٗا",
      "translation": "ئاوی لەکوڵ وکێم وزوخاو نەبێت\n",
      "footnotes": null
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 582,
      "verse_key": "78:26",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "جَزَآءٗ وِفَاقًا",
      "translation": "پاداش دەدرێنەوە بەپێ ی کردەوەیان\n",
      "footnotes": null
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 582,
      "verse_key": "78:27",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُمۡ كَانُواْ لَا يَرۡجُونَ حِسَابٗا",
      "translation": "چونکە بێگومان ئەوان بەتەمای لێپرسینەوە نەبوون\n",
      "footnotes": null
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 582,
      "verse_key": "78:28",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَكَذَّبُواْ بِـَٔايَٰتِنَا كِذَّابٗا",
      "translation": "وە ئایەتەکانی ئێمەیان بەدرۆ دەزانی چۆن بەدرۆ زانینێك\n",
      "footnotes": null
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 582,
      "verse_key": "78:29",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَكُلَّ شَيۡءٍ أَحۡصَيۡنَٰهُ كِتَٰبٗا",
      "translation": "لە کاتێکدا ھەموو شتێکمان ژماردووە (لە سەریان) بە نوسیین\n",
      "footnotes": null
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 582,
      "verse_key": "78:30",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَذُوقُواْ فَلَن نَّزِيدَكُمۡ إِلَّا عَذَابًا",
      "translation": "ئەمجا بچەژن(سزای خۆتان) ھیچتان بۆ زیاد ناکەین سزا نەبێت\n",
      "footnotes": null
    },
    {
      "id": 31,
      "verse_number": 31,
      "page_number": 583,
      "verse_key": "78:31",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ لِلۡمُتَّقِينَ مَفَازًا",
      "translation": "بێگومان جێگای پیرۆز وبەھرەمەندی ھەیە بۆ پارێزکاران ولەخواترسان\n",
      "footnotes": null
    },
    {
      "id": 32,
      "verse_number": 32,
      "page_number": 583,
      "verse_key": "78:32",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "حَدَآئِقَ وَأَعۡنَٰبٗا",
      "translation": "باخات وھەموو جۆرە ترێیەك\n",
      "footnotes": null
    },
    {
      "id": 33,
      "verse_number": 33,
      "page_number": 583,
      "verse_key": "78:33",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَكَوَاعِبَ أَتۡرَابٗا",
      "translation": "وە کچانی تازە مەمك کردووی ھاوتەمەن\n",
      "footnotes": null
    },
    {
      "id": 34,
      "verse_number": 34,
      "page_number": 583,
      "verse_key": "78:34",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَكَأۡسٗا دِهَاقٗا",
      "translation": "وە پەرداخی پڕ (لە شەراب)\n",
      "footnotes": null
    },
    {
      "id": 35,
      "verse_number": 35,
      "page_number": 583,
      "verse_key": "78:35",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَّا يَسۡمَعُونَ فِيهَا لَغۡوٗا وَلَا كِذَّٰبٗا",
      "translation": "قسەی بێھوودە ودرۆی تێدا نابیستن\n",
      "footnotes": null
    },
    {
      "id": 36,
      "verse_number": 36,
      "page_number": 583,
      "verse_key": "78:36",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "جَزَآءٗ مِّن رَّبِّكَ عَطَآءً حِسَابٗا",
      "translation": "(پاداش دەدرێنەوە بە) پاداشتێکی تەواو لەلایەن پەروەردگاری تۆوە\n",
      "footnotes": null
    },
    {
      "id": 37,
      "verse_number": 37,
      "page_number": 583,
      "verse_key": "78:37",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "رَّبِّ ٱلسَّمَٰوَٰتِ وَٱلۡأَرۡضِ وَمَا بَيۡنَهُمَا ٱلرَّحۡمَٰنِۖ لَا يَمۡلِكُونَ مِنۡهُ خِطَابٗا",
      "translation": "پەروەردگاری ئاسمانەکان وزەوی ونێوانی ئەوانە، خوایەکی بەخشندە ومیھرەبانە وکەس توانای تکا کردن و(قسە کردنی) نیە\n",
      "footnotes": null
    },
    {
      "id": 38,
      "verse_number": 38,
      "page_number": 583,
      "verse_key": "78:38",
      "juz_number": 30,
      "hizb_number": 59,
