{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 593,
      "verse_key": "89:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡفَجۡرِ",
      "translation": "سوێند بە بەرەبەیان\n",
      "footnotes": null
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 593,
      "verse_key": "89:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَيَالٍ عَشۡرٖ",
      "translation": "وە بە دە شەوی سەرەتای مانگی(ذی الحجە)\n",
      "footnotes": null
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 593,
      "verse_key": "89:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلشَّفۡعِ وَٱلۡوَتۡرِ",
      "translation": "وە بەتاك و بەجووت\n",
      "footnotes": null
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 593,
      "verse_key": "89:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّيۡلِ إِذَا يَسۡرِ",
      "translation": "وە بەشەوگار کاتێك کە تێدەپەڕێت\n",
      "footnotes": null
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 593,
      "verse_key": "89:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "هَلۡ فِي ذَٰلِكَ قَسَمٞ لِّذِي حِجۡرٍ",
      "translation": "ئایا ئەمانەی کە سوێندیان پێ خورا شایستەی سوێند پێ خواردن نین لای کەسانی تێگەیشتوو\n",
      "footnotes": null
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 593,
      "verse_key": "89:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَمۡ تَرَ كَيۡفَ فَعَلَ رَبُّكَ بِعَادٍ",
      "translation": "ئایا نابینیت کە پەروەردگاری تۆ چی کرد بە گەلی عاد\n",
      "footnotes": null
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 593,
      "verse_key": "89:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِرَمَ ذَاتِ ٱلۡعِمَادِ",
      "translation": "وە بەھۆزی ئیرەم ؟ خاوەنی پایە (و دەسەڵات)\n",
      "footnotes": null
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 593,
      "verse_key": "89:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّتِي لَمۡ يُخۡلَقۡ مِثۡلُهَا فِي ٱلۡبِلَٰدِ",
      "translation": "کە وێنەیان دروست نەکرابوو لە ھەموو ووڵاتدا\n",
      "footnotes": null
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 593,
      "verse_key": "89:9",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَثَمُودَ ٱلَّذِينَ جَابُواْ ٱلصَّخۡرَ بِٱلۡوَادِ",
      "translation": "وە(چی کرد) بە گەلی سەموود ئەوانەی کە(شاخی) بەردیان دەتاشیی لە شیوو دۆڵەکاندا\n",
      "footnotes": null
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 593,
      "verse_key": "89:10",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَفِرۡعَوۡنَ ذِي ٱلۡأَوۡتَادِ",
      "translation": "وە فیرعەونی خاوەن سنگی زۆر(وەك ئەھرامەکان، کە وەك سنگ چوون بە زەویدا)\n",
      "footnotes": null
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 593,
      "verse_key": "89:11",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِينَ طَغَوۡاْ فِي ٱلۡبِلَٰدِ",
      "translation": "ئەوانەی کە یاخی بوون لە وڵاتدا\n",
      "footnotes": null
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 593,
      "verse_key": "89:12",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَكۡثَرُواْ فِيهَا ٱلۡفَسَادَ",
      "translation": "وە زۆریان خراپە تێداکرد\n",
      "footnotes": null
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 593,
      "verse_key": "89:13",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَصَبَّ عَلَيۡهِمۡ رَبُّكَ سَوۡطَ عَذَابٍ",
      "translation": "ئەمجا پەروەردگاری تۆ دایگرتن بە قامچی سزا\n",
      "footnotes": null
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 593,
      "verse_key": "89:14",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ رَبَّكَ لَبِٱلۡمِرۡصَادِ",
      "translation": "بەڕاستی پەروەردگاری تۆ لە کەمینی ستەمكاراندایە\n",
      "footnotes": null
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 593,
      "verse_key": "89:15",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَمَّا ٱلۡإِنسَٰنُ إِذَا مَا ٱبۡتَلَىٰهُ رَبُّهُۥ فَأَكۡرَمَهُۥ وَنَعَّمَهُۥ فَيَقُولُ رَبِّيٓ أَكۡرَمَنِ",
      "translation": "جا کاتێك کە خوا ئادەمی تاقی کردەوە وڕێزی گرت ونازو نیعمەتی بەسەردا ڕشت، ئەوە دەڵێ پەروەردگارم ڕێزی گرتووم\n",
      "footnotes": null
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 593,
      "verse_key": "89:16",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَمَّآ إِذَا مَا ٱبۡتَلَىٰهُ فَقَدَرَ عَلَيۡهِ رِزۡقَهُۥ فَيَقُولُ رَبِّيٓ أَهَٰنَنِ",
      "translation": "وە ئەگەر تاقیكردەوە وڕۆزی کەم پێدا، ئەوسا دەڵێ پەروەردگارم ڕیسوای کردووم\n",
      "footnotes": null
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 593,
      "verse_key": "89:17",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّاۖ بَل لَّا تُكۡرِمُونَ ٱلۡيَتِيمَ",
      "translation": "نا، وانیە (کە ئێوە بیری لێ دەکەنەوە) بەڵکو ئێوە ڕێزی ھەتیوان ناگرن\n",
      "footnotes": null
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 593,
      "verse_key": "89:18",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَا تَحَٰٓضُّونَ عَلَىٰ طَعَامِ ٱلۡمِسۡكِينِ",
      "translation": "وە ھانی یەکتری نادەن لەسەر خواردەمەنی دان(وبەخێو کردنی) ھەژاران\n",
      "footnotes": null
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 593,
      "verse_key": "89:19",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَتَأۡكُلُونَ ٱلتُّرَاثَ أَكۡلٗا لَّمّٗا",
      "translation": "(بەڵکو سەرەڕای ئەمەش بەشە) میراتی(بەشداران) ھەمووی(بە ھەڵپە) دەخۆن\n",
      "footnotes": null
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 593,
      "verse_key": "89:20",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَتُحِبُّونَ ٱلۡمَالَ حُبّٗا جَمّٗا",
      "translation": "وە ماڵ وسامانتان خۆش دەوێت بە خۆش ویستنێکی زۆر\n",
      "footnotes": null
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 593,
      "verse_key": "89:21",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّآۖ إِذَا دُكَّتِ ٱلۡأَرۡضُ دَكّٗا دَكّٗا",
      "translation": "نابێ وا بکەن ! کاتێك کەزەوی بە توندی درا بەیەکدا وپارە پارە کرا\n",
      "footnotes": null
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 593,
      "verse_key": "89:22",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَجَآءَ رَبُّكَ وَٱلۡمَلَكُ صَفّٗا صَفّٗا",
      "translation": "(لەڕۆژی دواییدا) پەروەردگارت دێت ( بۆ لێپرسینەوەی خەڵکی، ھاتنێك شیاوی زاتی خۆی بێت، بڕوامان پێیەتی بەبێ ھیچ لێچواندن ولێکدانەوەیەك، وە چۆنیەتیەکەی نازانین، تەنھا خوا خۆی دەزانێت) وە فریشتەکانیش پۆل پۆل دێن\n",
      "footnotes": null
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 593,
      "verse_key": "89:23",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَجِاْيٓءَ يَوۡمَئِذِۭ بِجَهَنَّمَۚ يَوۡمَئِذٖ يَتَذَكَّرُ ٱلۡإِنسَٰنُ وَأَنَّىٰ لَهُ ٱلذِّكۡرَىٰ",
      "translation": "وە لەو ڕۆژەدا دۆزەخ دەھێنرێت و(نزیك دەكرێتەوە لە دۆزەخیەكان) لەو ڕۆژەدا ئینسان(کردەوەی خۆی) بیر دەکەوێتەوە، بەڵام ئەو بیرکەوتنەوەیە چ سوودێك بەو دەگەیەنێت؟\n",
      "footnotes": null
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 594,
      "verse_key": "89:24",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَقُولُ يَٰلَيۡتَنِي قَدَّمۡتُ لِحَيَاتِي",
      "translation": "(ئەمجا بە داخەوە) دەڵێ خۆزگە لە دونیادا کاری چاکەم پێش بخستایە بۆ ئەم ژیانەم\n",
      "footnotes": null
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 594,
      "verse_key": "89:25",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَيَوۡمَئِذٖ لَّا يُعَذِّبُ عَذَابَهُۥٓ أَحَدٞ",
      "translation": "ئەمجا لەو ڕۆژەدا کەس سزای کەس نادات وەکو سزای خوا\n",
      "footnotes": null
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 594,
      "verse_key": "89:26",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَا يُوثِقُ وَثَاقَهُۥٓ أَحَدٞ",
      "translation": "وە کەس کەلەپچەی کەس ناکات وەکو کەلەپچە کردنی خوا (بۆ بێ باوەڕان)\n",
      "footnotes": null
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 594,
      "verse_key": "89:27",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَٰٓأَيَّتُهَا ٱلنَّفۡسُ ٱلۡمُطۡمَئِنَّةُ",
      "translation": "(لەو ڕۆژەدا بە خوا پەرست دەووترێت ) ئەی نەفسی بە ئارام\n",
      "footnotes": null
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 594,
      "verse_key": "89:28",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱرۡجِعِيٓ إِلَىٰ رَبِّكِ رَاضِيَةٗ مَّرۡضِيَّةٗ",
      "translation": " بگەڕێرەوە بۆ لای پەروەردگاری خۆت بە ڕەزامەندی ولێ ڕازی بوون\n",
      "footnotes": null
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 594,
      "verse_key": "89:29",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَٱدۡخُلِي فِي عِبَٰدِي",
      "translation": "ئەمجا بچۆرە ناو بەندە چاکەکانی من\n",
      "footnotes": null
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 594,
      "verse_key": "89:30",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱدۡخُلِي جَنَّتِي",
      "translation": "و بچۆرە ناو بەھەشتی منەوە\n",
      "footnotes": null
    }
  ]
}