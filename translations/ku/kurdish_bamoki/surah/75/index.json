{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 577,
      "verse_key": "75:1",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَآ أُقۡسِمُ بِيَوۡمِ ٱلۡقِيَٰمَةِ",
      "translation": "سوێند دەخۆم بە ڕۆژی قیامەت\n",
      "footnotes": null
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 577,
      "verse_key": "75:2",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَآ أُقۡسِمُ بِٱلنَّفۡسِ ٱللَّوَّامَةِ",
      "translation": "وە سوێند دەخۆم بە نەفسی لۆمەکەر (لەسەر خراپە)\n",
      "footnotes": null
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 577,
      "verse_key": "75:3",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَيَحۡسَبُ ٱلۡإِنسَٰنُ أَلَّن نَّجۡمَعَ عِظَامَهُۥ",
      "translation": "ئایا مرۆڤ وا دەزانێت کە ئێسقانە (ڕزیوەکانی) کۆ ناکەینەوە\n",
      "footnotes": null
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 577,
      "verse_key": "75:4",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بَلَىٰ قَٰدِرِينَ عَلَىٰٓ أَن نُّسَوِّيَ بَنَانَهُۥ",
      "translation": "بەڵێ: بە تواناین لە سەر ڕێك خستنەوەی (نەقشی) سەر پەنجەکانی\n",
      "footnotes": null
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 577,
      "verse_key": "75:5",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بَلۡ يُرِيدُ ٱلۡإِنسَٰنُ لِيَفۡجُرَ أَمَامَهُۥ",
      "translation": "بەڵکو ئینسان دەیەوێت تا لە ژیان دایە ھەر تاوان وخراپە بکات\n",
      "footnotes": null
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 577,
      "verse_key": "75:6",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَسۡـَٔلُ أَيَّانَ يَوۡمُ ٱلۡقِيَٰمَةِ",
      "translation": "دەپرسێ: کەی ڕۆژی دوایی دێت ؟\n",
      "footnotes": null
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 577,
      "verse_key": "75:7",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِذَا بَرِقَ ٱلۡبَصَرُ",
      "translation": "کاتێ: کە چاو ئەبڵەق بوو ؟\n",
      "footnotes": null
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 577,
      "verse_key": "75:8",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَخَسَفَ ٱلۡقَمَرُ",
      "translation": "وە مانگ گیرا وتاریك بوو\n",
      "footnotes": null
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 577,
      "verse_key": "75:9",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَجُمِعَ ٱلشَّمۡسُ وَٱلۡقَمَرُ",
      "translation": "وە ڕۆژ ومانگ کۆ کرانەوە\n",
      "footnotes": null
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 577,
      "verse_key": "75:10",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَقُولُ ٱلۡإِنسَٰنُ يَوۡمَئِذٍ أَيۡنَ ٱلۡمَفَرُّ",
      "translation": "ئەو ڕۆژە مرۆڤ دەڵێت: لە کوێوە ھەڵبێین لە (سزای خوا)\n",
      "footnotes": null
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 577,
      "verse_key": "75:11",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّا لَا وَزَرَ",
      "translation": "نەخێر ھیچ پەناگەیەك نیە\n",
      "footnotes": null
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 577,
      "verse_key": "75:12",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَىٰ رَبِّكَ يَوۡمَئِذٍ ٱلۡمُسۡتَقَرُّ",
      "translation": "لەو ڕۆژەدا ئارامگا ھەر لای پەروەردگاری تۆیە\n",
      "footnotes": null
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 577,
      "verse_key": "75:13",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يُنَبَّؤُاْ ٱلۡإِنسَٰنُ يَوۡمَئِذِۭ بِمَا قَدَّمَ وَأَخَّرَ",
      "translation": "لەو ڕۆژەدا ئادەمیی ھەواڵی پێدەدرێت لەوەی کە پێشی خستووە ودوای خستووە (لە چاکە وخراپە)\n",
      "footnotes": null
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 577,
      "verse_key": "75:14",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بَلِ ٱلۡإِنسَٰنُ عَلَىٰ نَفۡسِهِۦ بَصِيرَةٞ",
      "translation": "بەڵکو ئادەمی ئاگادارە بەحاڵی خۆی\n",
      "footnotes": null
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 577,
      "verse_key": "75:15",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَوۡ أَلۡقَىٰ مَعَاذِيرَهُۥ",
      "translation": "با بڕوبیانووی زۆریش بھێنێتەوە\n",
      "footnotes": null
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 577,
      "verse_key": "75:16",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَا تُحَرِّكۡ بِهِۦ لِسَانَكَ لِتَعۡجَلَ بِهِۦٓ",
      "translation": "زمانت مەجوڵێنە (بەخوێندنەوەی قورئان لەکاتی دابەزینی وەحیدا) تا پەلەی تیدا بکەیت\n",
      "footnotes": null
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 577,
      "verse_key": "75:17",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ عَلَيۡنَا جَمۡعَهُۥ وَقُرۡءَانَهُۥ",
      "translation": "بە ڕاستی (وەك بەڵێنمان داوە) بێگومان لەسەر ئێمەیە کۆکردنەوەی لە سنگتدا وخوێندنەوەی\n",
      "footnotes": null
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 577,
      "verse_key": "75:18",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِذَا قَرَأۡنَٰهُ فَٱتَّبِعۡ قُرۡءَانَهُۥ",
      "translation": "ئەمجا کاتێ کە قورئانمان بۆ خوێندیتەوە (بە ھۆی جوبرەیلەوە) تۆش شوێن خوێندنەوەی ئەو بکەوە\n",
      "footnotes": null
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 577,
      "verse_key": "75:19",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ إِنَّ عَلَيۡنَا بَيَانَهُۥ",
      "translation": "لە پاشان دڵنیابە ڕوون کردنەوەشی لەسەر ئێمەیە\n",
      "footnotes": null
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 578,
      "verse_key": "75:20",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّا بَلۡ تُحِبُّونَ ٱلۡعَاجِلَةَ",
      "translation": "(زیندوو کردنەوە ھەر دەبێت) بەڵکو ئێوە ھەر دونیاتان خۆش دەوێت\n",
      "footnotes": null
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 578,
      "verse_key": "75:21",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَتَذَرُونَ ٱلۡأٓخِرَةَ",
      "translation": "گوێ بە ڕۆژی دوایی نادەن!\n",
      "footnotes": null
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 578,
      "verse_key": "75:22",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وُجُوهٞ يَوۡمَئِذٖ نَّاضِرَةٌ",
      "translation": "زۆر ڕوخسار لەو ڕۆژەدا گەشاوەن\n",
      "footnotes": null
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 578,
      "verse_key": "75:23",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَىٰ رَبِّهَا نَاظِرَةٞ",
      "translation": "تەماشای پەروەردگاریان دەکەن\n",
      "footnotes": null
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 578,
      "verse_key": "75:24",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَوُجُوهٞ يَوۡمَئِذِۭ بَاسِرَةٞ",
      "translation": "وە زۆر ڕوخساریش لەو ڕۆژەدا گرژ وتاڵن\n",
      "footnotes": null
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 578,
      "verse_key": "75:25",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تَظُنُّ أَن يُفۡعَلَ بِهَا فَاقِرَةٞ",
      "translation": "دەزانن کە سزای پشت شکێن دەدرێن\n",
      "footnotes": null
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 578,
      "verse_key": "75:26",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّآ إِذَا بَلَغَتِ ٱلتَّرَاقِيَ",
      "translation": "نەخێر (ئاوا ناچێتە سەر بۆتان، جا پەشیمان ببنەوە!) چونکە کاتێك کە گیان گەیشتە گەرو (لەکاتی کێشانیدا)\n",
      "footnotes": null
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 578,
      "verse_key": "75:27",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَقِيلَ مَنۡۜ رَاقٖ",
      "translation": "ئەوسا (ھەندێك لەوانەی کە بەدەوریدان) دەڵێن: کەسێك نیە چارەسەری بکات وفریای بکەوێت؟\n",
      "footnotes": null
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 578,
      "verse_key": "75:28",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَظَنَّ أَنَّهُ ٱلۡفِرَاقُ",
      "translation": "(ئەو کەسەی گیانی دەکێشرێت) دڵنیایە ئەوەی (تووشی بووە مردن و) جیابوونەوەیە\n",
      "footnotes": null
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 578,
      "verse_key": "75:29",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡتَفَّتِ ٱلسَّاقُ بِٱلسَّاقِ",
      "translation": "وە قاچ ولوولاقی بەیەکدا ئاڵا\n",
      "footnotes": null
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 578,
      "verse_key": "75:30",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَىٰ رَبِّكَ يَوۡمَئِذٍ ٱلۡمَسَاقُ",
      "translation": "ڕاپێچکردن لەو ڕۆژەدا بۆ لای پەروەردگارتە\n",
      "footnotes": null
    },
    {
      "id": 31,
      "verse_number": 31,
      "page_number": 578,
      "verse_key": "75:31",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلَا صَدَّقَ وَلَا صَلَّىٰ",
      "translation": "جا (کافر) نەبڕوای ھێناو نە نوێژی کرد\n",
      "footnotes": null
    },
    {
      "id": 32,
      "verse_number": 32,
      "page_number": 578,
      "verse_key": "75:32",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَٰكِن كَذَّبَ وَتَوَلَّىٰ",
      "translation": "بەڵکو ئاینی بەدرۆ دەزانی وپشتی تێ دەکرد\n",
      "footnotes": null
    },
    {
      "id": 33,
      "verse_number": 33,
      "page_number": 578,
      "verse_key": "75:33",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ ذَهَبَ إِلَىٰٓ أَهۡلِهِۦ يَتَمَطَّىٰٓ",
      "translation": "لە پاشان بە کەشخە وفیزەوە گەڕایەوە بۆ لای ماڵ ومنداڵی\n",
      "footnotes": null
    },
    {
      "id": 34,
      "verse_number": 34,
      "page_number": 578,
      "verse_key": "75:34",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَوۡلَىٰ لَكَ فَأَوۡلَىٰ",
      "translation": "تیاچوون بۆ تۆ (ئەی ئادەمی بێ بڕوا)، ئەمجا تیاچوون بۆ تۆ\n",
      "footnotes": null
    },
    {
      "id": 35,
      "verse_number": 35,
      "page_number": 578,
      "verse_key": "75:35",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ أَوۡلَىٰ لَكَ فَأَوۡلَىٰٓ",
      "translation": "دووبارە تیاچوون بۆ تۆ، ئەمجا تیاچوون بۆ تۆ\n",
      "footnotes": null
    },
    {
      "id": 36,
      "verse_number": 36,
      "page_number": 578,
      "verse_key": "75:36",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَيَحۡسَبُ ٱلۡإِنسَٰنُ أَن يُتۡرَكَ سُدًى",
      "translation": "ئایا مرۆڤ وا دادەنێت کە ھەروا بەرەڵا دەکرێت (وپشت گوێ دەخرێت)\n",
      "footnotes": null
    },
    {
      "id": 37,
      "verse_number": 37,
      "page_number": 578,
      "verse_key": "75:37",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَمۡ يَكُ نُطۡفَةٗ مِّن مَّنِيّٖ يُمۡنَىٰ",
      "translation": "ئایا ئەو دڵۆپە ئاوێك نەبوو لەتۆو کە دەڕژێنرایە (منداڵدانەوە)؟!\n",
      "footnotes": null
    },
    {
      "id": 38,
      "verse_number": 38,
      "page_number": 578,
      "verse_key": "75:38",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ كَانَ عَلَقَةٗ فَخَلَقَ فَسَوَّىٰ",
      "translation": "لە پاشان بوو بە پارچە خوێنێك، ئەمجا دروستیكرد وڕێکی خست\n",
      "footnotes": null
    },
    {
      "id": 39,
      "verse_number": 39,
      "page_number": 578,
      "verse_key": "75:39",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَجَعَلَ مِنۡهُ ٱلزَّوۡجَيۡنِ ٱلذَّكَرَ وَٱلۡأُنثَىٰٓ",
      "translation": "ئەمجا لەو تۆوە دوو جۆری دروستكرد نێر و مێ\n",
      "footnotes": null
    },
    {
      "id": 40,
      "verse_number": 40,
      "page_number": 578,
      "verse_key": "75:40",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَيۡسَ ذَٰلِكَ بِقَٰدِرٍ عَلَىٰٓ أَن يُحۡـِۧيَ ٱلۡمَوۡتَىٰ",
      "translation": "ئایا ئەو زاتە توانای نیە بەسەر ئەوەدا کە مردووان زیندوو بکاتەوە\n",
      "footnotes": null
    }
