{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 531,
      "verse_key": "55:1",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلرَّحۡمَٰنُ",
      "translation": "خوای بەخشندە ( ئەو خوایەیە )\n",
      "footnotes": null
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 531,
      "verse_key": "55:2",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلَّمَ ٱلۡقُرۡءَانَ",
      "translation": "کە قورئانی فێری(موحەممەد ﷺ وئادەمیزاد کرد، بەئاسانکردنی خوێندنەوە و لەبەرکردن وتێگەیشتنی مانا وواتاکانی)\n",
      "footnotes": null
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 531,
      "verse_key": "55:3",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "خَلَقَ ٱلۡإِنسَٰنَ",
      "translation": "ئادەمی دروست کرد\n",
      "footnotes": null
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 531,
      "verse_key": "55:4",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلَّمَهُ ٱلۡبَيَانَ",
      "translation": "وە فێری گفتوگۆی کرد\n",
      "footnotes": null
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 531,
      "verse_key": "55:5",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلشَّمۡسُ وَٱلۡقَمَرُ بِحُسۡبَانٖ",
      "translation": "ڕۆژ ومانگ بە حسابێکی زۆر وورد دێن ودەچن\n",
      "footnotes": null
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 531,
      "verse_key": "55:6",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلنَّجۡمُ وَٱلشَّجَرُ يَسۡجُدَانِ",
      "translation": "ئەستێرە وگژوگیا ودرەخت (بۆ خوا) سوژدە دەبەن\n",
      "footnotes": null
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 531,
      "verse_key": "55:7",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلسَّمَآءَ رَفَعَهَا وَوَضَعَ ٱلۡمِيزَانَ",
      "translation": "وە ئاسمانی بڵند وبەرزکردووەتەوە وە تەرازوی(عەدالەتی) داناوە\n",
      "footnotes": null
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 531,
      "verse_key": "55:8",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَّا تَطۡغَوۡاْ فِي ٱلۡمِيزَانِ",
      "translation": "(وە فەرمانی داوە) کە ھەرگیز زیادەڕۆی لەتەرازودا نەکەن\n",
      "footnotes": null
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 531,
      "verse_key": "55:9",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَقِيمُواْ ٱلۡوَزۡنَ بِٱلۡقِسۡطِ وَلَا تُخۡسِرُواْ ٱلۡمِيزَانَ",
      "translation": "وەکێشانەتان ڕێك بکێشن لە تەرازودا کەم وکووڕی مەکەن (سووك مەیکێشن)\n",
      "footnotes": null
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 531,
      "verse_key": "55:10",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡأَرۡضَ وَضَعَهَا لِلۡأَنَامِ",
      "translation": "وە زەوی بۆ خەڵکی ڕاخستووە\n",
      "footnotes": null
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 531,
      "verse_key": "55:11",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِيهَا فَٰكِهَةٞ وَٱلنَّخۡلُ ذَاتُ ٱلۡأَكۡمَامِ",
      "translation": "تێدایە میوەھات ودارخورمای خاوەن پۆرك\n",
      "footnotes": null
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 531,
      "verse_key": "55:12",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡحَبُّ ذُو ٱلۡعَصۡفِ وَٱلرَّيۡحَانُ",
      "translation": "وە دانەوێڵەی خاوەن کاو پەل وپۆدار وڕەیحانە\n",
      "footnotes": null
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 531,
      "verse_key": "55:13",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "جا بەکام لەنیعمەتەکانی پەروەردگارتان باوەڕ ناکەن؟\n",
      "footnotes": null
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 531,
      "verse_key": "55:14",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "خَلَقَ ٱلۡإِنسَٰنَ مِن صَلۡصَٰلٖ كَٱلۡفَخَّارِ",
      "translation": "ئادەمیی دروست کرد لەقوڕێکی ووشکەوە بووی وەك سواڵەت\n",
      "footnotes": null
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 531,
      "verse_key": "55:15",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَخَلَقَ ٱلۡجَآنَّ مِن مَّارِجٖ مِّن نَّارٖ",
      "translation": "وە جنۆکەی دروست کرد لە بڵێسەی ئاگرێکی بێ دووکەڵ\n",
      "footnotes": null
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 531,
      "verse_key": "55:16",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "جا بەکام لەنیعمەتەکانی پەروەردگارتان باوەڕ ناکەن؟\n",
      "footnotes": null
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 532,
      "verse_key": "55:17",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "رَبُّ ٱلۡمَشۡرِقَيۡنِ وَرَبُّ ٱلۡمَغۡرِبَيۡنِ",
      "translation": "(خوا) پەروەردگاری ھەر دوو ڕۆژھەڵات وپەروەردگاری ھەر دوو ڕۆژ ئاوایە\n",
      "footnotes": null
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 532,
      "verse_key": "55:18",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "جا بەکام لەنیعمەتەکانی پەروەردگارتان باوەڕ ناکەن؟\n",
      "footnotes": null
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 532,
      "verse_key": "55:19",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَرَجَ ٱلۡبَحۡرَيۡنِ يَلۡتَقِيَانِ",
      "translation": " دوو دەریای بەرەڵا کردووە کەدەگەن بەیەك\n",
      "footnotes": null
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 532,
      "verse_key": "55:20",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بَيۡنَهُمَا بَرۡزَخٞ لَّا يَبۡغِيَانِ",
      "translation": "لەنێوانیاندا بەربەستێك ھەیە تێکەڵی یەکتر نابن\n",
      "footnotes": null
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 532,
      "verse_key": "55:21",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "جا بەکام لەنیعمەتەکانی پەروەردگارتان باوەڕ ناکەن؟\n",
      "footnotes": null
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 532,
      "verse_key": "55:22",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَخۡرُجُ مِنۡهُمَا ٱللُّؤۡلُؤُ وَٱلۡمَرۡجَانُ",
      "translation": "لەو دوودەریایە مرواری ومەرجان دەردەچن (دروست دەبن)\n",
      "footnotes": null
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 532,
      "verse_key": "55:23",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "جا بەکام لەنیعمەتەکانی پەروەردگارتان باوەڕ ناکەن؟\n",
      "footnotes": null
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 532,
      "verse_key": "55:24",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَهُ ٱلۡجَوَارِ ٱلۡمُنشَـَٔاتُ فِي ٱلۡبَحۡرِ كَٱلۡأَعۡلَٰمِ",
      "translation": "وە بۆ خوایە ئەو کەشتیە گەورە دروستکراوانەی کە وەك شاخن لەدەریادا دەگەڕێن\n",
      "footnotes": null
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 532,
      "verse_key": "55:25",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "جا بەکام لەنیعمەتەکانی پەروەردگارتان باوەڕ ناکەن؟\n",
      "footnotes": null
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 532,
      "verse_key": "55:26",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كُلُّ مَنۡ عَلَيۡهَا فَانٖ",
      "translation": "ھەرچی دروستکراو لەسەر زەویدا ھەیە نامێنت وتیادەچێ\n",
      "footnotes": null
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 532,
      "verse_key": "55:27",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيَبۡقَىٰ وَجۡهُ رَبِّكَ ذُو ٱلۡجَلَٰلِ وَٱلۡإِكۡرَامِ",
      "translation": "وە ھەر ڕووی پیرۆزی پەروەردگارت دەمێنێت (لەم ئایەتەدا جێگیرکردنی سیفەتی [وجە = ڕوو] بۆ خوای گەورە ھاتووە، بەڵام بەشێوەیەك کە شایستەی گەورەیی وتەواوی زاتی خوا بێت، بەبێ ھیچ لێچواندن ولێکدانەوەیەك، وە بڕوامان پێیەتی وچۆنیەتیەکەی نازانین، تەنھا خوا دەیزانێت) کەخاوەن شکۆی ڕێزدارە\n",
      "footnotes": null
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 532,
      "verse_key": "55:28",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "جا بەکام لەنیعمەتەکانی پەروەردگارتان باوەڕ ناکەن؟\n",
      "footnotes": null
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 532,
      "verse_key": "55:29",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَسۡـَٔلُهُۥ مَن فِي ٱلسَّمَٰوَٰتِ وَٱلۡأَرۡضِۚ كُلَّ يَوۡمٍ هُوَ فِي شَأۡنٖ",
      "translation": "ئەوەی لەئاسمانەکان وزەوی دایە (ھەموو) داوای (پێویستیان) لەخوا دەکەن، ھەموو ڕۆژێ خوا لە (بەئەنجام گەیاندنی) کارێکی (گشتی بوونەوەر) دایە (یەك دەمرێنێت، یەك لەدایك دەبێت، یەکێك سەردەخات ویەکێکی تر دەشکێنێت، ھتد)\n",
      "footnotes": null
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 532,
      "verse_key": "55:30",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "جا بەکام لەنیعمەتەکانی پەروەردگارتان باوەڕ ناکەن؟\n",
      "footnotes": null
    },
    {
      "id": 31,
      "verse_number": 31,
      "page_number": 532,
      "verse_key": "55:31",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "سَنَفۡرُغُ لَكُمۡ أَيُّهَ ٱلثَّقَلَانِ",
      "translation": "لەمەولا موحاسەبەتان دەکەین ئەی گرۆی ئادەمی وجنۆکە\n",
      "footnotes": null
    },
    {
      "id": 32,
      "verse_number": 32,
      "page_number": 532,
      "verse_key": "55:32",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "جا بەکام لەنیعمەتەکانی پەروەردگارتان باوەڕ ناکەن؟\n",
      "footnotes": null
    },
    {
      "id": 33,
      "verse_number": 33,
      "page_number": 532,
      "verse_key": "55:33",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَٰمَعۡشَرَ ٱلۡجِنِّ وَٱلۡإِنسِ إِنِ ٱسۡتَطَعۡتُمۡ أَن تَنفُذُواْ مِنۡ أَقۡطَارِ ٱلسَّمَٰوَٰتِ وَٱلۡأَرۡضِ فَٱنفُذُواْۚ لَا تَنفُذُونَ إِلَّا بِسُلۡطَٰنٖ",
      "translation": "ئەی کۆمەڵی جنۆکە وئادەمی ئەگەر توانیتان دەرچن لەبەشەکانی ئاسمانەکان وزەوی ئەوە بڕۆن دەرچن، ناشتوانن مەگەر بەدەسەڵاتێك نەبێت\n",
      "footnotes": null
    },
    {
      "id": 34,
      "verse_number": 34,
      "page_number": 532,
      "verse_key": "55:34",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "جا بەکام لەنیعمەتەکانی پەروەردگارتان باوەڕ ناکەن؟\n",
      "footnotes": null
    },
    {
      "id": 35,
      "verse_number": 35,
      "page_number": 532,
      "verse_key": "55:35",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يُرۡسَلُ عَلَيۡكُمَا شُوَاظٞ مِّن نَّارٖ وَنُحَاسٞ فَلَا تَنتَصِرَانِ",
      "translation": "بۆتان دەنێرێ بڵێسەی ئاگر ومسی تواوە (یادوو کەڵی ئاگر) ئەوسا یارمەتی یەکترتان پێ نادرێت\n",
      "footnotes": null
    },
    {
      "id": 36,
      "verse_number": 36,
      "page_number": 532,
      "verse_key": "55:36",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "جا بەکام لەنیعمەتەکانی پەروەردگارتان باوەڕ ناکەن؟\n",
      "footnotes": null
    },
    {
      "id": 37,
      "verse_number": 37,
      "page_number": 532,
      "verse_key": "55:37",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِذَا ٱنشَقَّتِ ٱلسَّمَآءُ فَكَانَتۡ وَرۡدَةٗ كَٱلدِّهَانِ",
      "translation": "ئەمجا کاتێك ئاسمان لەت لەت بوو وەك گوڵی سوور، یان ڕۆنی ھەڵقرچاوی لێھات\n",
      "footnotes": null
    },
    {
      "id": 38,
      "verse_number": 38,
      "page_number": 532,
      "verse_key": "55:38",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "جا بەکام لەنیعمەتەکانی پەروەردگارتان باوەڕ ناکەن؟\n",
      "footnotes": null
    },
    {
      "id": 39,
      "verse_number": 39,
      "page_number": 532,
      "verse_key": "55:39",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَيَوۡمَئِذٖ لَّا يُسۡـَٔلُ عَن ذَنۢبِهِۦٓ إِنسٞ وَلَا جَآنّٞ",
      "translation": "ئەمجا لەو ڕۆژەدا پرسیار ناکرێت لەتاوانی ھیچ ئادەمی وجنۆکەیەك (چونکە ھەموو کەس لەو ڕۆژەدا بە ڕوویدا دەناسرێتەوە کە تاوانبارە یان چاکەکار)\n",
      "footnotes": null
    },
    {
      "id": 40,
      "verse_number": 40,
      "page_number": 532,
      "verse_key": "55:40",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "جا بەکام لەنیعمەتەکانی پەروەردگارتان باوەڕ ناکەن؟\n",
      "footnotes": null
    },
    {
      "id": 41,
      "verse_number": 41,
      "page_number": 533,
      "verse_key": "55:41",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يُعۡرَفُ ٱلۡمُجۡرِمُونَ بِسِيمَٰهُمۡ فَيُؤۡخَذُ بِٱلنَّوَٰصِي وَٱلۡأَقۡدَامِ",
      "translation": "خراپەکاران بەڕوخساریاندا دەناسرێنەوە، ئەمجا ڕائەکێشرێن بەمووی پێشی سەریان وبەقاچیان (بۆ ناو دۆزەخ)\n",
      "footnotes": null
    },
    {
      "id": 42,
      "verse_number": 42,
      "page_number": 533,
      "verse_key": "55:42",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "جا بەکام لەنیعمەتەکانی پەروەردگارتان باوەڕ ناکەن؟\n",
      "footnotes": null
    },
    {
      "id": 43,
      "verse_number": 43,
      "page_number": 533,
      "verse_key": "55:43",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "هَٰذِهِۦ جَهَنَّمُ ٱلَّتِي يُكَذِّبُ بِهَا ٱلۡمُجۡرِمُونَ",
      "translation": "ئەمە ئەو دۆزەخەیە کەتاوانباران بەدرۆیان دەزانی\n",
      "footnotes": null
    },
    {
      "id": 44,
      "verse_number": 44,
      "page_number": 533,
      "verse_key": "55:44",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَطُوفُونَ بَيۡنَهَا وَبَيۡنَ حَمِيمٍ ءَانٖ",
      "translation": "ھاتوچۆ ئەکەن لەنێوان ئەو (دۆزەخە) وئاوی زۆر گەرمدا\n",
      "footnotes": null
    },
    {
      "id": 45,
      "verse_number": 45,
      "page_number": 533,
      "verse_key": "55:45",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "جا بەکام لەنیعمەتەکانی پەروەردگارتان باوەڕ ناکەن؟\n",
      "footnotes": null
    },
    {
      "id": 46,
      "verse_number": 46,
      "page_number": 533,
      "verse_key": "55:46",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلِمَنۡ خَافَ مَقَامَ رَبِّهِۦ جَنَّتَانِ",
      "translation": "وە بۆ ئەو کەسەی لەوەستانی بەردەم پەروەردگاری بترسێت ھەیە دوو باخی (خۆش)\n",
      "footnotes": null
    },
    {
      "id": 47,
      "verse_number": 47,
      "page_number": 533,
      "verse_key": "55:47",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "جا بەکام لەنیعمەتەکانی پەروەردگارتان باوەڕ ناکەن؟\n",
      "footnotes": null
    },
    {
      "id": 48,
      "verse_number": 48,
      "page_number": 533,
      "verse_key": "55:48",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ذَوَاتَآ أَفۡنَانٖ",
      "translation": "(دوو باخەکە) خاوەنی میوە ودرەختی زۆرن\n",
      "footnotes": null
    },
    {
      "id": 49,
      "verse_number": 49,
      "page_number": 533,
      "verse_key": "55:49",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "جا بەکام لەنیعمەتەکانی پەروەردگارتان باوەڕ ناکەن؟\n",
      "footnotes": null
    },
    {
      "id": 50,
      "verse_number": 50,
      "page_number": 533,
      "verse_key": "55:50",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِيهِمَا عَيۡنَانِ تَجۡرِيَانِ",
      "translation": "تێیاندا ھەیە دوو ڕووبار وکانیاووی ڕەوان\n",
      "footnotes": null
    },
    {
      "id": 51,
      "verse_number": 51,
      "page_number": 533,
      "verse_key": "55:51",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "جا بەکام لەنیعمەتەکانی پەروەردگارتان باوەڕ ناکەن؟\n",
      "footnotes": null
    },
    {
      "id": 52,
      "verse_number": 52,
      "page_number": 533,
      "verse_key": "55:52",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِيهِمَا مِن كُلِّ فَٰكِهَةٖ زَوۡجَانِ",
      "translation": "ھەیە لە ناویاندا لەھەر میوەیەك دوو جۆر\n",
      "footnotes": null
    },
    {
      "id": 53,
      "verse_number": 53,
      "page_number": 533,
      "verse_key": "55:53",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "جا بەکام لەنیعمەتەکانی پەروەردگارتان باوەڕ ناکەن؟\n",
      "footnotes": null
    },
    {
      "id": 54,
      "verse_number": 54,
      "page_number": 533,
      "verse_key": "55:54",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مُتَّكِـِٔينَ عَلَىٰ فُرُشِۭ بَطَآئِنُهَا مِنۡ إِسۡتَبۡرَقٖۚ وَجَنَى ٱلۡجَنَّتَيۡنِ دَانٖ",
      "translation": "پاڵیان داوەتەوە لەسەر جۆرە فەرشێك کە بەرەکانیان لەئاوریشمی ئەستورە وە میوە لێ کردنەوەی ئەو دوو باخە نزیکە دەستە\n",
      "footnotes": null
    },
    {
      "id": 55,
      "verse_number": 55,
      "page_number": 533,
      "verse_key": "55:55",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "جا بەکام لەنیعمەتەکانی پەروەردگارتان باوەڕ ناکەن؟\n",
      "footnotes": null
    },
    {
      "id": 56,
      "verse_number": 56,
      "page_number": 533,
      "verse_key": "55:56",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِيهِنَّ قَٰصِرَٰتُ ٱلطَّرۡفِ لَمۡ يَطۡمِثۡهُنَّ إِنسٞ قَبۡلَهُمۡ وَلَا جَآنّٞ",
      "translation": "لەو باخانەدا ھەن حۆریانێك کەتەنھا چاویان بڕیوەتە مێردەکانیان پێش ئەوان دەستی ئادەمی وجنۆکەیان لێ نەکەوتووە\n",
      "footnotes": null
    },
    {
      "id": 57,
      "verse_number": 57,
      "page_number": 533,
      "verse_key": "55:57",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "جا بەکام لەنیعمەتەکانی پەروەردگارتان باوەڕ ناکەن؟\n",
      "footnotes": null
    },
    {
      "id": 58,
      "verse_number": 58,
      "page_number": 533,
      "verse_key": "55:58",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَأَنَّهُنَّ ٱلۡيَاقُوتُ وَٱلۡمَرۡجَانُ",
      "translation": "ئەو (حۆریانە) وەك یاقووت ومەرجان وان\n",
      "footnotes": null
    },
    {
      "id": 59,
      "verse_number": 59,
      "page_number": 533,
      "verse_key": "55:59",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "جا بەکام لەنیعمەتەکانی پەروەردگارتان باوەڕ ناکەن؟\n",
      "footnotes": null
    },
    {
      "id": 60,
      "verse_number": 60,
      "page_number": 533,
      "verse_key": "55:60",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "هَلۡ جَزَآءُ ٱلۡإِحۡسَٰنِ إِلَّا ٱلۡإِحۡسَٰنُ",
      "translation": "ئایا پاداشتی چاکە ھەر چاکە نیە؟\n",
      "footnotes": null
    },
    {
      "id": 61,
      "verse_number": 61,
      "page_number": 533,
      "verse_key": "55:61",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "جا بەکام لەنیعمەتەکانی پەروەردگارتان باوەڕ ناکەن؟\n",
      "footnotes": null
    },
    {
      "id": 62,
      "verse_number": 62,
      "page_number": 533,
      "verse_key": "55:62",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمِن دُونِهِمَا جَنَّتَانِ",
      "translation": "وە بێجگە لەو دوو باخەی (کەباسکران) دوو باخی تر ھەن\n",
      "footnotes": null
    },
    {
      "id": 63,
      "verse_number": 63,
      "page_number": 533,
      "verse_key": "55:63",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "جا بەکام لەنیعمەتەکانی پەروەردگارتان باوەڕ ناکەن؟\n",
      "footnotes": null
    },
    {
      "id": 64,
      "verse_number": 64,
      "page_number": 533,
      "verse_key": "55:64",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مُدۡهَآمَّتَانِ",
      "translation": "دوو باخی ئەوەندە سەوزن (لەسەوزیدا ڕەش دەنوێنن)\n",
      "footnotes": null
    },
    {
      "id": 65,
      "verse_number": 65,
      "page_number": 533,
      "verse_key": "55:65",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
