{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 591,
      "verse_key": "86:1",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلسَّمَآءِ وَٱلطَّارِقِ",
      "translation": "سوێند بە ئاسمان وئەوەی کە لەشەوا دەردەکەوێت\n",
      "footnotes": null
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 591,
      "verse_key": "86:2",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَآ أَدۡرَىٰكَ مَا ٱلطَّارِقُ",
      "translation": "تۆ چوزانیت دەرکەوتووی شەو چیە ؟\n",
      "footnotes": null
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 591,
      "verse_key": "86:3",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلنَّجۡمُ ٱلثَّاقِبُ",
      "translation": "ئەستێرەی درەخشانی تاریکی کون کەرە\n",
      "footnotes": null
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 591,
      "verse_key": "86:4",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِن كُلُّ نَفۡسٖ لَّمَّا عَلَيۡهَا حَافِظٞ",
      "translation": "(سوێند بەمانە) کەس نیە کە چاودێرێکی بەسەرەوە نەبێت\n",
      "footnotes": null
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 591,
      "verse_key": "86:5",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلۡيَنظُرِ ٱلۡإِنسَٰنُ مِمَّ خُلِقَ",
      "translation": " دەی با مرۆڤ سەرنج بدات کە لەچی دروستكراوە\n",
      "footnotes": null
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 591,
      "verse_key": "86:6",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "خُلِقَ مِن مَّآءٖ دَافِقٖ",
      "translation": "دروستكراوە لە ئاوێکی فڕێدراو (ھەڵقوڵاو)\n",
      "footnotes": null
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 591,
      "verse_key": "86:7",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَخۡرُجُ مِنۢ بَيۡنِ ٱلصُّلۡبِ وَٱلتَّرَآئِبِ",
      "translation": "کە دەردەچێت لە ناوەندی پشتی (پیاو) ودانەی (پەراسووی) سنگی (ژنەوە)\n",
      "footnotes": null
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 591,
      "verse_key": "86:8",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُۥ عَلَىٰ رَجۡعِهِۦ لَقَادِرٞ",
      "translation": "بەڕاستی خوا بەدەسەڵاتە لەسەر زیندوو کردنەوەی\n",
      "footnotes": null
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 591,
      "verse_key": "86:9",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَوۡمَ تُبۡلَى ٱلسَّرَآئِرُ",
      "translation": "لە ڕۆژێکدا کە ھەرچی نھێنی ھەیە ئاشکرا دەکرێت\n",
      "footnotes": null
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 591,
      "verse_key": "86:10",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَمَا لَهُۥ مِن قُوَّةٖ وَلَا نَاصِرٖ",
      "translation": " ئەو نە ھێزێک شك دەبات ونە پشتیوانێك\n",
      "footnotes": null
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 591,
      "verse_key": "86:11",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلسَّمَآءِ ذَاتِ ٱلرَّجۡعِ",
      "translation": "سوێند بە ئاسمانی خاوەن باران\n",
      "footnotes": null
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 591,
      "verse_key": "86:12",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡأَرۡضِ ذَاتِ ٱلصَّدۡعِ",
      "translation": "وە بەزەوی خاوەن درز وقڵییش\n",
      "footnotes": null
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 591,
      "verse_key": "86:13",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُۥ لَقَوۡلٞ فَصۡلٞ",
      "translation": "بێگومان ئەم(قورئانە)م وتەی ڕاست وجیاکەرەوەیە\n",
      "footnotes": null
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 591,
      "verse_key": "86:14",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا هُوَ بِٱلۡهَزۡلِ",
      "translation": "وە گاڵتە نیە\n",
      "footnotes": null
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 591,
      "verse_key": "86:15",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُمۡ يَكِيدُونَ كَيۡدٗا",
      "translation": "بێگومان بێ بڕواکان (پیلان دەگێڕن) وفێڵ دەکەن چۆن فێڵ کردنێك\n",
      "footnotes": null
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 591,
      "verse_key": "86:16",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَكِيدُ كَيۡدٗا",
      "translation": "وە منیش پیلان دەگێڕم (ونەخشە دەکێشم لەبەرامبەر پیلانی ئەواندا) چۆن پیلان ونەخشە کێشانێك\n",
      "footnotes": null
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 591,
      "verse_key": "86:17",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَمَهِّلِ ٱلۡكَٰفِرِينَ أَمۡهِلۡهُمۡ رُوَيۡدَۢا",
      "translation": "ئەمجا (تۆش ئەی پێغەمبەر ﷺ) مۆڵەتی بێ بڕواکان بدە، منیش کەمێك لێیان دەوەستم (پاشان دەبینیت چیان بەسەر دەھێنم بە سزادانایان)\n",
      "footnotes": null
    }
  ]
}