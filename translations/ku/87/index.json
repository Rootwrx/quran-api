{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 591,
      "verse_key": "87:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "سَبِّحِ ٱسۡمَ رَبِّكَ ٱلۡأَعۡلَى",
      "translation": "تەسبیحاتی پەروەردگاری ھەرە بڵندت بکە\n",
      "footnotes": null
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 591,
      "verse_key": "87:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِي خَلَقَ فَسَوَّىٰ",
      "translation": "ئەوەی(ھەموو شتێکی) دروستكردووە وڕێك وپێکی کردووە\n",
      "footnotes": null
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 591,
      "verse_key": "87:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّذِي قَدَّرَ فَهَدَىٰ",
      "translation": "وە ئەوەی کە نەخشەی کێشاوە(بۆ ھەموو شتێك) ئەمجا شارەزای کردووە\n",
      "footnotes": null
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 591,
      "verse_key": "87:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّذِيٓ أَخۡرَجَ ٱلۡمَرۡعَىٰ",
      "translation": "وە ئەوەی کە لەوەڕی دەرھێناوە\n",
      "footnotes": null
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 591,
      "verse_key": "87:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَجَعَلَهُۥ غُثَآءً أَحۡوَىٰ",
      "translation": "ئەمجا دەیکات بە پوشی ڕەش داگەڕاو\n",
      "footnotes": null
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 591,
      "verse_key": "87:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "سَنُقۡرِئُكَ فَلَا تَنسَىٰٓ",
      "translation": "(ئەی موحەممەد ﷺ قورئانت) بەسەردا دەخوێنینەوە وھەرگیز لە بیرت ناچێتەوە\n",
      "footnotes": null
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 591,
      "verse_key": "87:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَّا مَا شَآءَ ٱللَّهُۚ إِنَّهُۥ يَعۡلَمُ ٱلۡجَهۡرَ وَمَا يَخۡفَىٰ",
      "translation": "مەگەر ئەوەی کە خوا بیەوێت، بێگومان ئەو بە ھەموو شتێکی ئاشکرا وپەنھان ئاگادارە\n",
      "footnotes": null
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 591,
      "verse_key": "87:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَنُيَسِّرُكَ لِلۡيُسۡرَىٰ",
      "translation": "وە یارمەتیت دەدەین بۆ ڕێی ئاسان\n",
      "footnotes": null
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 591,
      "verse_key": "87:9",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَذَكِّرۡ إِن نَّفَعَتِ ٱلذِّكۡرَىٰ",
      "translation": "ئەمجا تۆش ھەردەم ئامۆژگاری بکە ئەگەر سوودی گەیاند\n",
      "footnotes": null
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 591,
      "verse_key": "87:10",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "سَيَذَّكَّرُ مَن يَخۡشَىٰ",
      "translation": "بێگومان ئەوەی لە خوا بترسێت پەندی لێ وەردەگرێ\n",
      "footnotes": null
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 591,
      "verse_key": "87:11",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيَتَجَنَّبُهَا ٱلۡأَشۡقَى",
      "translation": "وە ئەوەی زۆر خراپیش بێت خۆی لێ دوور دەخاتەوە\n",
      "footnotes": null
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 591,
      "verse_key": "87:12",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِي يَصۡلَى ٱلنَّارَ ٱلۡكُبۡرَىٰ",
      "translation": "ئەوەی دەچێتە ناو ئاگری ھەرە گەورەوە\n",
      "footnotes": null
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 591,
      "verse_key": "87:13",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ لَا يَمُوتُ فِيهَا وَلَا يَحۡيَىٰ",
      "translation": " لەپاشان نە دەمرێت تێیدا (تا ڕزگاری بێت) وە نە دەژی (تێیدا بە ئاسایی)\n",
      "footnotes": null
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 591,
      "verse_key": "87:14",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قَدۡ أَفۡلَحَ مَن تَزَكَّىٰ",
      "translation": "بەڕاستی سەرفراز بوو ئەوەی کە خۆی پاك ڕاگرت\n",
      "footnotes": null
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 591,
      "verse_key": "87:15",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَذَكَرَ ٱسۡمَ رَبِّهِۦ فَصَلَّىٰ",
      "translation": "یادی پەروەردگاری خۆی دەكات وکەوتە نوێژ کردن (وپاڕایەوە)\n",
      "footnotes": null
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 592,
      "verse_key": "87:16",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بَلۡ تُؤۡثِرُونَ ٱلۡحَيَوٰةَ ٱلدُّنۡيَا",
      "translation": "بەڵکو ئێوە ژیانی دونیا ھەڵدەبژێرن\n",
      "footnotes": null
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 592,
      "verse_key": "87:17",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡأٓخِرَةُ خَيۡرٞ وَأَبۡقَىٰٓ",
      "translation": "کەچی(ژیانی) دواڕۆژ باشتر وپایە دارترە\n",
      "footnotes": null
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 592,
      "verse_key": "87:18",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ هَٰذَا لَفِي ٱلصُّحُفِ ٱلۡأُولَىٰ",
      "translation": "بێگومان ئەمەی (کەباسکرا) لە نامەی پێشوەکاندایە\n",
      "footnotes": null
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 592,
      "verse_key": "87:19",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "صُحُفِ إِبۡرَٰهِيمَ وَمُوسَىٰ",
      "translation": " نامەکانی ئیبراھیم وموسا\n",
      "footnotes": null
    }
  ]
}