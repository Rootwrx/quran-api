      "sura": "113",
      "aya": "1",
      "arabic_text": "قُلۡ أَعُوذُ بِرَبِّ ٱلۡفَلَقِ",
      "translation": "(ئەی موحەممەد ﷺ) بڵێ: پەنا دەگرم بە دروستكەری بوونەوەر (بەپەروەردگاری بەرەبەیان)\n",
      "footnotes": null
    },
    {
      "id": "6227",
      "sura": "113",
      "aya": "2",
      "arabic_text": "مِن شَرِّ مَا خَلَقَ",
      "translation": "لە زیانی ئەوەی کە دروستی کردووە\n",
      "footnotes": null
    },
    {
      "id": "6228",
      "sura": "113",
      "aya": "3",
      "arabic_text": "وَمِن شَرِّ غَاسِقٍ إِذَا وَقَبَ",
      "translation": "وە لە زیانی تاریکی کاتێک کە دادێت\n",
      "footnotes": null
    },
    {
      "id": "6229",
      "sura": "113",
      "aya": "4",
      "arabic_text": "وَمِن شَرِّ ٱلنَّفَّٰثَٰتِ فِي ٱلۡعُقَدِ",
      "translation": "وە لە زیان وخراپەی(جادوگەران) کەپەت ودەزوو گرێ دەدەن وفووی پێدا دەكەن\n",
      "footnotes": null
    },
    {
      "id": "6230",
      "sura": "113",
      "aya": "5",
      "arabic_text": "وَمِن شَرِّ حَاسِدٍ إِذَا حَسَدَ",
      "translation": "وە لە زیانی بەخیل وحەسوود کاتێک کە حەسوودی دەبات\n",
      "footnotes": null
    }
  ]
}