{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 583,
      "verse_key": "79:1",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلنَّٰزِعَٰتِ غَرۡقٗا",
      "translation": "سوێند بەو فریشتانەی کە گیانی کافران(لە لەشیان) بە سەختی دەردەھێنن\n",
      "footnotes": null
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 583,
      "verse_key": "79:2",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلنَّٰشِطَٰتِ نَشۡطٗا",
      "translation": "وە بەو فریشتانەی کە گیانی باوەڕداران بە ئاسانی دەردەهێنن\n",
      "footnotes": null
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 583,
      "verse_key": "79:3",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلسَّٰبِحَٰتِ سَبۡحٗا",
      "translation": "وە سوێند بەو فریشتانەی کە خێرا فەرمانی خوا جێ بەجێ دەکەن\n",
      "footnotes": null
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 583,
      "verse_key": "79:4",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَٱلسَّٰبِقَٰتِ سَبۡقٗا",
      "translation": "وە سوێند بەو فریشتانەی کە پێشبڕكێ دەکەن (لە بەجێهێنانی فەرماندا)\n",
      "footnotes": null
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 583,
      "verse_key": "79:5",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَٱلۡمُدَبِّرَٰتِ أَمۡرٗا",
      "translation": "وە سوێند بەو فریشتانەی کە تەگبیری کاروبار دەکەن (واتە سوێند بەمانە ئەوەی بەڵێنتان پێدراوە دێتە دی)\n",
      "footnotes": null
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 583,
      "verse_key": "79:6",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَوۡمَ تَرۡجُفُ ٱلرَّاجِفَةُ",
      "translation": "ڕۆژێکە بومەلەرزە زەوی دەلەرزێنێ\n",
      "footnotes": null
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 583,
      "verse_key": "79:7",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تَتۡبَعُهَا ٱلرَّادِفَةُ",
      "translation": "بومەلەرزەیەکی تریشی بەدوایدا دێت، مردووان زیندوو دەبنەوە\n",
      "footnotes": null
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 583,
      "verse_key": "79:8",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قُلُوبٞ يَوۡمَئِذٖ وَاجِفَةٌ",
      "translation": "زۆر دڵ لەو ڕۆژەدا دەلەرزێت\n",
      "footnotes": null
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 583,
      "verse_key": "79:9",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَبۡصَٰرُهَا خَٰشِعَةٞ",
      "translation": "چاویان سەر بەرەو ژێر دەبێت\n",
      "footnotes": null
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 583,
      "verse_key": "79:10",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَقُولُونَ أَءِنَّا لَمَرۡدُودُونَ فِي ٱلۡحَافِرَةِ",
      "translation": "(ئەمڕۆ بەگاڵتەوە) دەڵێن ئاخۆ ئێمە(لە پاش مردن) دەگەڕێنرێینەوە بۆ ژیانی پێشوو؟\n",
      "footnotes": null
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 583,
      "verse_key": "79:11",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَءِذَا كُنَّا عِظَٰمٗا نَّخِرَةٗ",
      "translation": "ئایا کە بووین بە ئێسقانی ڕزییو (چۆن زیندوو دەبینەوە)\n",
      "footnotes": null
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 583,
      "verse_key": "79:12",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قَالُواْ تِلۡكَ إِذٗا كَرَّةٌ خَاسِرَةٞ",
      "translation": "ووتیان ئائەوە ئەگەر وابێت گەڕانەوەیەکی بەزیانە\n",
      "footnotes": null
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 583,
      "verse_key": "79:13",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِنَّمَا هِيَ زَجۡرَةٞ وَٰحِدَةٞ",
      "translation": "بێگومان ئەو زیندوو بوونەوەیە ھەر یەك نەعرەتەیە\n",
      "footnotes": null
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 583,
      "verse_key": "79:14",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِذَا هُم بِٱلسَّاهِرَةِ",
      "translation": "ئیتر کتوپڕ وان لە دەشتێکی سپی کاکی بە کاکیدا\n",
      "footnotes": null
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 583,
      "verse_key": "79:15",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "هَلۡ أَتَىٰكَ حَدِيثُ مُوسَىٰٓ",
      "translation": "ئایا داستانی مووسات پێ گەیشتووە\n",
      "footnotes": null
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 584,
      "verse_key": "79:16",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذۡ نَادَىٰهُ رَبُّهُۥ بِٱلۡوَادِ ٱلۡمُقَدَّسِ طُوًى",
      "translation": "کاتێك کە پەروەردگاری بانگی کرد لەشیووی پیرۆزی طوا وە\n",
      "footnotes": null
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 584,
      "verse_key": "79:17",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱذۡهَبۡ إِلَىٰ فِرۡعَوۡنَ إِنَّهُۥ طَغَىٰ",
      "translation": "(پێی ووت) بڕۆ بۆ لای فیرعەون(چونکە) بەڕاستی یاخیی بووە\n",
      "footnotes": null
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 584,
      "verse_key": "79:18",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَقُلۡ هَل لَّكَ إِلَىٰٓ أَن تَزَكَّىٰ",
      "translation": "وە پێی بڵێ: ئایا دەتەوێ پاك ببیتەوە (لە گوناە)\n",
      "footnotes": null
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 584,
      "verse_key": "79:19",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَهۡدِيَكَ إِلَىٰ رَبِّكَ فَتَخۡشَىٰ",
      "translation": "وە ڕێت نیشاندەم بۆ(ناسینی) پەروەردگارت تا لێی بترسییت\n",
      "footnotes": null
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 584,
      "verse_key": "79:20",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَرَىٰهُ ٱلۡأٓيَةَ ٱلۡكُبۡرَىٰ",
      "translation": "ئەمجا نیشانە ھەرە گەورەکەی نیشان دا (گۆچان لەگەڵ دەستە سپیەکەی)\n",
      "footnotes": null
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 584,
      "verse_key": "79:21",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَكَذَّبَ وَعَصَىٰ",
      "translation": "کەچی بە درۆی زانی وسەرپێچی کرد\n",
      "footnotes": null
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 584,
      "verse_key": "79:22",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ أَدۡبَرَ يَسۡعَىٰ",
      "translation": "لە پاشان پشتی ھەڵکرد وڕۆی بەپەلە\n",
      "footnotes": null
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 584,
      "verse_key": "79:23",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَحَشَرَ فَنَادَىٰ",
      "translation": "ئەمجا(جادوو گەرانی) کۆکردەوە وبانگی کردن\n",
      "footnotes": null
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 584,
      "verse_key": "79:24",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَقَالَ أَنَا۠ رَبُّكُمُ ٱلۡأَعۡلَىٰ",
      "translation": "پاشان ووتی: من پەروەردگاری ھەرە گەورەتانم\n",
      "footnotes": null
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 584,
      "verse_key": "79:25",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَخَذَهُ ٱللَّهُ نَكَالَ ٱلۡأٓخِرَةِ وَٱلۡأُولَىٰٓ",
      "translation": "ئەمجا خوا گرفتاری کرد بە سزای ھەردوو جیھان\n",
      "footnotes": null
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 584,
      "verse_key": "79:26",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ فِي ذَٰلِكَ لَعِبۡرَةٗ لِّمَن يَخۡشَىٰٓ",
      "translation": "بەڕاستی لەم داستانەدا پەند ھەیە بۆ کەسێك لەخوا بترسێت\n",
      "footnotes": null
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 584,
      "verse_key": "79:27",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ءَأَنتُمۡ أَشَدُّ خَلۡقًا أَمِ ٱلسَّمَآءُۚ بَنَىٰهَا",
      "translation": "ئایا دروستكردنی ئێوە گرانترە یان ئاسمان ؟ وە دروستیشی کرد\n",
      "footnotes": null
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 584,
      "verse_key": "79:28",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "رَفَعَ سَمۡكَهَا فَسَوَّىٰهَا",
      "translation": "باڵای بەرز کردەوە وڕێکی خست\n",
      "footnotes": null
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 584,
      "verse_key": "79:29",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَغۡطَشَ لَيۡلَهَا وَأَخۡرَجَ ضُحَىٰهَا",
      "translation": "وە شەوی ئەوی تاریك کرد ودەریخست(ڕووناکی) ڕۆژی\n",
      "footnotes": null
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 584,
      "verse_key": "79:30",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡأَرۡضَ بَعۡدَ ذَٰلِكَ دَحَىٰهَآ",
      "translation": "لە پاشان زەوی ڕاخست\n",
      "footnotes": null
    },
    {
      "id": 31,
      "verse_number": 31,
      "page_number": 584,
      "verse_key": "79:31",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَخۡرَجَ مِنۡهَا مَآءَهَا وَمَرۡعَىٰهَا",
      "translation": "ئاو ولەوەڕگای لێ دەرھێنا\n",
      "footnotes": null
    },
    {
      "id": 32,
      "verse_number": 32,
      "page_number": 584,
      "verse_key": "79:32",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡجِبَالَ أَرۡسَىٰهَا",
      "translation": "وە شاخەکانی پتەو دامەزراند\n",
      "footnotes": null
    },
    {
      "id": 33,
      "verse_number": 33,
      "page_number": 584,
      "verse_key": "79:33",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَتَٰعٗا لَّكُمۡ وَلِأَنۡعَٰمِكُمۡ",
      "translation": "بۆ بەھرەمەندی وژیانی خۆتان وئاژەڵتان\n",
      "footnotes": null
    },
    {
      "id": 34,
      "verse_number": 34,
      "page_number": 584,
      "verse_key": "79:34",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِذَا جَآءَتِ ٱلطَّآمَّةُ ٱلۡكُبۡرَىٰ",
      "translation": "ئەمجا کاتێك کە ڕووداوی گەورە ھات (کە ڕۆژی دواییە)\n",
      "footnotes": null
    },
    {
      "id": 35,
      "verse_number": 35,
      "page_number": 584,
      "verse_key": "79:35",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَوۡمَ يَتَذَكَّرُ ٱلۡإِنسَٰنُ مَا سَعَىٰ",
      "translation": "ئەو ڕۆژەی کە مرۆڤ کردەوەی خۆی تێدا بیر دەکەوێتەوە\n",
      "footnotes": null
    },
    {
      "id": 36,
      "verse_number": 36,
      "page_number": 584,
      "verse_key": "79:36",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَبُرِّزَتِ ٱلۡجَحِيمُ لِمَن يَرَىٰ",
      "translation": "وە دۆزەخ پیشاندرا بۆ ھەر کەسێك کە بڕوانێت\n",
      "footnotes": null
    },
    {
      "id": 37,
      "verse_number": 37,
      "page_number": 584,
      "verse_key": "79:37",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَمَّا مَن طَغَىٰ",
      "translation": "(لەو ڕۆژەدا چاکە وخراپە بەپێی کردەوە دابەش دەکرێت) ئەمجا ئەو کەسەی سەرکەش بووبێت\n",
      "footnotes": null
    },
    {
      "id": 38,
      "verse_number": 38,
      "page_number": 584,
      "verse_key": "79:38",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَءَاثَرَ ٱلۡحَيَوٰةَ ٱلدُّنۡيَا",
      "translation": "وە ژیانی دونیای ھەڵبژاردبێت\n",
      "footnotes": null
    },
    {
      "id": 39,
      "verse_number": 39,
      "page_number": 584,
      "verse_key": "79:39",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِنَّ ٱلۡجَحِيمَ هِيَ ٱلۡمَأۡوَىٰ",
      "translation": "ئەوە بێ گومان دۆزەخ جێگایەتی\n",
      "footnotes": null
    },
    {
      "id": 40,
      "verse_number": 40,
      "page_number": 584,
      "verse_key": "79:40",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَمَّا مَنۡ خَافَ مَقَامَ رَبِّهِۦ وَنَهَى ٱلنَّفۡسَ عَنِ ٱلۡهَوَىٰ",
      "translation": "وە ئەو کەسەی کە ترسابێت لە وەستانی بەردەم پەروەردگاری بۆ لێپرسینەوە وە بەرگری نەفسی خۆی کردبێت لە ئارەزووی خراپە\n",
      "footnotes": null
    },
    {
      "id": 41,
      "verse_number": 41,
      "page_number": 584,
      "verse_key": "79:41",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِنَّ ٱلۡجَنَّةَ هِيَ ٱلۡمَأۡوَىٰ",
      "translation": "ئەوە بێگومان بەھەشت جێگایەتی\n",
      "footnotes": null
    },
    {
      "id": 42,
      "verse_number": 42,
      "page_number": 584,
      "verse_key": "79:42",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَسۡـَٔلُونَكَ عَنِ ٱلسَّاعَةِ أَيَّانَ مُرۡسَىٰهَا",
      "translation": "(ئەو بێ باوەڕانە بەگاڵتە ولاقرتیەوە) پرسیارت لێ دەکەن دەربارەی ڕۆژی دوایی کەی دەبێت وکەی دێت\n",
      "footnotes": null
    },
    {
      "id": 43,
      "verse_number": 43,
      "page_number": 584,
      "verse_key": "79:43",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِيمَ أَنتَ مِن ذِكۡرَىٰهَآ",
      "translation": "تۆ چیت داوە لە بیر لێکردنەوەی ( تۆ چۆن لەم بارەوە دەدوێی)؟\n",
      "footnotes": null
    },
    {
      "id": 44,
      "verse_number": 44,
      "page_number": 584,
      "verse_key": "79:44",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَىٰ رَبِّكَ مُنتَهَىٰهَآ",
      "translation": "زانینی ئەوە ھەر لای پەروەردگارتە\n",
      "footnotes": null
    },
    {
      "id": 45,
      "verse_number": 45,
      "page_number": 584,
      "verse_key": "79:45",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّمَآ أَنتَ مُنذِرُ مَن يَخۡشَىٰهَا",
      "translation": "بێگومان تۆ ھەر ترسێنەری کەسێکیت کە لەو ڕۆژە بترسێت\n",
      "footnotes": null
    },
    {
      "id": 46,
      "verse_number": 46,
      "page_number": 584,
      "verse_key": "79:46",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَأَنَّهُمۡ يَوۡمَ يَرَوۡنَهَا لَمۡ يَلۡبَثُوٓاْ إِلَّا عَشِيَّةً أَوۡ ضُحَىٰهَا",
      "translation": "بێ بڕواکان کە ئەو ڕۆژە دەبینن وا دەزانن کە نەماونەتەوە ئێوارەیەك یا چێشتەنگاوێ نەبێت لە دونیادا\n",
      "footnotes": null
    }
  ]
}