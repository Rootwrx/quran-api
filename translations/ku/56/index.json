{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 534,
      "verse_key": "56:1",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذَا وَقَعَتِ ٱلۡوَاقِعَةُ",
      "translation": "کاتێك کە ڕووداوی گەورەی (قیامەت) ڕوویدا\n",
      "footnotes": null
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 534,
      "verse_key": "56:2",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَيۡسَ لِوَقۡعَتِهَا كَاذِبَةٌ",
      "translation": "لەڕودانیدا ھیچ درۆ وگومانێك نیە\n",
      "footnotes": null
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 534,
      "verse_key": "56:3",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "خَافِضَةٞ رَّافِعَةٌ",
      "translation": "نزم کەرەوەی (کەسانێکە و) بەرزکەرەوەیە (بۆ کەسانێکی تر)\n",
      "footnotes": null
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 534,
      "verse_key": "56:4",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذَا رُجَّتِ ٱلۡأَرۡضُ رَجّٗا",
      "translation": "کاتێك کە زەوی ھێنرایە جوڵان بە جوڵانێکی توند\n",
      "footnotes": null
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 534,
      "verse_key": "56:5",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَبُسَّتِ ٱلۡجِبَالُ بَسّٗا",
      "translation": "وە شاخەکان زۆر وورد کران بەورد کردن\n",
      "footnotes": null
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 534,
      "verse_key": "56:6",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَكَانَتۡ هَبَآءٗ مُّنۢبَثّٗا",
      "translation": "وە بوون بە تەپوتۆزی پرش وبڵاو\n",
      "footnotes": null
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 534,
      "verse_key": "56:7",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَكُنتُمۡ أَزۡوَٰجٗا ثَلَٰثَةٗ",
      "translation": "وە ئێوە دەبنە سێ کۆمەڵ (لە دوا ڕۆژدا)\n",
      "footnotes": null
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 534,
      "verse_key": "56:8",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَصۡحَٰبُ ٱلۡمَيۡمَنَةِ مَآ أَصۡحَٰبُ ٱلۡمَيۡمَنَةِ",
      "translation": "ئەمجا یارانی لای ڕاست ، کام یارانی لای ڕاست؟\n",
      "footnotes": null
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 534,
      "verse_key": "56:9",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَصۡحَٰبُ ٱلۡمَشۡـَٔمَةِ مَآ أَصۡحَٰبُ ٱلۡمَشۡـَٔمَةِ",
      "translation": "وە یارانی لای چەپ، کام یارانی لای چەپ؟\n",
      "footnotes": null
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 534,
      "verse_key": "56:10",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلسَّٰبِقُونَ ٱلسَّٰبِقُونَ",
      "translation": "وە پێشەکەوتوەکان ھەر خۆیانن پێش کەوتوو\n",
      "footnotes": null
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 534,
      "verse_key": "56:11",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أُوْلَٰٓئِكَ ٱلۡمُقَرَّبُونَ",
      "translation": "ئەوانەن نزیکانی (بارەگای خوا)\n",
      "footnotes": null
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 534,
      "verse_key": "56:12",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِي جَنَّٰتِ ٱلنَّعِيمِ",
      "translation": "لە بەھەشتانی پڕ نازو نیعمەتدان\n",
      "footnotes": null
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 534,
      "verse_key": "56:13",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُلَّةٞ مِّنَ ٱلۡأَوَّلِينَ",
      "translation": "کۆمەڵێکی زۆرن لە (گەلانی) پێشینان\n",
      "footnotes": null
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 534,
      "verse_key": "56:14",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَقَلِيلٞ مِّنَ ٱلۡأٓخِرِينَ",
      "translation": "وە کەمێکن لە پاشینان (لە ئوممەتی موحەممەد ﷺ)\n",
      "footnotes": null
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 534,
      "verse_key": "56:15",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلَىٰ سُرُرٖ مَّوۡضُونَةٖ",
      "translation": "لەسەر قەنەفە وکورسی چنراو بە ئاڵتون\n",
      "footnotes": null
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 534,
      "verse_key": "56:16",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مُّتَّكِـِٔينَ عَلَيۡهَا مُتَقَٰبِلِينَ",
      "translation": "پاڵ ئەدەنەوە لەسەری بەرامبەر بەیەکتری\n",
      "footnotes": null
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 535,
      "verse_key": "56:17",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَطُوفُ عَلَيۡهِمۡ وِلۡدَٰنٞ مُّخَلَّدُونَ",
      "translation": "تازە لاوی ھەمیشەیی بەسەریاندا دەگەڕێن\n",
      "footnotes": null
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 535,
      "verse_key": "56:18",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بِأَكۡوَابٖ وَأَبَارِيقَ وَكَأۡسٖ مِّن مَّعِينٖ",
      "translation": "بەپەرداخ و سوراحیی وگڵاسی پڕ لەشەرابی سەرچاوەی ڕەوان\n",
      "footnotes": null
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 535,
      "verse_key": "56:19",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَّا يُصَدَّعُونَ عَنۡهَا وَلَا يُنزِفُونَ",
      "translation": "نەسەریان پێی دەێشێت ونەسەرخۆش ئەبن پێی\n",
      "footnotes": null
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 535,
      "verse_key": "56:20",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَفَٰكِهَةٖ مِّمَّا يَتَخَيَّرُونَ",
      "translation": "میوەھاتیش کەخۆیان ھەڵی بژێرن\n",
      "footnotes": null
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 535,
      "verse_key": "56:21",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَحۡمِ طَيۡرٖ مِّمَّا يَشۡتَهُونَ",
      "translation": "وە گۆشتی ھەر باڵندەیەك کەحەزی لێ بکەن\n",
      "footnotes": null
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 535,
      "verse_key": "56:22",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَحُورٌ عِينٞ",
      "translation": "وەحۆریانی چاو گەش وچاو بەڵەك\n",
      "footnotes": null
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 535,
      "verse_key": "56:23",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَأَمۡثَٰلِ ٱللُّؤۡلُوِٕ ٱلۡمَكۡنُونِ",
      "translation": "وەك مرواری ناو سەدەف وان\n",
      "footnotes": null
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 535,
      "verse_key": "56:24",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "جَزَآءَۢ بِمَا كَانُواْ يَعۡمَلُونَ",
      "translation": "(ئەم نیعمەتانە) لەپاداشتی ئەو کردەوانەدایە کە ئەنجامیان ئەدا\n",
      "footnotes": null
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 535,
      "verse_key": "56:25",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَا يَسۡمَعُونَ فِيهَا لَغۡوٗا وَلَا تَأۡثِيمًا",
      "translation": "نایبیستن لەو بەھەشتانەدا نەقسەی بێھودە، وە نەتاوانبارکردن\n",
      "footnotes": null
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 535,
      "verse_key": "56:26",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَّا قِيلٗا سَلَٰمٗا سَلَٰمٗا",
      "translation": "جگە لەووتەی سڵاو وسەلام کردن (لەیەکتری)\n",
      "footnotes": null
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 535,
      "verse_key": "56:27",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَصۡحَٰبُ ٱلۡيَمِينِ مَآ أَصۡحَٰبُ ٱلۡيَمِينِ",
      "translation": "وە یارانی لای ڕاست کام یارانی لای ڕاست\n",
      "footnotes": null
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 535,
      "verse_key": "56:28",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِي سِدۡرٖ مَّخۡضُودٖ",
      "translation": "لەژێر سێبەری دارتاوگی بێ دڕکدان\n",
      "footnotes": null
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 535,
      "verse_key": "56:29",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَطَلۡحٖ مَّنضُودٖ",
      "translation": "وداری مۆزی ھۆنراو بەھێشووە مۆز\n",
      "footnotes": null
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 535,
      "verse_key": "56:30",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَظِلّٖ مَّمۡدُودٖ",
      "translation": "و سێبەری بەردەوام\n",
      "footnotes": null
    },
    {
      "id": 31,
      "verse_number": 31,
      "page_number": 535,
      "verse_key": "56:31",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَآءٖ مَّسۡكُوبٖ",
      "translation": "وەئاوی تاڤگەیی وئاوی ڕەوان\n",
      "footnotes": null
    },
    {
      "id": 32,
      "verse_number": 32,
      "page_number": 535,
      "verse_key": "56:32",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَفَٰكِهَةٖ كَثِيرَةٖ",
      "translation": "وەمیوەھاتی زۆر\n",
      "footnotes": null
    },
    {
      "id": 33,
      "verse_number": 33,
      "page_number": 535,
      "verse_key": "56:33",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَّا مَقۡطُوعَةٖ وَلَا مَمۡنُوعَةٖ",
      "translation": "کە ھەرگیز تەواو نابێ وقەدەغەش ناکرێ\n",
      "footnotes": null
    },
    {
      "id": 34,
      "verse_number": 34,
      "page_number": 535,
      "verse_key": "56:34",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَفُرُشٖ مَّرۡفُوعَةٍ",
      "translation": "ھەروەھا ڕاخەری بەرز وبڵندی تێدایە\n",
      "footnotes": null
    },
    {
      "id": 35,
      "verse_number": 35,
      "page_number": 535,
      "verse_key": "56:35",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّآ أَنشَأۡنَٰهُنَّ إِنشَآءٗ",
      "translation": " ئێمە حۆریەکانی بەھەشتمان دروست کردووە بەدروست کردنێکی زۆر جوان\n",
      "footnotes": null
    },
    {
      "id": 36,
      "verse_number": 36,
      "page_number": 535,
      "verse_key": "56:36",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَجَعَلۡنَٰهُنَّ أَبۡكَارًا",
      "translation": "وەگێڕاومانن بەحۆریانی ھەمیشە کچ (واتە : ھەرچەند ھاوسەرکانیان بچنە لایان خوای گەورە بۆیان دەکاتەوە بەکچ)\n",
      "footnotes": null
    },
    {
      "id": 37,
      "verse_number": 37,
      "page_number": 535,
      "verse_key": "56:37",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عُرُبًا أَتۡرَابٗا",
      "translation": "ھاوسەرەکانیان زۆر خۆش دەوێت وھاوتەمەنیانن\n",
      "footnotes": null
    },
    {
      "id": 38,
      "verse_number": 38,
      "page_number": 535,
      "verse_key": "56:38",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لِّأَصۡحَٰبِ ٱلۡيَمِينِ",
      "translation": "(ئەمانە ھەموو ) بۆ یارانی لای ڕاستن\n",
      "footnotes": null
    },
    {
      "id": 39,
      "verse_number": 39,
      "page_number": 535,
      "verse_key": "56:39",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُلَّةٞ مِّنَ ٱلۡأَوَّلِينَ",
      "translation": "کۆمەڵێکی زۆرن لە (گەلانی) پێشینان\n",
      "footnotes": null
    },
    {
      "id": 40,
      "verse_number": 40,
      "page_number": 535,
      "verse_key": "56:40",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَثُلَّةٞ مِّنَ ٱلۡأٓخِرِينَ",
      "translation": "کۆمەڵێکی زۆریش لەپاشینان (لە ئوممەتی موحەممەد ﷺ)\n",
      "footnotes": null
    },
    {
      "id": 41,
      "verse_number": 41,
      "page_number": 535,
      "verse_key": "56:41",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَصۡحَٰبُ ٱلشِّمَالِ مَآ أَصۡحَٰبُ ٱلشِّمَالِ",
      "translation": "وە ھاوڕێیانی لای چەپ کام ھاوڕێیانی لای چەپ؟\n",
      "footnotes": null
    },
    {
      "id": 42,
      "verse_number": 42,
      "page_number": 535,
      "verse_key": "56:42",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِي سَمُومٖ وَحَمِيمٖ",
      "translation": "لەناو گڕی دۆزەخ وئاوی لەکوڵدان\n",
      "footnotes": null
    },
    {
      "id": 43,
      "verse_number": 43,
      "page_number": 535,
      "verse_key": "56:43",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَظِلّٖ مِّن يَحۡمُومٖ",
      "translation": " وە لە سێبەری چڕە دووکەڵی ڕەشدان\n",
      "footnotes": null
    },
    {
      "id": 44,
      "verse_number": 44,
      "page_number": 535,
      "verse_key": "56:44",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَّا بَارِدٖ وَلَا كَرِيمٍ",
      "translation": "نە فێنکە و نە کەڵکیشی ھەیە\n",
      "footnotes": null
    },
    {
      "id": 45,
      "verse_number": 45,
      "page_number": 535,
      "verse_key": "56:45",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُمۡ كَانُواْ قَبۡلَ ذَٰلِكَ مُتۡرَفِينَ",
      "translation": "چونکە بەڕاستی ئەمانە پێشتر خۆشـگوزەران بوون\n",
      "footnotes": null
    },
    {
      "id": 46,
      "verse_number": 46,
      "page_number": 535,
      "verse_key": "56:46",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَكَانُواْ يُصِرُّونَ عَلَى ٱلۡحِنثِ ٱلۡعَظِيمِ",
      "translation": "وە بەردەوام بوون لەسەر گوناھە گەورەکە (ھاوەڵ دانان بۆ خوا)\n",
      "footnotes": null
    },
    {
      "id": 47,
      "verse_number": 47,
      "page_number": 535,
      "verse_key": "56:47",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَكَانُواْ يَقُولُونَ أَئِذَا مِتۡنَا وَكُنَّا تُرَابٗا وَعِظَٰمًا أَءِنَّا لَمَبۡعُوثُونَ",
      "translation": "وە دەیانووت: ئایا ئەگەر مردین وبووین بەخۆڵ وئێسکی ڕزیو ئایا ئێمە زیندوو ئەکرێینەوە\n",
      "footnotes": null
    },
    {
      "id": 48,
      "verse_number": 48,
      "page_number": 535,
      "verse_key": "56:48",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَوَءَابَآؤُنَا ٱلۡأَوَّلُونَ",
      "translation": "یاخود باوو باپیرانمان (زیندوو ئەکرێنەوە)\n",
      "footnotes": null
    },
    {
      "id": 49,
      "verse_number": 49,
      "page_number": 535,
      "verse_key": "56:49",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قُلۡ إِنَّ ٱلۡأَوَّلِينَ وَٱلۡأٓخِرِينَ",
      "translation": "(ئەی موحەممەد ﷺ) بڵێ: بێگومان ھەموو پێشینان وپاشینان\n",
      "footnotes": null
    },
    {
      "id": 50,
      "verse_number": 50,
      "page_number": 535,
      "verse_key": "56:50",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَمَجۡمُوعُونَ إِلَىٰ مِيقَٰتِ يَوۡمٖ مَّعۡلُومٖ",
      "translation": "کۆدەکرێنەوە وڕاپێچ دەکرێن بۆکاتی ڕۆژی دیاریكراو\n",
      "footnotes": null
    },
    {
      "id": 51,
      "verse_number": 51,
      "page_number": 536,
      "verse_key": "56:51",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ إِنَّكُمۡ أَيُّهَا ٱلضَّآلُّونَ ٱلۡمُكَذِّبُونَ",
      "translation": "پاشان ئێوە ئەی گومڕایانی بێ باوەڕان\n",
      "footnotes": null
    },
    {
      "id": 52,
      "verse_number": 52,
      "page_number": 536,
      "verse_key": "56:52",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَأٓكِلُونَ مِن شَجَرٖ مِّن زَقُّومٖ",
      "translation": "بێگومان لە(بەری) درەختی ژەقنەمووت (دەخۆن)\n",
      "footnotes": null
    },
    {
      "id": 53,
      "verse_number": 53,
      "page_number": 536,
      "verse_key": "56:53",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَمَالِـُٔونَ مِنۡهَا ٱلۡبُطُونَ",
      "translation": "جا وورگی خۆتانی لێ پڕدەکەن\n",
      "footnotes": null
    },
    {
      "id": 54,
      "verse_number": 54,
      "page_number": 536,
      "verse_key": "56:54",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَشَٰرِبُونَ عَلَيۡهِ مِنَ ٱلۡحَمِيمِ",
      "translation": "ئەمجا ئاوی کوڵاوی بەسەردا دەخۆنەوە\n",
      "footnotes": null
    },
    {
      "id": 55,
      "verse_number": 55,
      "page_number": 536,
      "verse_key": "56:55",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَشَٰرِبُونَ شُرۡبَ ٱلۡهِيمِ",
      "translation": "دەی خۆنەوە وەکو خواردنەوەی (حوشترانی) تینوو\n",
      "footnotes": null
    },
    {
      "id": 56,
      "verse_number": 56,
      "page_number": 536,
      "verse_key": "56:56",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "هَٰذَا نُزُلُهُمۡ يَوۡمَ ٱلدِّينِ",
      "translation": "ئەمە میوانداری ئەوانە لە ڕۆژی قیامەتدا\n",
      "footnotes": null
    },
    {
      "id": 57,
      "verse_number": 57,
      "page_number": 536,
      "verse_key": "56:57",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "نَحۡنُ خَلَقۡنَٰكُمۡ فَلَوۡلَا تُصَدِّقُونَ",
      "translation": "ئێمە دروستمان کردون ئەی بۆ بڕوا ناکەن (بەزیندوو بوونەوە)\n",
      "footnotes": null
    },
    {
      "id": 58,
      "verse_number": 58,
      "page_number": 536,
      "verse_key": "56:58",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَرَءَيۡتُم مَّا تُمۡنُونَ",
      "translation": "جا پێم بڵێن ئەو ئاوەی کە دەیڕێژنە ناو منداڵدانەوە؟\n",
      "footnotes": null
    },
    {
      "id": 59,
      "verse_number": 59,
      "page_number": 536,
      "verse_key": "56:59",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ءَأَنتُمۡ تَخۡلُقُونَهُۥٓ أَمۡ نَحۡنُ ٱلۡخَٰلِقُونَ",
      "translation": "ئایا ئێوە دروستی دەکەن؟ یان ئێمەین بەدی ھێنەرینی؟\n",
      "footnotes": null
    },
    {
      "id": 60,
      "verse_number": 60,
      "page_number": 536,
      "verse_key": "56:60",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "نَحۡنُ قَدَّرۡنَا بَيۡنَكُمُ ٱلۡمَوۡتَ وَمَا نَحۡنُ بِمَسۡبُوقِينَ",
      "translation": " ئێمە لەناوتاندا مردنمان‎ داناوە، وەکەس پێش دەسەڵاتی ئێمە ناکەوێ\n",
      "footnotes": null
    },
    {
      "id": 61,
      "verse_number": 61,
      "page_number": 536,
      "verse_key": "56:61",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلَىٰٓ أَن نُّبَدِّلَ أَمۡثَٰلَكُمۡ وَنُنشِئَكُمۡ فِي مَا لَا تَعۡلَمُونَ",
      "translation": "(بەدەسەڵاتین) کە لەجیاتی ئێوە نەوەی تر بھێنین وە دروستتان بکەینەوە بەشێوەیەك کەھیچی لێ نازانن\n",
      "footnotes": null
    },
    {
      "id": 62,
      "verse_number": 62,
      "page_number": 536,
      "verse_key": "56:62",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَقَدۡ عَلِمۡتُمُ ٱلنَّشۡأَةَ ٱلۡأُولَىٰ فَلَوۡلَا تَذَكَّرُونَ",
      "translation": "سوێند بەخوا بێگومان ئێوە لەبەدیهێنانی یەکەمی ئاگادارن ئیتر بۆ بیرناکەنەوە\n",
      "footnotes": null
    },
    {
      "id": 63,
      "verse_number": 63,
      "page_number": 536,
      "verse_key": "56:63",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَرَءَيۡتُم مَّا تَحۡرُثُونَ",
      "translation": "جا پێم بڵێن ئەو تۆوەی کەدەیچێنن\n",
      "footnotes": null
    },
    {
      "id": 64,
      "verse_number": 64,
      "page_number": 536,
      "verse_key": "56:64",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ءَأَنتُمۡ تَزۡرَعُونَهُۥٓ أَمۡ نَحۡنُ ٱلزَّٰرِعُونَ",
      "translation": "ئایا ئێوە دەیڕوێنن؟ یائێمەین دەیڕوێنین؟\n",
      "footnotes": null
    },
    {
      "id": 65,
      "verse_number": 65,
      "page_number": 536,
      "verse_key": "56:65",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَوۡ نَشَآءُ لَجَعَلۡنَٰهُ حُطَٰمٗا فَظَلۡتُمۡ تَفَكَّهُونَ",
      "translation": "ئەگەر بمان ویستایە ئەو (ڕواوە)مان دەگێڕا بە پووش و کا، ئەمجا دەستان دەکرد بەقسەی بێ کەڵك\n",
      "footnotes": null
    },
    {
      "id": 66,
      "verse_number": 66,
      "page_number": 536,
      "verse_key": "56:66",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّا لَمُغۡرَمُونَ",
      "translation": "(دەتانـووت) بەڕاستی زەرەرلێکەوتوو ئێمەین\n",
      "footnotes": null
    },
    {
      "id": 67,
      "verse_number": 67,
      "page_number": 536,
      "verse_key": "56:67",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بَلۡ نَحۡنُ مَحۡرُومُونَ",
      "translation": "بەڵکو ئێمە لە ڕزق وڕۆزی بێ بەشین\n",
      "footnotes": null
    },
    {
      "id": 68,
      "verse_number": 68,
      "page_number": 536,
      "verse_key": "56:68",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَرَءَيۡتُمُ ٱلۡمَآءَ ٱلَّذِي تَشۡرَبُونَ",
      "translation": "جا پێم بڵێن ئەو ئاوەی کەدەیخۆنەوە\n",
      "footnotes": null
    },
    {
      "id": 69,
      "verse_number": 69,
      "page_number": 536,
      "verse_key": "56:69",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ءَأَنتُمۡ أَنزَلۡتُمُوهُ مِنَ ٱلۡمُزۡنِ أَمۡ نَحۡنُ ٱلۡمُنزِلُونَ",
      "translation": "ئایا ئێوە لە ھەورەکانەوە دەیبارێنن یا ئێمە دەیبارێنین\n",
      "footnotes": null
    },
    {
      "id": 70,
      "verse_number": 70,
      "page_number": 536,
      "verse_key": "56:70",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَوۡ نَشَآءُ جَعَلۡنَٰهُ أُجَاجٗا فَلَوۡلَا تَشۡكُرُونَ",
      "translation": " ئەگەر بمان ویستایە دەمانکرد بەئاوێکی زۆر سوێری تاڵ، ئیتر بۆ سوپاسی خوا ناکەن\n",
      "footnotes": null
    },
    {
      "id": 71,
      "verse_number": 71,
      "page_number": 536,
      "verse_key": "56:71",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَرَءَيۡتُمُ ٱلنَّارَ ٱلَّتِي تُورُونَ",
      "translation": "پێم بڵێن ئەو ئاگرەی کە دایدەگیرسێنن\n",
      "footnotes": null
    },
    {
      "id": 72,
      "verse_number": 72,
      "page_number": 536,
      "verse_key": "56:72",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ءَأَنتُمۡ أَنشَأۡتُمۡ شَجَرَتَهَآ أَمۡ نَحۡنُ ٱلۡمُنشِـُٔونَ",
      "translation": "ئایا ئێوە دارەکەیتان بەدیهێناوە؟ یائێمە بەدیهێنەرینی؟\n",
      "footnotes": null
    },
    {
      "id": 73,
      "verse_number": 73,
      "page_number": 536,
      "verse_key": "56:73",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "نَحۡنُ جَعَلۡنَٰهَا تَذۡكِرَةٗ وَمَتَٰعٗا لِّلۡمُقۡوِينَ",
      "translation": "ئێمە ئەو (ئاگرە)مان کردووە بەھۆی یادخەرەوە(ی ئاگری دۆزەخ) وە (کردومانەتە) ھۆی کەڵک لێ وەرگرتن بۆ گەشتیاران\n",
      "footnotes": null
    },
    {
      "id": 74,
      "verse_number": 74,
      "page_number": 536,
      "verse_key": "56:74",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَسَبِّحۡ بِٱسۡمِ رَبِّكَ ٱلۡعَظِيمِ",
      "translation": "جا کەواتە یاد وتەسبیحاتی پەروەردگاری گەورەی خۆت بەپاکی بکە\n",
      "footnotes": null
    },
    {
      "id": 75,
      "verse_number": 75,
      "page_number": 536,
      "verse_key": "56:75",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "۞ فَلَآ أُقۡسِمُ بِمَوَٰقِعِ ٱلنُّجُومِ",
      "translation": "جا سوێند بێت بەشوێنی (ھەڵھاتن وئاوابوونی) ئەستێرەکان\n",
      "footnotes": null
    },
    {
      "id": 76,
      "verse_number": 76,
      "page_number": 536,
      "verse_key": "56:76",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِنَّهُۥ لَقَسَمٞ لَّوۡ تَعۡلَمُونَ عَظِيمٌ",
      "translation": "بەڕاستی ئەم سوێندە ئەگەر بزانن سوێندێکی زۆر گەورەیە\n",
      "footnotes": null
    },
    {
      "id": 77,
      "verse_number": 77,
      "page_number": 537,
      "verse_key": "56:77",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُۥ لَقُرۡءَانٞ كَرِيمٞ",
      "translation": "کە بێگومان ئەمە قورئانێکی گەورە وبەڕێزە\n",
      "footnotes": null
    },
    {
      "id": 78,
      "verse_number": 78,
      "page_number": 537,
      "verse_key": "56:78",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِي كِتَٰبٖ مَّكۡنُونٖ",
      "translation": "لەناو کتێبێکی شاراوەدایە (کەلوح المحفوظه)\n",
      "footnotes": null
    },
    {
      "id": 79,
      "verse_number": 79,
      "page_number": 537,
      "verse_key": "56:79",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَّا يَمَسُّهُۥٓ إِلَّا ٱلۡمُطَهَّرُونَ",
      "translation": "جگە لە (فریشتە) پاکەکان کەس دەستی لێ نادات\n",
      "footnotes": null
    },
    {
      "id": 80,
      "verse_number": 80,
      "page_number": 537,
      "verse_key": "56:80",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تَنزِيلٞ مِّن رَّبِّ ٱلۡعَٰلَمِينَ",
      "translation": "نێردراوەتە خوارەوە لە لایەن پەروەردگاری جیھانیانەوە\n",
      "footnotes": null
    },
    {
      "id": 81,
      "verse_number": 81,
      "page_number": 537,
      "verse_key": "56:81",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَبِهَٰذَا ٱلۡحَدِيثِ أَنتُم مُّدۡهِنُونَ",
      "translation": " دەی ئایا ئێوە بەم قورئانە باوەڕ ناکەن\n",
      "footnotes": null
    },
    {
      "id": 82,
      "verse_number": 82,
      "page_number": 537,
      "verse_key": "56:82",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَتَجۡعَلُونَ رِزۡقَكُمۡ أَنَّكُمۡ تُكَذِّبُونَ",
      "translation": "وە لەجیاتی سوپاسگوزاری لەسەر ڕۆزیتان ئێوە باوەڕ ناھێنن\n",
      "footnotes": null
    },
    {
      "id": 83,
      "verse_number": 83,
      "page_number": 537,
      "verse_key": "56:83",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلَوۡلَآ إِذَا بَلَغَتِ ٱلۡحُلۡقُومَ",
      "translation": "دەی بۆچی کاتێك گیانی (نەخۆشەکەتان) دەگاتە گەرووی\n",
      "footnotes": null
    },
    {
      "id": 84,
      "verse_number": 84,
      "page_number": 537,
      "verse_key": "56:84",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنتُمۡ حِينَئِذٖ تَنظُرُونَ",
      "translation": "وە ئێوە لەو کاتەدا تەماشا دەکەن\n",
      "footnotes": null
    },
    {
      "id": 85,
      "verse_number": 85,
      "page_number": 537,
      "verse_key": "56:85",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَنَحۡنُ أَقۡرَبُ إِلَيۡهِ مِنكُمۡ وَلَٰكِن لَّا تُبۡصِرُونَ",
      "translation": "لەوکاتەدا (فریشتەکانمان) لە ئێوە نزیکترن لەو (کەسەی لەسەرە مەرگدایە) بەڵام ئێوە (فریشتەکانی) ئێمە نابینن\n",
      "footnotes": null
    },
    {
      "id": 86,
      "verse_number": 86,
      "page_number": 537,
      "verse_key": "56:86",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلَوۡلَآ إِن كُنتُمۡ غَيۡرَ مَدِينِينَ",
      "translation": "ئەگەر ئێوە (زیندوو ناکرێنەوەو)پاداشت نادرێنەوە؟\n",
      "footnotes": null
    },
    {
      "id": 87,
      "verse_number": 87,
      "page_number": 537,
      "verse_key": "56:87",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تَرۡجِعُونَهَآ إِن كُنتُمۡ صَٰدِقِينَ",
      "translation": "ڕۆحی (مردوەکەتان) بگێڕنەوە ئەگەر ڕاست دەکەن\n",
      "footnotes": null
    },
    {
      "id": 88,
      "verse_number": 88,
      "page_number": 537,
      "verse_key": "56:88",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَمَّآ إِن كَانَ مِنَ ٱلۡمُقَرَّبِينَ",
      "translation": "ئەمجا ئەگەر ئەو (کەسەی لە گیانە ڵادایە) لەنزیکان بێ(لەخوا)\n",
      "footnotes": null
    },
    {
      "id": 89,
      "verse_number": 89,
      "page_number": 537,
      "verse_key": "56:89",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَرَوۡحٞ وَرَيۡحَانٞ وَجَنَّتُ نَعِيمٖ",
      "translation": "ئەوە حەسانەوە وبۆنی خۆش وبەھەشتی پڕ لەناز ونیعمەتی بۆ ھەیە\n",
      "footnotes": null
    },
    {
      "id": 90,
      "verse_number": 90,
      "page_number": 537,
      "verse_key": "56:90",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَمَّآ إِن كَانَ مِنۡ أَصۡحَٰبِ ٱلۡيَمِينِ",
      "translation": "وە ئەگەر لەھاوەڵانی دەستی ڕاست بێت\n",
      "footnotes": null
    },
    {
      "id": 91,
      "verse_number": 91,
      "page_number": 537,
      "verse_key": "56:91",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَسَلَٰمٞ لَّكَ مِنۡ أَصۡحَٰبِ ٱلۡيَمِينِ",
      "translation": "ئەوە سەلامەتی و بێ زیانی بۆ تۆ (چونکە) تۆ لەیارانی دەستی ڕاستی\n",
      "footnotes": null
    },
    {
      "id": 92,
      "verse_number": 92,
      "page_number": 537,
      "verse_key": "56:92",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَمَّآ إِن كَانَ مِنَ ٱلۡمُكَذِّبِينَ ٱلضَّآلِّينَ",
      "translation": "بەڵام ئەگەر لە بێ باوەڕانی گومڕا بێت\n",
      "footnotes": null
    },
    {
      "id": 93,
      "verse_number": 93,
      "page_number": 537,
      "verse_key": "56:93",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَنُزُلٞ مِّنۡ حَمِيمٖ",
      "translation": " ئەوە(شوێنی) میوانداریەکەی ئێجگار گەرمە\n",
      "footnotes": null
    },
    {
      "id": 94,
      "verse_number": 94,
      "page_number": 537,
      "verse_key": "56:94",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَتَصۡلِيَةُ جَحِيمٍ",
      "translation": "وەچوونە ناو دۆزەخە\n",
      "footnotes": null
    },
    {
      "id": 95,
      "verse_number": 95,
      "page_number": 537,
      "verse_key": "56:95",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ هَٰذَا لَهُوَ حَقُّ ٱلۡيَقِينِ",
      "translation": "بەڕاستی ئەوەی (کـە لەم سورەتەدا باسکرا) حەق وبێـگومانە\n",
      "footnotes": null
    },
    {
      "id": 96,
      "verse_number": 96,
      "page_number": 537,
      "verse_key": "56:96",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَسَبِّحۡ بِٱسۡمِ رَبِّكَ ٱلۡعَظِيمِ",
      "translation": "جا کەواتە یاد وتەسبیحاتی پەروەردگاری گەورەی خۆت بەپاکی بکە\n",
      "footnotes": null
    }
  ]
}