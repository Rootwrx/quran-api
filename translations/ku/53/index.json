{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 526,
      "verse_key": "53:1",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلنَّجۡمِ إِذَا هَوَىٰ",
      "translation": "سوێند بە ئەستێرە کاتێك ئاوا دەبێت\n",
      "footnotes": null
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 526,
      "verse_key": "53:2",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَا ضَلَّ صَاحِبُكُمۡ وَمَا غَوَىٰ",
      "translation": "ھاوڕێکەتان (موحەممەد ﷺ) گومڕا نەبووە وە لە ڕاستی لای نەداوە\n",
      "footnotes": null
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 526,
      "verse_key": "53:3",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا يَنطِقُ عَنِ ٱلۡهَوَىٰٓ",
      "translation": "وە لە ھەوا وھەوسەوە قسە ناکات (کە قورئان دەخوێنێت)\n",
      "footnotes": null
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 526,
      "verse_key": "53:4",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنۡ هُوَ إِلَّا وَحۡيٞ يُوحَىٰ",
      "translation": "ھەرچی دەیڵێت (قورئانەو) نیگا دەکرێت بۆی\n",
      "footnotes": null
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 526,
      "verse_key": "53:5",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلَّمَهُۥ شَدِيدُ ٱلۡقُوَىٰ",
      "translation": "(جوبرەیلی) زۆر بەتوانا فێری کردووە\n",
      "footnotes": null
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 526,
      "verse_key": "53:6",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ذُو مِرَّةٖ فَٱسۡتَوَىٰ",
      "translation": "خاوەنی بیرو ھۆشی بەھێزە، جا لەسەر شێوەی ڕاستەقینەی خۆی ڕاوەستاوە\n",
      "footnotes": null
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 526,
      "verse_key": "53:7",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَهُوَ بِٱلۡأُفُقِ ٱلۡأَعۡلَىٰ",
      "translation": "کە بە ئاسۆی ھەرە بەرزی ئاسمانەوە بوو\n",
      "footnotes": null
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 526,
      "verse_key": "53:8",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ دَنَا فَتَدَلَّىٰ",
      "translation": "پاشان نزیك بوویەوە (لە موحەممەد ﷺ) و ئەمجا ھاتە خوارەوە\n",
      "footnotes": null
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 526,
      "verse_key": "53:9",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَكَانَ قَابَ قَوۡسَيۡنِ أَوۡ أَدۡنَىٰ",
      "translation": "تا بە ئەندازەی دوو کەوان یان کەمتری(ماوە بوو لەگەڵ موحەممەددا ﷺ)\n",
      "footnotes": null
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 526,
      "verse_key": "53:10",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَوۡحَىٰٓ إِلَىٰ عَبۡدِهِۦ مَآ أَوۡحَىٰ",
      "translation": "ئەمجا (خوا) نیگای کرد بۆ بەندەی خۆی، ئای چ نیگایەك بوو\n",
      "footnotes": null
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 526,
      "verse_key": "53:11",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَا كَذَبَ ٱلۡفُؤَادُ مَا رَأَىٰٓ",
      "translation": "ئەوەی (موحەممەد ﷺ) بەچاوی سەر بینی دڵی (بڕوای پێ کرد و) بە درۆی نەزانی\n",
      "footnotes": null
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 526,
      "verse_key": "53:12",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَتُمَٰرُونَهُۥ عَلَىٰ مَا يَرَىٰ",
      "translation": "جا ئایا (بێ باوەڕان) ئێوە دەمەقاڵێی لەگەڵ دەکەن دەربارەی ئەوەی کە بە چاوی خۆی دەی بینێت\n",
      "footnotes": null
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 526,
      "verse_key": "53:13",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَقَدۡ رَءَاهُ نَزۡلَةً أُخۡرَىٰ",
      "translation": "سوێند بەخوا بێگومان جارێکی تر جوبرەئیلی بینیوە\n",
      "footnotes": null
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 526,
      "verse_key": "53:14",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عِندَ سِدۡرَةِ ٱلۡمُنتَهَىٰ",
      "translation": "لای درەختی(سیدرەتولمونتـەھا)\n",
      "footnotes": null
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 526,
      "verse_key": "53:15",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عِندَهَا جَنَّةُ ٱلۡمَأۡوَىٰٓ",
      "translation": "کە لای ئەوەوەیە بەھەشتی نیشتەجێ بوون\n",
      "footnotes": null
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 526,
      "verse_key": "53:16",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذۡ يَغۡشَى ٱلسِّدۡرَةَ مَا يَغۡشَىٰ",
      "translation": "ئەو کاتەی (پێغەمَبەرﷺ) ئەو شتە گرنگ وسەر سوڕھێنەرەی بینی کە سیدرەی داپۆشی چۆن داپۆشینێك\n",
      "footnotes": null
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 526,
      "verse_key": "53:17",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَا زَاغَ ٱلۡبَصَرُ وَمَا طَغَىٰ",
      "translation": "چاوی ئەو (موحەممەد ﷺ) بەملاولادا نەیـدەڕوانی (لە مەبەست تێ نەدەپەڕی)\n",
      "footnotes": null
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 526,
      "verse_key": "53:18",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَقَدۡ رَأَىٰ مِنۡ ءَايَٰتِ رَبِّهِ ٱلۡكُبۡرَىٰٓ",
      "translation": "سوێند بەخوا بێگومان بینی گەورەترین بەڵگە ونیشانەی پەروەردگاری\n",
      "footnotes": null
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 526,
      "verse_key": "53:19",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَرَءَيۡتُمُ ٱللَّٰتَ وَٱلۡعُزَّىٰ",
      "translation": "دەی پێم بڵێن ئایا لات و عوززا (کە دووبتن)\n",
      "footnotes": null
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 526,
      "verse_key": "53:20",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَنَوٰةَ ٱلثَّالِثَةَ ٱلۡأُخۡرَىٰٓ",
      "translation": "وە بتی سێهەم کە مەناتە (ئایا ھیچیان پێ دەکرێت)\n",
      "footnotes": null
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 526,
      "verse_key": "53:21",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَكُمُ ٱلذَّكَرُ وَلَهُ ٱلۡأُنثَىٰ",
      "translation": "ئایا ڕاستە ئێوە کوڕان بۆ خۆتان وکچان بۆ خودا بەڕەوا وچاك دەزانن؟\n",
      "footnotes": null
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 526,
      "verse_key": "53:22",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تِلۡكَ إِذٗا قِسۡمَةٞ ضِيزَىٰٓ",
      "translation": "ئەگەر وابێت دابەشکردنێکی نادروست دەبێ\n",
      "footnotes": null
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 526,
      "verse_key": "53:23",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنۡ هِيَ إِلَّآ أَسۡمَآءٞ سَمَّيۡتُمُوهَآ أَنتُمۡ وَءَابَآؤُكُم مَّآ أَنزَلَ ٱللَّهُ بِهَا مِن سُلۡطَٰنٍۚ إِن يَتَّبِعُونَ إِلَّا ٱلظَّنَّ وَمَا تَهۡوَى ٱلۡأَنفُسُۖ وَلَقَدۡ جَآءَهُم مِّن رَّبِّهِمُ ٱلۡهُدَىٰٓ",
      "translation": "ئەم بتانە ھەر ناوانێکی ڕوتن کە ئێوە و باو وباپیرانتان لێتان ناون (بەبێ ناوەڕۆك) خوا ھیچ بەڵگەیەکی لەسەر پەرستراوی ئەوان نەناردووەتە خوارەوە، تەنھا شوێنی گومان وئەو شتە دەکەون کە نەفسی خۆیان حەزی لێیەتی، لە کاتێکدا کە بەڕاستی لەلایەن پەروەردگاریانەوە ھیدایەتیان بۆ ھاتووە\n",
      "footnotes": null
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 526,
      "verse_key": "53:24",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَمۡ لِلۡإِنسَٰنِ مَا تَمَنَّىٰ",
      "translation": "ئایا مرۆڤ ئارەزووی ھەرچی کرد بۆی دێتە دی (نەخێر)\n",
      "footnotes": null
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 526,
      "verse_key": "53:25",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلِلَّهِ ٱلۡأٓخِرَةُ وَٱلۡأُولَىٰ",
      "translation": "چونکە ھەر خوایە خاوەنی دونیا ودواڕۆژە\n",
      "footnotes": null
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 526,
      "verse_key": "53:26",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "۞ وَكَم مِّن مَّلَكٖ فِي ٱلسَّمَٰوَٰتِ لَا تُغۡنِي شَفَٰعَتُهُمۡ شَيۡـًٔا إِلَّا مِنۢ بَعۡدِ أَن يَأۡذَنَ ٱللَّهُ لِمَن يَشَآءُ وَيَرۡضَىٰٓ",
      "translation": "وە چەندەھا فریشتە کە لە ئاسمانەکاندا ھەن تکا وشەفاعەتیان ھیچ سوودی نیە، مەگەر دوای ئەوەی کە خوا مۆڵەت بدات بۆ ھەرکەسێك کە خۆی بیەوێت وڕازی بێت پێی\n",
      "footnotes": null
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 527,
      "verse_key": "53:27",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ ٱلَّذِينَ لَا يُؤۡمِنُونَ بِٱلۡأٓخِرَةِ لَيُسَمُّونَ ٱلۡمَلَٰٓئِكَةَ تَسۡمِيَةَ ٱلۡأُنثَىٰ",
      "translation": "بەڕاستی ئەوانەی کە باوەڕ بـە ڕۆژی دوایی ناھێنن، فریشتەکان بە کچی (خوا) ناو دەبەن\n",
      "footnotes": null
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 527,
      "verse_key": "53:28",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا لَهُم بِهِۦ مِنۡ عِلۡمٍۖ إِن يَتَّبِعُونَ إِلَّا ٱلظَّنَّۖ وَإِنَّ ٱلظَّنَّ لَا يُغۡنِي مِنَ ٱلۡحَقِّ شَيۡـٔٗا",
      "translation": " لە کاتێکدا ھیچ زانیاریەکیان دەربارەی نیە، تەنھا دوای گومان دەکەون وە بەڕاستی گومان ھیچ سوودێکی نیە لەزانینی ھەق وڕاستیدا\n",
      "footnotes": null
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 527,
      "verse_key": "53:29",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَعۡرِضۡ عَن مَّن تَوَلَّىٰ عَن ذِكۡرِنَا وَلَمۡ يُرِدۡ إِلَّا ٱلۡحَيَوٰةَ ٱلدُّنۡيَا",
      "translation": "دەی کەواتە تۆش وازبھێنە لەو کەسە پشت دەکاتە قورئان ویادی ئێمە، وە بێجگە ژیانی دونیا ھیچی تری ناوێت\n",
      "footnotes": null
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 527,
      "verse_key": "53:30",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ذَٰلِكَ مَبۡلَغُهُم مِّنَ ٱلۡعِلۡمِۚ إِنَّ رَبَّكَ هُوَ أَعۡلَمُ بِمَن ضَلَّ عَن سَبِيلِهِۦ وَهُوَ أَعۡلَمُ بِمَنِ ٱهۡتَدَىٰ",
      "translation": "ئەمە ئەوپەڕی زانیاری وتێگەیشتنیانە لەزانیاریدا، بەڕاستی پەروەردگارت چاك دەزانێت بەوەی لە ڕێبازەکەی گومڕا بووە وە چاك دەزانێت بەوەی کە ڕێنووین بووە\n",
      "footnotes": null
    },
    {
      "id": 31,
      "verse_number": 31,
      "page_number": 527,
      "verse_key": "53:31",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلِلَّهِ مَا فِي ٱلسَّمَٰوَٰتِ وَمَا فِي ٱلۡأَرۡضِ لِيَجۡزِيَ ٱلَّذِينَ أَسَٰٓـُٔواْ بِمَا عَمِلُواْ وَيَجۡزِيَ ٱلَّذِينَ أَحۡسَنُواْ بِٱلۡحُسۡنَى",
      "translation": "وە خوا خاوەنێتی ھەرچی لەئاسمانـەکان وزەویدایە، تا سزای ئەوانە بدات کە خراپەیان کردووە بە پێی کردەوەکانیان وە پاداشتی ئەوانە بداتەوە کەچاکەیان کردووە بەچاکەتر (لەوەی کردویانە)\n",
      "footnotes": null
    },
    {
      "id": 32,
      "verse_number": 32,
      "page_number": 527,
      "verse_key": "53:32",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِينَ يَجۡتَنِبُونَ كَبَٰٓئِرَ ٱلۡإِثۡمِ وَٱلۡفَوَٰحِشَ إِلَّا ٱللَّمَمَۚ إِنَّ رَبَّكَ وَٰسِعُ ٱلۡمَغۡفِرَةِۚ هُوَ أَعۡلَمُ بِكُمۡ إِذۡ أَنشَأَكُم مِّنَ ٱلۡأَرۡضِ وَإِذۡ أَنتُمۡ أَجِنَّةٞ فِي بُطُونِ أُمَّهَٰتِكُمۡۖ فَلَا تُزَكُّوٓاْ أَنفُسَكُمۡۖ هُوَ أَعۡلَمُ بِمَنِ ٱتَّقَىٰٓ",
      "translation": "ئەوانەی کە لەگوناھی گەورە وکردەوەی دزێو خۆیان دەپارێزن جگە تاوانی بچوك نەبێت، بەڕاستی پەروەردگارت لێبوردنی فراوانە، ئەو لەھەموو کەس بەحاڵی ئێوە زاناترە لەکاتێکدا کە لەزەوی دروستی کردون وە کاتێك کە ئێوە کۆرپەلەن ولەسکی دایکتاندان، کەواتە خۆتان بەچاك وپاك دامەنێن، خوا زاناترە بەو کەسەی زیاتر لە خواترسە\n",
      "footnotes": null
    },
    {
      "id": 33,
      "verse_number": 33,
      "page_number": 527,
      "verse_key": "53:33",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَرَءَيۡتَ ٱلَّذِي تَوَلَّىٰ",
      "translation": "جا ئایا بینیت ئەو کەسەی کە پشتی ھەڵکرد (لەباوەڕ)\n",
      "footnotes": null
    },
    {
      "id": 34,
      "verse_number": 34,
      "page_number": 527,
      "verse_key": "53:34",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَعۡطَىٰ قَلِيلٗا وَأَكۡدَىٰٓ",
      "translation": "(لەوەی بڕیاری دابوو) کەمێکی بەخشی وئەوەی تری گرتەوە\n",
      "footnotes": null
    },
    {
      "id": 35,
      "verse_number": 35,
      "page_number": 527,
      "verse_key": "53:35",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَعِندَهُۥ عِلۡمُ ٱلۡغَيۡبِ فَهُوَ يَرَىٰٓ",
      "translation": "ئایا (ئەو کەسە) زانیاری پەنھانی لایەو بەچاو دەیبینێت\n",
      "footnotes": null
    },
    {
      "id": 36,
      "verse_number": 36,
      "page_number": 527,
      "verse_key": "53:36",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَمۡ لَمۡ يُنَبَّأۡ بِمَا فِي صُحُفِ مُوسَىٰ",
      "translation": "یان ئایا ئاگادار نەکرابوو بەوەی کە لەپەڕاوەکانی موسادا بوو\n",
      "footnotes": null
    },
    {
      "id": 37,
      "verse_number": 37,
      "page_number": 527,
      "verse_key": "53:37",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِبۡرَٰهِيمَ ٱلَّذِي وَفَّىٰٓ",
      "translation": "(وە بەوەی کە لەپەڕاوی ئیبراھیمدا بوو) ئەو ئیبراھیمەی کە پەیمانی خۆی بەچاکی بەجێ ھێنا\n",
      "footnotes": null
    },
    {
      "id": 38,
      "verse_number": 38,
      "page_number": 527,
      "verse_key": "53:38",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَّا تَزِرُ وَازِرَةٞ وِزۡرَ أُخۡرَىٰ",
      "translation": "(لەو پەڕاوانەدا ھاتووە) کە ھیچ تاوانبارێك گوناھی تاوانبارێکی تر ھەڵناگرێت\n",
      "footnotes": null
    },
    {
      "id": 39,
      "verse_number": 39,
      "page_number": 527,
      "verse_key": "53:39",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَن لَّيۡسَ لِلۡإِنسَٰنِ إِلَّا مَا سَعَىٰ",
      "translation": "وەبێگومان ئادەمی جگە لەکۆششی خۆی خاوەنی ھیچی تر نیە (لە دواڕۆژدا)\n",
      "footnotes": null
    },
    {
      "id": 40,
      "verse_number": 40,
      "page_number": 527,
      "verse_key": "53:40",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّ سَعۡيَهُۥ سَوۡفَ يُرَىٰ",
      "translation": "و بێگومان ھەوڵ کۆششەکەی لە داھاتوو (لەقیامەت)دا دەبینرێتەوە\n",
      "footnotes": null
    },
    {
      "id": 41,
      "verse_number": 41,
      "page_number": 527,
      "verse_key": "53:41",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ يُجۡزَىٰهُ ٱلۡجَزَآءَ ٱلۡأَوۡفَىٰ",
      "translation": "پاشان پاداشت دەدرێتەوە بەتێر وتەواوی\n",
      "footnotes": null
    },
    {
      "id": 42,
      "verse_number": 42,
      "page_number": 527,
      "verse_key": "53:42",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّ إِلَىٰ رَبِّكَ ٱلۡمُنتَهَىٰ",
      "translation": "وە بەڕاستی ھەموو کارێک سەرەنجامەکەی ھەر بۆ لای پەروەردگارت دەگەڕێتەوە\n",
      "footnotes": null
    },
    {
      "id": 43,
      "verse_number": 43,
      "page_number": 527,
      "verse_key": "53:43",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّهُۥ هُوَ أَضۡحَكَ وَأَبۡكَىٰ",
      "translation": "وە بێگومان ھەر ئەویش بەندەکان دەخاتە گریان وپێکەنین\n",
      "footnotes": null
    },
    {
      "id": 44,
      "verse_number": 44,
      "page_number": 527,
      "verse_key": "53:44",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّهُۥ هُوَ أَمَاتَ وَأَحۡيَا",
      "translation": "وە بێگومان ھەر ئەوە دەیانمرێنێت وزیندووشیان دەکاتەوە\n",
      "footnotes": null
    },
    {
      "id": 45,
      "verse_number": 45,
      "page_number": 528,
      "verse_key": "53:45",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّهُۥ خَلَقَ ٱلزَّوۡجَيۡنِ ٱلذَّكَرَ وَٱلۡأُنثَىٰ",
      "translation": "وەبەڕاستی ھەر ئەوە کە جوتێ نێر ومێی دروستكردووە\n",
      "footnotes": null
    },
    {
      "id": 46,
      "verse_number": 46,
      "page_number": 528,
      "verse_key": "53:46",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مِن نُّطۡفَةٍ إِذَا تُمۡنَىٰ",
      "translation": "لە نوتفەیەك (دڵۆپە ئاوێك)کاتێك دەڕژێنرێتە منداڵـدانەوە\n",
      "footnotes": null
    },
    {
      "id": 47,
      "verse_number": 47,
      "page_number": 528,
      "verse_key": "53:47",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّ عَلَيۡهِ ٱلنَّشۡأَةَ ٱلۡأُخۡرَىٰ",
      "translation": "وە بەڕاستی ھەر لەسەر خوایە دروست کردنەوەی لەڕۆژی دواییدا\n",
      "footnotes": null
    },
    {
      "id": 48,
      "verse_number": 48,
      "page_number": 528,
      "verse_key": "53:48",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّهُۥ هُوَ أَغۡنَىٰ وَأَقۡنَىٰ",
      "translation": "وە بێگومان ھەر ئەویشە کەخەڵکی دەوڵەمەند وھەژار دەکات\n",
      "footnotes": null
    },
    {
      "id": 49,
      "verse_number": 49,
      "page_number": 528,
      "verse_key": "53:49",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّهُۥ هُوَ رَبُّ ٱلشِّعۡرَىٰ",
      "translation": "وە بەڕاستی ھەر ئەوە پەروەردگاری ئەستێرەی شیعرا ( شیعرا : ئەستێرەیەك بوو خەڵکانێك لەسەردەمی نەفامیدا دەیانپەرست)\n",
      "footnotes": null
    },
    {
      "id": 50,
      "verse_number": 50,
      "page_number": 528,
      "verse_key": "53:50",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّهُۥٓ أَهۡلَكَ عَادًا ٱلۡأُولَىٰ",
      "translation": "وەبێگومان ھەر ئەو خوایە گەلی عادی یەکەمی لەناو برد\n",
      "footnotes": null
    },
    {
      "id": 51,
      "verse_number": 51,
      "page_number": 528,
      "verse_key": "53:51",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَثَمُودَاْ فَمَآ أَبۡقَىٰ",
      "translation": "گەلی سەموودیشی لەناوبرد وکەسی نەھێشتن\n",
      "footnotes": null
    },
    {
      "id": 52,
      "verse_number": 52,
      "page_number": 528,
      "verse_key": "53:52",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَقَوۡمَ نُوحٖ مِّن قَبۡلُۖ إِنَّهُمۡ كَانُواْ هُمۡ أَظۡلَمَ وَأَطۡغَىٰ",
      "translation": "وەگەلی نووحیشی (لەناو برد) لەپێش (عاد وسەمود)دا کەبەڕاستی ئەوان ستەم کارتر وسەر کەش تربوون\n",
      "footnotes": null
    },
    {
      "id": 53,
      "verse_number": 53,
      "page_number": 528,
      "verse_key": "53:53",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡمُؤۡتَفِكَةَ أَهۡوَىٰ",
      "translation": "وەگوندە سەروژێرکراوەکانی (بەزەویدا) بردە خوارەوە (کەگوندەکانی گەلی لوط بوون)\n",
      "footnotes": null
    },
    {
      "id": 54,
      "verse_number": 54,
      "page_number": 528,
      "verse_key": "53:54",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَغَشَّىٰهَا مَا غَشَّىٰ",
      "translation": "ئەمجا ئەو گوندانەی داپۆشی (بەجۆرەھا سزای سەخت) چۆن داپۆشینێك\n",
      "footnotes": null
    },
    {
      "id": 55,
      "verse_number": 55,
      "page_number": 528,
      "verse_key": "53:55",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكَ تَتَمَارَىٰ",
      "translation": "لەکام ناز ونیعمەتەکانی پەروەردگارت دوو دڵیت\n",
      "footnotes": null
    },
    {
      "id": 56,
      "verse_number": 56,
      "page_number": 528,
      "verse_key": "53:56",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "هَٰذَا نَذِيرٞ مِّنَ ٱلنُّذُرِ ٱلۡأُولَىٰٓ",
      "translation": "ئەم (پێغەمبەرە) ترسێنەرێکە لە (پێغەمبەرە) ترسێنەرەکانی پێشوو\n",
      "footnotes": null
    },
    {
      "id": 57,
      "verse_number": 57,
      "page_number": 528,
      "verse_key": "53:57",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَزِفَتِ ٱلۡأٓزِفَةُ",
      "translation": "ڕۆژی دوایی زۆر نزیك بووەتەوە\n",
      "footnotes": null
    },
    {
      "id": 58,
      "verse_number": 58,
      "page_number": 528,
      "verse_key": "53:58",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَيۡسَ لَهَا مِن دُونِ ٱللَّهِ كَاشِفَةٌ",
      "translation": " جگە لەخوای گەورە کەس نیە ئاشکرای بکات (وبزانێت کەی دێت)\n",
      "footnotes": null
    },
    {
      "id": 59,
      "verse_number": 59,
      "page_number": 528,
      "verse_key": "53:59",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَمِنۡ هَٰذَا ٱلۡحَدِيثِ تَعۡجَبُونَ",
      "translation": "جـا ئایا لەم قورئانە سەرتان سوڕ دەمێنێت؟!\n",
      "footnotes": null
    },
    {
      "id": 60,
      "verse_number": 60,
      "page_number": 528,
      "verse_key": "53:60",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَتَضۡحَكُونَ وَلَا تَبۡكُونَ",
      "translation": "وە پێدەکەنن وناگرین!\n",
      "footnotes": null
    },
    {
      "id": 61,
      "verse_number": 61,
      "page_number": 528,
      "verse_key": "53:61",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنتُمۡ سَٰمِدُونَ",
      "translation": "وە ئێوە سەر گەرم و بێ ئاگان\n",
      "footnotes": null
    },
    {
      "id": 62,
      "verse_number": 62,
      "page_number": 528,
      "verse_key": "53:62",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": "obligatory",
      "sajdah_number": 12,
      "text": "فَٱسۡجُدُواْۤ لِلَّهِۤ وَٱعۡبُدُواْ۩",
      "translation": "دەی سوژدە بەرن بۆ خوا وپەرستشی بکەن\n",
      "footnotes": null
    }
  ]
}