{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 599,
      "verse_key": "100:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡعَٰدِيَٰتِ ضَبۡحٗا",
      "translation": "سوێند بە ئەسپانی تاودەر(کە دەپڕمێنن) چۆن پڕمەیەك (لەکاتی غەزاکردندا)\n",
      "footnotes": null
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 599,
      "verse_key": "100:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَٱلۡمُورِيَٰتِ قَدۡحٗا",
      "translation": "وە سوێند بە ئاگر ھەڵگیرسێنەران لەبەرد بەناڵی سمیان\n",
      "footnotes": null
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 599,
      "verse_key": "100:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَٱلۡمُغِيرَٰتِ صُبۡحٗا",
      "translation": "وە سوێند بە (ئەسپانی) ھێرش بەران لە بەیانیاندا\n",
      "footnotes": null
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 599,
      "verse_key": "100:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَثَرۡنَ بِهِۦ نَقۡعٗا",
      "translation": "کە بەوە تەپ وتۆز ھەڵدەستێنن\n",
      "footnotes": null
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 599,
      "verse_key": "100:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَوَسَطۡنَ بِهِۦ جَمۡعًا",
      "translation": " وە بەو تەپ وتۆزەوە، خۆیان دەکەن بە ناوەڕاستی کۆمەڵی دوژمندا\n",
      "footnotes": null
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 599,
      "verse_key": "100:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ ٱلۡإِنسَٰنَ لِرَبِّهِۦ لَكَنُودٞ",
      "translation": "بەڕاستی مرۆڤ بۆ پەروەردگاری نا سوپاسە\n",
      "footnotes": null
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 599,
      "verse_key": "100:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِنَّهُۥ عَلَىٰ ذَٰلِكَ لَشَهِيدٞ",
      "translation": "وە بێگومان خۆی لەسەر ئەم ناسوپاسیە شایەتە\n",
      "footnotes": null
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 599,
      "verse_key": "100:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِنَّهُۥ لِحُبِّ ٱلۡخَيۡرِ لَشَدِيدٌ",
      "translation": "وە بەڕاستی مرۆڤ بۆ خۆشەویستی ماڵی دونیا زۆر بەتین وپەرۆشە\n",
      "footnotes": null
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 599,
      "verse_key": "100:9",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "۞ أَفَلَا يَعۡلَمُ إِذَا بُعۡثِرَ مَا فِي ٱلۡقُبُورِ",
      "translation": "دەی ئایا نازانی کاتێك کە مردووی ناو گۆڕەکان زیندوو کرانەوە و(ھێنرانە دەرەوە)\n",
      "footnotes": null
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 600,
      "verse_key": "100:10",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَحُصِّلَ مَا فِي ٱلصُّدُورِ",
      "translation": "وە ئاشکرا بوو ھەرچی ڕازی ناو دڵانە\n",
      "footnotes": null
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 600,
      "verse_key": "100:11",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ رَبَّهُم بِهِمۡ يَوۡمَئِذٖ لَّخَبِيرُۢ",
      "translation": " بێگومان پەروەردگاریان لەو ڕۆژەدا ئاگادارە بە کردەوەیان\n",
      "footnotes": null
    }
  ]
}