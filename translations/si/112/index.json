{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 604,
      "verse_key": "112:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قُلۡ هُوَ ٱللَّهُ أَحَدٌ",
      "translation": "(නබිවරය!) නුඹ පවසනු. ඔහු එනම් අල්ලාහ් ඒකීය ය.",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 604,
      "verse_key": "112:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱللَّهُ ٱلصَّمَدُ",
      "translation": "අල්ලාහ් ස්වාධීන ය.",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 604,
      "verse_key": "112:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَمۡ يَلِدۡ وَلَمۡ يُولَدۡ",
      "translation": "ඔහු ජනිත නො කළේ ය. තවද ජනිත කරනු නො ලැබුවේ ය.",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 604,
      "verse_key": "112:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَمۡ يَكُن لَّهُۥ كُفُوًا أَحَدُۢ",
      "translation": "තවද ඔහුට සමාන කිසිවෙකු නැත.",
      "footnotes": ""
    }
  ]
}