{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 534,
      "verse_key": "56:1",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذَا وَقَعَتِ ٱلۡوَاقِعَةُ",
      "translation": "Sε Asεm (kεseε a εrebesie) no nya si a,",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 534,
      "verse_key": "56:2",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَيۡسَ لِوَقۡعَتِهَا كَاذِبَةٌ",
      "translation": "Nkontompo biara nni si a ebesie no ho.",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 534,
      "verse_key": "56:3",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "خَافِضَةٞ رَّافِعَةٌ",
      "translation": "(Saa Da no), yεbεbrε (nnipa no) bi ase, ama (ebinom nso) so.",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 534,
      "verse_key": "56:4",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذَا رُجَّتِ ٱلۡأَرۡضُ رَجّٗا",
      "translation": "Sε asaase woso nwosoeε (denden) no,",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 534,
      "verse_key": "56:5",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَبُسَّتِ ٱلۡجِبَالُ بَسّٗا",
      "translation": "Na yɛbubu mmepͻ gͻsͻgͻsͻ –",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 534,
      "verse_key": "56:6",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَكَانَتۡ هَبَآءٗ مُّنۢبَثّٗا",
      "translation": "Na εyε sε mfuturo a εretu hwete a,",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 534,
      "verse_key": "56:7",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَكُنتُمۡ أَزۡوَٰجٗا ثَلَٰثَةٗ",
      "translation": "(Saa Da no) mobεyε akuo ahodoͻ mmiεnsa.",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 534,
      "verse_key": "56:8",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَصۡحَٰبُ ٱلۡمَيۡمَنَةِ مَآ أَصۡحَٰبُ ٱلۡمَيۡمَنَةِ",
      "translation": "Wͻn a wͻyε Nifa so foͻ no; hwan nom ne wͻn a wͻyε Nifa so foͻ no ?",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 534,
      "verse_key": "56:9",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَصۡحَٰبُ ٱلۡمَشۡـَٔمَةِ مَآ أَصۡحَٰبُ ٱلۡمَشۡـَٔمَةِ",
      "translation": "Ɛne wͻn a wͻyε Benkum so foͻ no; hwan nom ne wͻn a wͻyε benkum so foͻ no?",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 534,
      "verse_key": "56:10",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلسَّٰبِقُونَ ٱلسَّٰبِقُونَ",
      "translation": "Ɛne (gyidie ho nwuma mu) adikanfoͻ no; wͻn ne (daakye no mu) adikanfoͻ.",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 534,
      "verse_key": "56:11",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أُوْلَٰٓئِكَ ٱلۡمُقَرَّبُونَ",
      "translation": "Woinom na wͻbεbεn (Nyankopͻn),",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 534,
      "verse_key": "56:12",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِي جَنَّٰتِ ٱلنَّعِيمِ",
      "translation": "Wͻ Anigyeε Aheman no mu.",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 534,
      "verse_key": "56:13",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُلَّةٞ مِّنَ ٱلۡأَوَّلِينَ",
      "translation": "Dodoͻ no bɛfiri tetefoͻ no mu,",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 534,
      "verse_key": "56:14",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَقَلِيلٞ مِّنَ ٱلۡأٓخِرِينَ",
      "translation": "Ԑna kakraabi nso bɛfiri nkyirimma no mu.",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 534,
      "verse_key": "56:15",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلَىٰ سُرُرٖ مَّوۡضُونَةٖ",
      "translation": "Wͻbεtena ahennwa a yεde sika kͻkͻͻ ne dwetε na anwene so.",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 534,
      "verse_key": "56:16",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مُّتَّكِـِٔينَ عَلَيۡهَا مُتَقَٰبِلِينَ",
      "translation": "Wͻbεgyegye ada mu ahwε wͻn ho wͻn ho anim.",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 535,
      "verse_key": "56:17",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَطُوفُ عَلَيۡهِمۡ وِلۡدَٰنٞ مُّخَلَّدُونَ",
      "translation": "Mmeranteε a wͻte nkwa mu daa na wͻnnwu da bɛdi wͻn hͻ akͻneaba (asom wͻn),",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 535,
      "verse_key": "56:18",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بِأَكۡوَابٖ وَأَبَارِيقَ وَكَأۡسٖ مِّن مَّعِينٖ",
      "translation": "Wͻde nkoraa ne sanya nkuruwaa ne kyansee a nsa korͻgyee wͻ mu.",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 535,
      "verse_key": "56:19",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَّا يُصَدَّعُونَ عَنۡهَا وَلَا يُنزِفُونَ",
      "translation": "Wͻn ti mmpae wͻn, na wͻmmoro nso,",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 535,
      "verse_key": "56:20",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَفَٰكِهَةٖ مِّمَّا يَتَخَيَّرُونَ",
      "translation": "Ne nnuaba biara a wͻn ara bεhwε mu ayi deε wͻpε.",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 535,
      "verse_key": "56:21",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَحۡمِ طَيۡرٖ مِّمَّا يَشۡتَهُونَ",
      "translation": "Ne nnomaa nnam biara a wͻn kͻn dͻ.",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 535,
      "verse_key": "56:22",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَحُورٌ عِينٞ",
      "translation": "Ne (mmaa) akronkron ahoͻfεfoͻ a wͻn ani akεse-akεse,",
      "footnotes": ""
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 535,
      "verse_key": "56:23",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَأَمۡثَٰلِ ٱللُّؤۡلُوِٕ ٱلۡمَكۡنُونِ",
      "translation": "(Wͻn ho fε) tesε nhwene pa bi a yɛahyε da de asie abͻ ho ban.",
      "footnotes": ""
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 535,
      "verse_key": "56:24",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "جَزَآءَۢ بِمَا كَانُواْ يَعۡمَلُونَ",
      "translation": "(Woi) yε akatua ma dwuma (pa) a wͻdiiε no.",
      "footnotes": ""
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 535,
      "verse_key": "56:25",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَا يَسۡمَعُونَ فِيهَا لَغۡوٗا وَلَا تَأۡثِيمًا",
      "translation": "Wͻnte nsεm hunu anaa kasa bͻne biara wͻ hͻ.",
      "footnotes": ""
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 535,
      "verse_key": "56:26",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَّا قِيلٗا سَلَٰمٗا سَلَٰمٗا",
      "translation": "Gyesε ‘Salaaman Salaaman’ Asomdweε! Asomdweε!",
      "footnotes": ""
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 535,
      "verse_key": "56:27",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَصۡحَٰبُ ٱلۡيَمِينِ مَآ أَصۡحَٰبُ ٱلۡيَمِينِ",
      "translation": "Ɛna Nifasofoͻ no; hwan nom ne Nifaso foͻ no?",
      "footnotes": ""
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 535,
      "verse_key": "56:28",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِي سِدۡرٖ مَّخۡضُودٖ",
      "translation": "(Wͻn wͻ) nnua bi a εho yε trͻmtrͻm a nkasεε nni ho bi mu,",
      "footnotes": ""
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 535,
      "verse_key": "56:29",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَطَلۡحٖ مَّنضُودٖ",
      "translation": "Ne nkwadu a abͻ esa-esa,",
      "footnotes": ""
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 535,
      "verse_key": "56:30",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَظِلّٖ مَّمۡدُودٖ",
      "translation": "Ne nwunu a εbae trε mu,",
      "footnotes": ""
    },
    {
      "id": 31,
      "verse_number": 31,
      "page_number": 535,
      "verse_key": "56:31",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَآءٖ مَّسۡكُوبٖ",
      "translation": "Ne nsuo a εtene ba (abrε biara),",
      "footnotes": ""
    },
    {
      "id": 32,
      "verse_number": 32,
      "page_number": 535,
      "verse_key": "56:32",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَفَٰكِهَةٖ كَثِيرَةٖ",
      "translation": "Ne nnuaba a abu soͻ,",
      "footnotes": ""
    },
    {
      "id": 33,
      "verse_number": 33,
      "page_number": 535,
      "verse_key": "56:33",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَّا مَقۡطُوعَةٖ وَلَا مَمۡنُوعَةٖ",
      "translation": "(Nnuaba no berε) to ntwa, na yɛnsi wͻn ho kwan nso.",
      "footnotes": ""
    },
    {
      "id": 34,
      "verse_number": 34,
      "page_number": 535,
      "verse_key": "56:34",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَفُرُشٖ مَّرۡفُوعَةٍ",
      "translation": "(Wͻn) wͻ aban soroͻ a εwͻ soro so.",
      "footnotes": ""
    },
    {
      "id": 35,
      "verse_number": 35,
      "page_number": 535,
      "verse_key": "56:35",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّآ أَنشَأۡنَٰهُنَّ إِنشَآءٗ",
      "translation": "Nokorε sε Yɛabͻ wͻn (mmeͻε foforͻ) paa,",
      "footnotes": ""
    },
    {
      "id": 36,
      "verse_number": 36,
      "page_number": 535,
      "verse_key": "56:36",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَجَعَلۡنَٰهُنَّ أَبۡكَارًا",
      "translation": "Ɛna Yɛama wͻn ayε mmabunu,",
      "footnotes": ""
    },
    {
      "id": 37,
      "verse_number": 37,
      "page_number": 535,
      "verse_key": "56:37",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عُرُبًا أَتۡرَابٗا",
      "translation": "Ɔdͻ ne ahomeka (wͻ wͻn mu); na wͻn (nyinaa) bͻ atipεn.",
      "footnotes": ""
    },
    {
      "id": 38,
      "verse_number": 38,
      "page_number": 535,
      "verse_key": "56:38",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لِّأَصۡحَٰبِ ٱلۡيَمِينِ",
      "translation": "(Woi) wͻ Nifasofoͻ no.",
      "footnotes": ""
    },
    {
      "id": 39,
      "verse_number": 39,
      "page_number": 535,
      "verse_key": "56:39",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُلَّةٞ مِّنَ ٱلۡأَوَّلِينَ",
      "translation": "Dodoͻ no bɛfiri tetefoͻ no mu,",
      "footnotes": ""
    },
    {
      "id": 40,
      "verse_number": 40,
      "page_number": 535,
      "verse_key": "56:40",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَثُلَّةٞ مِّنَ ٱلۡأٓخِرِينَ",
      "translation": "Na dodoͻ no nso afiri nkyirimma yi mu.",
      "footnotes": ""
    },
    {
      "id": 41,
      "verse_number": 41,
      "page_number": 535,
      "verse_key": "56:41",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَصۡحَٰبُ ٱلشِّمَالِ مَآ أَصۡحَٰبُ ٱلشِّمَالِ",
      "translation": "Ɛna Benkum so foͻ no; hwan nom ne Benkum so foͻ no?",
      "footnotes": ""
    },
    {
      "id": 42,
      "verse_number": 42,
      "page_number": 535,
      "verse_key": "56:42",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِي سَمُومٖ وَحَمِيمٖ",
      "translation": "(Wͻn) wͻ mframa hyew ne nsuo hyeε mu,",
      "footnotes": ""
    },
    {
      "id": 43,
      "verse_number": 43,
      "page_number": 535,
      "verse_key": "56:43",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَظِلّٖ مِّن يَحۡمُومٖ",
      "translation": "Ne wisie tumm nwunu mu,",
      "footnotes": ""
    },
    {
      "id": 44,
      "verse_number": 44,
      "page_number": 535,
      "verse_key": "56:44",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَّا بَارِدٖ وَلَا كَرِيمٍ",
      "translation": "(Saa wisie no) nyε nwunu na (wͻnnya emu) anigyeε anaa papa biara.",
      "footnotes": ""
    },
    {
      "id": 45,
      "verse_number": 45,
      "page_number": 535,
      "verse_key": "56:45",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُمۡ كَانُواْ قَبۡلَ ذَٰلِكَ مُتۡرَفِينَ",
      "translation": "Nokorε sε, kane no na wͻgye wͻn ani,",
      "footnotes": ""
    },
    {
      "id": 46,
      "verse_number": 46,
      "page_number": 535,
      "verse_key": "56:46",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَكَانُواْ يُصِرُّونَ عَلَى ٱلۡحِنثِ ٱلۡعَظِيمِ",
      "translation": "Na wͻtͻ kͻ bͻne akεseε so.",
      "footnotes": ""
    },
    {
      "id": 47,
      "verse_number": 47,
      "page_number": 535,
      "verse_key": "56:47",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَكَانُواْ يَقُولُونَ أَئِذَا مِتۡنَا وَكُنَّا تُرَابٗا وَعِظَٰمًا أَءِنَّا لَمَبۡعُوثُونَ",
      "translation": "Na wͻka sε: “Sε yɛwu na yεyε mfuturo ne nnompe a, nokorε sε yεbεsan anyane yεn?",
      "footnotes": ""
    },
    {
      "id": 48,
      "verse_number": 48,
      "page_number": 535,
      "verse_key": "56:48",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَوَءَابَآؤُنَا ٱلۡأَوَّلُونَ",
      "translation": "Anaasε yεn agyanom tetefoͻ no (nso)?",
      "footnotes": ""
    },
    {
      "id": 49,
      "verse_number": 49,
      "page_number": 535,
      "verse_key": "56:49",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قُلۡ إِنَّ ٱلۡأَوَّلِينَ وَٱلۡأٓخِرِينَ",
      "translation": "(Nkͻmhyεni), ka sε: “Tetefoͻ no ne nkyirimma no (nyinaa),",
      "footnotes": ""
    },
    {
      "id": 50,
      "verse_number": 50,
      "page_number": 535,
      "verse_key": "56:50",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَمَجۡمُوعُونَ إِلَىٰ مِيقَٰتِ يَوۡمٖ مَّعۡلُومٖ",
      "translation": "Nokorε sε yεbεboa wͻn ano akͻ εberε a yɛahyε no wͻ saa Da no mu no“.",
      "footnotes": ""
    },
    {
      "id": 51,
      "verse_number": 51,
      "page_number": 536,
      "verse_key": "56:51",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ إِنَّكُمۡ أَيُّهَا ٱلضَّآلُّونَ ٱلۡمُكَذِّبُونَ",
      "translation": "Afei mo ayerafoͻ no a mofaa (nokorε no) nkontompo no,",
      "footnotes": ""
    },
    {
      "id": 52,
      "verse_number": 52,
      "page_number": 536,
      "verse_key": "56:52",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَأٓكِلُونَ مِن شَجَرٖ مِّن زَقُّومٖ",
      "translation": "Mobɛdidi afiri dua Zaqqum so,",
      "footnotes": ""
    },
    {
      "id": 53,
      "verse_number": 53,
      "page_number": 536,
      "verse_key": "56:53",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَمَالِـُٔونَ مِنۡهَا ٱلۡبُطُونَ",
      "translation": "Na mode ahyε (mo) afru ma,",
      "footnotes": ""
    },
    {
      "id": 54,
      "verse_number": 54,
      "page_number": 536,
      "verse_key": "56:54",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَشَٰرِبُونَ عَلَيۡهِ مِنَ ٱلۡحَمِيمِ",
      "translation": "Na moanom nsuo hyeε agu so;",
      "footnotes": ""
    },
    {
      "id": 55,
      "verse_number": 55,
      "page_number": 536,
      "verse_key": "56:55",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَشَٰرِبُونَ شُرۡبَ ٱلۡهِيمِ",
      "translation": "Na (mo) bεnom no sεdeε yoma a sukͻm de no renom nsuo.",
      "footnotes": ""
    },
    {
      "id": 56,
      "verse_number": 56,
      "page_number": 536,
      "verse_key": "56:56",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "هَٰذَا نُزُلُهُمۡ يَوۡمَ ٱلدِّينِ",
      "translation": "Woi na εbεyε wͻn brεsuo a yεde bεma wͻn akwaaba wͻ (daakye) akatua Da no.",
      "footnotes": ""
    },
    {
      "id": 57,
      "verse_number": 57,
      "page_number": 536,
      "verse_key": "56:57",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "نَحۡنُ خَلَقۡنَٰكُمۡ فَلَوۡلَا تُصَدِّقُونَ",
      "translation": "Yεn na Yεbͻͻ mo na afei adεn nti na monye nnie?",
      "footnotes": ""
    },
    {
      "id": 58,
      "verse_number": 58,
      "page_number": 536,
      "verse_key": "56:58",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَرَءَيۡتُم مَّا تُمۡنُونَ",
      "translation": "Moahunu (ahobaeε) a mohwie gu (ͻbaa awodeε mu) no?",
      "footnotes": ""
    },
    {
      "id": 59,
      "verse_number": 59,
      "page_number": 536,
      "verse_key": "56:59",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ءَأَنتُمۡ تَخۡلُقُونَهُۥٓ أَمۡ نَحۡنُ ٱلۡخَٰلِقُونَ",
      "translation": "Mo na mobͻ (akͻdaa no) anaasε Yε ne Ɔbͻͻ Adeε no?",
      "footnotes": ""
    },
    {
      "id": 60,
      "verse_number": 60,
      "page_number": 536,
      "verse_key": "56:60",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "نَحۡنُ قَدَّرۡنَا بَيۡنَكُمُ ٱلۡمَوۡتَ وَمَا نَحۡنُ بِمَسۡبُوقِينَ",
      "translation": "Yεn na Yɛahyehyε owuo de ato mo soͻ; na entumi mmu mfa Yεn so-",
      "footnotes": ""
    },
    {
      "id": 61,
      "verse_number": 61,
      "page_number": 536,
      "verse_key": "56:61",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلَىٰٓ أَن نُّبَدِّلَ أَمۡثَٰلَكُمۡ وَنُنشِئَكُمۡ فِي مَا لَا تَعۡلَمُونَ",
      "translation": "-Sε Yεbεsesa mo tebea yi abͻ mo akͻ (tebea foforͻ) a mo nnim mu.",
      "footnotes": ""
    },
    {
      "id": 62,
      "verse_number": 62,
      "page_number": 536,
      "verse_key": "56:62",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَقَدۡ عَلِمۡتُمُ ٱلنَّشۡأَةَ ٱلۡأُولَىٰ فَلَوۡلَا تَذَكَّرُونَ",
      "translation": "Ampa sε, monim mmͻeε a edi kan no; afei adεn na monfa nkͻ adwen-dwene mu?",
      "footnotes": ""
    },
    {
      "id": 63,
      "verse_number": 63,
      "page_number": 536,
      "verse_key": "56:63",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَرَءَيۡتُم مَّا تَحۡرُثُونَ",
      "translation": "Moahunu (nneεma) a modua no?",
      "footnotes": ""
    },
    {
      "id": 64,
      "verse_number": 64,
      "page_number": 536,
      "verse_key": "56:64",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ءَأَنتُمۡ تَزۡرَعُونَهُۥٓ أَمۡ نَحۡنُ ٱلزَّٰرِعُونَ",
      "translation": "Mo na moma no fifiri na enyin, anaasε Yεn na Yεma no fifiri na enyin?",
      "footnotes": ""
    },
    {
      "id": 65,
      "verse_number": 65,
      "page_number": 536,
      "verse_key": "56:65",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَوۡ نَشَآءُ لَجَعَلۡنَٰهُ حُطَٰمٗا فَظَلۡتُمۡ تَفَكَّهُونَ",
      "translation": "Sε Yεpε a anka Yεbεma no ayam (anaasε Yεbεma no asεe) ama mo atwa adwo-",
      "footnotes": ""
    },
    {
      "id": 66,
      "verse_number": 66,
      "page_number": 536,
      "verse_key": "56:66",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّا لَمُغۡرَمُونَ",
      "translation": "(Ama moasu sε): “Nokorε yɛabͻ ka.”",
      "footnotes": ""
    },
    {
      "id": 67,
      "verse_number": 67,
      "page_number": 536,
      "verse_key": "56:67",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بَلۡ نَحۡنُ مَحۡرُومُونَ",
      "translation": "Aane, yɛahwere (y’agyapadeε).",
      "footnotes": ""
    },
    {
      "id": 68,
      "verse_number": 68,
      "page_number": 536,
      "verse_key": "56:68",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَرَءَيۡتُمُ ٱلۡمَآءَ ٱلَّذِي تَشۡرَبُونَ",
      "translation": "Moahunu nsuo a monom no?",
      "footnotes": ""
    },
    {
      "id": 69,
      "verse_number": 69,
      "page_number": 536,
      "verse_key": "56:69",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ءَأَنتُمۡ أَنزَلۡتُمُوهُ مِنَ ٱلۡمُزۡنِ أَمۡ نَحۡنُ ٱلۡمُنزِلُونَ",
      "translation": "Mo na mosiane firi mununkum no mu ba, anaasε Yεn na Yɛsiane ba?",
      "footnotes": ""
    },
    {
      "id": 70,
      "verse_number": 70,
      "page_number": 536,
      "verse_key": "56:70",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَوۡ نَشَآءُ جَعَلۡنَٰهُ أُجَاجٗا فَلَوۡلَا تَشۡكُرُونَ",
      "translation": "Sε Yεpε a anka Yεbεma no ayε nkyene-nkyene. Adεn na afei monna (Nyankopͻn) aseε?",
      "footnotes": ""
    },
    {
      "id": 71,
      "verse_number": 71,
      "page_number": 536,
      "verse_key": "56:71",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَرَءَيۡتُمُ ٱلنَّارَ ٱلَّتِي تُورُونَ",
      "translation": "Moahunu egya a mosͻ no?",
      "footnotes": ""
    },
    {
      "id": 72,
      "verse_number": 72,
      "page_number": 536,
      "verse_key": "56:72",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ءَأَنتُمۡ أَنشَأۡتُمۡ شَجَرَتَهَآ أَمۡ نَحۡنُ ٱلۡمُنشِـُٔونَ",
      "translation": "Mona mobͻ ne nnua no anaasε Yεn na Yεbͻeε?",
      "footnotes": ""
    },
    {
      "id": 73,
      "verse_number": 73,
      "page_number": 536,
      "verse_key": "56:73",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "نَحۡنُ جَعَلۡنَٰهَا تَذۡكِرَةٗ وَمَتَٰعٗا لِّلۡمُقۡوِينَ",
      "translation": "Yɛayε egya no nkaeε (a mode bεkae Amanehunu gya no), εsan nso wͻ mfasoͻ de ma onipa biara a ohia gya, ne εserε so akwantufoͻ no.",
      "footnotes": ""
    },
    {
      "id": 74,
      "verse_number": 74,
      "page_number": 536,
      "verse_key": "56:74",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَسَبِّحۡ بِٱسۡمِ رَبِّكَ ٱلۡعَظِيمِ",
      "translation": "Enti bͻ wo Wura Nyankopͻn Kεseε no abodin.",
      "footnotes": ""
    },
    {
      "id": 75,
      "verse_number": 75,
      "page_number": 536,
      "verse_key": "56:75",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "۞ فَلَآ أُقۡسِمُ بِمَوَٰقِعِ ٱلنُّجُومِ",
      "translation": "Mede faako a nsoromma kͻtͻ di nse!",
      "footnotes": ""
    },
    {
      "id": 76,
      "verse_number": 76,
      "page_number": 536,
      "verse_key": "56:76",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِنَّهُۥ لَقَسَمٞ لَّوۡ تَعۡلَمُونَ عَظِيمٌ",
      "translation": "Nokorε sε, εyε nsedie a εso paa sε anka monim a.",
      "footnotes": ""
    },
    {
      "id": 77,
      "verse_number": 77,
      "page_number": 537,
      "verse_key": "56:77",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُۥ لَقُرۡءَانٞ كَرِيمٞ",
      "translation": "Nokorε sε, εyε Qur’aan animuonyam (Nwoma) noa,",
      "footnotes": ""
    },
    {
      "id": 78,
      "verse_number": 78,
      "page_number": 537,
      "verse_key": "56:78",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِي كِتَٰبٖ مَّكۡنُونٖ",
      "translation": "Ɛwͻ Nwoma a yεabͻ ho ban no mu.",
      "footnotes": ""
    },
    {
      "id": 79,
      "verse_number": 79,
      "page_number": 537,
      "verse_key": "56:79",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَّا يَمَسُّهُۥٓ إِلَّا ٱلۡمُطَهَّرُونَ",
      "translation": "Abᴐfoᴐ ahoteefoᴐ no na wᴐsᴐ mu.",
      "footnotes": ""
    },
    {
      "id": 80,
      "verse_number": 80,
      "page_number": 537,
      "verse_key": "56:80",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تَنزِيلٞ مِّن رَّبِّ ٱلۡعَٰلَمِينَ",
      "translation": "(Qur’aan) yε Adiyisεm a efri Abͻdeε nyinaa Wura Nyankopͻn no hͻ.",
      "footnotes": ""
    },
    {
      "id": 81,
      "verse_number": 81,
      "page_number": 537,
      "verse_key": "56:81",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَبِهَٰذَا ٱلۡحَدِيثِ أَنتُم مُّدۡهِنُونَ",
      "translation": "Enti saa Asεm yi na mobu no animtia?",
      "footnotes": ""
    },
    {
      "id": 82,
      "verse_number": 82,
      "page_number": 537,
      "verse_key": "56:82",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَتَجۡعَلُونَ رِزۡقَكُمۡ أَنَّكُمۡ تُكَذِّبُونَ",
      "translation": "Ɛna moayε po a mopo (Qur’aan no sε εnam so) na monya mo akͻnhoma?",
      "footnotes": ""
    },
    {
      "id": 83,
      "verse_number": 83,
      "page_number": 537,
      "verse_key": "56:83",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلَوۡلَآ إِذَا بَلَغَتِ ٱلۡحُلۡقُومَ",
      "translation": "Afei adεn na sε (obi a ͻrewuo nkwa no) duru ne mene mu a (monsi nkwa no kwan)?",
      "footnotes": ""
    },
    {
      "id": 84,
      "verse_number": 84,
      "page_number": 537,
      "verse_key": "56:84",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنتُمۡ حِينَئِذٖ تَنظُرُونَ",
      "translation": "Na saa berε no na morehwε.",
      "footnotes": ""
    },
    {
      "id": 85,
      "verse_number": 85,
      "page_number": 537,
      "verse_key": "56:85",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَنَحۡنُ أَقۡرَبُ إِلَيۡهِ مِنكُمۡ وَلَٰكِن لَّا تُبۡصِرُونَ",
      "translation": "(Saa berε no) Yεbεn no kyεn mo, nanso monhunu.",
      "footnotes": ""
    },
    {
      "id": 86,
      "verse_number": 86,
      "page_number": 537,
      "verse_key": "56:86",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلَوۡلَآ إِن كُنتُمۡ غَيۡرَ مَدِينِينَ",
      "translation": "Sε anka monhyε tumi bi ase a mobɛbu akonta a,",
      "footnotes": ""
    },
    {
      "id": 87,
      "verse_number": 87,
      "page_number": 537,
      "verse_key": "56:87",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تَرۡجِعُونَهَآ إِن كُنتُمۡ صَٰدِقِينَ",
      "translation": "Adεn na momma (nkwa) no nsan n’akyi (nkͻ onipa no mu bio) sε moyε nokwafoͻ ampa a?",
      "footnotes": ""
    },
    {
      "id": 88,
      "verse_number": 88,
      "page_number": 537,
      "verse_key": "56:88",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَمَّآ إِن كَانَ مِنَ ٱلۡمُقَرَّبِينَ",
      "translation": "Enti sε (saa nipa no) ka wͻn a wͻbεn (Nyankopͻn) no ho a,",
      "footnotes": ""
    },
    {
      "id": 89,
      "verse_number": 89,
      "page_number": 537,
      "verse_key": "56:89",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَرَوۡحٞ وَرَيۡحَانٞ وَجَنَّتُ نَعِيمٖ",
      "translation": "(Ɔbɛnya) ahomegyeε, ne akͻnhoma ne Anigyeε Aheman no.",
      "footnotes": ""
    },
    {
      "id": 90,
      "verse_number": 90,
      "page_number": 537,
      "verse_key": "56:90",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَمَّآ إِن كَانَ مِنۡ أَصۡحَٰبِ ٱلۡيَمِينِ",
      "translation": "Na sε ͻka Nifasofoͻ no nso ho a,",
      "footnotes": ""
    },
    {
      "id": 91,
      "verse_number": 91,
      "page_number": 537,
      "verse_key": "56:91",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَسَلَٰمٞ لَّكَ مِنۡ أَصۡحَٰبِ ٱلۡيَمِينِ",
      "translation": "Ɛneε asomdwoeε nka wo, Nifasofoͻ no.",
      "footnotes": ""
    },
    {
      "id": 92,
      "verse_number": 92,
      "page_number": 537,
      "verse_key": "56:92",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَمَّآ إِن كَانَ مِنَ ٱلۡمُكَذِّبِينَ ٱلضَّآلِّينَ",
      "translation": "Na mmom sε ͻka wͻn a wͻpo nokorԑ, ayerafoͻ no ho nso a,",
      "footnotes": ""
    },
    {
      "id": 93,
      "verse_number": 93,
      "page_number": 537,
      "verse_key": "56:93",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَنُزُلٞ مِّنۡ حَمِيمٖ",
      "translation": "Ɛneε nsuo-hyeε na yεde bεma no brεsuo,",
      "footnotes": ""
    },
    {
      "id": 94,
      "verse_number": 94,
      "page_number": 537,
      "verse_key": "56:94",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَتَصۡلِيَةُ جَحِيمٍ",
      "translation": "Na yɛakͻhye no wͻ Amanehunu gya no mu.",
      "footnotes": ""
    },
    {
      "id": 95,
      "verse_number": 95,
      "page_number": 537,
      "verse_key": "56:95",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ هَٰذَا لَهُوَ حَقُّ ٱلۡيَقِينِ",
      "translation": "Nokorε sε, ampa woi ne Nokorε pefee no.",
      "footnotes": ""
    },
    {
      "id": 96,
      "verse_number": 96,
      "page_number": 537,
      "verse_key": "56:96",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَسَبِّحۡ بِٱسۡمِ رَبِّكَ ٱلۡعَظِيمِ",
      "translation": "Enti bͻ wo Wura Nyankopͻn Kεseε no abodin tontom no.",
      "footnotes": ""
    }
  ]
}