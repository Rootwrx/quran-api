{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 602,
      "verse_key": "106:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لِإِيلَٰفِ قُرَيۡشٍ",
      "translation": "Deε akokwa Quraishfoͻ no,",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 602,
      "verse_key": "106:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِۦلَٰفِهِمۡ رِحۡلَةَ ٱلشِّتَآءِ وَٱلصَّيۡفِ",
      "translation": "Deε akokwa wͻn a εne akwantuo a wͻtu no wͻ awͻ berε ne ahuhu berε mu no nti,",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 602,
      "verse_key": "106:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلۡيَعۡبُدُواْ رَبَّ هَٰذَا ٱلۡبَيۡتِ",
      "translation": "Wͻnsom Efie wei (Ka’ba) Wura.",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 602,
      "verse_key": "106:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِيٓ أَطۡعَمَهُم مِّن جُوعٖ وَءَامَنَهُم مِّنۡ خَوۡفِۭ",
      "translation": "(Nyankopͻn) no a, Ɔbͻ wͻn akͻnhoma fri ͻkͻm mu, na Ɔma wͻn akomatͻyamu firi ehuu mu no.",
      "footnotes": ""
    }
  ]
}