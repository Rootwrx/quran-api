{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 602,
      "verse_key": "107:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَرَءَيۡتَ ٱلَّذِي يُكَذِّبُ بِٱلدِّينِ",
      "translation": "Znaš li ti onoga koji Sud poriče? ",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 602,
      "verse_key": "107:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَذَٰلِكَ ٱلَّذِي يَدُعُّ ٱلۡيَتِيمَ",
      "translation": "Pa to je onaj koji grubo odbija siroče. ",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 602,
      "verse_key": "107:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَا يَحُضُّ عَلَىٰ طَعَامِ ٱلۡمِسۡكِينِ",
      "translation": "i koji da se nahrani siromah - ne podstiče. ",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 602,
      "verse_key": "107:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَوَيۡلٞ لِّلۡمُصَلِّينَ",
      "translation": "Pa teško klanjačima, ",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 602,
      "verse_key": "107:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِينَ هُمۡ عَن صَلَاتِهِمۡ سَاهُونَ",
      "translation": "onima koji su nemarni prema svome namazu.",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 602,
      "verse_key": "107:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِينَ هُمۡ يُرَآءُونَ",
      "translation": "koji se samo ljudima pokazuju  ",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 602,
      "verse_key": "107:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيَمۡنَعُونَ ٱلۡمَاعُونَ",
      "translation": "čak potrepštine koje se na posudbu daju oni ne daju!",
      "footnotes": ""
    }
  ]
}