{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 603,
      "verse_key": "111:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تَبَّتۡ يَدَآ أَبِي لَهَبٖ وَتَبَّ",
      "translation": "Yarahonye Abu Lahabi, aragahona ku kubera ukubangamira kwiwe Intumwa Muhamadi (nyakugira amahoro n’impuhwe z’Imana Allah).",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 603,
      "verse_key": "111:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَآ أَغۡنَىٰ عَنۡهُ مَالُهُۥ وَمَا كَسَبَ",
      "translation": "Amatungo yiwe, eka n’abana biwe, nta co bizomumarira ngo bimukingire ibihano vy’Imana Allah.",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 603,
      "verse_key": "111:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "سَيَصۡلَىٰ نَارٗا ذَاتَ لَهَبٖ",
      "translation": "Azokwinjira mu muriro ururumba cane, maze awote;",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 603,
      "verse_key": "111:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱمۡرَأَتُهُۥ حَمَّالَةَ ٱلۡحَطَبِ",
      "translation": "Ari kumwe n’umugore wiwe (Ummu Jamiila), yahora yikorera amahwa akayatega mu nzira Intumwa y’Imana (nyakugira amahoro n’impuhwe z’Imana Allah) icamwo;",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 603,
      "verse_key": "111:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِي جِيدِهَا حَبۡلٞ مِّن مَّسَدِۭ",
      "translation": "Uyo mukenyezi, azotabwa mu muriro aboheshejwe mw’izosi ikiziriko gitsivye neza.",
      "footnotes": ""
    }
  ]
}