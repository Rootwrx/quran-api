{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 575,
      "verse_key": "74:1",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَٰٓأَيُّهَا ٱلۡمُدَّثِّرُ",
      "translation": "Эй, бурканиб ётган!",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 575,
      "verse_key": "74:2",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قُمۡ فَأَنذِرۡ",
      "translation": "Туринг ва огоҳлантиринг!",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 575,
      "verse_key": "74:3",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَرَبَّكَ فَكَبِّرۡ",
      "translation": "Парвардигорингизни улуғланг!",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 575,
      "verse_key": "74:4",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَثِيَابَكَ فَطَهِّرۡ",
      "translation": "Либосларингизни покланг!",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 575,
      "verse_key": "74:5",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلرُّجۡزَ فَٱهۡجُرۡ",
      "translation": "Бутлардан йироқ бўлинг!",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 575,
      "verse_key": "74:6",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَا تَمۡنُن تَسۡتَكۡثِرُ",
      "translation": "Кўп санаб, миннат қилманг.",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 575,
      "verse_key": "74:7",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلِرَبِّكَ فَٱصۡبِرۡ",
      "translation": "Парвардигорингиз учун сабр қилинг.",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 575,
      "verse_key": "74:8",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِذَا نُقِرَ فِي ٱلنَّاقُورِ",
      "translation": "Бурғу чалинганида.",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 575,
      "verse_key": "74:9",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَذَٰلِكَ يَوۡمَئِذٖ يَوۡمٌ عَسِيرٌ",
      "translation": "Ана ўша кун қийин кундир!",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 575,
      "verse_key": "74:10",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلَى ٱلۡكَٰفِرِينَ غَيۡرُ يَسِيرٖ",
      "translation": "Кофирларга осон бўлмаган (кундир)!",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 575,
      "verse_key": "74:11",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ذَرۡنِي وَمَنۡ خَلَقۡتُ وَحِيدٗا",
      "translation": "Мен якка ҳолида яратган кимсани Ўзимга қўйиб беринг.",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 575,
      "verse_key": "74:12",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَجَعَلۡتُ لَهُۥ مَالٗا مَّمۡدُودٗا",
      "translation": "Унга мўл-кўл мол-давлат бердим.",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 575,
      "verse_key": "74:13",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَبَنِينَ شُهُودٗا",
      "translation": "Ва ҳозиру нозир фарзандлар ҳам.",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 575,
      "verse_key": "74:14",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَهَّدتُّ لَهُۥ تَمۡهِيدٗا",
      "translation": "Унга осонлаштириб қўйдим.",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 575,
      "verse_key": "74:15",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ يَطۡمَعُ أَنۡ أَزِيدَ",
      "translation": "Кейин у янада зиёда қилишимни тама қилур.",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 575,
      "verse_key": "74:16",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّآۖ إِنَّهُۥ كَانَ لِأٓيَٰتِنَا عَنِيدٗا",
      "translation": "Асло! У Бизнинг оятларимизга қаршилик қилгувчи эди.",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 575,
      "verse_key": "74:17",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "سَأُرۡهِقُهُۥ صَعُودًا",
      "translation": "Яқинда Мен уни бир машаққатга дучор қиламан.",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 576,
      "verse_key": "74:18",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُۥ فَكَّرَ وَقَدَّرَ",
      "translation": "Дарҳақиқат, у ўйлади ва режа тузди.",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 576,
      "verse_key": "74:19",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَقُتِلَ كَيۡفَ قَدَّرَ",
      "translation": "Ҳалок қилингур, қандай режа тузди-я?!",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 576,
      "verse_key": "74:20",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ قُتِلَ كَيۡفَ قَدَّرَ",
      "translation": "Яна ҳалок қилингур, қандай режа тузди-я?!",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 576,
      "verse_key": "74:21",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ نَظَرَ",
      "translation": "Кейин назар солди.",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 576,
      "verse_key": "74:22",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ عَبَسَ وَبَسَرَ",
      "translation": "Кейин афтини буриштирди ва қовоғини солди.",
      "footnotes": ""
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 576,
      "verse_key": "74:23",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ أَدۡبَرَ وَٱسۡتَكۡبَرَ",
      "translation": "Кейин юз ўгирди ва мутакаббирлик қилди.",
      "footnotes": ""
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 576,
      "verse_key": "74:24",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَقَالَ إِنۡ هَٰذَآ إِلَّا سِحۡرٞ يُؤۡثَرُ",
      "translation": "Деди: «Албатта, бу нақл қилинган сеҳрдан ўзга нарса эмас».",
      "footnotes": ""
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 576,
      "verse_key": "74:25",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنۡ هَٰذَآ إِلَّا قَوۡلُ ٱلۡبَشَرِ",
      "translation": "«Бу инсон сўзидан бошқа нарса эмас».",
      "footnotes": ""
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 576,
      "verse_key": "74:26",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "سَأُصۡلِيهِ سَقَرَ",
      "translation": "Яқинда уни Сақарга киритурман.",
      "footnotes": ""
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 576,
      "verse_key": "74:27",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَآ أَدۡرَىٰكَ مَا سَقَرُ",
      "translation": "Сақарнинг нима эканини сиз қаердан ҳам билар эдингиз.",
      "footnotes": ""
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 576,
      "verse_key": "74:28",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَا تُبۡقِي وَلَا تَذَرُ",
      "translation": "У на қолдиради ва на тарк этади.",
      "footnotes": ""
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 576,
      "verse_key": "74:29",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَوَّاحَةٞ لِّلۡبَشَرِ",
      "translation": "У терини қаттиқ куйдирувчидир.",
      "footnotes": ""
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 576,
      "verse_key": "74:30",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلَيۡهَا تِسۡعَةَ عَشَرَ",
      "translation": "Унинг устида ўн тўққизта бор.",
      "footnotes": ""
    },
    {
      "id": 31,
      "verse_number": 31,
      "page_number": 576,
      "verse_key": "74:31",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا جَعَلۡنَآ أَصۡحَٰبَ ٱلنَّارِ إِلَّا مَلَٰٓئِكَةٗۖ وَمَا جَعَلۡنَا عِدَّتَهُمۡ إِلَّا فِتۡنَةٗ لِّلَّذِينَ كَفَرُواْ لِيَسۡتَيۡقِنَ ٱلَّذِينَ أُوتُواْ ٱلۡكِتَٰبَ وَيَزۡدَادَ ٱلَّذِينَ ءَامَنُوٓاْ إِيمَٰنٗا وَلَا يَرۡتَابَ ٱلَّذِينَ أُوتُواْ ٱلۡكِتَٰبَ وَٱلۡمُؤۡمِنُونَ وَلِيَقُولَ ٱلَّذِينَ فِي قُلُوبِهِم مَّرَضٞ وَٱلۡكَٰفِرُونَ مَاذَآ أَرَادَ ٱللَّهُ بِهَٰذَا مَثَلٗاۚ كَذَٰلِكَ يُضِلُّ ٱللَّهُ مَن يَشَآءُ وَيَهۡدِي مَن يَشَآءُۚ وَمَا يَعۡلَمُ جُنُودَ رَبِّكَ إِلَّا هُوَۚ وَمَا هِيَ إِلَّا ذِكۡرَىٰ لِلۡبَشَرِ",
      "translation": "Дўзах эгалари - қўриқчиларини фақат фаришталар қилдик. Уларнинг сонини куфр келтирганлар учун синовдан ўзга нарса қилмадик. Токи китоб берилганлар аниқ билиб олсинлар ва иймон келтирганларнинг иймони янада зиёда бўлсин. Китоб берилганлар ҳам, мўминлар ҳам шубҳага бормасинлар. Қалбида марази бор кимсалар ва кофирлар эса: «Бу билан Аллоҳ нимани мисол қилмоқчи бўляпти», деб қолсинлар. Аллоҳ мана шундай Ўзи истаган одамни адаштиради ва Ўзи истаган одамни ҳидоят қилади. Парвардигорингизнинг лашкарларини Ўзидан ўзга ҳеч кимса билмас. У башарият учун бир эслатма, холос.",
      "footnotes": ""
    },
    {
      "id": 32,
      "verse_number": 32,
      "page_number": 576,
      "verse_key": "74:32",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّا وَٱلۡقَمَرِ",
      "translation": "Йўқ! Ойга қасам.",
      "footnotes": ""
    },
    {
      "id": 33,
      "verse_number": 33,
      "page_number": 576,
      "verse_key": "74:33",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّيۡلِ إِذۡ أَدۡبَرَ",
      "translation": "Ўтиб кетган тунга қасам.",
      "footnotes": ""
    },
    {
      "id": 34,
      "verse_number": 34,
      "page_number": 576,
      "verse_key": "74:34",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلصُّبۡحِ إِذَآ أَسۡفَرَ",
      "translation": "Ёришиб келаётган тонгга қасам.",
      "footnotes": ""
    },
    {
      "id": 35,
      "verse_number": 35,
      "page_number": 576,
      "verse_key": "74:35",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهَا لَإِحۡدَى ٱلۡكُبَرِ",
      "translation": "Дарҳақиқат, у улканларнинг биридир.",
      "footnotes": ""
    },
    {
      "id": 36,
      "verse_number": 36,
      "page_number": 576,
      "verse_key": "74:36",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "نَذِيرٗا لِّلۡبَشَرِ",
      "translation": "У башарият учун огоҳлантирувчидир.",
      "footnotes": ""
    },
    {
      "id": 37,
      "verse_number": 37,
      "page_number": 576,
      "verse_key": "74:37",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لِمَن شَآءَ مِنكُمۡ أَن يَتَقَدَّمَ أَوۡ يَتَأَخَّرَ",
      "translation": "Сизлардан пешқадам бўлишни ёки ортда қолишни хоҳлаётган кимсалар учундир.",
      "footnotes": ""
    },
    {
      "id": 38,
      "verse_number": 38,
      "page_number": 576,
      "verse_key": "74:38",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كُلُّ نَفۡسِۭ بِمَا كَسَبَتۡ رَهِينَةٌ",
      "translation": "Ҳар бир жон ўзи қилиб ўтган ишнинг гаровидир.",
      "footnotes": ""
    },
    {
      "id": 39,
      "verse_number": 39,
      "page_number": 576,
      "verse_key": "74:39",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَّآ أَصۡحَٰبَ ٱلۡيَمِينِ",
      "translation": "Ўнг тараф соҳиблари бундан мустаснодир.",
      "footnotes": ""
    },
    {
      "id": 40,
      "verse_number": 40,
      "page_number": 576,
      "verse_key": "74:40",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِي جَنَّٰتٖ يَتَسَآءَلُونَ",
      "translation": "Улар жаннатларда бир-бирларидан сўрайдилар.",
      "footnotes": ""
    },
    {
      "id": 41,
      "verse_number": 41,
      "page_number": 576,
      "verse_key": "74:41",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَنِ ٱلۡمُجۡرِمِينَ",
      "translation": "Жиноятчилар ҳақида.",
      "footnotes": ""
    },
    {
      "id": 42,
      "verse_number": 42,
      "page_number": 576,
      "verse_key": "74:42",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَا سَلَكَكُمۡ فِي سَقَرَ",
      "translation": "\"Сизларни Сақарга нима киритди?\"",
      "footnotes": ""
    },
    {
      "id": 43,
      "verse_number": 43,
      "page_number": 576,
      "verse_key": "74:43",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قَالُواْ لَمۡ نَكُ مِنَ ٱلۡمُصَلِّينَ",
      "translation": "Дедилар: «Намоз ўқигувчилардан бўлмагандик.",
      "footnotes": ""
    },
    {
      "id": 44,
      "verse_number": 44,
      "page_number": 576,
      "verse_key": "74:44",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَمۡ نَكُ نُطۡعِمُ ٱلۡمِسۡكِينَ",
      "translation": "Мискинга таом бермагандик.",
      "footnotes": ""
    },
    {
      "id": 45,
      "verse_number": 45,
      "page_number": 576,
      "verse_key": "74:45",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَكُنَّا نَخُوضُ مَعَ ٱلۡخَآئِضِينَ",
      "translation": "Шўнғувчилар билан бирга шўнғир эдик.",
      "footnotes": ""
    },
    {
      "id": 46,
      "verse_number": 46,
      "page_number": 576,
      "verse_key": "74:46",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَكُنَّا نُكَذِّبُ بِيَوۡمِ ٱلدِّينِ",
      "translation": "Қиёмат кунини ёлғонга чиқарар эдик.",
      "footnotes": ""
    },
    {
      "id": 47,
      "verse_number": 47,
      "page_number": 576,
      "verse_key": "74:47",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "حَتَّىٰٓ أَتَىٰنَا ٱلۡيَقِينُ",
      "translation": "То бизга ўлим келгунига қадар (шундай эдик)».",
      "footnotes": ""
    },
    {
      "id": 48,
      "verse_number": 48,
      "page_number": 577,
      "verse_key": "74:48",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَمَا تَنفَعُهُمۡ شَفَٰعَةُ ٱلشَّٰفِعِينَ",
      "translation": "Энди уларга оқловчиларнинг оқлови фойда бермас!",
      "footnotes": ""
    },
    {
      "id": 49,
      "verse_number": 49,
      "page_number": 577,
      "verse_key": "74:49",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَمَا لَهُمۡ عَنِ ٱلتَّذۡكِرَةِ مُعۡرِضِينَ",
      "translation": "Уларга нима бўлдики, эслатмадан юз ўгирадилар?",
      "footnotes": ""
    },
    {
      "id": 50,
      "verse_number": 50,
      "page_number": 577,
      "verse_key": "74:50",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَأَنَّهُمۡ حُمُرٞ مُّسۡتَنفِرَةٞ",
      "translation": "Бамисоли улар ёввойи эшаклар.",
      "footnotes": ""
    },
    {
      "id": 51,
      "verse_number": 51,
      "page_number": 577,
      "verse_key": "74:51",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَرَّتۡ مِن قَسۡوَرَةِۭ",
      "translation": "Арслондан қочгани каби.",
      "footnotes": ""
    },
    {
      "id": 52,
      "verse_number": 52,
      "page_number": 577,
      "verse_key": "74:52",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بَلۡ يُرِيدُ كُلُّ ٱمۡرِيٕٖ مِّنۡهُمۡ أَن يُؤۡتَىٰ صُحُفٗا مُّنَشَّرَةٗ",
      "translation": "Балки улардан ҳар бир киши ўзига очиқ саҳифалар берилишини истаётгандир.",
      "footnotes": ""
    },
    {
      "id": 53,
      "verse_number": 53,
      "page_number": 577,
      "verse_key": "74:53",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّاۖ بَل لَّا يَخَافُونَ ٱلۡأٓخِرَةَ",
      "translation": "Йўқ! Балки улар охиратдан қўрқмаслар.",
      "footnotes": ""
    },
    {
      "id": 54,
      "verse_number": 54,
      "page_number": 577,
      "verse_key": "74:54",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّآ إِنَّهُۥ تَذۡكِرَةٞ",
      "translation": "Йўқ! Албатта, у эслатмадир.",
      "footnotes": ""
    },
    {
      "id": 55,
      "verse_number": 55,
      "page_number": 577,
      "verse_key": "74:55",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَمَن شَآءَ ذَكَرَهُۥ",
      "translation": "Бас, ким истаса, ундан эслатма-ибрат олур.",
      "footnotes": ""
    },
    {
      "id": 56,
      "verse_number": 56,
      "page_number": 577,
      "verse_key": "74:56",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا يَذۡكُرُونَ إِلَّآ أَن يَشَآءَ ٱللَّهُۚ هُوَ أَهۡلُ ٱلتَّقۡوَىٰ وَأَهۡلُ ٱلۡمَغۡفِرَةِ",
      "translation": "Аллоҳ хоҳлаганидагина эслатма-ибрат оладилар. У тақво аҳлидир, мағфират аҳлидир.",
      "footnotes": ""
    }
  ]
}