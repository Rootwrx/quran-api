{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 586,
      "verse_key": "81:1",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذَا ٱلشَّمۡسُ كُوِّرَتۡ",
      "translation": "Қуёш ўралиб қолганида;",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 586,
      "verse_key": "81:2",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلنُّجُومُ ٱنكَدَرَتۡ",
      "translation": "Юлдузлар тўкилганида;",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 586,
      "verse_key": "81:3",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡجِبَالُ سُيِّرَتۡ",
      "translation": "Тоғлар жойидан қўзғатилганида;",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 586,
      "verse_key": "81:4",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡعِشَارُ عُطِّلَتۡ",
      "translation": "Ҳомиласига ўн ой тўлган бўғоз туялар қаровсиз қолганида;",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 586,
      "verse_key": "81:5",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡوُحُوشُ حُشِرَتۡ",
      "translation": "Ваҳший ҳайвонлар бир жойга тўпланганида;",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 586,
      "verse_key": "81:6",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡبِحَارُ سُجِّرَتۡ",
      "translation": "Денгизлар аланга олиб ёнганида;",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 586,
      "verse_key": "81:7",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلنُّفُوسُ زُوِّجَتۡ",
      "translation": "Жонлар жуфтланганида;",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 586,
      "verse_key": "81:8",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡمَوۡءُۥدَةُ سُئِلَتۡ",
      "translation": "Тириклай кўмилган қиздан сўралганида;",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 586,
      "verse_key": "81:9",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بِأَيِّ ذَنۢبٖ قُتِلَتۡ",
      "translation": "Қандай гуноҳ сабабли ўлдирилгани ҳақида;",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 586,
      "verse_key": "81:10",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلصُّحُفُ نُشِرَتۡ",
      "translation": "Саҳифалар очилганида;",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 586,
      "verse_key": "81:11",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلسَّمَآءُ كُشِطَتۡ",
      "translation": "Осмон сидириб олинганида;",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 586,
      "verse_key": "81:12",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡجَحِيمُ سُعِّرَتۡ",
      "translation": "Жаҳаннам қиздирилганида;",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 586,
      "verse_key": "81:13",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡجَنَّةُ أُزۡلِفَتۡ",
      "translation": "Жаннат яқинлаштирилганида;",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 586,
      "verse_key": "81:14",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلِمَتۡ نَفۡسٞ مَّآ أَحۡضَرَتۡ",
      "translation": "Ҳар бир жон нима ҳозирлаб келганини биладир.",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 586,
      "verse_key": "81:15",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلَآ أُقۡسِمُ بِٱلۡخُنَّسِ",
      "translation": "Кўздан ғойиб бўлгувчи юлдузларга қасам ичаман.",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 586,
      "verse_key": "81:16",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلۡجَوَارِ ٱلۡكُنَّسِ",
      "translation": "Юрувчи, беркинувчиларга.",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 586,
      "verse_key": "81:17",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّيۡلِ إِذَا عَسۡعَسَ",
      "translation": "Келиб-кетаётган кечага.",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 586,
      "verse_key": "81:18",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلصُّبۡحِ إِذَا تَنَفَّسَ",
      "translation": "Ва отаётган тонгга (қасам ичурманки).",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 586,
      "verse_key": "81:19",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُۥ لَقَوۡلُ رَسُولٖ كَرِيمٖ",
      "translation": "У шак-шубҳасиз, улуғ элчининг (Аллоҳдан келтирган) сўзидир!",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 586,
      "verse_key": "81:20",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ذِي قُوَّةٍ عِندَ ذِي ٱلۡعَرۡشِ مَكِينٖ",
      "translation": "У қувватли, Аршнинг соҳиби наздида мартабаси олий.",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 586,
      "verse_key": "81:21",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مُّطَاعٖ ثَمَّ أَمِينٖ",
      "translation": "Итоат этилгувчи ва ишончлидир.",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 586,
      "verse_key": "81:22",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا صَاحِبُكُم بِمَجۡنُونٖ",
      "translation": "Сизнинг соҳибингиз мажнун эмасдир.",
      "footnotes": ""
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 586,
      "verse_key": "81:23",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَقَدۡ رَءَاهُ بِٱلۡأُفُقِ ٱلۡمُبِينِ",
      "translation": "Дарҳақиқат, у (Жаброил алайҳиссалом)ни очиқ уфқда кўрди.",
      "footnotes": ""
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 586,
      "verse_key": "81:24",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا هُوَ عَلَى ٱلۡغَيۡبِ بِضَنِينٖ",
      "translation": "Ва у ғайбга бахил эмасдир.",
      "footnotes": ""
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 586,
      "verse_key": "81:25",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا هُوَ بِقَوۡلِ شَيۡطَٰنٖ رَّجِيمٖ",
      "translation": "У (Қуръони Карим) тошбўрон қилинган шайтоннинг сўзи эмасдир.",
      "footnotes": ""
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 586,
      "verse_key": "81:26",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَيۡنَ تَذۡهَبُونَ",
      "translation": "Бас, қаён бормоқдасиз?!",
      "footnotes": ""
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 586,
      "verse_key": "81:27",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنۡ هُوَ إِلَّا ذِكۡرٞ لِّلۡعَٰلَمِينَ",
      "translation": "У бутун оламлар учун бир эслатмадан бошқа нарса эмас.",
      "footnotes": ""
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 586,
      "verse_key": "81:28",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لِمَن شَآءَ مِنكُمۡ أَن يَسۡتَقِيمَ",
      "translation": "Орангиздаги тўғри йўлдан бормоқни истаганлар учун.",
      "footnotes": ""
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 586,
      "verse_key": "81:29",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا تَشَآءُونَ إِلَّآ أَن يَشَآءَ ٱللَّهُ رَبُّ ٱلۡعَٰلَمِينَ",
      "translation": "Бутун оламлар Парвардигори Аллоҳ хоҳласагина хоҳларсизлар.",
      "footnotes": ""
    }
  ]
}