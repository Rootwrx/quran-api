{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 591,
      "verse_key": "86:1",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلسَّمَآءِ وَٱلطَّارِقِ",
      "translation": "Осмонга қасам, тунги йўловчига қасам.",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 591,
      "verse_key": "86:2",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَآ أَدۡرَىٰكَ مَا ٱلطَّارِقُ",
      "translation": "Тунги йўловчи нима эканини қаердан ҳам билар эдингиз.",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 591,
      "verse_key": "86:3",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلنَّجۡمُ ٱلثَّاقِبُ",
      "translation": "У пориллаб турувчи юлдуздир.",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 591,
      "verse_key": "86:4",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِن كُلُّ نَفۡسٖ لَّمَّا عَلَيۡهَا حَافِظٞ",
      "translation": "Ҳар бир жоннинг устида ёд олгувчи бордир.",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 591,
      "verse_key": "86:5",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلۡيَنظُرِ ٱلۡإِنسَٰنُ مِمَّ خُلِقَ",
      "translation": "Энди инсон ўзининг нимадан яралганига бир боқсин!",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 591,
      "verse_key": "86:6",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "خُلِقَ مِن مَّآءٖ دَافِقٖ",
      "translation": "У отилиб чиқувчи бир сувдан яралган-ку!",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 591,
      "verse_key": "86:7",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَخۡرُجُ مِنۢ بَيۡنِ ٱلصُّلۡبِ وَٱلتَّرَآئِبِ",
      "translation": "У орқа умуртқа ва кўкрак суяги орасидан чиқади.",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 591,
      "verse_key": "86:8",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُۥ عَلَىٰ رَجۡعِهِۦ لَقَادِرٞ",
      "translation": "Албатта, У Зот уни қайта тирилтиришга Қодирдир.",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 591,
      "verse_key": "86:9",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَوۡمَ تُبۡلَى ٱلسَّرَآئِرُ",
      "translation": "Сирлар фош бўладиган кунда.",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 591,
      "verse_key": "86:10",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَمَا لَهُۥ مِن قُوَّةٖ وَلَا نَاصِرٖ",
      "translation": "Бас, унинг учун бирон куч ҳам, ёрдамчи ҳам бўлмас!",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 591,
      "verse_key": "86:11",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلسَّمَآءِ ذَاتِ ٱلرَّجۡعِ",
      "translation": "Қайта-қайта тушгувчи (ёмғир)нинг эгаси бўлмиш осмонга қасам;",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 591,
      "verse_key": "86:12",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡأَرۡضِ ذَاتِ ٱلصَّدۡعِ",
      "translation": "Ёриб чиқувчининг эгаси бўлган ерга қасам.",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 591,
      "verse_key": "86:13",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُۥ لَقَوۡلٞ فَصۡلٞ",
      "translation": "Албатта, у ажратувчи сўздир.",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 591,
      "verse_key": "86:14",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا هُوَ بِٱلۡهَزۡلِ",
      "translation": "У ҳазил эмасдир.",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 591,
      "verse_key": "86:15",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُمۡ يَكِيدُونَ كَيۡدٗا",
      "translation": "Албатта, улар ҳийла қиладилар.",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 591,
      "verse_key": "86:16",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَكِيدُ كَيۡدٗا",
      "translation": "Мен ҳам бир ҳийла қилурман.",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 591,
      "verse_key": "86:17",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَمَهِّلِ ٱلۡكَٰفِرِينَ أَمۡهِلۡهُمۡ رُوَيۡدَۢا",
      "translation": "Бас, кофирларга муҳлат беринг. Ҳа, уларга озгина муҳлат беринг.",
      "footnotes": ""
    }
  ]
}