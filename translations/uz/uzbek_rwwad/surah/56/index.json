{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 534,
      "verse_key": "56:1",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذَا وَقَعَتِ ٱلۡوَاقِعَةُ",
      "translation": "Воқеа рўй берганда.",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 534,
      "verse_key": "56:2",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَيۡسَ لِوَقۡعَتِهَا كَاذِبَةٌ",
      "translation": "Унинг рўй беришини ёлғон дегувчи бўлмас.",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 534,
      "verse_key": "56:3",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "خَافِضَةٞ رَّافِعَةٌ",
      "translation": "Пастлатувчи. Кўтаргувчи.",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 534,
      "verse_key": "56:4",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذَا رُجَّتِ ٱلۡأَرۡضُ رَجّٗا",
      "translation": "Ер қаттиқ ларзага келганда.",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 534,
      "verse_key": "56:5",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَبُسَّتِ ٱلۡجِبَالُ بَسّٗا",
      "translation": "Тоғлар титилиб кетганда.",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 534,
      "verse_key": "56:6",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَكَانَتۡ هَبَآءٗ مُّنۢبَثّٗا",
      "translation": "Чанг-тўзонга айланганда.",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 534,
      "verse_key": "56:7",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَكُنتُمۡ أَزۡوَٰجٗا ثَلَٰثَةٗ",
      "translation": "Сиз уч тоифа бўлганингизда.",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 534,
      "verse_key": "56:8",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَصۡحَٰبُ ٱلۡمَيۡمَنَةِ مَآ أَصۡحَٰبُ ٱلۡمَيۡمَنَةِ",
      "translation": "Бас, ўнг тараф эгалари. Ўнг тараф эгалари нима?",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 534,
      "verse_key": "56:9",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَصۡحَٰبُ ٱلۡمَشۡـَٔمَةِ مَآ أَصۡحَٰبُ ٱلۡمَشۡـَٔمَةِ",
      "translation": "Шум тараф эгалари. Шум тараф эгалари нима?",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 534,
      "verse_key": "56:10",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلسَّٰبِقُونَ ٱلسَّٰبِقُونَ",
      "translation": "Пешқадамлар, пешқадамлар.",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 534,
      "verse_key": "56:11",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أُوْلَٰٓئِكَ ٱلۡمُقَرَّبُونَ",
      "translation": "Ана ўшалар муқарраблардир.",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 534,
      "verse_key": "56:12",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِي جَنَّٰتِ ٱلنَّعِيمِ",
      "translation": "Ноз-неъмат жаннатларида.",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 534,
      "verse_key": "56:13",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُلَّةٞ مِّنَ ٱلۡأَوَّلِينَ",
      "translation": "Олдингилардан кўпчилик.",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 534,
      "verse_key": "56:14",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَقَلِيلٞ مِّنَ ٱلۡأٓخِرِينَ",
      "translation": "Кейингилардан озчилик.",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 534,
      "verse_key": "56:15",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلَىٰ سُرُرٖ مَّوۡضُونَةٖ",
      "translation": "Тўқилган сўриларда.",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 534,
      "verse_key": "56:16",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مُّتَّكِـِٔينَ عَلَيۡهَا مُتَقَٰبِلِينَ",
      "translation": "Бир-бирларига рўбарў бўлиб, ястаниб ўтирадилар.",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 535,
      "verse_key": "56:17",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَطُوفُ عَلَيۡهِمۡ وِلۡدَٰنٞ مُّخَلَّدُونَ",
      "translation": "Атрофларида ёш болалар ҳар доим айланиб турадилар.",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 535,
      "verse_key": "56:18",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بِأَكۡوَابٖ وَأَبَارِيقَ وَكَأۡسٖ مِّن مَّعِينٖ",
      "translation": "Оқар булоқдан қадаҳлар, кўзалар ва косалар ила.",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 535,
      "verse_key": "56:19",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَّا يُصَدَّعُونَ عَنۡهَا وَلَا يُنزِفُونَ",
      "translation": "Улардан бошлари ҳам оғримас, маст ҳам бўлиб қолмаслар.",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 535,
      "verse_key": "56:20",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَفَٰكِهَةٖ مِّمَّا يَتَخَيَّرُونَ",
      "translation": "Хоҳлаган меваларини ҳам.",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 535,
      "verse_key": "56:21",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَحۡمِ طَيۡرٖ مِّمَّا يَشۡتَهُونَ",
      "translation": "Иштаҳалари тортган қуш гўштини ҳам.",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 535,
      "verse_key": "56:22",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَحُورٌ عِينٞ",
      "translation": "Оҳу кўзли ҳурлар ҳам бор.",
      "footnotes": ""
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 535,
      "verse_key": "56:23",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَأَمۡثَٰلِ ٱللُّؤۡلُوِٕ ٱلۡمَكۡنُونِ",
      "translation": "Худди яшириб қўйилган дур каби.",
      "footnotes": ""
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 535,
      "verse_key": "56:24",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "جَزَآءَۢ بِمَا كَانُواْ يَعۡمَلُونَ",
      "translation": "Қилиб ўтган амалларининг мукофоти.",
      "footnotes": ""
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 535,
      "verse_key": "56:25",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَا يَسۡمَعُونَ فِيهَا لَغۡوٗا وَلَا تَأۡثِيمًا",
      "translation": "У ерда беҳуда гапни ҳам, гуноҳни ҳам эшитмаслар.",
      "footnotes": ""
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 535,
      "verse_key": "56:26",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَّا قِيلٗا سَلَٰمٗا سَلَٰمٗا",
      "translation": "Фақат «Салом, салом», дейилганинигина эшитарлар.",
      "footnotes": ""
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 535,
      "verse_key": "56:27",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَصۡحَٰبُ ٱلۡيَمِينِ مَآ أَصۡحَٰبُ ٱلۡيَمِينِ",
      "translation": "Ўнг тараф эгалари. Ўнг тараф эгалари нима?",
      "footnotes": ""
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 535,
      "verse_key": "56:28",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِي سِدۡرٖ مَّخۡضُودٖ",
      "translation": "Тикансиз бутазорларда.",
      "footnotes": ""
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 535,
      "verse_key": "56:29",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَطَلۡحٖ مَّنضُودٖ",
      "translation": "Тизилган бананзорларда.",
      "footnotes": ""
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 535,
      "verse_key": "56:30",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَظِلّٖ مَّمۡدُودٖ",
      "translation": "Ёйилган сояда.",
      "footnotes": ""
    },
    {
      "id": 31,
      "verse_number": 31,
      "page_number": 535,
      "verse_key": "56:31",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَآءٖ مَّسۡكُوبٖ",
      "translation": "Оқизиб қўйилган сувларда.",
      "footnotes": ""
    },
    {
      "id": 32,
      "verse_number": 32,
      "page_number": 535,
      "verse_key": "56:32",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَفَٰكِهَةٖ كَثِيرَةٖ",
      "translation": "Мўл-кўл меваларда.",
      "footnotes": ""
    },
    {
      "id": 33,
      "verse_number": 33,
      "page_number": 535,
      "verse_key": "56:33",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَّا مَقۡطُوعَةٖ وَلَا مَمۡنُوعَةٖ",
      "translation": "Тугамайдиган ва ман ҳам қилинмайдиган.",
      "footnotes": ""
    },
    {
      "id": 34,
      "verse_number": 34,
      "page_number": 535,
      "verse_key": "56:34",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَفُرُشٖ مَّرۡفُوعَةٍ",
      "translation": "Баланд кўтарилган кўрпачаларда.",
      "footnotes": ""
    },
    {
      "id": 35,
      "verse_number": 35,
      "page_number": 535,
      "verse_key": "56:35",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّآ أَنشَأۡنَٰهُنَّ إِنشَآءٗ",
      "translation": "Дарҳақиқат, Биз уларни дафъатан яратдик.",
      "footnotes": ""
    },
    {
      "id": 36,
      "verse_number": 36,
      "page_number": 535,
      "verse_key": "56:36",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَجَعَلۡنَٰهُنَّ أَبۡكَارًا",
      "translation": "Уларни бокиралар қилдик.",
      "footnotes": ""
    },
    {
      "id": 37,
      "verse_number": 37,
      "page_number": 535,
      "verse_key": "56:37",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عُرُبًا أَتۡرَابٗا",
      "translation": "Тенгдош маҳбубалар.",
      "footnotes": ""
    },
    {
      "id": 38,
      "verse_number": 38,
      "page_number": 535,
      "verse_key": "56:38",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لِّأَصۡحَٰبِ ٱلۡيَمِينِ",
      "translation": "Ўнг тараф эгалари учун.",
      "footnotes": ""
    },
    {
      "id": 39,
      "verse_number": 39,
      "page_number": 535,
      "verse_key": "56:39",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُلَّةٞ مِّنَ ٱلۡأَوَّلِينَ",
      "translation": "Олдингилардан ҳам кўпчилик.",
      "footnotes": ""
    },
    {
      "id": 40,
      "verse_number": 40,
      "page_number": 535,
      "verse_key": "56:40",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَثُلَّةٞ مِّنَ ٱلۡأٓخِرِينَ",
      "translation": "Кейингилардан ҳам кўпчилик.",
      "footnotes": ""
    },
    {
      "id": 41,
      "verse_number": 41,
      "page_number": 535,
      "verse_key": "56:41",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَصۡحَٰبُ ٱلشِّمَالِ مَآ أَصۡحَٰبُ ٱلشِّمَالِ",
      "translation": "Чап тараф эгалари. Чап тараф эгалари нима?",
      "footnotes": ""
    },
    {
      "id": 42,
      "verse_number": 42,
      "page_number": 535,
      "verse_key": "56:42",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِي سَمُومٖ وَحَمِيمٖ",
      "translation": "Самум ва қайноқ сув ичрадирлар.",
      "footnotes": ""
    },
    {
      "id": 43,
      "verse_number": 43,
      "page_number": 535,
      "verse_key": "56:43",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَظِلّٖ مِّن يَحۡمُومٖ",
      "translation": "Қора тутундан иборат соядалар.",
      "footnotes": ""
    },
    {
      "id": 44,
      "verse_number": 44,
      "page_number": 535,
      "verse_key": "56:44",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَّا بَارِدٖ وَلَا كَرِيمٍ",
      "translation": "Салқин ҳам эмас, фойдали ҳам эмас.",
      "footnotes": ""
    },
    {
      "id": 45,
      "verse_number": 45,
      "page_number": 535,
      "verse_key": "56:45",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُمۡ كَانُواْ قَبۡلَ ذَٰلِكَ مُتۡرَفِينَ",
      "translation": "Дарҳақиқат, улар бундан олдин маишатпараст эдилар.",
      "footnotes": ""
    },
    {
      "id": 46,
      "verse_number": 46,
      "page_number": 535,
      "verse_key": "56:46",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَكَانُواْ يُصِرُّونَ عَلَى ٱلۡحِنثِ ٱلۡعَظِيمِ",
      "translation": "Саркашлик билан катта гуноҳларни қилаверардилар.",
      "footnotes": ""
    },
    {
      "id": 47,
      "verse_number": 47,
      "page_number": 535,
      "verse_key": "56:47",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَكَانُواْ يَقُولُونَ أَئِذَا مِتۡنَا وَكُنَّا تُرَابٗا وَعِظَٰمًا أَءِنَّا لَمَبۡعُوثُونَ",
      "translation": "«Ўлиб, тупроқ ва суяклар бўлиб кетганимизда яна қайта тириламизми?» - дер эдилар.",
      "footnotes": ""
    },
    {
      "id": 48,
      "verse_number": 48,
      "page_number": 535,
      "verse_key": "56:48",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَوَءَابَآؤُنَا ٱلۡأَوَّلُونَ",
      "translation": "Олдинги ота-боболаримиз ҳам-а?",
      "footnotes": ""
    },
    {
      "id": 49,
      "verse_number": 49,
      "page_number": 535,
      "verse_key": "56:49",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قُلۡ إِنَّ ٱلۡأَوَّلِينَ وَٱلۡأٓخِرِينَ",
      "translation": "Айтинг: «Олдингилару кейингилар",
      "footnotes": ""
    },
    {
      "id": 50,
      "verse_number": 50,
      "page_number": 535,
      "verse_key": "56:50",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَمَجۡمُوعُونَ إِلَىٰ مِيقَٰتِ يَوۡمٖ مَّعۡلُومٖ",
      "translation": "Маълум кундаги белгиланган вақтда тўплангувчидирлар».",
      "footnotes": ""
    },
    {
      "id": 51,
      "verse_number": 51,
      "page_number": 536,
      "verse_key": "56:51",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ إِنَّكُمۡ أَيُّهَا ٱلضَّآلُّونَ ٱلۡمُكَذِّبُونَ",
      "translation": "Кейин сизлар, эй ёлғон дегувчи гумроҳлар.",
      "footnotes": ""
    },
    {
      "id": 52,
      "verse_number": 52,
      "page_number": 536,
      "verse_key": "56:52",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَأٓكِلُونَ مِن شَجَرٖ مِّن زَقُّومٖ",
      "translation": "Албатта, заққум дарахтидан егувчисизлар.",
      "footnotes": ""
    },
    {
      "id": 53,
      "verse_number": 53,
      "page_number": 536,
      "verse_key": "56:53",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَمَالِـُٔونَ مِنۡهَا ٱلۡبُطُونَ",
      "translation": "Бас, ўшандан қоринларни тўлдиргувчисизлар.",
      "footnotes": ""
    },
    {
      "id": 54,
      "verse_number": 54,
      "page_number": 536,
      "verse_key": "56:54",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَشَٰرِبُونَ عَلَيۡهِ مِنَ ٱلۡحَمِيمِ",
      "translation": "Устидан қайноқ сув ичгувчисизлар.",
      "footnotes": ""
    },
    {
      "id": 55,
      "verse_number": 55,
      "page_number": 536,
      "verse_key": "56:55",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَشَٰرِبُونَ شُرۡبَ ٱلۡهِيمِ",
      "translation": "Ташна туядек ичгувчисизлар.",
      "footnotes": ""
    },
    {
      "id": 56,
      "verse_number": 56,
      "page_number": 536,
      "verse_key": "56:56",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "هَٰذَا نُزُلُهُمۡ يَوۡمَ ٱلدِّينِ",
      "translation": "Уларнинг Қиёмат кунидаги зиёфатлари мана шу.",
      "footnotes": ""
    },
    {
      "id": 57,
      "verse_number": 57,
      "page_number": 536,
      "verse_key": "56:57",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "نَحۡنُ خَلَقۡنَٰكُمۡ فَلَوۡلَا تُصَدِّقُونَ",
      "translation": "Сизларни Биз яратганмиз. Тасдиқ этмайсизларми?",
      "footnotes": ""
    },
    {
      "id": 58,
      "verse_number": 58,
      "page_number": 536,
      "verse_key": "56:58",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَرَءَيۡتُم مَّا تُمۡنُونَ",
      "translation": "Сизлар тўкадиган маний ҳақида ўйлаб кўрдингларми?",
      "footnotes": ""
    },
    {
      "id": 59,
      "verse_number": 59,
      "page_number": 536,
      "verse_key": "56:59",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ءَأَنتُمۡ تَخۡلُقُونَهُۥٓ أَمۡ نَحۡنُ ٱلۡخَٰلِقُونَ",
      "translation": "Уни сизлар яратасизларми ёки Биз яратгувчимизми?",
      "footnotes": ""
    },
    {
      "id": 60,
      "verse_number": 60,
      "page_number": 536,
      "verse_key": "56:60",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "نَحۡنُ قَدَّرۡنَا بَيۡنَكُمُ ٱلۡمَوۡتَ وَمَا نَحۡنُ بِمَسۡبُوقِينَ",
      "translation": "Орангиздаги ўлимни ҳам Биз тақдир қилиб қўйганмиз ва Биз ожизлар эмасмиз.",
      "footnotes": ""
    },
    {
      "id": 61,
      "verse_number": 61,
      "page_number": 536,
      "verse_key": "56:61",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلَىٰٓ أَن نُّبَدِّلَ أَمۡثَٰلَكُمۡ وَنُنشِئَكُمۡ فِي مَا لَا تَعۡلَمُونَ",
      "translation": "Ўрнингизга ўхшашларингизни алмаштириб қўйишга ва сизларни ўзларингиз билмайдиган бир шаклда пайдо қилишга.",
      "footnotes": ""
    },
    {
      "id": 62,
      "verse_number": 62,
      "page_number": 536,
      "verse_key": "56:62",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَقَدۡ عَلِمۡتُمُ ٱلنَّشۡأَةَ ٱلۡأُولَىٰ فَلَوۡلَا تَذَكَّرُونَ",
      "translation": "Дарҳақиқат, дастлабки пайдо бўлишни билдинглар. Бас, эслатма олмайсизларми?",
      "footnotes": ""
    },
    {
      "id": 63,
      "verse_number": 63,
      "page_number": 536,
      "verse_key": "56:63",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَرَءَيۡتُم مَّا تَحۡرُثُونَ",
      "translation": "Экаётган нарсангиз ҳақида ўйлаб кўрдингизми?",
      "footnotes": ""
    },
    {
      "id": 64,
      "verse_number": 64,
      "page_number": 536,
      "verse_key": "56:64",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ءَأَنتُمۡ تَزۡرَعُونَهُۥٓ أَمۡ نَحۡنُ ٱلزَّٰرِعُونَ",
      "translation": "Уни сизлар ундирасизларми ёки Биз ундиргувчилармизми?",
      "footnotes": ""
    },
    {
      "id": 65,
      "verse_number": 65,
      "page_number": 536,
      "verse_key": "56:65",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَوۡ نَشَآءُ لَجَعَلۡنَٰهُ حُطَٰمٗا فَظَلۡتُمۡ تَفَكَّهُونَ",
      "translation": "Агар истасак, уни қуруқ чўп қилиб юборган бўлар эдик. Бас, ҳайрон бўлиб қолар эдинглар.",
      "footnotes": ""
    },
    {
      "id": 66,
      "verse_number": 66,
      "page_number": 536,
      "verse_key": "56:66",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّا لَمُغۡرَمُونَ",
      "translation": "«Дарҳақиқат, биз зарар кўргувчилармиз.",
      "footnotes": ""
    },
    {
      "id": 67,
      "verse_number": 67,
      "page_number": 536,
      "verse_key": "56:67",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بَلۡ نَحۡنُ مَحۡرُومُونَ",
      "translation": "Йўқ, биз маҳрумлармиз», (деб қолар эдинглар).",
      "footnotes": ""
    },
    {
      "id": 68,
      "verse_number": 68,
      "page_number": 536,
      "verse_key": "56:68",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَرَءَيۡتُمُ ٱلۡمَآءَ ٱلَّذِي تَشۡرَبُونَ",
      "translation": "Ўзингиз ичадиган сув ҳақида ўйлаб кўрдингларми?",
      "footnotes": ""
    },
    {
      "id": 69,
      "verse_number": 69,
      "page_number": 536,
      "verse_key": "56:69",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ءَأَنتُمۡ أَنزَلۡتُمُوهُ مِنَ ٱلۡمُزۡنِ أَمۡ نَحۡنُ ٱلۡمُنزِلُونَ",
      "translation": "Уни булутлардан сизлар ёғдирдингларми ёки Биз ёғдиргувчимизми?",
      "footnotes": ""
    },
    {
      "id": 70,
      "verse_number": 70,
      "page_number": 536,
      "verse_key": "56:70",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَوۡ نَشَآءُ جَعَلۡنَٰهُ أُجَاجٗا فَلَوۡلَا تَشۡكُرُونَ",
      "translation": "Агар истасак, уни шўр қилиб қўйган бўлар эдик. Бас, шукр қилмайсизларми?",
      "footnotes": ""
    },
    {
      "id": 71,
      "verse_number": 71,
      "page_number": 536,
      "verse_key": "56:71",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَرَءَيۡتُمُ ٱلنَّارَ ٱلَّتِي تُورُونَ",
      "translation": "Ўзингиз ёқадиган олов ҳақида ўйлаб кўрдингларми?",
      "footnotes": ""
    },
    {
      "id": 72,
      "verse_number": 72,
      "page_number": 536,
      "verse_key": "56:72",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ءَأَنتُمۡ أَنشَأۡتُمۡ شَجَرَتَهَآ أَمۡ نَحۡنُ ٱلۡمُنشِـُٔونَ",
      "translation": "Унинг дарахтини сизлар пайдо қилдингларми ёки Биз пайдо қилгувчимизми?",
      "footnotes": ""
    },
    {
      "id": 73,
      "verse_number": 73,
      "page_number": 536,
      "verse_key": "56:73",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "نَحۡنُ جَعَلۡنَٰهَا تَذۡكِرَةٗ وَمَتَٰعٗا لِّلۡمُقۡوِينَ",
      "translation": "Биз уни бир эслатма ва йўловчилар учун бир манфаат қилдик.",
      "footnotes": ""
    },
    {
      "id": 74,
      "verse_number": 74,
      "page_number": 536,
      "verse_key": "56:74",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَسَبِّحۡ بِٱسۡمِ رَبِّكَ ٱلۡعَظِيمِ",
      "translation": "Бас, улуғ Парвардигорингиз номини поклаб тасбеҳ айтинг.",
      "footnotes": ""
    },
    {
      "id": 75,
      "verse_number": 75,
      "page_number": 536,
      "verse_key": "56:75",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "۞ فَلَآ أُقۡسِمُ بِمَوَٰقِعِ ٱلنُّجُومِ",
      "translation": "Юлдузларнинг ботар жойларига қасам ичурман.",
      "footnotes": ""
    },
    {
      "id": 76,
      "verse_number": 76,
      "page_number": 536,
      "verse_key": "56:76",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِنَّهُۥ لَقَسَمٞ لَّوۡ تَعۡلَمُونَ عَظِيمٌ",
      "translation": "Ва бу, агар билсангиз, улуғ қасам.",
      "footnotes": ""
    },
    {
      "id": 77,
      "verse_number": 77,
      "page_number": 537,
      "verse_key": "56:77",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُۥ لَقُرۡءَانٞ كَرِيمٞ",
      "translation": "Албатта, у улуғ Қуръондир.",
      "footnotes": ""
    },
    {
      "id": 78,
      "verse_number": 78,
      "page_number": 537,
      "verse_key": "56:78",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِي كِتَٰبٖ مَّكۡنُونٖ",
      "translation": "Асралган Китобда.",
      "footnotes": ""
    },
    {
      "id": 79,
      "verse_number": 79,
      "page_number": 537,
      "verse_key": "56:79",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَّا يَمَسُّهُۥٓ إِلَّا ٱلۡمُطَهَّرُونَ",
      "translation": "Уни фақат покланганларгина ушлайдилар.",
      "footnotes": ""
    },
    {
      "id": 80,
      "verse_number": 80,
      "page_number": 537,
      "verse_key": "56:80",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تَنزِيلٞ مِّن رَّبِّ ٱلۡعَٰلَمِينَ",
      "translation": "Оламлар Парвардигори тарафидан нозил қилинган.",
      "footnotes": ""
    },
    {
      "id": 81,
      "verse_number": 81,
      "page_number": 537,
      "verse_key": "56:81",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَبِهَٰذَا ٱلۡحَدِيثِ أَنتُم مُّدۡهِنُونَ",
      "translation": "Мана шу сўзни менсимасдан инкор қиляпсизларми?",
      "footnotes": ""
    },
    {
      "id": 82,
      "verse_number": 82,
      "page_number": 537,
      "verse_key": "56:82",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَتَجۡعَلُونَ رِزۡقَكُمۡ أَنَّكُمۡ تُكَذِّبُونَ",
      "translation": "Мана шу ёлғон дейишингизни ўз ризқингиз қилиб оляпсизларми?",
      "footnotes": ""
    },
    {
      "id": 83,
      "verse_number": 83,
      "page_number": 537,
      "verse_key": "56:83",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلَوۡلَآ إِذَا بَلَغَتِ ٱلۡحُلۡقُومَ",
      "translation": "Жонингиз ҳалқумга келганда.",
      "footnotes": ""
    },
    {
      "id": 84,
      "verse_number": 84,
      "page_number": 537,
      "verse_key": "56:84",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنتُمۡ حِينَئِذٖ تَنظُرُونَ",
      "translation": "Ҳолбуки, ўзларингиз ўша пайтда қараб турасизлар.",
      "footnotes": ""
    },
    {
      "id": 85,
      "verse_number": 85,
      "page_number": 537,
      "verse_key": "56:85",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَنَحۡنُ أَقۡرَبُ إِلَيۡهِ مِنكُمۡ وَلَٰكِن لَّا تُبۡصِرُونَ",
      "translation": "Биз эса унга сизлардан ҳам яқинроқмиз. Лекин сизлар кўрмайсизлар.",
      "footnotes": ""
    },
    {
      "id": 86,
      "verse_number": 86,
      "page_number": 537,
      "verse_key": "56:86",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلَوۡلَآ إِن كُنتُمۡ غَيۡرَ مَدِينِينَ",
      "translation": "Агар сизлар ҳисоб-китобга тортилмайдиган бўлсангизлар.",
      "footnotes": ""
    },
    {
      "id": 87,
      "verse_number": 87,
      "page_number": 537,
      "verse_key": "56:87",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تَرۡجِعُونَهَآ إِن كُنتُمۡ صَٰدِقِينَ",
      "translation": "Агар ростгўй бўлсангизлар, уни жойига қайтаринглар-чи.",
      "footnotes": ""
    },
    {
      "id": 88,
      "verse_number": 88,
      "page_number": 537,
      "verse_key": "56:88",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَمَّآ إِن كَانَ مِنَ ٱلۡمُقَرَّبِينَ",
      "translation": "Бас, агар у яқин қилинганлардан бўлса,",
      "footnotes": ""
    },
    {
      "id": 89,
      "verse_number": 89,
      "page_number": 537,
      "verse_key": "56:89",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَرَوۡحٞ وَرَيۡحَانٞ وَجَنَّتُ نَعِيمٖ",
      "translation": "Бас, роҳат, ёқимли ризқ ва неъматларга тўла жаннат (уникидир).",
      "footnotes": ""
    },
    {
      "id": 90,
      "verse_number": 90,
      "page_number": 537,
      "verse_key": "56:90",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَمَّآ إِن كَانَ مِنۡ أَصۡحَٰبِ ٱلۡيَمِينِ",
      "translation": "Агар ўнг тараф эгаларидан бўлса,",
      "footnotes": ""
    },
    {
      "id": 91,
      "verse_number": 91,
      "page_number": 537,
      "verse_key": "56:91",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَسَلَٰمٞ لَّكَ مِنۡ أَصۡحَٰبِ ٱلۡيَمِينِ",
      "translation": "Бас, сенга ўнг тараф эгаларидан салом бўлсин.",
      "footnotes": ""
    },
    {
      "id": 92,
      "verse_number": 92,
      "page_number": 537,
      "verse_key": "56:92",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَمَّآ إِن كَانَ مِنَ ٱلۡمُكَذِّبِينَ ٱلضَّآلِّينَ",
      "translation": "Агар ёлғонга чиқарувчи гумроҳлардан бўлса,",
      "footnotes": ""
    },
    {
      "id": 93,
      "verse_number": 93,
      "page_number": 537,
      "verse_key": "56:93",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَنُزُلٞ مِّنۡ حَمِيمٖ",
      "translation": "Бас, (унга) ўта қайноқ сувдан иборат зиёфат бор.",
      "footnotes": ""
    },
    {
      "id": 94,
      "verse_number": 94,
      "page_number": 537,
      "verse_key": "56:94",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَتَصۡلِيَةُ جَحِيمٍ",
      "translation": "Ва дўзахда ёниш.",
      "footnotes": ""
    },
    {
      "id": 95,
      "verse_number": 95,
      "page_number": 537,
      "verse_key": "56:95",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ هَٰذَا لَهُوَ حَقُّ ٱلۡيَقِينِ",
      "translation": "Албатта, бу айни ҳақиқатдир.",
      "footnotes": ""
    },
    {
      "id": 96,
      "verse_number": 96,
      "page_number": 537,
      "verse_key": "56:96",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَسَبِّحۡ بِٱسۡمِ رَبِّكَ ٱلۡعَظِيمِ",
      "translation": "Бас, улуғ Парвардигорингиз номини поклаб тасбеҳ айтинг.",
      "footnotes": ""
    }
  ]
}