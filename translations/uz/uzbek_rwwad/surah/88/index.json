{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 592,
      "verse_key": "88:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "هَلۡ أَتَىٰكَ حَدِيثُ ٱلۡغَٰشِيَةِ",
      "translation": "Сизга ўраб олгувчи ҳақида хабар келдими?",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 592,
      "verse_key": "88:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وُجُوهٞ يَوۡمَئِذٍ خَٰشِعَةٌ",
      "translation": "У кунда эгилиб қолган юзлар бўлур.",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 592,
      "verse_key": "88:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَامِلَةٞ نَّاصِبَةٞ",
      "translation": "Меҳнат-машаққатдан ҳорғин.",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 592,
      "verse_key": "88:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تَصۡلَىٰ نَارًا حَامِيَةٗ",
      "translation": "(Бу юзлар) қизиган дўзахга кирур!",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 592,
      "verse_key": "88:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تُسۡقَىٰ مِنۡ عَيۡنٍ ءَانِيَةٖ",
      "translation": "Қайнаб турган булоқдан суғорилур!",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 592,
      "verse_key": "88:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَّيۡسَ لَهُمۡ طَعَامٌ إِلَّا مِن ضَرِيعٖ",
      "translation": "Уларга тикандан бошқа таом бўлмас.",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 592,
      "verse_key": "88:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَّا يُسۡمِنُ وَلَا يُغۡنِي مِن جُوعٖ",
      "translation": "У семиртирмайди ҳам, очлиқдан халос ҳам қилмайди!",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 592,
      "verse_key": "88:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وُجُوهٞ يَوۡمَئِذٖ نَّاعِمَةٞ",
      "translation": "У кунда хурсанд чеҳралар ҳам бўлур.",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 592,
      "verse_key": "88:9",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لِّسَعۡيِهَا رَاضِيَةٞ",
      "translation": "Ўз ишидан рози.",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 592,
      "verse_key": "88:10",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِي جَنَّةٍ عَالِيَةٖ",
      "translation": "Олий жаннатдадирлар.",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 592,
      "verse_key": "88:11",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَّا تَسۡمَعُ فِيهَا لَٰغِيَةٗ",
      "translation": "У жойда бирон беҳуда сўз эшитмаслар.",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 592,
      "verse_key": "88:12",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِيهَا عَيۡنٞ جَارِيَةٞ",
      "translation": "У жойда оқар булоқ бордир.",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 592,
      "verse_key": "88:13",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِيهَا سُرُرٞ مَّرۡفُوعَةٞ",
      "translation": "У жойда баланд сўрилар.",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 592,
      "verse_key": "88:14",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَكۡوَابٞ مَّوۡضُوعَةٞ",
      "translation": "Тайёрлаб қўйилган қадаҳлар бор.",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 592,
      "verse_key": "88:15",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَنَمَارِقُ مَصۡفُوفَةٞ",
      "translation": "Тизиб қўйилган ёстиқлар.",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 592,
      "verse_key": "88:16",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَزَرَابِيُّ مَبۡثُوثَةٌ",
      "translation": "Ҳамма ёққа тўшалган гиламлар бордир.",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 592,
      "verse_key": "88:17",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَلَا يَنظُرُونَ إِلَى ٱلۡإِبِلِ كَيۡفَ خُلِقَتۡ",
      "translation": "Ахир улар туянинг қандай яратилганига боқмайдиларми?",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 592,
      "verse_key": "88:18",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِلَى ٱلسَّمَآءِ كَيۡفَ رُفِعَتۡ",
      "translation": "Осмоннинг қандай кўтариб қўйилганига?",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 592,
      "verse_key": "88:19",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِلَى ٱلۡجِبَالِ كَيۡفَ نُصِبَتۡ",
      "translation": "Тоғларнинг қандай ўрнатилганига?",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 592,
      "verse_key": "88:20",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِلَى ٱلۡأَرۡضِ كَيۡفَ سُطِحَتۡ",
      "translation": "Ернинг қандай ёйиб-текислаб қўйилганига?",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 592,
      "verse_key": "88:21",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَذَكِّرۡ إِنَّمَآ أَنتَ مُذَكِّرٞ",
      "translation": "Бас, эслатинг! Сиз эслатувчидирсиз, холос.",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 592,
      "verse_key": "88:22",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَّسۡتَ عَلَيۡهِم بِمُصَيۡطِرٍ",
      "translation": "Сиз уларнинг устида зўравонлик билан ҳукм юргизгувчи эмассиз.",
      "footnotes": ""
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 592,
      "verse_key": "88:23",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَّا مَن تَوَلَّىٰ وَكَفَرَ",
      "translation": "Юз ўгирган ва кофир бўлган кимса бундан мустасно.",
      "footnotes": ""
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 592,
      "verse_key": "88:24",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَيُعَذِّبُهُ ٱللَّهُ ٱلۡعَذَابَ ٱلۡأَكۡبَرَ",
      "translation": "Уни Аллоҳ энг катта азоб билан азоблар!",
      "footnotes": ""
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 592,
      "verse_key": "88:25",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ إِلَيۡنَآ إِيَابَهُمۡ",
      "translation": "Албатта, уларнинг қайтиши Бизгадир.",
      "footnotes": ""
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 592,
      "verse_key": "88:26",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ إِنَّ عَلَيۡنَا حِسَابَهُم",
      "translation": "Сўнгра уларни ҳисоб-китоб қилиш ҳам ёлғиз Бизнинг зиммамиздадир!",
      "footnotes": ""
    }
  ]
}