{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 595,
      "verse_key": "92:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّيۡلِ إِذَا يَغۡشَىٰ",
      "translation": "M wẽen ne yʋngã t'a sã n wa lutẽ.",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 595,
      "verse_key": "92:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلنَّهَارِ إِذَا تَجَلَّىٰ",
      "translation": "La ne wĩndgã t'a sã n wɑ puki.",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 595,
      "verse_key": "92:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا خَلَقَ ٱلذَّكَرَ وَٱلۡأُنثَىٰٓ",
      "translation": "La a sẽn naane, rao la paga.",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 595,
      "verse_key": "92:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ سَعۡيَكُمۡ لَشَتَّىٰ",
      "translation": "Ad yãmb tʋʋmã yaa toay-toaya.",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 595,
      "verse_key": "92:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَمَّا مَنۡ أَعۡطَىٰ وَٱتَّقَىٰ",
      "translation": "Rẽnd ned ning sẽn kõ la a zoe Wẽnde.",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 595,
      "verse_key": "92:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَصَدَّقَ بِٱلۡحُسۡنَىٰ",
      "translation": "La a kõ sõmã wã sɩda.",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 595,
      "verse_key": "92:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَسَنُيَسِّرُهُۥ لِلۡيُسۡرَىٰ",
      "translation": "Tõnd na n yols-a-la ne yol-yole.",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 595,
      "verse_key": "92:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَمَّا مَنۢ بَخِلَ وَٱسۡتَغۡنَىٰ",
      "translation": "La ned ning sẽn maan beed la a sek a menga.",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 595,
      "verse_key": "92:9",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَكَذَّبَ بِٱلۡحُسۡنَىٰ",
      "translation": "La a yags sõmbã.",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 595,
      "verse_key": "92:10",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَسَنُيَسِّرُهُۥ لِلۡعُسۡرَىٰ",
      "translation": "D na n yols-a lame ne tʋʋm-wẽense tɩ sõmɑ yɩ toog ne-ɑ.",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 595,
      "verse_key": "92:11",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا يُغۡنِي عَنۡهُ مَالُهُۥٓ إِذَا تَرَدَّىٰٓ",
      "translation": "La a arzεkã pa tõe n sεk-a, t'a sã n wa halk ye.",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 595,
      "verse_key": "92:12",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ عَلَيۡنَا لَلۡهُدَىٰ",
      "translation": "Ad kãndgrã bee ne Tõndo.",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 595,
      "verse_key": "92:13",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِنَّ لَنَا لَلۡأٓخِرَةَ وَٱلۡأُولَىٰ",
      "translation": "La ad Yaoolmã la Rãeenmã yaa Tõnd n so.",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 595,
      "verse_key": "92:14",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَنذَرۡتُكُمۡ نَارٗا تَلَظَّىٰ",
      "translation": "Tɩ M bugsda yãmb ne bugum sẽn witi.",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 596,
      "verse_key": "92:15",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَا يَصۡلَىٰهَآ إِلَّا ٱلۡأَشۡقَى",
      "translation": "Pa kẽed-a , ralla zu-be-neda.",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 596,
      "verse_key": "92:16",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِي كَذَّبَ وَتَوَلَّىٰ",
      "translation": "Sẽn yags-a la ɑ gõd kuidga.",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 596,
      "verse_key": "92:17",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَسَيُجَنَّبُهَا ٱلۡأَتۡقَى",
      "translation": "La b na n zãag-a-la ne wẽn-zoεt-kɑsengã, (Abuubɑkɑr sidiik, Wẽnd yɑrd be ɑ yĩngɑ ).",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 596,
      "verse_key": "92:18",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِي يُؤۡتِي مَالَهُۥ يَتَزَكَّىٰ",
      "translation": "Sẽn kõt-a n baood yɩlgre.",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 596,
      "verse_key": "92:19",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا لِأَحَدٍ عِندَهُۥ مِن نِّعۡمَةٖ تُجۡزَىٰٓ",
      "translation": "La ned ba a yembr pa tar mɑɑn-neer ɑ nengẽ t'ɑ mɑɑnd n nɑ n rol ye.",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 596,
      "verse_key": "92:20",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَّا ٱبۡتِغَآءَ وَجۡهِ رَبِّهِ ٱلۡأَعۡلَىٰ",
      "translation": "Rallame tɩ yaa a Soab-zẽkdgã yard baoob bɑlɑ.",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 596,
      "verse_key": "92:21",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَسَوۡفَ يَرۡضَىٰ",
      "translation": "La yɩ bilfu, a na wa n yard bũmb ning A sẽn rol-ɑ wã.",
      "footnotes": ""
    }
  ]
}