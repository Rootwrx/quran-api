{
  "result": [
    {
      "id": "6059",
      "sura": "92",
      "aya": "1",
      "arabic_text": "وَٱلَّيۡلِ إِذَا يَغۡشَىٰ",
      "translation": "M wẽen ne yʋngã t'a sã n wa lutẽ.",
      "footnotes": ""
    },
    {
      "id": "6060",
      "sura": "92",
      "aya": "2",
      "arabic_text": "وَٱلنَّهَارِ إِذَا تَجَلَّىٰ",
      "translation": "La ne wĩndgã t'a sã n wɑ puki.",
      "footnotes": ""
    },
    {
      "id": "6061",
      "sura": "92",
      "aya": "3",
      "arabic_text": "وَمَا خَلَقَ ٱلذَّكَرَ وَٱلۡأُنثَىٰٓ",
      "translation": "La a sẽn naane, rao la paga.",
      "footnotes": ""
    },
    {
      "id": "6062",
      "sura": "92",
      "aya": "4",
      "arabic_text": "إِنَّ سَعۡيَكُمۡ لَشَتَّىٰ",
      "translation": "Ad yãmb tʋʋmã yaa toay-toaya.",
      "footnotes": ""
    },
    {
      "id": "6063",
      "sura": "92",
      "aya": "5",
      "arabic_text": "فَأَمَّا مَنۡ أَعۡطَىٰ وَٱتَّقَىٰ",
      "translation": "Rẽnd ned ning sẽn kõ la a zoe Wẽnde.",
      "footnotes": ""
    },
    {
      "id": "6064",
      "sura": "92",
      "aya": "6",
      "arabic_text": "وَصَدَّقَ بِٱلۡحُسۡنَىٰ",
      "translation": "La a kõ sõmã wã sɩda.",
      "footnotes": ""
    },
    {
      "id": "6065",
      "sura": "92",
      "aya": "7",
      "arabic_text": "فَسَنُيَسِّرُهُۥ لِلۡيُسۡرَىٰ",
      "translation": "Tõnd na n yols-a-la ne yol-yole.",
      "footnotes": ""
    },
    {
      "id": "6066",
      "sura": "92",
      "aya": "8",
      "arabic_text": "وَأَمَّا مَنۢ بَخِلَ وَٱسۡتَغۡنَىٰ",
      "translation": "La ned ning sẽn maan beed la a sek a menga.",
      "footnotes": ""
    },
    {
      "id": "6067",
      "sura": "92",
      "aya": "9",
      "arabic_text": "وَكَذَّبَ بِٱلۡحُسۡنَىٰ",
      "translation": "La a yags sõmbã.",
      "footnotes": ""
    },
    {
      "id": "6068",
      "sura": "92",
      "aya": "10",
      "arabic_text": "فَسَنُيَسِّرُهُۥ لِلۡعُسۡرَىٰ",
      "translation": "D na n yols-a lame ne tʋʋm-wẽense tɩ sõmɑ yɩ toog ne-ɑ.",
      "footnotes": ""
    },
    {
      "id": "6069",
      "sura": "92",
      "aya": "11",
      "arabic_text": "وَمَا يُغۡنِي عَنۡهُ مَالُهُۥٓ إِذَا تَرَدَّىٰٓ",
      "translation": "La a arzεkã pa tõe n sεk-a, t'a sã n wa halk ye.",
      "footnotes": ""
    },
    {
      "id": "6070",
      "sura": "92",
      "aya": "12",
      "arabic_text": "إِنَّ عَلَيۡنَا لَلۡهُدَىٰ",
      "translation": "Ad kãndgrã bee ne Tõndo.",
      "footnotes": ""
    },
    {
      "id": "6071",
      "sura": "92",
      "aya": "13",
      "arabic_text": "وَإِنَّ لَنَا لَلۡأٓخِرَةَ وَٱلۡأُولَىٰ",
      "translation": "La ad Yaoolmã la Rãeenmã yaa Tõnd n so.",
      "footnotes": ""
    },
    {
      "id": "6072",
      "sura": "92",
      "aya": "14",
      "arabic_text": "فَأَنذَرۡتُكُمۡ نَارٗا تَلَظَّىٰ",
      "translation": "Tɩ M bugsda yãmb ne bugum sẽn witi.",
      "footnotes": ""
    },
    {
      "id": "6073",
      "sura": "92",
      "aya": "15",
      "arabic_text": "لَا يَصۡلَىٰهَآ إِلَّا ٱلۡأَشۡقَى",
      "translation": "Pa kẽed-a , ralla zu-be-neda.",
      "footnotes": ""
    },
    {
      "id": "6074",
      "sura": "92",
      "aya": "16",
      "arabic_text": "ٱلَّذِي كَذَّبَ وَتَوَلَّىٰ",
      "translation": "Sẽn yags-a la ɑ gõd kuidga.",
      "footnotes": ""
    },
    {
      "id": "6075",
      "sura": "92",
      "aya": "17",
      "arabic_text": "وَسَيُجَنَّبُهَا ٱلۡأَتۡقَى",
      "translation": "La b na n zãag-a-la ne wẽn-zoεt-kɑsengã, (Abuubɑkɑr sidiik, Wẽnd yɑrd be ɑ yĩngɑ ).",
      "footnotes": ""
    },
    {
      "id": "6076",
      "sura": "92",
      "aya": "18",
      "arabic_text": "ٱلَّذِي يُؤۡتِي مَالَهُۥ يَتَزَكَّىٰ",
      "translation": "Sẽn kõt-a n baood yɩlgre.",
      "footnotes": ""
    },
    {
      "id": "6077",
      "sura": "92",
      "aya": "19",
      "arabic_text": "وَمَا لِأَحَدٍ عِندَهُۥ مِن نِّعۡمَةٖ تُجۡزَىٰٓ",
      "translation": "La ned ba a yembr pa tar mɑɑn-neer ɑ nengẽ t'ɑ mɑɑnd n nɑ n rol ye.",
      "footnotes": ""
    },
    {
      "id": "6078",
      "sura": "92",
      "aya": "20",
      "arabic_text": "إِلَّا ٱبۡتِغَآءَ وَجۡهِ رَبِّهِ ٱلۡأَعۡلَىٰ",
      "translation": "Rallame tɩ yaa a Soab-zẽkdgã yard baoob bɑlɑ.",
      "footnotes": ""
    },
    {
      "id": "6079",
      "sura": "92",
      "aya": "21",
      "arabic_text": "وَلَسَوۡفَ يَرۡضَىٰ",
      "translation": "La yɩ bilfu, a na wa n yard bũmb ning A sẽn rol-ɑ wã.",
      "footnotes": ""
    }
  ]
}