{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 585,
      "verse_key": "80:1",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَبَسَ وَتَوَلَّىٰٓ",
      "translation": "A gãnem a neng n gũd a zãn-kuidga.",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 585,
      "verse_key": "80:2",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَن جَآءَهُ ٱلۡأَعۡمَىٰ",
      "translation": "Tɩ zõangã waa a nengẽ.",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 585,
      "verse_key": "80:3",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا يُدۡرِيكَ لَعَلَّهُۥ يَزَّكَّىٰٓ",
      "translation": "La bõe n wilgd foo, sãnd-sãnde a na n yɩlgame?",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 585,
      "verse_key": "80:4",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَوۡ يَذَّكَّرُ فَتَنفَعَهُ ٱلذِّكۡرَىٰٓ",
      "translation": "Ma t'a tẽeg-a tɩ tẽegrã naf-a?",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 585,
      "verse_key": "80:5",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَمَّا مَنِ ٱسۡتَغۡنَىٰ",
      "translation": "La ned ning sẽn sek a menga.",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 585,
      "verse_key": "80:6",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَنتَ لَهُۥ تَصَدَّىٰ",
      "translation": "Tɩ fo yãgd a sεgbo,",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 585,
      "verse_key": "80:7",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا عَلَيۡكَ أَلَّا يَزَّكَّىٰ",
      "translation": "A pa kãndg pa pak foo ye.",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 585,
      "verse_key": "80:8",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَمَّا مَن جَآءَكَ يَسۡعَىٰ",
      "translation": "La sã n ya ned ning sẽn yãgd n wat fo nengẽ wã.",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 585,
      "verse_key": "80:9",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَهُوَ يَخۡشَىٰ",
      "translation": "Tɩ yẽnda yaa sẽn zoet Wẽnde.",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 585,
      "verse_key": "80:10",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَنتَ عَنۡهُ تَلَهَّىٰ",
      "translation": "Lɑ fo sẽn tar soagl ne kɩfr-dãmbã n bɑs ɑ sεgbã.",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 585,
      "verse_key": "80:11",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّآ إِنَّهَا تَذۡكِرَةٞ",
      "translation": "Pa woto ye! Ad a yaa tẽegre.",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 585,
      "verse_key": "80:12",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَمَن شَآءَ ذَكَرَهُۥ",
      "translation": "Rẽnd ned ning sẽn tʋll-a bɩ a tẽeg-A, lɑ ɑ pɑɑm sɑgell ne Alkʋrãɑnã.",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 585,
      "verse_key": "80:13",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِي صُحُفٖ مُّكَرَّمَةٖ",
      "translation": "N be karaas-waoogds pʋsẽ.",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 585,
      "verse_key": "80:14",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَّرۡفُوعَةٖ مُّطَهَّرَةِۭ",
      "translation": "N zẽk n yɩlge.",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 585,
      "verse_key": "80:15",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بِأَيۡدِي سَفَرَةٖ",
      "translation": "N be mɑlεgs nusẽ.",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 585,
      "verse_key": "80:16",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كِرَامِۭ بَرَرَةٖ",
      "translation": "N yaa wagl n yaa sak-n-tũ-rãmba.",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 585,
      "verse_key": "80:17",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قُتِلَ ٱلۡإِنسَٰنُ مَآ أَكۡفَرَهُۥ",
      "translation": "B kãaba ninsaal kɩfre, a sõm-zɩɩlmã yĩnga.",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 585,
      "verse_key": "80:18",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مِنۡ أَيِّ شَيۡءٍ خَلَقَهُۥ",
      "translation": "Yaa bõe la A naan ne-a?",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 585,
      "verse_key": "80:19",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مِن نُّطۡفَةٍ خَلَقَهُۥ فَقَدَّرَهُۥ",
      "translation": "Yaa mani la A naan ne-a n kord-a.",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 585,
      "verse_key": "80:20",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ ٱلسَّبِيلَ يَسَّرَهُۥ",
      "translation": "Rẽnda poorẽ n yols a sorã t'ɑ yi ɑ mɑ wã pʋgẽ.",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 585,
      "verse_key": "80:21",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ أَمَاتَهُۥ فَأَقۡبَرَهُۥ",
      "translation": "Rẽ poorẽ t'A kʋ-a n kẽes a kabrẽ.",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 585,
      "verse_key": "80:22",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ إِذَا شَآءَ أَنشَرَهُۥ",
      "translation": "Rẽnda poorẽ, A sã n wa tʋlle, A na n yik-a lame.",
      "footnotes": ""
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 585,
      "verse_key": "80:23",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّا لَمَّا يَقۡضِ مَآ أَمَرَهُۥ",
      "translation": "Pa woto ye! A pa tũ b sẽn sagl-a wã ye.",
      "footnotes": ""
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 585,
      "verse_key": "80:24",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلۡيَنظُرِ ٱلۡإِنسَٰنُ إِلَىٰ طَعَامِهِۦٓ",
      "translation": "Bɩ ninsaal ges a rɩɩbã, ɑ sẽn yɩt to-to.",
      "footnotes": ""
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 585,
      "verse_key": "80:25",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَنَّا صَبَبۡنَا ٱلۡمَآءَ صَبّٗا",
      "translation": "Tɩ Tõnd kiigda koomã ne kiigri.",
      "footnotes": ""
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 585,
      "verse_key": "80:26",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ شَقَقۡنَا ٱلۡأَرۡضَ شَقّٗا",
      "translation": "Rẽ poorẽ n pãrg tẽngã pãrgr tεkẽ",
      "footnotes": ""
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 585,
      "verse_key": "80:27",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَنۢبَتۡنَا فِيهَا حَبّٗا",
      "translation": "N buls a pʋgẽ bõn-bεεla.",
      "footnotes": ""
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 585,
      "verse_key": "80:28",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَعِنَبٗا وَقَضۡبٗا",
      "translation": "La sibã la yam-maasã (zẽ-biisi).",
      "footnotes": ""
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 585,
      "verse_key": "80:29",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَزَيۡتُونٗا وَنَخۡلٗا",
      "translation": "La Zaeentʋʋn (olɩvɩye) la tamar-tɩɩse.",
      "footnotes": ""
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 585,
      "verse_key": "80:30",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَحَدَآئِقَ غُلۡبٗا",
      "translation": "La pʋt sẽn tob bĩmm.",
      "footnotes": ""
    },
    {
      "id": 31,
      "verse_number": 31,
      "page_number": 585,
      "verse_key": "80:31",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَفَٰكِهَةٗ وَأَبّٗا",
      "translation": "La tɩ-biis la yamdo.",
      "footnotes": ""
    },
    {
      "id": 32,
      "verse_number": 32,
      "page_number": 585,
      "verse_key": "80:32",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَّتَٰعٗا لَّكُمۡ وَلِأَنۡعَٰمِكُمۡ",
      "translation": "Tɩ yaa wʋm-noog ne yãmb la y rũmsã.",
      "footnotes": ""
    },
    {
      "id": 33,
      "verse_number": 33,
      "page_number": 585,
      "verse_key": "80:33",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِذَا جَآءَتِ ٱلصَّآخَّةُ",
      "translation": "La tãs-kεgengã sã n wa.",
      "footnotes": ""
    },
    {
      "id": 34,
      "verse_number": 34,
      "page_number": 585,
      "verse_key": "80:34",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَوۡمَ يَفِرُّ ٱلۡمَرۡءُ مِنۡ أَخِيهِ",
      "translation": "Raar ned sẽn na n zoe n bas a ma-biiga.",
      "footnotes": ""
    },
    {
      "id": 35,
      "verse_number": 35,
      "page_number": 585,
      "verse_key": "80:35",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأُمِّهِۦ وَأَبِيهِ",
      "translation": "La a ma, la a saamba.",
      "footnotes": ""
    },
    {
      "id": 36,
      "verse_number": 36,
      "page_number": 585,
      "verse_key": "80:36",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَصَٰحِبَتِهِۦ وَبَنِيهِ",
      "translation": "La a pag la a koamba.",
      "footnotes": ""
    },
    {
      "id": 37,
      "verse_number": 37,
      "page_number": 585,
      "verse_key": "80:37",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لِكُلِّ ٱمۡرِيٕٖ مِّنۡهُمۡ يَوۡمَئِذٖ شَأۡنٞ يُغۡنِيهِ",
      "translation": "Ned fãa yell sεk-a-la raaren-kãngã.",
      "footnotes": ""
    },
    {
      "id": 38,
      "verse_number": 38,
      "page_number": 585,
      "verse_key": "80:38",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وُجُوهٞ يَوۡمَئِذٖ مُّسۡفِرَةٞ",
      "translation": "Raaren-kãngã, nems sẽn pɩnd bee be.",
      "footnotes": ""
    },
    {
      "id": 39,
      "verse_number": 39,
      "page_number": 585,
      "verse_key": "80:39",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ضَاحِكَةٞ مُّسۡتَبۡشِرَةٞ",
      "translation": "N yaa sẽn laade, n yaa sẽn yolse.",
      "footnotes": ""
    },
    {
      "id": 40,
      "verse_number": 40,
      "page_number": 585,
      "verse_key": "80:40",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَوُجُوهٞ يَوۡمَئِذٍ عَلَيۡهَا غَبَرَةٞ",
      "translation": "Nems me sẽn yaa puddd bee raarã.",
      "footnotes": ""
    },
    {
      "id": 41,
      "verse_number": 41,
      "page_number": 585,
      "verse_key": "80:41",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تَرۡهَقُهَا قَتَرَةٌ",
      "translation": "Sablem n lud-ba.",
      "footnotes": ""
    },
    {
      "id": 42,
      "verse_number": 42,
      "page_number": 585,
      "verse_key": "80:42",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أُوْلَٰٓئِكَ هُمُ ٱلۡكَفَرَةُ ٱلۡفَجَرَةُ",
      "translation": "Bãmb rãmbã la kɩfr-dãmbã n yaa nin-wẽnsã.",
      "footnotes": ""
    }
  ]
}