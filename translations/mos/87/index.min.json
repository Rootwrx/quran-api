{
  "verses": [
    {
      "number": "1",
      "arabic_text": "سَبِّحِ ٱسۡمَ رَبِّكَ ٱلۡأَعۡلَى",
      "translation": "yɩlg f Sob-zẽkdgã yʋʋrã."
    },
    {
      "number": "2",
      "arabic_text": "ٱلَّذِي خَلَقَ فَسَوَّىٰ",
      "translation": "Sẽn naan-ɑ ninsɑɑlã n zems a taabã."
    },
    {
      "number": "3",
      "arabic_text": "وَٱلَّذِي قَدَّرَ فَهَدَىٰ",
      "translation": "La sẽn kord-a bõn-nɑɑndsã b yɑlẽ gilli, n peeg-bɑ n tʋg sẽn nɑ zems ne-bɑ."
    },
    {
      "number": "4",
      "arabic_text": "وَٱلَّذِيٓ أَخۡرَجَ ٱلۡمَرۡعَىٰ",
      "translation": "Sẽn yiisd-a yamdã.(rũms wãmneg zĩiga)."
    },
    {
      "number": "5",
      "arabic_text": "فَجَعَلَهُۥ غُثَآءً أَحۡوَىٰ",
      "translation": "N leb n wa maan-a t'a kʋɩ n sobge."
    },
    {
      "number": "6",
      "arabic_text": "سَنُقۡرِئُكَ فَلَا تَنسَىٰٓ",
      "translation": "M na n karmɑ foo tɩ f pa yĩm ye."
    },
    {
      "number": "7",
      "arabic_text": "إِلَّا مَا شَآءَ ٱللَّهُۚ إِنَّهُۥ يَعۡلَمُ ٱلۡجَهۡرَ وَمَا يَخۡفَىٰ",
      "translation": "Rẽnda bũmb ning Wẽnd sẽn wa n tʋllã; ad Yẽ n mi sẽn vẽnege, la bũmb ning sẽn solge."
    },
    {
      "number": "8",
      "arabic_text": "وَنُيَسِّرُكَ لِلۡيُسۡرَىٰ",
      "translation": "La D yols-f ne yol-yole."
    },
    {
      "number": "9",
      "arabic_text": "فَذَكِّرۡ إِن نَّفَعَتِ ٱلذِّكۡرَىٰ",
      "translation": "Bɩ f tẽeg tɩ tẽegrã sã n nafdẽ."
    },
    {
      "number": "10",
      "arabic_text": "سَيَذَّكَّرُ مَن يَخۡشَىٰ",
      "translation": "Ned ning sẽn yaeesd-a nɑ tẽege."
    },
    {
      "number": "11",
      "arabic_text": "وَيَتَجَنَّبُهَا ٱلۡأَشۡقَى",
      "translation": "La zu-be-ned zãagd-a lame."
    },
    {
      "number": "12",
      "arabic_text": "ٱلَّذِي يَصۡلَى ٱلنَّارَ ٱلۡكُبۡرَىٰ",
      "translation": "Ning sẽn kẽed bug-kẽengã."
    },
    {
      "number": "13",
      "arabic_text": "ثُمَّ لَا يَمُوتُ فِيهَا وَلَا يَحۡيَىٰ",
      "translation": "Rẽ poorẽ a pa kiid a pʋgẽ, la a pa vɩɩmsd ye."
    },
    {
      "number": "14",
      "arabic_text": "قَدۡ أَفۡلَحَ مَن تَزَكَّىٰ",
      "translation": "Hakɩɩka sẽn yɩlg-a tɩlgame."
    },
    {
      "number": "15",
      "arabic_text": "وَذَكَرَ ٱسۡمَ رَبِّهِۦ فَصَلَّىٰ",
      "translation": "La a Tẽeg a Soaba yʋʋr n pʋʋse."
    },
    {
      "number": "16",
      "arabic_text": "بَلۡ تُؤۡثِرُونَ ٱلۡحَيَوٰةَ ٱلدُّنۡيَا",
      "translation": "La bala yãmb yɩɩsda dũni vɩɩmã."
    },
    {
      "number": "17",
      "arabic_text": "وَٱلۡأٓخِرَةُ خَيۡرٞ وَأَبۡقَىٰٓ",
      "translation": "La yaoolem raarã sõmb n yɩɩde, n leb n yɩɩd ne paabo."
    },
    {
      "number": "18",
      "arabic_text": "إِنَّ هَٰذَا لَفِي ٱلصُّحُفِ ٱلۡأُولَىٰ",
      "translation": "Ad woto n be rẽenem gaf-rãmbẽ wã."
    },
    {
      "number": "19",
      "arabic_text": "صُحُفِ إِبۡرَٰهِيمَ وَمُوسَىٰ",
      "translation": "A ɭbraahɩɩm ne a Musa gaf-rãmbã."
    }
  ]
}