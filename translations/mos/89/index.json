{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 593,
      "verse_key": "89:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡفَجۡرِ",
      "translation": "M wẽen ne fagiri",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 593,
      "verse_key": "89:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَيَالٍ عَشۡرٖ",
      "translation": "La M wẽen ne yʋnd piigã,",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 593,
      "verse_key": "89:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلشَّفۡعِ وَٱلۡوَتۡرِ",
      "translation": "La ne sõd-yiibu la lugum (sõd-yiib-n-tõatga).",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 593,
      "verse_key": "89:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّيۡلِ إِذَا يَسۡرِ",
      "translation": "La ne yʋngã t'a sã n wa gũd kuidga.",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 593,
      "verse_key": "89:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "هَلۡ فِي ذَٰلِكَ قَسَمٞ لِّذِي حِجۡرٍ",
      "translation": "Rẽ yĩnga yεl nins sẽn togsã, wẽeneg bee be n sek yam soab bɩ?",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 593,
      "verse_key": "89:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَمۡ تَرَ كَيۡفَ فَعَلَ رَبُّكَ بِعَادٍ",
      "translation": "Fo pa yã wãn-wãn la fo Soabã maan a ʿAad rãmba?",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 593,
      "verse_key": "89:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِرَمَ ذَاتِ ٱلۡعِمَادِ",
      "translation": "ɭram rãmb sẽn yaa zeng-raad rãmba,",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 593,
      "verse_key": "89:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّتِي لَمۡ يُخۡلَقۡ مِثۡلُهَا فِي ٱلۡبِلَٰدِ",
      "translation": "B sẽn pa naan b bilgr tẽmsã pʋga?",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 593,
      "verse_key": "89:9",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَثَمُودَ ٱلَّذِينَ جَابُواْ ٱلصَّخۡرَ بِٱلۡوَادِ",
      "translation": "La Ṯamuud rãmb sẽn yaa neb nins sẽn pees pĩigã ko-soogẽ wã?",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 593,
      "verse_key": "89:10",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَفِرۡعَوۡنَ ذِي ٱلۡأَوۡتَادِ",
      "translation": "La a Fɩr-ʿaoon sẽn yaa bay soabã?",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 593,
      "verse_key": "89:11",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِينَ طَغَوۡاْ فِي ٱلۡبِلَٰدِ",
      "translation": "Sẽn maan-b n zʋʋg tẽmsã pʋga.",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 593,
      "verse_key": "89:12",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَكۡثَرُواْ فِيهَا ٱلۡفَسَادَ",
      "translation": "N wʋsg sãang a pʋgẽ.",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 593,
      "verse_key": "89:13",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَصَبَّ عَلَيۡهِمۡ رَبُّكَ سَوۡطَ عَذَابٍ",
      "translation": "Tɩ b Soabã kiig b zug naong kõbg toor-toore.",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 593,
      "verse_key": "89:14",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ رَبَّكَ لَبِٱلۡمِرۡصَادِ",
      "translation": "Ad fo Soabã yaa gũusda.",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 593,
      "verse_key": "89:15",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَمَّا ٱلۡإِنسَٰنُ إِذَا مَا ٱبۡتَلَىٰهُ رَبُّهُۥ فَأَكۡرَمَهُۥ وَنَعَّمَهُۥ فَيَقُولُ رَبِّيٓ أَكۡرَمَنِ",
      "translation": "La sã n yaa ne ninsaala, a Soabã sã n wa zarb-a n na n gese, n waoog-a, n neem-a, la a sẽn yete: \"mam Soabã waooga maam\".",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 593,
      "verse_key": "89:16",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَمَّآ إِذَا مَا ٱبۡتَلَىٰهُ فَقَدَرَ عَلَيۡهِ رِزۡقَهُۥ فَيَقُولُ رَبِّيٓ أَهَٰنَنِ",
      "translation": "La A sã n wa zarb-a, n weoog a rɩtlã a zugu, la a sẽn yete: \"mam Soabã yaala maam\".",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 593,
      "verse_key": "89:17",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّاۖ بَل لَّا تُكۡرِمُونَ ٱلۡيَتِيمَ",
      "translation": "Pa woto ye! Y pa waoogd kɩɩb ye.",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 593,
      "verse_key": "89:18",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَا تَحَٰٓضُّونَ عَلَىٰ طَعَامِ ٱلۡمِسۡكِينِ",
      "translation": "Y leb n pa pirsd taab ne ninbãan-neb rɩlgr ye.",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 593,
      "verse_key": "89:19",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَتَأۡكُلُونَ ٱلتُّرَاثَ أَكۡلٗا لَّمّٗا",
      "translation": "La y rɩt faadã ne kãas-n-rɩ.",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 593,
      "verse_key": "89:20",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَتُحِبُّونَ ٱلۡمَالَ حُبّٗا جَمّٗا",
      "translation": "La y nong arzεkã nong-kεgenga.",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 593,
      "verse_key": "89:21",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّآۖ إِذَا دُكَّتِ ٱلۡأَرۡضُ دَكّٗا دَكّٗا",
      "translation": "Pa woto ye! Lɑk Tẽn-gãngã sã n wa rãmb rãmbg tεkẽ.",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 593,
      "verse_key": "89:22",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَجَآءَ رَبُّكَ وَٱلۡمَلَكُ صَفّٗا صَفّٗا",
      "translation": "Tɩ fo Soabã wa, la malεgsã, rulg-rulga.",
      "footnotes": ""
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 593,
      "verse_key": "89:23",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَجِاْيٓءَ يَوۡمَئِذِۭ بِجَهَنَّمَۚ يَوۡمَئِذٖ يَتَذَكَّرُ ٱلۡإِنسَٰنُ وَأَنَّىٰ لَهُ ٱلذِّكۡرَىٰ",
      "translation": "Tɩ b wa ne Gεhannem bɑ-rɑɑrã; ra-kãng raare ninsaal tẽegdame, la tẽegr bεε ne-a?",
      "footnotes": ""
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 594,
      "verse_key": "89:24",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَقُولُ يَٰلَيۡتَنِي قَدَّمۡتُ لِحَيَاتِي",
      "translation": "A yetame: \"kɑeto, Mam sã n da maana neer tɩ reng taoore mam beoog vɩɩmã yĩnga, pɑ são\".",
      "footnotes": ""
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 594,
      "verse_key": "89:25",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَيَوۡمَئِذٖ لَّا يُعَذِّبُ عَذَابَهُۥٓ أَحَدٞ",
      "translation": "La ra-kãngã, ned pa namsd wala A namsgã buud ye.",
      "footnotes": ""
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 594,
      "verse_key": "89:26",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَا يُوثِقُ وَثَاقَهُۥٓ أَحَدٞ",
      "translation": "Ned bɑ ɑ yembr me pa sẽbd wala A sẽbrã ye.",
      "footnotes": ""
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 594,
      "verse_key": "89:27",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَٰٓأَيَّتُهَا ٱلنَّفۡسُ ٱلۡمُطۡمَئِنَّةُ",
      "translation": "Yaa foo yõor sẽn tar bas-m-yam lɑ yĩn-yõgnego!",
      "footnotes": ""
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 594,
      "verse_key": "89:28",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱرۡجِعِيٓ إِلَىٰ رَبِّكِ رَاضِيَةٗ مَّرۡضِيَّةٗ",
      "translation": "Leb f Soabã nengẽ, tɩ f yaa sẽn yɑr-A n yɑɑ b sẽn yard-a soaba.",
      "footnotes": ""
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 594,
      "verse_key": "89:29",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَٱدۡخُلِي فِي عِبَٰدِي",
      "translation": "Bɩ f kẽ Mam yemb-sõmsã pʋgẽ.",
      "footnotes": ""
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 594,
      "verse_key": "89:30",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱدۡخُلِي جَنَّتِي",
      "translation": "La f kẽ Mam Arzãnã.",
      "footnotes": ""
    }
  ]
}