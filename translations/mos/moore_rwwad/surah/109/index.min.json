{
  "verses": [
    {
      "number": "1",
      "arabic_text": "قُلۡ يَٰٓأَيُّهَا ٱلۡكَٰفِرُونَ",
      "translation": "Yeele: \"yaa yãmb ɑ sõm-zɩtbã!"
    },
    {
      "number": "2",
      "arabic_text": "لَآ أَعۡبُدُ مَا تَعۡبُدُونَ",
      "translation": "Mam pa na n tũ yãmb sẽn tũud bũmb ningã ye."
    },
    {
      "number": "3",
      "arabic_text": "وَلَآ أَنتُمۡ عَٰبِدُونَ مَآ أَعۡبُدُ",
      "translation": "Yãmb me pa tũudb bũmb ning mam sẽn tũudã ye."
    },
    {
      "number": "4",
      "arabic_text": "وَلَآ أَنَا۠ عَابِدٞ مَّا عَبَدتُّمۡ",
      "translation": "Ma me pa tũud ne yãmb sẽn tũ bũmb ningã ye."
    },
    {
      "number": "5",
      "arabic_text": "وَلَآ أَنتُمۡ عَٰبِدُونَ مَآ أَعۡبُدُ",
      "translation": "Yãmb me pa tũudb ne bũmb ning mam sẽn tũudã ye."
    },
    {
      "number": "6",
      "arabic_text": "لَكُمۡ دِينُكُمۡ وَلِيَ دِينِ",
      "translation": "Yãmb ne y sorã y sẽn nɑngã, maam me ne m sore Wẽnd sẽn sik n kõ mɑɑmã."
    }
  ]
}