{
  "result": [
    {
      "id": "6024",
      "sura": "90",
      "aya": "1",
      "arabic_text": "لَآ أُقۡسِمُ بِهَٰذَا ٱلۡبَلَدِ",
      "translation": "M na wẽend ne tẽn-kãngã!",
      "footnotes": ""
    },
    {
      "id": "6025",
      "sura": "90",
      "aya": "2",
      "arabic_text": "وَأَنتَ حِلُّۢ بِهَٰذَا ٱلۡبَلَدِ",
      "translation": "Lɑ sɑkdɑ ne foo f sẽn mɑɑn fãɑ tẽn-kãngã.",
      "footnotes": ""
    },
    {
      "id": "6026",
      "sura": "90",
      "aya": "3",
      "arabic_text": "وَوَالِدٖ وَمَا وَلَدَ",
      "translation": "La M wẽen ne roagda, la a sẽn rog bũmb ningã.",
      "footnotes": ""
    },
    {
      "id": "6027",
      "sura": "90",
      "aya": "4",
      "arabic_text": "لَقَدۡ خَلَقۡنَا ٱلۡإِنسَٰنَ فِي كَبَدٍ",
      "translation": "Sɩd la hakɩɩka D naana ninsaal t'a be yaams pʋgẽ.",
      "footnotes": ""
    },
    {
      "id": "6028",
      "sura": "90",
      "aya": "5",
      "arabic_text": "أَيَحۡسَبُ أَن لَّن يَقۡدِرَ عَلَيۡهِ أَحَدٞ",
      "translation": "A tẽedame tɩ ned ba a yembr pa tõe-a ye.",
      "footnotes": ""
    },
    {
      "id": "6029",
      "sura": "90",
      "aya": "6",
      "arabic_text": "يَقُولُ أَهۡلَكۡتُ مَالٗا لُّبَدًا",
      "translation": "A yetame:\"M halka arzεk wʋsgo\"",
      "footnotes": ""
    },
    {
      "id": "6030",
      "sura": "90",
      "aya": "7",
      "arabic_text": "أَيَحۡسَبُ أَن لَّمۡ يَرَهُۥٓ أَحَدٌ",
      "translation": "A tẽedame tɩ ned ba a yembr pa ne-a ye.",
      "footnotes": ""
    },
    {
      "id": "6031",
      "sura": "90",
      "aya": "8",
      "arabic_text": "أَلَمۡ نَجۡعَل لَّهُۥ عَيۡنَيۡنِ",
      "translation": "Tõnd pa maan nin a yiib n kõ-a sɩdɑ?",
      "footnotes": ""
    },
    {
      "id": "6032",
      "sura": "90",
      "aya": "9",
      "arabic_text": "وَلِسَانٗا وَشَفَتَيۡنِ",
      "translation": "La zɩlemd la no-bɩt a yiibu?",
      "footnotes": ""
    },
    {
      "id": "6033",
      "sura": "90",
      "aya": "10",
      "arabic_text": "وَهَدَيۡنَٰهُ ٱلنَّجۡدَيۡنِ",
      "translation": "La M kãndg-a soy a yiibã?",
      "footnotes": ""
    },
    {
      "id": "6034",
      "sura": "90",
      "aya": "11",
      "arabic_text": "فَلَا ٱقۡتَحَمَ ٱلۡعَقَبَةَ",
      "translation": "A pɑ nɑ n gãndg gidrã (so-toogã) n pɑɑm n tɑ Arzẽnɑ.",
      "footnotes": ""
    },
    {
      "id": "6035",
      "sura": "90",
      "aya": "12",
      "arabic_text": "وَمَآ أَدۡرَىٰكَ مَا ٱلۡعَقَبَةُ",
      "translation": "La bõe n wilgd foo bõe la gidr gãndgre.",
      "footnotes": ""
    },
    {
      "id": "6036",
      "sura": "90",
      "aya": "13",
      "arabic_text": "فَكُّ رَقَبَةٍ",
      "translation": "Yaa yamb lodg n bas burkĩndi.",
      "footnotes": ""
    },
    {
      "id": "6037",
      "sura": "90",
      "aya": "14",
      "arabic_text": "أَوۡ إِطۡعَٰمٞ فِي يَوۡمٖ ذِي مَسۡغَبَةٖ",
      "translation": "Ma rɩlgre, kom raare.",
      "footnotes": ""
    },
    {
      "id": "6038",
      "sura": "90",
      "aya": "15",
      "arabic_text": "يَتِيمٗا ذَا مَقۡرَبَةٍ",
      "translation": "Kɩɩb sẽn yaa rog-pẽta.",
      "footnotes": ""
    },
    {
      "id": "6039",
      "sura": "90",
      "aya": "16",
      "arabic_text": "أَوۡ مِسۡكِينٗا ذَا مَتۡرَبَةٖ",
      "translation": "Ma nimbãan-ned (mɩskɩɩn) sẽn yaa tom-vũnumda.",
      "footnotes": ""
    },
    {
      "id": "6040",
      "sura": "90",
      "aya": "17",
      "arabic_text": "ثُمَّ كَانَ مِنَ ٱلَّذِينَ ءَامَنُواْ وَتَوَاصَوۡاْ بِٱلصَّبۡرِ وَتَوَاصَوۡاْ بِٱلۡمَرۡحَمَةِ",
      "translation": "Rẽ poorẽ a yɩɩme n be neb nins sẽn kõt sɩdã pʋgẽ, la b sagl taab ne sugri, la b sagl taab ne yolsgo.",
      "footnotes": ""
    },
    {
      "id": "6041",
      "sura": "90",
      "aya": "18",
      "arabic_text": "أُوْلَٰٓئِكَ أَصۡحَٰبُ ٱلۡمَيۡمَنَةِ",
      "translation": "Bãmb rãmb la Rɩtg rãmba.",
      "footnotes": ""
    },
    {
      "id": "6042",
      "sura": "90",
      "aya": "19",
      "arabic_text": "وَٱلَّذِينَ كَفَرُواْ بِـَٔايَٰتِنَا هُمۡ أَصۡحَٰبُ ٱلۡمَشۡـَٔمَةِ",
      "translation": "La neb nins sẽn kɩɩs-b Tõnd tagmas-rãmbã, bãmb la goabgã rãmba.",
      "footnotes": ""
    },
    {
      "id": "6043",
      "sura": "90",
      "aya": "20",
      "arabic_text": "عَلَيۡهِمۡ نَارٞ مُّؤۡصَدَةُۢ",
      "translation": "Bug võgends n be b zugẽ.",
      "footnotes": ""
    }
  ]
}