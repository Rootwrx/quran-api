{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 594,
      "verse_key": "90:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَآ أُقۡسِمُ بِهَٰذَا ٱلۡبَلَدِ",
      "translation": "M na wẽend ne tẽn-kãngã!",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 594,
      "verse_key": "90:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنتَ حِلُّۢ بِهَٰذَا ٱلۡبَلَدِ",
      "translation": "Lɑ sɑkdɑ ne foo f sẽn mɑɑn fãɑ tẽn-kãngã.",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 594,
      "verse_key": "90:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَوَالِدٖ وَمَا وَلَدَ",
      "translation": "La M wẽen ne roagda, la a sẽn rog bũmb ningã.",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 594,
      "verse_key": "90:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَقَدۡ خَلَقۡنَا ٱلۡإِنسَٰنَ فِي كَبَدٍ",
      "translation": "Sɩd la hakɩɩka D naana ninsaal t'a be yaams pʋgẽ.",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 594,
      "verse_key": "90:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَيَحۡسَبُ أَن لَّن يَقۡدِرَ عَلَيۡهِ أَحَدٞ",
      "translation": "A tẽedame tɩ ned ba a yembr pa tõe-a ye.",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 594,
      "verse_key": "90:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَقُولُ أَهۡلَكۡتُ مَالٗا لُّبَدًا",
      "translation": "A yetame:\"M halka arzεk wʋsgo\"",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 594,
      "verse_key": "90:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَيَحۡسَبُ أَن لَّمۡ يَرَهُۥٓ أَحَدٌ",
      "translation": "A tẽedame tɩ ned ba a yembr pa ne-a ye.",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 594,
      "verse_key": "90:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَمۡ نَجۡعَل لَّهُۥ عَيۡنَيۡنِ",
      "translation": "Tõnd pa maan nin a yiib n kõ-a sɩdɑ?",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 594,
      "verse_key": "90:9",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلِسَانٗا وَشَفَتَيۡنِ",
      "translation": "La zɩlemd la no-bɩt a yiibu?",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 594,
      "verse_key": "90:10",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَهَدَيۡنَٰهُ ٱلنَّجۡدَيۡنِ",
      "translation": "La M kãndg-a soy a yiibã?",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 594,
      "verse_key": "90:11",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلَا ٱقۡتَحَمَ ٱلۡعَقَبَةَ",
      "translation": "A pɑ nɑ n gãndg gidrã (so-toogã) n pɑɑm n tɑ Arzẽnɑ.",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 594,
      "verse_key": "90:12",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَآ أَدۡرَىٰكَ مَا ٱلۡعَقَبَةُ",
      "translation": "La bõe n wilgd foo bõe la gidr gãndgre.",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 594,
      "verse_key": "90:13",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَكُّ رَقَبَةٍ",
      "translation": "Yaa yamb lodg n bas burkĩndi.",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 594,
      "verse_key": "90:14",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَوۡ إِطۡعَٰمٞ فِي يَوۡمٖ ذِي مَسۡغَبَةٖ",
      "translation": "Ma rɩlgre, kom raare.",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 594,
      "verse_key": "90:15",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَتِيمٗا ذَا مَقۡرَبَةٍ",
      "translation": "Kɩɩb sẽn yaa rog-pẽta.",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 594,
      "verse_key": "90:16",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَوۡ مِسۡكِينٗا ذَا مَتۡرَبَةٖ",
      "translation": "Ma nimbãan-ned (mɩskɩɩn) sẽn yaa tom-vũnumda.",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 594,
      "verse_key": "90:17",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ كَانَ مِنَ ٱلَّذِينَ ءَامَنُواْ وَتَوَاصَوۡاْ بِٱلصَّبۡرِ وَتَوَاصَوۡاْ بِٱلۡمَرۡحَمَةِ",
      "translation": "Rẽ poorẽ a yɩɩme n be neb nins sẽn kõt sɩdã pʋgẽ, la b sagl taab ne sugri, la b sagl taab ne yolsgo.",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 594,
      "verse_key": "90:18",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أُوْلَٰٓئِكَ أَصۡحَٰبُ ٱلۡمَيۡمَنَةِ",
      "translation": "Bãmb rãmb la Rɩtg rãmba.",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 594,
      "verse_key": "90:19",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّذِينَ كَفَرُواْ بِـَٔايَٰتِنَا هُمۡ أَصۡحَٰبُ ٱلۡمَشۡـَٔمَةِ",
      "translation": "La neb nins sẽn kɩɩs-b Tõnd tagmas-rãmbã, bãmb la goabgã rãmba.",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 594,
      "verse_key": "90:20",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلَيۡهِمۡ نَارٞ مُّؤۡصَدَةُۢ",
      "translation": "Bug võgends n be b zugẽ.",
      "footnotes": ""
    }
  ]
}