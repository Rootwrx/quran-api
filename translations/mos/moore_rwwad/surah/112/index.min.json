{
  "verses": [
    {
      "number": "1",
      "arabic_text": "قُلۡ هُوَ ٱللَّهُ أَحَدٌ",
      "translation": "Yeele: \"Yẽnda la Wẽnde, A yaa A Yembre."
    },
    {
      "number": "2",
      "arabic_text": "ٱللَّهُ ٱلصَّمَدُ",
      "translation": "Wẽnd la b tikda."
    },
    {
      "number": "3",
      "arabic_text": "لَمۡ يَلِدۡ وَلَمۡ يُولَدۡ",
      "translation": "A pa roge, b pa rog-A."
    },
    {
      "number": "4",
      "arabic_text": "وَلَمۡ يَكُن لَّهُۥ كُفُوًا أَحَدُۢ",
      "translation": "Mams-n-taag me ba a yembr ka ne-A ye\"."
    }
  ]
}