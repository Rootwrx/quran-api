{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 600,
      "verse_key": "101:1",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلۡقَارِعَةُ",
      "translation": "Wɑkɑt ning sẽn Koagsd nebã sũyã!",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 600,
      "verse_key": "101:2",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَا ٱلۡقَارِعَةُ",
      "translation": "Wɑkɑt sẽn koagsd nebã sũyã yɑɑ bʋgo?",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 600,
      "verse_key": "101:3",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَآ أَدۡرَىٰكَ مَا ٱلۡقَارِعَةُ",
      "translation": "La bõe n wildg foo koagsdã sẽn ya-a soaba?",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 600,
      "verse_key": "101:4",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَوۡمَ يَكُونُ ٱلنَّاسُ كَٱلۡفَرَاشِ ٱلۡمَبۡثُوثِ",
      "translation": "Yɑɑ raar ning nebã sẽn na n yɩ wala pilimpigd sẽn sãeege.",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 600,
      "verse_key": "101:5",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَتَكُونُ ٱلۡجِبَالُ كَٱلۡعِهۡنِ ٱلۡمَنفُوشِ",
      "translation": "Tɩ tãnsã lebg wala Pʋmpʋm kõbd b sẽn yelg-ɑ.",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 600,
      "verse_key": "101:6",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَمَّا مَن ثَقُلَتۡ مَوَٰزِينُهُۥ",
      "translation": "Sã n yaa ned ning tʋʋm-sõng mɑgb sẽn zɩsgã.",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 600,
      "verse_key": "101:7",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَهُوَ فِي عِيشَةٖ رَّاضِيَةٖ",
      "translation": "Yẽnda bee vɩɩm pʋg b sẽn yard-ɑ.",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 600,
      "verse_key": "101:8",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَمَّا مَنۡ خَفَّتۡ مَوَٰزِينُهُۥ",
      "translation": "La sã n yaa ned ning tʋʋm-sõms mɑgb sẽn yɩ faagɑ, tɩ wẽngã wil-ɑ.",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 600,
      "verse_key": "101:9",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأُمُّهُۥ هَاوِيَةٞ",
      "translation": "Rẽnd yẽ gãag ziig yaa (hɑlkr lʋɩɩsẽ) Gεhɑnem pʋgẽ.",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 600,
      "verse_key": "101:10",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَآ أَدۡرَىٰكَ مَا هِيَهۡ",
      "translation": "La bõe n wilgd foo bõe la yẽ?",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 600,
      "verse_key": "101:11",
      "juz_number": 30,
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "نَارٌ حَامِيَةُۢ",
      "translation": "Yaa bug-tʋʋlga.",
      "footnotes": ""
    }
  ]
}