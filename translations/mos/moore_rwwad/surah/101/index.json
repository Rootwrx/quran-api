{
  "result": [
    {
      "id": "6158",
      "sura": "101",
      "aya": "1",
      "arabic_text": "ٱلۡقَارِعَةُ",
      "translation": "Wɑkɑt ning sẽn Koagsd nebã sũyã!",
      "footnotes": ""
    },
    {
      "id": "6159",
      "sura": "101",
      "aya": "2",
      "arabic_text": "مَا ٱلۡقَارِعَةُ",
      "translation": "Wɑkɑt sẽn koagsd nebã sũyã yɑɑ bʋgo?",
      "footnotes": ""
    },
    {
      "id": "6160",
      "sura": "101",
      "aya": "3",
      "arabic_text": "وَمَآ أَدۡرَىٰكَ مَا ٱلۡقَارِعَةُ",
      "translation": "La bõe n wildg foo koagsdã sẽn ya-a soaba?",
      "footnotes": ""
    },
    {
      "id": "6161",
      "sura": "101",
      "aya": "4",
      "arabic_text": "يَوۡمَ يَكُونُ ٱلنَّاسُ كَٱلۡفَرَاشِ ٱلۡمَبۡثُوثِ",
      "translation": "Yɑɑ raar ning nebã sẽn na n yɩ wala pilimpigd sẽn sãeege.",
      "footnotes": ""
    },
    {
      "id": "6162",
      "sura": "101",
      "aya": "5",
      "arabic_text": "وَتَكُونُ ٱلۡجِبَالُ كَٱلۡعِهۡنِ ٱلۡمَنفُوشِ",
      "translation": "Tɩ tãnsã lebg wala Pʋmpʋm kõbd b sẽn yelg-ɑ.",
      "footnotes": ""
    },
    {
      "id": "6163",
      "sura": "101",
      "aya": "6",
      "arabic_text": "فَأَمَّا مَن ثَقُلَتۡ مَوَٰزِينُهُۥ",
      "translation": "Sã n yaa ned ning tʋʋm-sõng mɑgb sẽn zɩsgã.",
      "footnotes": ""
    },
    {
      "id": "6164",
      "sura": "101",
      "aya": "7",
      "arabic_text": "فَهُوَ فِي عِيشَةٖ رَّاضِيَةٖ",
      "translation": "Yẽnda bee vɩɩm pʋg b sẽn yard-ɑ.",
      "footnotes": ""
    },
    {
      "id": "6165",
      "sura": "101",
      "aya": "8",
      "arabic_text": "وَأَمَّا مَنۡ خَفَّتۡ مَوَٰزِينُهُۥ",
      "translation": "La sã n yaa ned ning tʋʋm-sõms mɑgb sẽn yɩ faagɑ, tɩ wẽngã wil-ɑ.",
      "footnotes": ""
    },
    {
      "id": "6166",
      "sura": "101",
      "aya": "9",
      "arabic_text": "فَأُمُّهُۥ هَاوِيَةٞ",
      "translation": "Rẽnd yẽ gãag ziig yaa (hɑlkr lʋɩɩsẽ) Gεhɑnem pʋgẽ.",
      "footnotes": ""
    },
    {
      "id": "6167",
      "sura": "101",
      "aya": "10",
      "arabic_text": "وَمَآ أَدۡرَىٰكَ مَا هِيَهۡ",
      "translation": "La bõe n wilgd foo bõe la yẽ?",
      "footnotes": ""
    },
    {
      "id": "6168",
      "sura": "101",
      "aya": "11",
      "arabic_text": "نَارٌ حَامِيَةُۢ",
      "translation": "Yaa bug-tʋʋlga.",
      "footnotes": ""
    }
  ]
}