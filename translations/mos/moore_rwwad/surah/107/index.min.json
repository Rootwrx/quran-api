{
  "verses": [
    {
      "number": "1",
      "arabic_text": "أَرَءَيۡتَ ٱلَّذِي يُكَذِّبُ بِٱلدِّينِ",
      "translation": "F pa yã ned ning sẽn yagsd-a Rolbã rɑɑrã?"
    },
    {
      "number": "2",
      "arabic_text": "فَذَٰلِكَ ٱلَّذِي يَدُعُّ ٱلۡيَتِيمَ",
      "translation": "Yẽ la ned ning sẽn tusd kɩɩb tus-kεgenga."
    },
    {
      "number": "3",
      "arabic_text": "وَلَا يَحُضُّ عَلَىٰ طَعَامِ ٱلۡمِسۡكِينِ",
      "translation": "A leb n pa pirsd ne nimbãan-ned rɩlgr ye."
    },
    {
      "number": "4",
      "arabic_text": "فَوَيۡلٞ لِّلۡمُصَلِّينَ",
      "translation": "Rẽnd wɩɩbd be ne pʋʋsdba."
    },
    {
      "number": "5",
      "arabic_text": "ٱلَّذِينَ هُمۡ عَن صَلَاتِهِمۡ سَاهُونَ",
      "translation": "Pʋʋsdb nins sẽn maand yam yaal-yaal ne b Pʋʋsgã."
    },
    {
      "number": "6",
      "arabic_text": "ٱلَّذِينَ هُمۡ يُرَآءُونَ",
      "translation": "Neb nins sẽn yãngd nebã."
    },
    {
      "number": "7",
      "arabic_text": "وَيَمۡنَعُونَ ٱلۡمَاعُونَ",
      "translation": "La b mongd sõngr teedo."
    }
  ]
}