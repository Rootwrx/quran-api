{
  "verses": [
    {
      "number": "1",
      "arabic_text": "قُلۡ أَعُوذُ بِرَبِّ ٱلۡفَلَقِ",
      "translation": "Yeele : \"M kota gũudum ne yibeoog Soabã."
    },
    {
      "number": "2",
      "arabic_text": "مِن شَرِّ مَا خَلَقَ",
      "translation": "N yi A bõn-naandsã wẽnga."
    },
    {
      "number": "3",
      "arabic_text": "وَمِن شَرِّ غَاسِقٍ إِذَا وَقَبَ",
      "translation": "N leb n yi yʋng wẽng t`a sã n wa limi."
    },
    {
      "number": "4",
      "arabic_text": "وَمِن شَرِّ ٱلنَّفَّٰثَٰتِ فِي ٱلۡعُقَدِ",
      "translation": "N leb n yi tɩ-sɑood-pogsã wẽngã, b sẽn tʋbsd yoadã pʋgẽ wã."
    },
    {
      "number": "5",
      "arabic_text": "وَمِن شَرِّ حَاسِدٍ إِذَا حَسَدَ",
      "translation": "N leb n yi sũ-ki-ned wẽnga, t'a sã n wa kĩidẽ (maan sũkĩiri)\"."
    }
  ]
}