{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 586,
      "verse_key": "81:1",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذَا ٱلشَّمۡسُ كُوِّرَتۡ",
      "translation": "B sã n wa põd wĩndgã n lobge.",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 586,
      "verse_key": "81:2",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلنُّجُومُ ٱنكَدَرَتۡ",
      "translation": "Ãdsã sã n wa lʋɩtẽ.",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 586,
      "verse_key": "81:3",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡجِبَالُ سُيِّرَتۡ",
      "translation": "B sã n wa kẽnes tãmsã.",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 586,
      "verse_key": "81:4",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡعِشَارُ عُطِّلَتۡ",
      "translation": "B sã n wa maan yam-kaalg ne yʋgem tʋtsã.",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 586,
      "verse_key": "81:5",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡوُحُوشُ حُشِرَتۡ",
      "translation": "B sã n wa tigim we-rũmsã.",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 586,
      "verse_key": "81:6",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡبِحَارُ سُجِّرَتۡ",
      "translation": "B sã n wa yõgneg moagã.",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 586,
      "verse_key": "81:7",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلنُّفُوسُ زُوِّجَتۡ",
      "translation": "B sã n wa pẽneg yõyã ne b wõns n taabse.",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 586,
      "verse_key": "81:8",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡمَوۡءُۥدَةُ سُئِلَتۡ",
      "translation": "Tɩ kom-pugli b sẽn bus vɩɩsã B sã n wa sokd-bɑ.",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 586,
      "verse_key": "81:9",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بِأَيِّ ذَنۢبٖ قُتِلَتۡ",
      "translation": "Yaa bõe beegr yĩng la b kʋ-ba ?",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 586,
      "verse_key": "81:10",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلصُّحُفُ نُشِرَتۡ",
      "translation": "B sã n wa pʋɩ tʋʋmã karaase.",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 586,
      "verse_key": "81:11",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلسَّمَآءُ كُشِطَتۡ",
      "translation": "Lɑ saagã sã n wa fõke.",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 586,
      "verse_key": "81:12",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡجَحِيمُ سُعِّرَتۡ",
      "translation": "Lɑ B sã n wa widg Gεhɩɩme.",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 586,
      "verse_key": "81:13",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡجَنَّةُ أُزۡلِفَتۡ",
      "translation": "Lɑ Arzãnã B sã n wa pẽneg-ɑ.",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 586,
      "verse_key": "81:14",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلِمَتۡ نَفۡسٞ مَّآ أَحۡضَرَتۡ",
      "translation": "Yõor na bãng a sẽn tʋme.",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 586,
      "verse_key": "81:15",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلَآ أُقۡسِمُ بِٱلۡخُنَّسِ",
      "translation": "M wẽen ne ãd-solgdsã.",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 586,
      "verse_key": "81:16",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلۡجَوَارِ ٱلۡكُنَّسِ",
      "translation": "Ãd-kẽnds sẽn menemd tɩ zĩigã sã n yẽegã.",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 586,
      "verse_key": "81:17",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلَّيۡلِ إِذَا عَسۡعَسَ",
      "translation": "La ne yʋng t'a sã n wa limsi.",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 586,
      "verse_key": "81:18",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلصُّبۡحِ إِذَا تَنَفَّسَ",
      "translation": "La ne yibeoogo, t'a sã n wa vẽnege.",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 586,
      "verse_key": "81:19",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهُۥ لَقَوۡلُ رَسُولٖ كَرِيمٖ",
      "translation": "Ad yẽnda (Alkʋrãɑnɑ) yaa Wẽnd goɑm tɩ Tẽn-tʋʋm ɑ Gɩbriil sẽn yaa wagellã tɑɑs-ɑ.",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 586,
      "verse_key": "81:20",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ذِي قُوَّةٍ عِندَ ذِي ٱلۡعَرۡشِ مَكِينٖ",
      "translation": "Sẽn yaa pãng-kεgeng soaba, n tɑr ziir al-ʿars-zẽkdgã Soab nengẽ.",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 586,
      "verse_key": "81:21",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مُّطَاعٖ ثَمَّ أَمِينٖ",
      "translation": "N yaa b sẽn tũud-a soaba, n leb n yaa bas-m-yam soaba.",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 586,
      "verse_key": "81:22",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا صَاحِبُكُم بِمَجۡنُونٖ",
      "translation": "La yãmb tũud-n-taagã pa gãeeng ye.",
      "footnotes": ""
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 586,
      "verse_key": "81:23",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَقَدۡ رَءَاهُ بِٱلۡأُفُقِ ٱلۡمُبِينِ",
      "translation": "La hakɩɩka Tẽn-tʋʋmã yãa ɑ Gibriil sɑɑgã ween-n-vɩɩgã pʋgẽ.",
      "footnotes": ""
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 586,
      "verse_key": "81:24",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا هُوَ عَلَى ٱلۡغَيۡبِ بِضَنِينٖ",
      "translation": "La ad a pa yɩ n na maan beed ne yel-soaandɩ wã ye.",
      "footnotes": ""
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 586,
      "verse_key": "81:25",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا هُوَ بِقَوۡلِ شَيۡطَٰنٖ رَّجِيمٖ",
      "translation": "La a pa Sʋɩtãan b sẽn rig n zãag yolsgã gomd ye.",
      "footnotes": ""
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 586,
      "verse_key": "81:26",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَيۡنَ تَذۡهَبُونَ",
      "translation": "La yaa yε la yãmb rabda?",
      "footnotes": ""
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 586,
      "verse_key": "81:27",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنۡ هُوَ إِلَّا ذِكۡرٞ لِّلۡعَٰلَمِينَ",
      "translation": "A (Alkʋrãanã) pa yɩ ralla tẽegr ne bõn-naandsã.",
      "footnotes": ""
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 586,
      "verse_key": "81:28",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لِمَن شَآءَ مِنكُمۡ أَن يَسۡتَقِيمَ",
      "translation": "Ned ning sẽn wa n tʋll-a yãmb pʋgẽ bɩ a tẽeg tɩrga.",
      "footnotes": ""
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 586,
      "verse_key": "81:29",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا تَشَآءُونَ إِلَّآ أَن يَشَآءَ ٱللَّهُ رَبُّ ٱلۡعَٰلَمِينَ",
      "translation": "La yãmb pa tõe n tʋlla, ralla Wẽnd sã n tʋlla; bõn-naandsã Gʋʋndã.",
      "footnotes": ""
    }
  ]
}