{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 580,
      "verse_key": "77:1",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡمُرۡسَلَٰتِ عُرۡفٗا",
      "translation": "M wẽen ne sebdã sẽn fugdã.",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 580,
      "verse_key": "77:2",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَٱلۡعَٰصِفَٰتِ عَصۡفٗا",
      "translation": "La ne sob-kugdsã kugb tεkẽ.",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 580,
      "verse_key": "77:3",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلنَّٰشِرَٰتِ نَشۡرٗا",
      "translation": "La ne sob-fãeeds fãeegr tεkẽ.",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 580,
      "verse_key": "77:4",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَٱلۡفَٰرِقَٰتِ فَرۡقٗا",
      "translation": "La M wẽen ne Mɑlεk-rãmb nins sẽn sigd ne sẽn wεlgd sɩd lɑ zĩri sʋkã.",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 580,
      "verse_key": "77:5",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَٱلۡمُلۡقِيَٰتِ ذِكۡرًا",
      "translation": "La ne Mɑlεk-rãmb nins sẽn sigd ne wɑhɩ wã n leemsd aaya-rãmbã.",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 580,
      "verse_key": "77:6",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عُذۡرًا أَوۡ نُذۡرًا",
      "translation": "Padmes yĩng ma bugsg yĩnga.",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 580,
      "verse_key": "77:7",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّمَا تُوعَدُونَ لَوَٰقِعٞ",
      "translation": "Bũmb ning b sẽn pʋlem yãmbã na n yɩɩme.",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 580,
      "verse_key": "77:8",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِذَا ٱلنُّجُومُ طُمِسَتۡ",
      "translation": "La ãdsã sã n wa yẽese.",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 580,
      "verse_key": "77:9",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلسَّمَآءُ فُرِجَتۡ",
      "translation": "La saagã sã n wa pãrse.",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 580,
      "verse_key": "77:10",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلۡجِبَالُ نُسِفَتۡ",
      "translation": "La tãmsã sã n wa vigsi.",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 580,
      "verse_key": "77:11",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا ٱلرُّسُلُ أُقِّتَتۡ",
      "translation": "La tẽn-tʋʋmbã sã n wa ta b wakatã tɩ b tigim-bɑ.",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 580,
      "verse_key": "77:12",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لِأَيِّ يَوۡمٍ أُجِّلَتۡ",
      "translation": "La rabʋr yĩng la b yaoond yεla?",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 580,
      "verse_key": "77:13",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لِيَوۡمِ ٱلۡفَصۡلِ",
      "translation": "Yaa welgrã raar yĩnga.",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 580,
      "verse_key": "77:14",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَآ أَدۡرَىٰكَ مَا يَوۡمُ ٱلۡفَصۡلِ",
      "translation": "La bõe n wilgd foo welgr raar sẽn yaa a soaba?",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 580,
      "verse_key": "77:15",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ",
      "translation": "Wɩɩbd be ne zĩrimbe-rãmbã ra-kãng raare.",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 580,
      "verse_key": "77:16",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَمۡ نُهۡلِكِ ٱلۡأَوَّلِينَ",
      "translation": "Rẽ yĩngã Tõnd pa halk reng taoor rãmbã,",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 580,
      "verse_key": "77:17",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ نُتۡبِعُهُمُ ٱلۡأٓخِرِينَ",
      "translation": "Rẽ poorẽ tɩ d pʋglg bãmb neb a taabã?",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 580,
      "verse_key": "77:18",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَذَٰلِكَ نَفۡعَلُ بِٱلۡمُجۡرِمِينَ",
      "translation": "Woto bal la D tʋm ne nin-wẽnsã.",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 580,
      "verse_key": "77:19",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ",
      "translation": "Wɩɩbd be ne zĩrimbe-rãmbã ra-kãng raare.",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 581,
      "verse_key": "77:20",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَمۡ نَخۡلُقكُّم مِّن مَّآءٖ مَّهِينٖ",
      "translation": "Rẽ yĩnga D pa naan yãmb n yi ko-yaalga,",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 581,
      "verse_key": "77:21",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَجَعَلۡنَٰهُ فِي قَرَارٖ مَّكِينٍ",
      "translation": "Tɩ D ning-a tabendg zĩig sẽn tar gũubu.",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 581,
      "verse_key": "77:22",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِلَىٰ قَدَرٖ مَّعۡلُومٖ",
      "translation": "N tʋg wakat b sẽn mi.",
      "footnotes": ""
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 581,
      "verse_key": "77:23",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَقَدَرۡنَا فَنِعۡمَ ٱلۡقَٰدِرُونَ",
      "translation": "Tɩ Tõnd kord-ɑ nɑɑnegã, ad Tõnd kordg be neere.",
      "footnotes": ""
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 581,
      "verse_key": "77:24",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ",
      "translation": "Wɩɩbd be ne zĩrimbe-rãmbã ra-kãng raare.",
      "footnotes": ""
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 581,
      "verse_key": "77:25",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَمۡ نَجۡعَلِ ٱلۡأَرۡضَ كِفَاتًا",
      "translation": "Tõnd pa maan tẽngã tɩ yaa kʋmbda,",
      "footnotes": ""
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 581,
      "verse_key": "77:26",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَحۡيَآءٗ وَأَمۡوَٰتٗا",
      "translation": "Vɩtbã la kiidbã,",
      "footnotes": ""
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 581,
      "verse_key": "77:27",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَجَعَلۡنَا فِيهَا رَوَٰسِيَ شَٰمِخَٰتٖ وَأَسۡقَيۡنَٰكُم مَّآءٗ فُرَاتٗا",
      "translation": "La D ning a pʋgẽ tãens sẽn lugi, la D yũnug yãmb koom sẽn noome?",
      "footnotes": ""
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 581,
      "verse_key": "77:28",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ",
      "translation": "Wɩɩbd be ne zĩrimbe-rãmbã ra-kãng raare.",
      "footnotes": ""
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 581,
      "verse_key": "77:29",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱنطَلِقُوٓاْ إِلَىٰ مَا كُنتُم بِهِۦ تُكَذِّبُونَ",
      "translation": "Kẽng-y n tʋg bũmb ning y sẽn da yagsdã.",
      "footnotes": ""
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 581,
      "verse_key": "77:30",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱنطَلِقُوٓاْ إِلَىٰ ظِلّٖ ذِي ثَلَٰثِ شُعَبٖ",
      "translation": "Kẽng-y n tʋg maasem sẽn yaa buk a tãabo (gεhanm bug zõosẽ wã).",
      "footnotes": ""
    },
    {
      "id": 31,
      "verse_number": 31,
      "page_number": 581,
      "verse_key": "77:31",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَّا ظَلِيلٖ وَلَا يُغۡنِي مِنَ ٱللَّهَبِ",
      "translation": "Ka maasem, a pa gɩdgd bug-vilingã me tɩ pɑ tɑ-yã.",
      "footnotes": ""
    },
    {
      "id": 32,
      "verse_number": 32,
      "page_number": 581,
      "verse_key": "77:32",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّهَا تَرۡمِي بِشَرَرٖ كَٱلۡقَصۡرِ",
      "translation": "Ad a fãeeda bug-pãrs sẽn yaa wala gãsdo,",
      "footnotes": ""
    },
    {
      "id": 33,
      "verse_number": 33,
      "page_number": 581,
      "verse_key": "77:33",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَأَنَّهُۥ جِمَٰلَتٞ صُفۡرٞ",
      "translation": "A yaa wala yʋgm sɑblɑ.",
      "footnotes": ""
    },
    {
      "id": 34,
      "verse_number": 34,
      "page_number": 581,
      "verse_key": "77:34",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ",
      "translation": "Wɩɩbd be ne zĩrimbe-rãmbã ra-kãng raare.",
      "footnotes": ""
    },
    {
      "id": 35,
      "verse_number": 35,
      "page_number": 581,
      "verse_key": "77:35",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "هَٰذَا يَوۡمُ لَا يَنطِقُونَ",
      "translation": "Yãoã yaa raar b sẽn pa gomde.",
      "footnotes": ""
    },
    {
      "id": 36,
      "verse_number": 36,
      "page_number": 581,
      "verse_key": "77:36",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَا يُؤۡذَنُ لَهُمۡ فَيَعۡتَذِرُونَ",
      "translation": "La b pa kõt-b sor me tɩ b padem ye.",
      "footnotes": ""
    },
    {
      "id": 37,
      "verse_number": 37,
      "page_number": 581,
      "verse_key": "77:37",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ",
      "translation": "Wɩɩbd be ne zĩrimbe-rãmbã ra-kãng raare.",
      "footnotes": ""
    },
    {
      "id": 38,
      "verse_number": 38,
      "page_number": 581,
      "verse_key": "77:38",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "هَٰذَا يَوۡمُ ٱلۡفَصۡلِۖ جَمَعۡنَٰكُمۡ وَٱلۡأَوَّلِينَ",
      "translation": "Yãoã yaa welgrã raare, Tõnd tigma yãmb la rẽenem rãmba.",
      "footnotes": ""
    },
    {
      "id": 39,
      "verse_number": 39,
      "page_number": 581,
      "verse_key": "77:39",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِن كَانَ لَكُمۡ كَيۡدٞ فَكِيدُونِ",
      "translation": "Y sã n tara rabr bɩ y rabs-ma.",
      "footnotes": ""
    },
    {
      "id": 40,
      "verse_number": 40,
      "page_number": 581,
      "verse_key": "77:40",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ",
      "translation": "Wɩɩbd be ne zĩrimbe-rãmbã ra-kãng raare.",
      "footnotes": ""
    },
    {
      "id": 41,
      "verse_number": 41,
      "page_number": 581,
      "verse_key": "77:41",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ ٱلۡمُتَّقِينَ فِي ظِلَٰلٖ وَعُيُونٖ",
      "translation": "Ad wẽn-zoεtbã bee maasem pʋgẽ la ko-ninẽ.",
      "footnotes": ""
    },
    {
      "id": 42,
      "verse_number": 42,
      "page_number": 581,
      "verse_key": "77:42",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَفَوَٰكِهَ مِمَّا يَشۡتَهُونَ",
      "translation": "La tɩ-biisẽ bãmb sẽn nong a soaba.",
      "footnotes": ""
    },
    {
      "id": 43,
      "verse_number": 43,
      "page_number": 581,
      "verse_key": "77:43",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كُلُواْ وَٱشۡرَبُواْ هَنِيٓـَٔۢا بِمَا كُنتُمۡ تَعۡمَلُونَ",
      "translation": "Rɩ-y la y yũ, yaa beelg ne yãmb y sẽn da yɩ n tʋmdẽ wã sɑbɑb.",
      "footnotes": ""
    },
    {
      "id": 44,
      "verse_number": 44,
      "page_number": 581,
      "verse_key": "77:44",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّا كَذَٰلِكَ نَجۡزِي ٱلۡمُحۡسِنِينَ",
      "translation": "Ad yaa woto la Tõnd keood manegdbã.",
      "footnotes": ""
    },
    {
      "id": 45,
      "verse_number": 45,
      "page_number": 581,
      "verse_key": "77:45",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ",
      "translation": "Wɩɩbd be ne zĩrimbe-rãmbã ra-kãng raare.",
      "footnotes": ""
    },
    {
      "id": 46,
      "verse_number": 46,
      "page_number": 581,
      "verse_key": "77:46",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كُلُواْ وَتَمَتَّعُواْ قَلِيلًا إِنَّكُم مُّجۡرِمُونَ",
      "translation": "Rɩ-y-yã la y wʋmsd noog bilfu bala, ad yãmb yaa nin-wẽnse.",
      "footnotes": ""
    },
    {
      "id": 47,
      "verse_number": 47,
      "page_number": 581,
      "verse_key": "77:47",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ",
      "translation": "Wɩɩbd be ne zĩrimbe-rãmbã ra-kãng raare.",
      "footnotes": ""
    },
    {
      "id": 48,
      "verse_number": 48,
      "page_number": 581,
      "verse_key": "77:48",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِذَا قِيلَ لَهُمُ ٱرۡكَعُواْ لَا يَرۡكَعُونَ",
      "translation": "B sã n yeel-b tɩ b maan rukuu, b pa maand ye.",
      "footnotes": ""
    },
    {
      "id": 49,
      "verse_number": 49,
      "page_number": 581,
      "verse_key": "77:49",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ",
      "translation": "Wɩɩbd be ne zĩri-bemrãmbã ra-kãng raare.",
      "footnotes": ""
    },
    {
      "id": 50,
      "verse_number": 50,
      "page_number": 581,
      "verse_key": "77:50",
      "juz_number": 29,
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ حَدِيثِۭ بَعۡدَهُۥ يُؤۡمِنُونَ",
      "translation": "La yaa gomd bʋg la b kõt sɩda a (Alkʋrãanã) loogr poorẽ, ( tɩ y sã n pɑ kõ-sɩd ne Kɑrãɑnã)?",
      "footnotes": ""
    }
  ]
}