  "verses": [
    {
      "number": "1",
      "arabic_text": "إِنَّآ أَعۡطَيۡنَٰكَ ٱلۡكَوۡثَرَ",
      "translation": "Ad Tõnd kõo foom sõma wʋsgo."
    },
    {
      "number": "2",
      "arabic_text": "فَصَلِّ لِرَبِّكَ وَٱنۡحَرۡ",
      "translation": "Bɩ f pʋʋs f Soabã yĩng la f kõse."
    },
    {
      "number": "3",
      "arabic_text": "إِنَّ شَانِئَكَ هُوَ ٱلۡأَبۡتَرُ",
      "translation": "Ad fo gẽedã, yẽ vũun bal n wãage."
    }
  ]
