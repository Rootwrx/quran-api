{
  "result": [
    {
      "id": "5830",
      "sura": "82",
      "aya": "1",
      "arabic_text": "إِذَا ٱلسَّمَآءُ ٱنفَطَرَتۡ",
      "translation": "Saagã sã n wa pãrge.",
      "footnotes": ""
    },
    {
      "id": "5831",
      "sura": "82",
      "aya": "2",
      "arabic_text": "وَإِذَا ٱلۡكَوَاكِبُ ٱنتَثَرَتۡ",
      "translation": "La ãdsã sã n wa lʋɩtẽ.",
      "footnotes": ""
    },
    {
      "id": "5832",
      "sura": "82",
      "aya": "3",
      "arabic_text": "وَإِذَا ٱلۡبِحَارُ فُجِّرَتۡ",
      "translation": "La moagã sã n wa pus n yek tɑɑbɑ.",
      "footnotes": ""
    },
    {
      "id": "5833",
      "sura": "82",
      "aya": "4",
      "arabic_text": "وَإِذَا ٱلۡقُبُورُ بُعۡثِرَتۡ",
      "translation": "La kabɑ wã sã n wa bugsdẽ.",
      "footnotes": ""
    },
    {
      "id": "5834",
      "sura": "82",
      "aya": "5",
      "arabic_text": "عَلِمَتۡ نَفۡسٞ مَّا قَدَّمَتۡ وَأَخَّرَتۡ",
      "translation": "Yõor na bãng a sẽn tʋm tɩ reng taoor la a sẽn tʋm tɩ yaoole.",
      "footnotes": ""
    },
    {
      "id": "5835",
      "sura": "82",
      "aya": "6",
      "arabic_text": "يَٰٓأَيُّهَا ٱلۡإِنسَٰنُ مَا غَرَّكَ بِرَبِّكَ ٱلۡكَرِيمِ",
      "translation": "Yaa foo ninsaalã! Bõe n belg foom ne fo Soab wagellã,",
      "footnotes": ""
    },
    {
      "id": "5836",
      "sura": "82",
      "aya": "7",
      "arabic_text": "ٱلَّذِي خَلَقَكَ فَسَوَّىٰكَ فَعَدَلَكَ",
      "translation": "Ning sẽn naan-a foo, n zems-f taab n tẽeg-f tɩrga?",
      "footnotes": ""
    },
    {
      "id": "5837",
      "sura": "82",
      "aya": "8",
      "arabic_text": "فِيٓ أَيِّ صُورَةٖ مَّا شَآءَ رَكَّبَكَ",
      "translation": "Nɑɑnd ning A sẽn tʋll la A sigd foo.",
      "footnotes": ""
    },
    {
      "id": "5838",
      "sura": "82",
      "aya": "9",
      "arabic_text": "كَلَّا بَلۡ تُكَذِّبُونَ بِٱلدِّينِ",
      "translation": "Pa woto ye! Y kɩɩsda Rolbã raare.",
      "footnotes": ""
    },
    {
      "id": "5839",
      "sura": "82",
      "aya": "10",
      "arabic_text": "وَإِنَّ عَلَيۡكُمۡ لَحَٰفِظِينَ",
      "translation": "La ad gũusdb n be yãmb zutu,",
      "footnotes": ""
    },
    {
      "id": "5840",
      "sura": "82",
      "aya": "11",
      "arabic_text": "كِرَامٗا كَٰتِبِينَ",
      "translation": "N yaa wagla, n yaa gʋlsdba.",
      "footnotes": ""
    },
    {
      "id": "5841",
      "sura": "82",
      "aya": "12",
      "arabic_text": "يَعۡلَمُونَ مَا تَفۡعَلُونَ",
      "translation": "B mii yãmb sẽn tʋmdã.",
      "footnotes": ""
    },
    {
      "id": "5842",
      "sura": "82",
      "aya": "13",
      "arabic_text": "إِنَّ ٱلۡأَبۡرَارَ لَفِي نَعِيمٖ",
      "translation": "Ad manegdbã bee neema pʋga.",
      "footnotes": ""
    },
    {
      "id": "5843",
      "sura": "82",
      "aya": "14",
      "arabic_text": "وَإِنَّ ٱلۡفُجَّارَ لَفِي جَحِيمٖ",
      "translation": "La ad nin-wẽnsã bee Gεhɩɩm pʋgẽ.",
      "footnotes": ""
    },
    {
      "id": "5844",
      "sura": "82",
      "aya": "15",
      "arabic_text": "يَصۡلَوۡنَهَا يَوۡمَ ٱلدِّينِ",
      "translation": "B kẽed-a lame Rolbã raare.",
      "footnotes": ""
    },
    {
      "id": "5845",
      "sura": "82",
      "aya": "16",
      "arabic_text": "وَمَا هُمۡ عَنۡهَا بِغَآئِبِينَ",
      "translation": "La b pa na n tol n lill n bas-a ye.",
      "footnotes": ""
    },
    {
      "id": "5846",
      "sura": "82",
      "aya": "17",
      "arabic_text": "وَمَآ أَدۡرَىٰكَ مَا يَوۡمُ ٱلدِّينِ",
      "translation": "La bõe n wilg foo Rolbã raare?",
      "footnotes": ""
    },
    {
      "id": "5847",
      "sura": "82",
      "aya": "18",
      "arabic_text": "ثُمَّ مَآ أَدۡرَىٰكَ مَا يَوۡمُ ٱلدِّينِ",
      "translation": "Rẽ poorẽ, bõe n wilgd foo Rolbã raare?",
      "footnotes": ""
    },
    {
      "id": "5848",
      "sura": "82",
      "aya": "19",
      "arabic_text": "يَوۡمَ لَا تَمۡلِكُ نَفۡسٞ لِّنَفۡسٖ شَيۡـٔٗاۖ وَٱلۡأَمۡرُ يَوۡمَئِذٖ لِّلَّهِ",
      "translation": "Raar ning yõor sẽn pa so a yõ to ne ba fʋɩ; raaren-kãngã, yellã bee ne Wẽnde.",
      "footnotes": ""
    }
  ]
}