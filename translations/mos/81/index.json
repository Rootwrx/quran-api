{
  "result": [
    {
      "id": "5801",
      "sura": "81",
      "aya": "1",
      "arabic_text": "إِذَا ٱلشَّمۡسُ كُوِّرَتۡ",
      "translation": "B sã n wa põd wĩndgã n lobge.",
      "footnotes": ""
    },
    {
      "id": "5802",
      "sura": "81",
      "aya": "2",
      "arabic_text": "وَإِذَا ٱلنُّجُومُ ٱنكَدَرَتۡ",
      "translation": "Ãdsã sã n wa lʋɩtẽ.",
      "footnotes": ""
    },
    {
      "id": "5803",
      "sura": "81",
      "aya": "3",
      "arabic_text": "وَإِذَا ٱلۡجِبَالُ سُيِّرَتۡ",
      "translation": "B sã n wa kẽnes tãmsã.",
      "footnotes": ""
    },
    {
      "id": "5804",
      "sura": "81",
      "aya": "4",
      "arabic_text": "وَإِذَا ٱلۡعِشَارُ عُطِّلَتۡ",
      "translation": "B sã n wa maan yam-kaalg ne yʋgem tʋtsã.",
      "footnotes": ""
    },
    {
      "id": "5805",
      "sura": "81",
      "aya": "5",
      "arabic_text": "وَإِذَا ٱلۡوُحُوشُ حُشِرَتۡ",
      "translation": "B sã n wa tigim we-rũmsã.",
      "footnotes": ""
    },
    {
      "id": "5806",
      "sura": "81",
      "aya": "6",
      "arabic_text": "وَإِذَا ٱلۡبِحَارُ سُجِّرَتۡ",
      "translation": "B sã n wa yõgneg moagã.",
      "footnotes": ""
    },
    {
      "id": "5807",
      "sura": "81",
      "aya": "7",
      "arabic_text": "وَإِذَا ٱلنُّفُوسُ زُوِّجَتۡ",
      "translation": "B sã n wa pẽneg yõyã ne b wõns n taabse.",
      "footnotes": ""
    },
    {
      "id": "5808",
      "sura": "81",
      "aya": "8",
      "arabic_text": "وَإِذَا ٱلۡمَوۡءُۥدَةُ سُئِلَتۡ",
      "translation": "Tɩ kom-pugli b sẽn bus vɩɩsã B sã n wa sokd-bɑ.",
      "footnotes": ""
    },
    {
      "id": "5809",
      "sura": "81",
      "aya": "9",
      "arabic_text": "بِأَيِّ ذَنۢبٖ قُتِلَتۡ",
      "translation": "Yaa bõe beegr yĩng la b kʋ-ba ?",
      "footnotes": ""
    },
    {
      "id": "5810",
      "sura": "81",
      "aya": "10",
      "arabic_text": "وَإِذَا ٱلصُّحُفُ نُشِرَتۡ",
      "translation": "B sã n wa pʋɩ tʋʋmã karaase.",
      "footnotes": ""
    },
    {
      "id": "5811",
      "sura": "81",
      "aya": "11",
      "arabic_text": "وَإِذَا ٱلسَّمَآءُ كُشِطَتۡ",
      "translation": "Lɑ saagã sã n wa fõke.",
      "footnotes": ""
    },
    {
      "id": "5812",
      "sura": "81",
      "aya": "12",
      "arabic_text": "وَإِذَا ٱلۡجَحِيمُ سُعِّرَتۡ",
      "translation": "Lɑ B sã n wa widg Gεhɩɩme.",
      "footnotes": ""
    },
    {
      "id": "5813",
      "sura": "81",
      "aya": "13",
      "arabic_text": "وَإِذَا ٱلۡجَنَّةُ أُزۡلِفَتۡ",
      "translation": "Lɑ Arzãnã B sã n wa pẽneg-ɑ.",
      "footnotes": ""
    },
    {
      "id": "5814",
      "sura": "81",
      "aya": "14",
      "arabic_text": "عَلِمَتۡ نَفۡسٞ مَّآ أَحۡضَرَتۡ",
      "translation": "Yõor na bãng a sẽn tʋme.",
      "footnotes": ""
    },
    {
      "id": "5815",
      "sura": "81",
      "aya": "15",
      "arabic_text": "فَلَآ أُقۡسِمُ بِٱلۡخُنَّسِ",
      "translation": "M wẽen ne ãd-solgdsã.",
      "footnotes": ""
    },
    {
      "id": "5816",
      "sura": "81",
      "aya": "16",
      "arabic_text": "ٱلۡجَوَارِ ٱلۡكُنَّسِ",
      "translation": "Ãd-kẽnds sẽn menemd tɩ zĩigã sã n yẽegã.",
      "footnotes": ""
    },
    {
      "id": "5817",
      "sura": "81",
      "aya": "17",
      "arabic_text": "وَٱلَّيۡلِ إِذَا عَسۡعَسَ",
      "translation": "La ne yʋng t'a sã n wa limsi.",
      "footnotes": ""
    },
    {
      "id": "5818",
      "sura": "81",
      "aya": "18",
      "arabic_text": "وَٱلصُّبۡحِ إِذَا تَنَفَّسَ",
      "translation": "La ne yibeoogo, t'a sã n wa vẽnege.",
      "footnotes": ""
    },
    {
      "id": "5819",
      "sura": "81",
      "aya": "19",
      "arabic_text": "إِنَّهُۥ لَقَوۡلُ رَسُولٖ كَرِيمٖ",
      "translation": "Ad yẽnda (Alkʋrãɑnɑ) yaa Wẽnd goɑm tɩ Tẽn-tʋʋm ɑ Gɩbriil sẽn yaa wagellã tɑɑs-ɑ.",
      "footnotes": ""
    },
    {
      "id": "5820",
      "sura": "81",
      "aya": "20",
      "arabic_text": "ذِي قُوَّةٍ عِندَ ذِي ٱلۡعَرۡشِ مَكِينٖ",
      "translation": "Sẽn yaa pãng-kεgeng soaba, n tɑr ziir al-ʿars-zẽkdgã Soab nengẽ.",
      "footnotes": ""
    },
    {
      "id": "5821",
      "sura": "81",
      "aya": "21",
      "arabic_text": "مُّطَاعٖ ثَمَّ أَمِينٖ",
      "translation": "N yaa b sẽn tũud-a soaba, n leb n yaa bas-m-yam soaba.",
      "footnotes": ""
    },
    {
      "id": "5822",
      "sura": "81",
      "aya": "22",
      "arabic_text": "وَمَا صَاحِبُكُم بِمَجۡنُونٖ",
      "translation": "La yãmb tũud-n-taagã pa gãeeng ye.",
      "footnotes": ""
    },
    {
      "id": "5823",
      "sura": "81",
      "aya": "23",
      "arabic_text": "وَلَقَدۡ رَءَاهُ بِٱلۡأُفُقِ ٱلۡمُبِينِ",
      "translation": "La hakɩɩka Tẽn-tʋʋmã yãa ɑ Gibriil sɑɑgã ween-n-vɩɩgã pʋgẽ.",
      "footnotes": ""
    },
    {
      "id": "5824",
      "sura": "81",
      "aya": "24",
      "arabic_text": "وَمَا هُوَ عَلَى ٱلۡغَيۡبِ بِضَنِينٖ",
      "translation": "La ad a pa yɩ n na maan beed ne yel-soaandɩ wã ye.",
      "footnotes": ""
    },
    {
      "id": "5825",
      "sura": "81",
      "aya": "25",
      "arabic_text": "وَمَا هُوَ بِقَوۡلِ شَيۡطَٰنٖ رَّجِيمٖ",
      "translation": "La a pa Sʋɩtãan b sẽn rig n zãag yolsgã gomd ye.",
      "footnotes": ""
    },
    {
      "id": "5826",
      "sura": "81",
      "aya": "26",
      "arabic_text": "فَأَيۡنَ تَذۡهَبُونَ",
      "translation": "La yaa yε la yãmb rabda?",
      "footnotes": ""
    },
    {
      "id": "5827",
      "sura": "81",
      "aya": "27",
      "arabic_text": "إِنۡ هُوَ إِلَّا ذِكۡرٞ لِّلۡعَٰلَمِينَ",
      "translation": "A (Alkʋrãanã) pa yɩ ralla tẽegr ne bõn-naandsã.",
      "footnotes": ""
    },
    {
      "id": "5828",
      "sura": "81",
      "aya": "28",
      "arabic_text": "لِمَن شَآءَ مِنكُمۡ أَن يَسۡتَقِيمَ",
      "translation": "Ned ning sẽn wa n tʋll-a yãmb pʋgẽ bɩ a tẽeg tɩrga.",
      "footnotes": ""
    },
    {
      "id": "5829",
      "sura": "81",
      "aya": "29",
      "arabic_text": "وَمَا تَشَآءُونَ إِلَّآ أَن يَشَآءَ ٱللَّهُ رَبُّ ٱلۡعَٰلَمِينَ",
      "translation": "La yãmb pa tõe n tʋlla, ralla Wẽnd sã n tʋlla; bõn-naandsã Gʋʋndã.",
      "footnotes": ""
    }
  ]
}