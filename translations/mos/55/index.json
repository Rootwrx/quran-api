{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 531,
      "verse_key": "55:1",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلرَّحۡمَٰنُ",
      "translation": "Yols-kεgeng Naaba.",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 531,
      "verse_key": "55:2",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلَّمَ ٱلۡقُرۡءَانَ",
      "translation": "A wilga Alkʋrãanã.",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 531,
      "verse_key": "55:3",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "خَلَقَ ٱلۡإِنسَٰنَ",
      "translation": "A naana ninsaalã.",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 531,
      "verse_key": "55:4",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلَّمَهُ ٱلۡبَيَانَ",
      "translation": "A wilg-a-la zelem-pεka.",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 531,
      "verse_key": "55:5",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلشَّمۡسُ وَٱلۡقَمَرُ بِحُسۡبَانٖ",
      "translation": "Wĩndgã ne kiuugã, yaa geelg tεkẽ.",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 531,
      "verse_key": "55:6",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلنَّجۡمُ وَٱلشَّجَرُ يَسۡجُدَانِ",
      "translation": "Adsã ne tɩɩsã maanda Suguudu.",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 531,
      "verse_key": "55:7",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلسَّمَآءَ رَفَعَهَا وَوَضَعَ ٱلۡمِيزَانَ",
      "translation": "La saagã, A zẽk-a lame, la A ning tẽeg tɩrga.",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 531,
      "verse_key": "55:8",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَّا تَطۡغَوۡاْ فِي ٱلۡمِيزَانِ",
      "translation": "Tɩ y ra kedg magbẽ wã ye.",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 531,
      "verse_key": "55:9",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَقِيمُواْ ٱلۡوَزۡنَ بِٱلۡقِسۡطِ وَلَا تُخۡسِرُواْ ٱلۡمِيزَانَ",
      "translation": "La y yãnes ne magbã ne tẽeg tɩrg la y ra boog magbã ye.",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 531,
      "verse_key": "55:10",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡأَرۡضَ وَضَعَهَا لِلۡأَنَامِ",
      "translation": "La Tẽnga, A rɩgl-a-la bõn-naandsã yĩnga.",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 531,
      "verse_key": "55:11",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِيهَا فَٰكِهَةٞ وَٱلنَّخۡلُ ذَاتُ ٱلۡأَكۡمَامِ",
      "translation": "Tɩ-biis be a pʋgẽ la tamar-tɩɩse sẽn tar wʋʋdo.",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 531,
      "verse_key": "55:12",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡحَبُّ ذُو ٱلۡعَصۡفِ وَٱلرَّيۡحَانُ",
      "translation": "La bõn-bεεl sẽn tar pʋgd la (Rɑyhɑɑn) mõog sẽn tɑr yũ-noodo.",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 531,
      "verse_key": "55:13",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "La yaa yãmb y yiibã Soabã neema bʋg la y yagsda?",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 531,
      "verse_key": "55:14",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "خَلَقَ ٱلۡإِنسَٰنَ مِن صَلۡصَٰلٖ كَٱلۡفَخَّارِ",
      "translation": "A naana ninsaalã ne tãm-maasr sẽn kʋɩ n lebg koe-koe wala yagd sẽn zugi.",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 531,
      "verse_key": "55:15",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَخَلَقَ ٱلۡجَآنَّ مِن مَّارِجٖ مِّن نَّارٖ",
      "translation": "la A naan zĩn-dãmbã ne bug-witim.",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 531,
      "verse_key": "55:16",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "La yaa yãmb y yiibã Soabã neema bʋg la y yagsda?",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 532,
      "verse_key": "55:17",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "رَبُّ ٱلۡمَشۡرِقَيۡنِ وَرَبُّ ٱلۡمَغۡرِبَيۡنِ",
      "translation": "Wĩnto-pʋkdgẽ a yiib Naab la wĩnto-lʋɩɩngẽ a yiib Naaba.",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 532,
      "verse_key": "55:18",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "La yaa yãmb y yiibã Soabã neema bʋg la y yagsda?",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 532,
      "verse_key": "55:19",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَرَجَ ٱلۡبَحۡرَيۡنِ يَلۡتَقِيَانِ",
      "translation": "A basa mogã a yiibã tɩ b yekd taaba.",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 532,
      "verse_key": "55:20",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بَيۡنَهُمَا بَرۡزَخٞ لَّا يَبۡغِيَانِ",
      "translation": "B yiibã sʋka kogr n be, b pa zʋʋgd taab ye.",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 532,
      "verse_key": "55:21",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "La yaa yãmb y yiibã Soabã neema bʋg la y yagsda?",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 532,
      "verse_key": "55:22",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَخۡرُجُ مِنۡهُمَا ٱللُّؤۡلُؤُ وَٱلۡمَرۡجَانُ",
      "translation": "Yitame, b yiibã pʋgẽ, Lʋʋlʋʋ la Margεεne.",
      "footnotes": ""
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 532,
      "verse_key": "55:23",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "La yaa yãmb y yiibã Soabã neema bʋg la y yagsda?",
      "footnotes": ""
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 532,
      "verse_key": "55:24",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَهُ ٱلۡجَوَارِ ٱلۡمُنشَـَٔاتُ فِي ٱلۡبَحۡرِ كَٱلۡأَعۡلَٰمِ",
      "translation": "La a tara kogend-zãnds mogrã pʋgẽ tɩ yaa wala tãmse.",
      "footnotes": ""
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 532,
      "verse_key": "55:25",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "La yaa yãmb y yiibã Soabã neema bʋg la y yagsda?",
      "footnotes": ""
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 532,
      "verse_key": "55:26",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كُلُّ مَنۡ عَلَيۡهَا فَانٖ",
      "translation": "Sẽn be (Tẽngã) zug fãa nɑ n sɑɑme.",
      "footnotes": ""
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 532,
      "verse_key": "55:27",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَيَبۡقَىٰ وَجۡهُ رَبِّكَ ذُو ٱلۡجَلَٰلِ وَٱلۡإِكۡرَامِ",
      "translation": "Fo Soabã bal n paade, Waooglem Naaba, la Waoogr Naaba.",
      "footnotes": ""
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 532,
      "verse_key": "55:28",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "La yaa yãmb y yiibã Soabã neema bʋg la y yagsda?",
      "footnotes": ""
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 532,
      "verse_key": "55:29",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَسۡـَٔلُهُۥ مَن فِي ٱلسَّمَٰوَٰتِ وَٱلۡأَرۡضِۚ كُلَّ يَوۡمٍ هُوَ فِي شَأۡنٖ",
      "translation": "Sẽn be saasẽ la tẽng bõosd-A lame, raar fãa A bee tʋʋmd pʋgẽ.",
      "footnotes": ""
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 532,
      "verse_key": "55:30",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "La yaa yãmb y yiibã Soabã neema bʋg la y yagsda?",
      "footnotes": ""
    },
    {
      "id": 31,
      "verse_number": 31,
      "page_number": 532,
      "verse_key": "55:31",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "سَنَفۡرُغُ لَكُمۡ أَيُّهَ ٱلثَّقَلَانِ",
      "translation": "D na n yãkɑ wɑkɑt yãmb (bʋʋdã) yĩnga, yãmb ɑ bõn-zɩsd a yiibu(ninsɑɑlbã lɑ zĩndãmbã)!",
      "footnotes": ""
    },
    {
      "id": 32,
      "verse_number": 32,
      "page_number": 532,
      "verse_key": "55:32",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "La yaa yãmb y yiibã Soabã neema bʋg la y yagsda?",
      "footnotes": ""
    },
    {
      "id": 33,
      "verse_number": 33,
      "page_number": 532,
      "verse_key": "55:33",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَٰمَعۡشَرَ ٱلۡجِنِّ وَٱلۡإِنسِ إِنِ ٱسۡتَطَعۡتُمۡ أَن تَنفُذُواْ مِنۡ أَقۡطَارِ ٱلسَّمَٰوَٰتِ وَٱلۡأَرۡضِ فَٱنفُذُواْۚ لَا تَنفُذُونَ إِلَّا بِسُلۡطَٰنٖ",
      "translation": "Yaa yãmb zĩn-rãmb la ninsaalb zãma, yãmb sã n tõe n yi saasã la Tẽngã vɩʋʋgẽ, bɩ y yi, yãmb pa yit rẽnda ne tõogo.",
      "footnotes": ""
    },
    {
      "id": 34,
      "verse_number": 34,
      "page_number": 532,
      "verse_key": "55:34",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "La yaa yãmb y yiibã Soabã neema bʋg la y yagsda?",
      "footnotes": ""
    },
    {
      "id": 35,
      "verse_number": 35,
      "page_number": 532,
      "verse_key": "55:35",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يُرۡسَلُ عَلَيۡكُمَا شُوَاظٞ مِّن نَّارٖ وَنُحَاسٞ فَلَا تَنتَصِرَانِ",
      "translation": "B tʋmsda yãmb y yiibã zug bug-pãrs la bug-zõose (ma zũud sẽn yẽnege) tɩ yãmb y yiibã pa paamd sõngr ye.",
      "footnotes": ""
    },
    {
      "id": 36,
      "verse_number": 36,
      "page_number": 532,
      "verse_key": "55:36",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "La yaa yãmb y yiibã Soabã neema bʋg la y yagsda?",
      "footnotes": ""
    },
    {
      "id": 37,
      "verse_number": 37,
      "page_number": 532,
      "verse_key": "55:37",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِذَا ٱنشَقَّتِ ٱلسَّمَآءُ فَكَانَتۡ وَرۡدَةٗ كَٱلدِّهَانِ",
      "translation": "La saagã sã n wa pãrg n lebg miuug wala rãngo.",
      "footnotes": ""
    },
    {
      "id": 38,
      "verse_number": 38,
      "page_number": 532,
      "verse_key": "55:38",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "La yaa yãmb y yiibã Soabã neema bʋg la y yagsda?",
      "footnotes": ""
    },
    {
      "id": 39,
      "verse_number": 39,
      "page_number": 532,
      "verse_key": "55:39",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَيَوۡمَئِذٖ لَّا يُسۡـَٔلُ عَن ذَنۢبِهِۦٓ إِنسٞ وَلَا جَآنّٞ",
      "translation": "La ba-raare, zĩn-dãmb la ninsaalb pa soakd-A b zunuub yell ye.",
      "footnotes": ""
    },
    {
      "id": 40,
      "verse_number": 40,
      "page_number": 532,
      "verse_key": "55:40",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "La yaa yãmb y yiibã Soabã neema bʋg la y yagsda?",
      "footnotes": ""
    },
    {
      "id": 41,
      "verse_number": 41,
      "page_number": 533,
      "verse_key": "55:41",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يُعۡرَفُ ٱلۡمُجۡرِمُونَ بِسِيمَٰهُمۡ فَيُؤۡخَذُ بِٱلنَّوَٰصِي وَٱلۡأَقۡدَامِ",
      "translation": "B bãngda nin-wẽensã ne b tagmasã, tɩ b yõkd-b ne rĩ-gĩin la na-tɑgdɑ.",
      "footnotes": ""
    },
    {
      "id": 42,
      "verse_number": 42,
      "page_number": 533,
      "verse_key": "55:42",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "La yaa yãmb y yiibã Soabã neema bʋg la y yagsda?",
      "footnotes": ""
    },
    {
      "id": 43,
      "verse_number": 43,
      "page_number": 533,
      "verse_key": "55:43",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "هَٰذِهِۦ جَهَنَّمُ ٱلَّتِي يُكَذِّبُ بِهَا ٱلۡمُجۡرِمُونَ",
      "translation": "Yaa wãnde la Gεhɑnnem ning nin-wẽensã sẽn yagsd-a wã.",
      "footnotes": ""
    },
    {
      "id": 44,
      "verse_number": 44,
      "page_number": 533,
      "verse_key": "55:44",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَطُوفُونَ بَيۡنَهَا وَبَيۡنَ حَمِيمٍ ءَانٖ",
      "translation": "B gild a (Bʋgmã) sʋka la ko-kaeeds sẽn ta tεk sʋka.",
      "footnotes": ""
    },
    {
      "id": 45,
      "verse_number": 45,
      "page_number": 533,
      "verse_key": "55:45",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "La yaa yãmb y yiibã Soabã neema bʋg la y yagsda?",
      "footnotes": ""
    },
    {
      "id": 46,
      "verse_number": 46,
      "page_number": 533,
      "verse_key": "55:46",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلِمَنۡ خَافَ مَقَامَ رَبِّهِۦ جَنَّتَانِ",
      "translation": "La be ne ned ning sẽn yaeesd yɑlsgã ɑ Soabã tɑoorã Arzãn a yiibu.",
      "footnotes": ""
    },
    {
      "id": 47,
      "verse_number": 47,
      "page_number": 533,
      "verse_key": "55:47",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "La yaa yãmb y yiibã Soabã neema bʋg la y yagsda?",
      "footnotes": ""
    },
    {
      "id": 48,
      "verse_number": 48,
      "page_number": 533,
      "verse_key": "55:48",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ذَوَاتَآ أَفۡنَانٖ",
      "translation": "Arzẽn ɑ yiibu sẽn tar wil a yiib sẽn ya bãmm,",
      "footnotes": ""
    },
    {
      "id": 49,
      "verse_number": 49,
      "page_number": 533,
      "verse_key": "55:49",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "La yaa yãmb y yiibã Soabã neema bʋg la y yagsda?",
      "footnotes": ""
    },
    {
      "id": 50,
      "verse_number": 50,
      "page_number": 533,
      "verse_key": "55:50",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِيهِمَا عَيۡنَانِ تَجۡرِيَانِ",
      "translation": "Be b yiibã pʋgẽ, ko-nin a yiib n zoete.",
      "footnotes": ""
    },
    {
      "id": 51,
      "verse_number": 51,
      "page_number": 533,
      "verse_key": "55:51",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "La yaa yãmb y yiibã Soabã neema bʋg la y yagsda?",
      "footnotes": ""
    },
    {
      "id": 52,
      "verse_number": 52,
      "page_number": 533,
      "verse_key": "55:52",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِيهِمَا مِن كُلِّ فَٰكِهَةٖ زَوۡجَانِ",
      "translation": "Be a yiibã pʋgẽ, sẽn yi tɩ-biis fãa buudu, kõbg a yiibu.",
      "footnotes": ""
    },
    {
      "id": 53,
      "verse_number": 53,
      "page_number": 533,
      "verse_key": "55:53",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "La yaa yãmb y yiibã Soabã neema bʋg la y yagsda?",
      "footnotes": ""
    },
    {
      "id": 54,
      "verse_number": 54,
      "page_number": 533,
      "verse_key": "55:54",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مُتَّكِـِٔينَ عَلَىٰ فُرُشِۭ بَطَآئِنُهَا مِنۡ إِسۡتَبۡرَقٖۚ وَجَنَى ٱلۡجَنَّتَيۡنِ دَانٖ",
      "translation": "B yaa sẽn kɩl gãt zutu, a fu-tɩʋʋdsã yaa ɭstabrak (fu-faas sẽn bʋgse) la arzãn a yiibã tɩ-biisã pẽeme.",
      "footnotes": ""
    },
    {
      "id": 55,
      "verse_number": 55,
      "page_number": 533,
      "verse_key": "55:55",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "La yaa yãmb y yiibã Soabã neema bʋg la y yagsda?",
      "footnotes": ""
    },
    {
      "id": 56,
      "verse_number": 56,
      "page_number": 533,
      "verse_key": "55:56",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِيهِنَّ قَٰصِرَٰتُ ٱلطَّرۡفِ لَمۡ يَطۡمِثۡهُنَّ إِنسٞ قَبۡلَهُمۡ وَلَا جَآنّٞ",
      "translation": "Ges koees rãmb n (Huur Aεεn) be b pʋgẽ, ninsaalb pa sɩɩs-b bãmb (sɩdbã) taoore, pa zĩn-dãmba.",
      "footnotes": ""
    },
    {
      "id": 57,
      "verse_number": 57,
      "page_number": 533,
      "verse_key": "55:57",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "La yaa yãmb y yiibã Soabã neema bʋg la y yagsda?",
      "footnotes": ""
    },
    {
      "id": 58,
      "verse_number": 58,
      "page_number": 533,
      "verse_key": "55:58",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَأَنَّهُنَّ ٱلۡيَاقُوتُ وَٱلۡمَرۡجَانُ",
      "translation": "B yaa wala Yaakʋʋt la Margεεn.",
      "footnotes": ""
    },
    {
      "id": 59,
      "verse_number": 59,
      "page_number": 533,
      "verse_key": "55:59",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "La yaa yãmb y yiibã Soabã neema bʋg la y yagsda?",
      "footnotes": ""
    },
    {
      "id": 60,
      "verse_number": 60,
      "page_number": 533,
      "verse_key": "55:60",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "هَلۡ جَزَآءُ ٱلۡإِحۡسَٰنِ إِلَّا ٱلۡإِحۡسَٰنُ",
      "translation": "Rẽ yĩnga manegr tara rolb tɩ sã n pa manegre?",
      "footnotes": ""
    },
    {
      "id": 61,
      "verse_number": 61,
      "page_number": 533,
      "verse_key": "55:61",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "La yaa yãmb y yiibã Soabã neema bʋg la y yagsda?",
      "footnotes": ""
    },
    {
      "id": 62,
      "verse_number": 62,
      "page_number": 533,
      "verse_key": "55:62",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمِن دُونِهِمَا جَنَّتَانِ",
      "translation": "Sẽn leb n paasd bãmb b yiibã, yaa arzãn a yiib yεsa.",
      "footnotes": ""
    },
    {
      "id": 63,
      "verse_number": 63,
      "page_number": 533,
      "verse_key": "55:63",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "La yaa yãmb y yiibã Soabã neema bʋg la y yagsda?",
      "footnotes": ""
    },
    {
      "id": 64,
      "verse_number": 64,
      "page_number": 533,
      "verse_key": "55:64",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مُدۡهَآمَّتَانِ",
      "translation": "Sẽn yaa kẽeg n bĩmi.",
      "footnotes": ""
    },
    {
      "id": 65,
      "verse_number": 65,
      "page_number": 533,
      "verse_key": "55:65",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "La yaa yãmb y yiibã Soabã neemãa bʋg la y yagsda?",
      "footnotes": ""
    },
    {
      "id": 66,
      "verse_number": 66,
      "page_number": 533,
      "verse_key": "55:66",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِيهِمَا عَيۡنَانِ نَضَّاخَتَانِ",
      "translation": "Ko-nin a yiib n be b yiibã pʋgẽ n fudgdẽ.",
      "footnotes": ""
    },
    {
      "id": 67,
      "verse_number": 67,
      "page_number": 533,
      "verse_key": "55:67",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "La yaa yãmb y yiibã Soabã neema bʋg la y yagsda?",
      "footnotes": ""
    },
    {
      "id": 68,
      "verse_number": 68,
      "page_number": 534,
      "verse_key": "55:68",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِيهِمَا فَٰكِهَةٞ وَنَخۡلٞ وَرُمَّانٞ",
      "translation": "Tɩ-biis n be b yiibã pʋgẽ, la tamar-tɩɩs la Rommaan (gerenadiye).",
      "footnotes": ""
    },
    {
      "id": 69,
      "verse_number": 69,
      "page_number": 534,
      "verse_key": "55:69",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "La yaa yãmb y yiibã Soabã neema bʋg la y yagsda?",
      "footnotes": ""
    },
    {
      "id": 70,
      "verse_number": 70,
      "page_number": 534,
      "verse_key": "55:70",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِيهِنَّ خَيۡرَٰتٌ حِسَانٞ",
      "translation": "Pʋg-sõms n be a pʋgẽ n yaa pʋg-neeba.",
      "footnotes": ""
    },
    {
      "id": 71,
      "verse_number": 71,
      "page_number": 534,
      "verse_key": "55:71",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "La yaa yãmb y yiibã Soabã neema bʋg la y yagsda?",
      "footnotes": ""
    },
    {
      "id": 72,
      "verse_number": 72,
      "page_number": 534,
      "verse_key": "55:72",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "حُورٞ مَّقۡصُورَٰتٞ فِي ٱلۡخِيَامِ",
      "translation": "Pʋg-nin-sabl sẽn ya yõk-m-mens-rãmb n be zãn tẽngre.",
      "footnotes": ""
    },
    {
      "id": 73,
      "verse_number": 73,
      "page_number": 534,
      "verse_key": "55:73",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "La yaa yãmb y yiibã Soabã neema bʋg la y yagsda?",
      "footnotes": ""
    },
    {
      "id": 74,
      "verse_number": 74,
      "page_number": 534,
      "verse_key": "55:74",
      "juz_number": 27,
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَمۡ يَطۡمِثۡهُنَّ إِنسٞ قَبۡلَهُمۡ وَلَا جَآنّٞ",
      "translation": "Ninsaalb pa sɩɩs-ba, bãmb (sɩdbã) taoor ye, pa zĩn-dãmba.",
      "footnotes": ""
    },
    {
      "id": 75,
