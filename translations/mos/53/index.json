{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 526,
      "verse_key": "53:1",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلنَّجۡمِ إِذَا هَوَىٰ",
      "translation": "M wẽen ne ãdg t'a sã n wa lʋɩ.",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 526,
      "verse_key": "53:2",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَا ضَلَّ صَاحِبُكُمۡ وَمَا غَوَىٰ",
      "translation": "Yãmb tũud-n-taagã pa menme, a pa kedg me ye.",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 526,
      "verse_key": "53:3",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا يَنطِقُ عَنِ ٱلۡهَوَىٰٓ",
      "translation": "A pa gomd a yõor yamleoog me ye.",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 526,
      "verse_key": "53:4",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنۡ هُوَ إِلَّا وَحۡيٞ يُوحَىٰ",
      "translation": "Rẽndame tɩ yaa wahɩ sẽn tʋmse.",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 526,
      "verse_key": "53:5",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَلَّمَهُۥ شَدِيدُ ٱلۡقُوَىٰ",
      "translation": "Pãn-kεgeng soab (a Gibriilã) n tɑɑ-a ɑ nengẽ.",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 526,
      "verse_key": "53:6",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ذُو مِرَّةٖ فَٱسۡتَوَىٰ",
      "translation": "Nɑɑnd sẽn be neerã, t'ɑ be ɑ yɑlẽ wã tɩ yɑɑ vẽeneg tɩ nɑbiyɑɑmã ne-ɑ.",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 526,
      "verse_key": "53:7",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَهُوَ بِٱلۡأُفُقِ ٱلۡأَعۡلَىٰ",
      "translation": "T'a be yĩngr-yĩngri.",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 526,
      "verse_key": "53:8",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ دَنَا فَتَدَلَّىٰ",
      "translation": "Rẽ poorẽ t'a (Gibriilɑ) pẽneg n sig n kolge.",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 526,
      "verse_key": "53:9",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَكَانَ قَابَ قَوۡسَيۡنِ أَوۡ أَدۡنَىٰ",
      "translation": "Tɩ yaa wala tãp ne a to sʋk vɩʋʋgo, ma sẽn paood n pa ta rẽ.",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 526,
      "verse_key": "53:10",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَوۡحَىٰٓ إِلَىٰ عَبۡدِهِۦ مَآ أَوۡحَىٰ",
      "translation": "T'ɑ togs Wẽnd yɑmbɑ Mʋhɑmmɑd, bũmb ning A sẽn tʋms-ɑ t'ɑ tɑɑsã.",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 526,
      "verse_key": "53:11",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَا كَذَبَ ٱلۡفُؤَادُ مَا رَأَىٰٓ",
      "translation": "Nɑbiyɑɑmã sũurã pa yag bũmb ning a sẽn yã wã ye.",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 526,
      "verse_key": "53:12",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَتُمَٰرُونَهُۥ عَلَىٰ مَا يَرَىٰ",
      "translation": "Rẽ yĩnga, yãmb wẽeda ne-a no-koεεm bũmb ning a sẽn yã wã zugu?",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 526,
      "verse_key": "53:13",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلَقَدۡ رَءَاهُ نَزۡلَةً أُخۡرَىٰ",
      "translation": "La sɩd la hakɩɩka, a leb n yã-a lame yiib-n-soaba.",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 526,
      "verse_key": "53:14",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عِندَ سِدۡرَةِ ٱلۡمُنتَهَىٰ",
      "translation": "Tɑɑb-tεk Mugnugẽ wã (yaa tɩɩg sẽn be saasẽ wã n yaa rʋʋd fãa tεk zĩiga).",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 526,
      "verse_key": "53:15",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عِندَهَا جَنَّةُ ٱلۡمَأۡوَىٰٓ",
      "translation": "Ma'awaa Arzãnã bee beenẽ ( (lallg zĩig Arzãn) ma (gãag zĩig arzãna) ).",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 526,
      "verse_key": "53:16",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِذۡ يَغۡشَى ٱلسِّدۡرَةَ مَا يَغۡشَىٰ",
      "translation": "Wakat ning sẽn wa n sigd Sɩdratã(tɩɩgã) zugã, sẽn yɑɑ Wẽnd sɑgellã, bũmb ning sẽn pɑtɑr to wã.",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 526,
      "verse_key": "53:17",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَا زَاغَ ٱلۡبَصَرُ وَمَا طَغَىٰ",
      "translation": "Nɑbiyɑɑmã ninfrã pa wẽnege, a pa kedg me.",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 526,
      "verse_key": "53:18",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَقَدۡ رَأَىٰ مِنۡ ءَايَٰتِ رَبِّهِ ٱلۡكُبۡرَىٰٓ",
      "translation": "Sɩd la hakɩɩka a yãa a Soabã yel-soalem bεd-bεdã sãnda.",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 526,
      "verse_key": "53:19",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَرَءَيۡتُمُ ٱللَّٰتَ وَٱلۡعُزَّىٰ",
      "translation": "Rẽ yĩnga yãmb tẽedame tɩ rʋbsã ɑ Laat ne a ʿƲzzaa?",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 526,
      "verse_key": "53:20",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَنَوٰةَ ٱلثَّالِثَةَ ٱلۡأُخۡرَىٰٓ",
      "translation": "La a Manaat sẽn yaa tãaben-soabã nafdame?.",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 526,
      "verse_key": "53:21",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَكُمُ ٱلذَّكَرُ وَلَهُ ٱلۡأُنثَىٰ",
      "translation": "Yãmb n tar roapã yãmb sẽn nong-b rãmbã, tɩ Yẽ yaool n tar pagbã yãmb sẽn pɑ kisb rãmb n pɑ rɑt-bɑ?",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 526,
      "verse_key": "53:22",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تِلۡكَ إِذٗا قِسۡمَةٞ ضِيزَىٰٓ",
      "translation": "Ad rẽnda yaa zãmb pʋɩɩb moasã wae!",
      "footnotes": ""
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 526,
      "verse_key": "53:23",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنۡ هِيَ إِلَّآ أَسۡمَآءٞ سَمَّيۡتُمُوهَآ أَنتُمۡ وَءَابَآؤُكُم مَّآ أَنزَلَ ٱللَّهُ بِهَا مِن سُلۡطَٰنٍۚ إِن يَتَّبِعُونَ إِلَّا ٱلظَّنَّ وَمَا تَهۡوَى ٱلۡأَنفُسُۖ وَلَقَدۡ جَآءَهُم مِّن رَّبِّهِمُ ٱلۡهُدَىٰٓ",
      "translation": "B pa yɩ rẽnda yʋy bala, yãmb sẽn pʋde, yãmb ne y saam-rãmba, Wẽnd pa sik tagmas a yĩng ye. B sẽn tũudã pa yɩ rẽnda sãmbg lɑ b yõyã yɑm-leedo. La sɩd la hakɩɩka, kãndgr waa bãmb nengẽ sẽn yi b Soabẽ wã.",
      "footnotes": ""
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 526,
      "verse_key": "53:24",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَمۡ لِلۡإِنسَٰنِ مَا تَمَنَّىٰ",
      "translation": "Bɩ ninsaal paamda a sẽn tẽed lɑ ɑ saagdẽ wã tɩ rʋbsɑ n sugs-ɑ lɑme wã?",
      "footnotes": ""
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 526,
      "verse_key": "53:25",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلِلَّهِ ٱلۡأٓخِرَةُ وَٱلۡأُولَىٰ",
      "translation": "Rẽnd yaoolem la rãeenem (Lɑɑhrã ne Dũni wã) bee ne Wẽnde t'A kõt ɑ sẽn rɑt-ɑ.",
      "footnotes": ""
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 526,
      "verse_key": "53:26",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "۞ وَكَم مِّن مَّلَكٖ فِي ٱلسَّمَٰوَٰتِ لَا تُغۡنِي شَفَٰعَتُهُمۡ شَيۡـًٔا إِلَّا مِنۢ بَعۡدِ أَن يَأۡذَنَ ٱللَّهُ لِمَن يَشَآءُ وَيَرۡضَىٰٓ",
      "translation": "La sõor waoog malεgs sẽn be saasẽ wã, tɩ b susg pa nafd ba fʋɩ rẽnda Wẽnd sẽn kõ sor poore, A sẽn rat-a soaba la A yardẽ.",
      "footnotes": ""
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 527,
      "verse_key": "53:27",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "إِنَّ ٱلَّذِينَ لَا يُؤۡمِنُونَ بِٱلۡأٓخِرَةِ لَيُسَمُّونَ ٱلۡمَلَٰٓئِكَةَ تَسۡمِيَةَ ٱلۡأُنثَىٰ",
      "translation": "Ad neb nins sẽn pa kõt sɩd ne yaoolem raarã, bãmb pʋtɑ malεgsã pog-yʋy.",
      "footnotes": ""
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 527,
      "verse_key": "53:28",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا لَهُم بِهِۦ مِنۡ عِلۡمٍۖ إِن يَتَّبِعُونَ إِلَّا ٱلظَّنَّۖ وَإِنَّ ٱلظَّنَّ لَا يُغۡنِي مِنَ ٱلۡحَقِّ شَيۡـٔٗا",
      "translation": "La bãmb pa tar bãngr ne rẽnda ye, b sẽn tũudã pa yɩ rẽnda sãmbg bala, la ad sãmbg pa tõe n gɩgd sɩd ba fʋɩ.",
      "footnotes": ""
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 527,
      "verse_key": "53:29",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَعۡرِضۡ عَن مَّن تَوَلَّىٰ عَن ذِكۡرِنَا وَلَمۡ يُرِدۡ إِلَّا ٱلۡحَيَوٰةَ ٱلدُّنۡيَا",
      "translation": "Bɩ f ball n bas ned ning sẽn gũt-a a kuidg n yit Tõnd yʋʋrã tẽegre, n pa rat sã n pa Dũni vɩɩmã.",
      "footnotes": ""
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 527,
      "verse_key": "53:30",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ذَٰلِكَ مَبۡلَغُهُم مِّنَ ٱلۡعِلۡمِۚ إِنَّ رَبَّكَ هُوَ أَعۡلَمُ بِمَن ضَلَّ عَن سَبِيلِهِۦ وَهُوَ أَعۡلَمُ بِمَنِ ٱهۡتَدَىٰ",
      "translation": "Rẽ la b bãngrã tεka, ad fo Soabã n yɩɩd minim ne ned ning sẽn menem-a n yi A Sorã zugu, Yẽ n leb n yɩɩd minim ne ned ning sẽn kãndg-a.",
      "footnotes": ""
    },
    {
      "id": 31,
      "verse_number": 31,
      "page_number": 527,
      "verse_key": "53:31",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَلِلَّهِ مَا فِي ٱلسَّمَٰوَٰتِ وَمَا فِي ٱلۡأَرۡضِ لِيَجۡزِيَ ٱلَّذِينَ أَسَٰٓـُٔواْ بِمَا عَمِلُواْ وَيَجۡزِيَ ٱلَّذِينَ أَحۡسَنُواْ بِٱلۡحُسۡنَى",
      "translation": "La Wẽnd n so bũmb ning sẽn be saasẽ, la sẽn be tẽng zugu, bʋɩl yĩng t'A na rol neb nins sẽn wẽng-bã b sẽn tʋmã, la A rol neb nins sẽn maneg-bã ne manegre.",
      "footnotes": ""
    },
    {
      "id": 32,
      "verse_number": 32,
      "page_number": 527,
      "verse_key": "53:32",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ٱلَّذِينَ يَجۡتَنِبُونَ كَبَٰٓئِرَ ٱلۡإِثۡمِ وَٱلۡفَوَٰحِشَ إِلَّا ٱللَّمَمَۚ إِنَّ رَبَّكَ وَٰسِعُ ٱلۡمَغۡفِرَةِۚ هُوَ أَعۡلَمُ بِكُمۡ إِذۡ أَنشَأَكُم مِّنَ ٱلۡأَرۡضِ وَإِذۡ أَنتُمۡ أَجِنَّةٞ فِي بُطُونِ أُمَّهَٰتِكُمۡۖ فَلَا تُزَكُّوٓاْ أَنفُسَكُمۡۖ هُوَ أَعۡلَمُ بِمَنِ ٱتَّقَىٰٓ",
      "translation": "Neb nins sẽn zãagd zunuub-bεdã la yel-beedã ralla zunuub-bõonesã, ad fo Soabã yaa yaaf-yaleng Soaba, Yẽ n mi yãmb n yɩɩda, wakat ning A sẽn wa n yẽbg yãmb n yi Tẽngẽ wã, la wakat ning yãmb sẽn wa n yaa bõn-vɩʋʋs n be yãmb ma-rãmbã pʋse, bɩ y ra pẽg y mens ye, Yẽ n yɩɩd minim ne ned ning sẽn zoet-a Wẽnde.",
      "footnotes": ""
    },
    {
      "id": 33,
      "verse_number": 33,
      "page_number": 527,
      "verse_key": "53:33",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَرَءَيۡتَ ٱلَّذِي تَوَلَّىٰ",
      "translation": "La rẽ yĩnga fo pa yã ned ning sẽn gõd a kuidga?",
      "footnotes": ""
    },
    {
      "id": 34,
      "verse_number": 34,
      "page_number": 527,
      "verse_key": "53:34",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَعۡطَىٰ قَلِيلٗا وَأَكۡدَىٰٓ",
      "translation": "La a kõ bilfu, la a monge?",
      "footnotes": ""
    },
    {
      "id": 35,
      "verse_number": 35,
      "page_number": 527,
      "verse_key": "53:35",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَعِندَهُۥ عِلۡمُ ٱلۡغَيۡبِ فَهُوَ يَرَىٰٓ",
      "translation": "A tara bãng soaandag tɩ yaa yẽ n ne bɩ?",
      "footnotes": ""
    },
    {
      "id": 36,
      "verse_number": 36,
      "page_number": 527,
      "verse_key": "53:36",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَمۡ لَمۡ يُنَبَّأۡ بِمَا فِي صُحُفِ مُوسَىٰ",
      "translation": "Bɩ b pa kõ-a kɩbar ne bũmb ning sẽn be a Muusa gafẽ wã,",
      "footnotes": ""
    },
    {
      "id": 37,
      "verse_number": 37,
      "page_number": 527,
      "verse_key": "53:37",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَإِبۡرَٰهِيمَ ٱلَّذِي وَفَّىٰٓ",
      "translation": "La a ɭbraahɩɩm sẽn pids a pʋlengã?",
      "footnotes": ""
    },
    {
      "id": 38,
      "verse_number": 38,
      "page_number": 527,
      "verse_key": "53:38",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَلَّا تَزِرُ وَازِرَةٞ وِزۡرَ أُخۡرَىٰ",
      "translation": "Tɩ ned fãɑ pa na n tʋk a to zunuub ye.",
      "footnotes": ""
    },
    {
      "id": 39,
      "verse_number": 39,
      "page_number": 527,
      "verse_key": "53:39",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَن لَّيۡسَ لِلۡإِنسَٰنِ إِلَّا مَا سَعَىٰ",
      "translation": "La pa be ne ninsaal ralla a sẽn tʋmã.",
      "footnotes": ""
    },
    {
      "id": 40,
      "verse_number": 40,
      "page_number": 527,
      "verse_key": "53:40",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّ سَعۡيَهُۥ سَوۡفَ يُرَىٰ",
      "translation": "La a tʋʋmã b na yã.",
      "footnotes": ""
    },
    {
      "id": 41,
      "verse_number": 41,
      "page_number": 527,
      "verse_key": "53:41",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ يُجۡزَىٰهُ ٱلۡجَزَآءَ ٱلۡأَوۡفَىٰ",
      "translation": "Rẽ poorẽ tɩ b roal-a rol-pidsdga.",
      "footnotes": ""
    },
    {
      "id": 42,
      "verse_number": 42,
      "page_number": 527,
      "verse_key": "53:42",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّ إِلَىٰ رَبِّكَ ٱلۡمُنتَهَىٰ",
      "translation": "La yaa fo Soabẽ wã la tεk zĩiga.",
      "footnotes": ""
    },
    {
      "id": 43,
      "verse_number": 43,
      "page_number": 527,
      "verse_key": "53:43",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّهُۥ هُوَ أَضۡحَكَ وَأَبۡكَىٰ",
      "translation": "La yell sẽn yalẽ, Yẽ n kɩt tɩ b laadẽ la A kɩt tɩ b kũmdẽ.",
      "footnotes": ""
    },
    {
      "id": 44,
      "verse_number": 44,
      "page_number": 527,
      "verse_key": "53:44",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّهُۥ هُوَ أَمَاتَ وَأَحۡيَا",
      "translation": "La yell sẽn yalẽ, Yẽ n kʋʋd la A vɩɩmsde.",
      "footnotes": ""
    },
    {
      "id": 45,
      "verse_number": 45,
      "page_number": 528,
      "verse_key": "53:45",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّهُۥ خَلَقَ ٱلزَّوۡجَيۡنِ ٱلذَّكَرَ وَٱلۡأُنثَىٰ",
      "translation": "La yell sẽn yalẽ, A naana kõbg a yiibã: rao la paga.",
      "footnotes": ""
    },
    {
      "id": 46,
      "verse_number": 46,
      "page_number": 528,
      "verse_key": "53:46",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مِن نُّطۡفَةٍ إِذَا تُمۡنَىٰ",
      "translation": "Sẽn tũ ne mani wakat ning a sẽn wat n kiigdẽ wã.",
      "footnotes": ""
    },
    {
      "id": 47,
      "verse_number": 47,
      "page_number": 528,
      "verse_key": "53:47",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّ عَلَيۡهِ ٱلنَّشۡأَةَ ٱلۡأُخۡرَىٰ",
      "translation": "La ad yẽbg-zẽngã bee ne Yẽnda.",
      "footnotes": ""
    },
    {
      "id": 48,
      "verse_number": 48,
      "page_number": 528,
      "verse_key": "53:48",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّهُۥ هُوَ أَغۡنَىٰ وَأَقۡنَىٰ",
      "translation": "La yel sẽn yalẽ, Yẽ n maan rakãaglem la A leb n kõ arzεka.",
      "footnotes": ""
    },
    {
      "id": 49,
      "verse_number": 49,
      "page_number": 528,
      "verse_key": "53:49",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّهُۥ هُوَ رَبُّ ٱلشِّعۡرَىٰ",
      "translation": "La yell sẽn yalẽ, Yẽnda la Sɩʿɩraa Soabã (yaa ãdg yʋʋre tɩ zɩdbã rɑ tũu-ɑ).",
      "footnotes": ""
    },
    {
      "id": 50,
      "verse_number": 50,
      "page_number": 528,
      "verse_key": "53:50",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنَّهُۥٓ أَهۡلَكَ عَادًا ٱلۡأُولَىٰ",
      "translation": "La yell sẽn yalẽ, A halka rẽenem ʿAadã (ɑ Hʋʋd nebã).",
      "footnotes": ""
    },
    {
      "id": 51,
      "verse_number": 51,
      "page_number": 528,
      "verse_key": "53:51",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَثَمُودَاْ فَمَآ أَبۡقَىٰ",
      "translation": "La a Ṯamuud, la A ka kell ye.",
      "footnotes": ""
    },
    {
      "id": 52,
      "verse_number": 52,
      "page_number": 528,
      "verse_key": "53:52",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَقَوۡمَ نُوحٖ مِّن قَبۡلُۖ إِنَّهُمۡ كَانُواْ هُمۡ أَظۡلَمَ وَأَطۡغَىٰ",
      "translation": "La a Nuuh nebã hal pĩnda, ad bãmba, bãmb ra yɩɩme n yɩɩd ne wẽgd la kengre.",
      "footnotes": ""
    },
    {
      "id": 53,
      "verse_number": 53,
      "page_number": 528,
      "verse_key": "53:53",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَٱلۡمُؤۡتَفِكَةَ أَهۡوَىٰ",
      "translation": "La a Mutafɩkaat (a Lʋʋt neba), A kibind-b lame.",
      "footnotes": ""
    },
    {
      "id": 54,
      "verse_number": 54,
      "page_number": 528,
      "verse_key": "53:54",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَغَشَّىٰهَا مَا غَشَّىٰ",
      "translation": "Tɩ bõr-ba, bũmb ning sẽn bõr-ba.",
      "footnotes": ""
    },
    {
      "id": 55,
      "verse_number": 55,
      "page_number": 528,
      "verse_key": "53:55",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَبِأَيِّ ءَالَآءِ رَبِّكَ تَتَمَارَىٰ",
      "translation": "La yaa fo Soabã neema bʋgo la f maand sika?",
      "footnotes": ""
    },
    {
      "id": 56,
      "verse_number": 56,
      "page_number": 528,
      "verse_key": "53:56",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "هَٰذَا نَذِيرٞ مِّنَ ٱلنُّذُرِ ٱلۡأُولَىٰٓ",
      "translation": "Yãoã yaa bugsd sẽn wõnd bugsdbã sẽn reng tɑɑrã buudu.",
      "footnotes": ""
    },
    {
      "id": 57,
      "verse_number": 57,
      "page_number": 528,
      "verse_key": "53:57",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَزِفَتِ ٱلۡأٓزِفَةُ",
      "translation": "Pẽnegdã (Dũni yikrã) pẽnegame.",
      "footnotes": ""
    },
    {
      "id": 58,
      "verse_number": 58,
      "page_number": 528,
      "verse_key": "53:58",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لَيۡسَ لَهَا مِن دُونِ ٱللَّهِ كَاشِفَةٌ",
      "translation": "A pa tar pãrgda, zẽng tɩ pa Wẽnd ye.",
      "footnotes": ""
    },
    {
      "id": 59,
      "verse_number": 59,
      "page_number": 528,
      "verse_key": "53:59",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَفَمِنۡ هَٰذَا ٱلۡحَدِيثِ تَعۡجَبُونَ",
      "translation": "Gom kãngã la yãmb maan yel-soalmã?",
      "footnotes": ""
    },
    {
      "id": 60,
      "verse_number": 60,
      "page_number": 528,
      "verse_key": "53:60",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَتَضۡحَكُونَ وَلَا تَبۡكُونَ",
      "translation": "La y laadẽ, n pa kũmdẽ?",
      "footnotes": ""
    },
    {
      "id": 61,
      "verse_number": 61,
      "page_number": 528,
      "verse_key": "53:61",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَنتُمۡ سَٰمِدُونَ",
      "translation": "Tɩ yãmb yaa sẽn wɩsde.",
      "footnotes": ""
    },
    {
      "id": 62,
      "verse_number": 62,
      "page_number": 528,
      "verse_key": "53:62",
      "juz_number": 27,
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "sajdah_type": "obligatory",
      "sajdah_number": 12,
      "text": "فَٱسۡجُدُواْۤ لِلَّهِۤ وَٱعۡبُدُواْ۩",
      "translation": "Bɩ y maan suguud n kõ Wẽnde, la y tũ-A.",
      "footnotes": ""
    }
  ]
}