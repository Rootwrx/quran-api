{
  "verses": [
    {
      "number": "1",
      "arabic_text": "قُلۡ أَعُوذُ بِرَبِّ ٱلنَّاسِ",
      "translation": "1. Loba: Nasengi libateli epai ya Nkolo ya bato."
    },
    {
      "number": "2",
      "arabic_text": "مَلِكِ ٱلنَّاسِ",
      "translation": "2. Mokonzi ya bato."
    },
    {
      "number": "3",
      "arabic_text": "إِلَٰهِ ٱلنَّاسِ",
      "translation": "3. Nzambe ya bato."
    },
    {
      "number": "4",
      "arabic_text": "مِن شَرِّ ٱلۡوَسۡوَاسِ ٱلۡخَنَّاسِ",
      "translation": "4. Kowuta na komekama ya milimo mabe."
    },
    {
      "number": "5",
      "arabic_text": "ٱلَّذِي يُوَسۡوِسُ فِي صُدُورِ ٱلنَّاسِ",
      "translation": "5. Oyo azali kofula makanisi mabe o’mitema mia bato."
    },
    {
      "number": "6",
      "arabic_text": "مِنَ ٱلۡجِنَّةِ وَٱلنَّاسِ",
      "translation": "6. Azala okati ya bilima to bato."
    }
  ]
}