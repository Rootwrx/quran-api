{
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "page_number": 585,
      "verse_key": "80:1",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "عَبَسَ وَتَوَلَّىٰٓ",
      "translation": "1. Akangi elongi mpe apesi mokongo.",
      "footnotes": ""
    },
    {
      "id": 2,
      "verse_number": 2,
      "page_number": 585,
      "verse_key": "80:2",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَن جَآءَهُ ٱلۡأَعۡمَىٰ",
      "translation": "2. Mpo ete mokufi miso aye epai naye.",
      "footnotes": ""
    },
    {
      "id": 3,
      "verse_number": 3,
      "page_number": 585,
      "verse_key": "80:3",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا يُدۡرِيكَ لَعَلَّهُۥ يَزَّكَّىٰٓ",
      "translation": "3. Nani akoyebisa yo ntango mosusu ye alingi komipetola?",
      "footnotes": ""
    },
    {
      "id": 4,
      "verse_number": 4,
      "page_number": 585,
      "verse_key": "80:4",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَوۡ يَذَّكَّرُ فَتَنفَعَهُ ٱلذِّكۡرَىٰٓ",
      "translation": "4. To mpe kokanisa mpo ete mateya esalisa ye?",
      "footnotes": ""
    },
    {
      "id": 5,
      "verse_number": 5,
      "page_number": 585,
      "verse_key": "80:5",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَمَّا مَنِ ٱسۡتَغۡنَىٰ",
      "translation": "5. Mpona moto oyo amitondisi na lolendo mpona bozwi naye,",
      "footnotes": ""
    },
    {
      "id": 6,
      "verse_number": 6,
      "page_number": 585,
      "verse_key": "80:6",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَنتَ لَهُۥ تَصَدَّىٰ",
      "translation": "6. Ozali koyamba ye na lombango.",
      "footnotes": ""
    },
    {
      "id": 7,
      "verse_number": 7,
      "page_number": 585,
      "verse_key": "80:7",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَمَا عَلَيۡكَ أَلَّا يَزَّكَّىٰ",
      "translation": "7. Kasi bopetwami naye ezali na maboko nayo te.",
      "footnotes": ""
    },
    {
      "id": 8,
      "verse_number": 8,
      "page_number": 585,
      "verse_key": "80:8",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأَمَّا مَن جَآءَكَ يَسۡعَىٰ",
      "translation": "8. Kasi mpona oyo aye na lombango epai nayo,",
      "footnotes": ""
    },
    {
      "id": 9,
      "verse_number": 9,
      "page_number": 585,
      "verse_key": "80:9",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَهُوَ يَخۡشَىٰ",
      "translation": "9. Naye azali na bobangi,",
      "footnotes": ""
    },
    {
      "id": 10,
      "verse_number": 10,
      "page_number": 585,
      "verse_key": "80:10",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَنتَ عَنۡهُ تَلَهَّىٰ",
      "translation": "10. Ozali kotala ye te.",
      "footnotes": ""
    },
    {
      "id": 11,
      "verse_number": 11,
      "page_number": 585,
      "verse_key": "80:11",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّآ إِنَّهَا تَذۡكِرَةٞ",
      "translation": "11. Kosala lisusu boye te oyo ezali ya soló liteya.",
      "footnotes": ""
    },
    {
      "id": 12,
      "verse_number": 12,
      "page_number": 585,
      "verse_key": "80:12",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَمَن شَآءَ ذَكَرَهُۥ",
      "translation": "12. Oyo alingi akanisa yango.",
      "footnotes": ""
    },
    {
      "id": 13,
      "verse_number": 13,
      "page_number": 585,
      "verse_key": "80:13",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فِي صُحُفٖ مُّكَرَّمَةٖ",
      "translation": "13. Ezwami kati ya makomi ya lokumu,",
      "footnotes": ""
    },
    {
      "id": 14,
      "verse_number": 14,
      "page_number": 585,
      "verse_key": "80:14",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَّرۡفُوعَةٖ مُّطَهَّرَةِۭ",
      "translation": "14. Etombwami mpe epetolami,",
      "footnotes": ""
    },
    {
      "id": 15,
      "verse_number": 15,
      "page_number": 585,
      "verse_key": "80:15",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "بِأَيۡدِي سَفَرَةٖ",
      "translation": "15. O’maboko ma bantoma,",
      "footnotes": ""
    },
    {
      "id": 16,
      "verse_number": 16,
      "page_number": 585,
      "verse_key": "80:16",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كِرَامِۭ بَرَرَةٖ",
      "translation": "16. Baye ya lokumu тре botosi.",
      "footnotes": ""
    },
    {
      "id": 17,
      "verse_number": 17,
      "page_number": 585,
      "verse_key": "80:17",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "قُتِلَ ٱلۡإِنسَٰنُ مَآ أَكۡفَرَهُۥ",
      "translation": "17. Moto amibomi na bopengwi naye.",
      "footnotes": ""
    },
    {
      "id": 18,
      "verse_number": 18,
      "page_number": 585,
      "verse_key": "80:18",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مِنۡ أَيِّ شَيۡءٍ خَلَقَهُۥ",
      "translation": "18. Na eloko nini akelamaki?",
      "footnotes": ""
    },
    {
      "id": 19,
      "verse_number": 19,
      "page_number": 585,
      "verse_key": "80:19",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مِن نُّطۡفَةٍ خَلَقَهُۥ فَقَدَّرَهُۥ",
      "translation": "19. Akelamaki kowuta na mayi ya mokongo, akeli ye mpe akokiseli ye mokano mwa ye.",
      "footnotes": ""
    },
    {
      "id": 20,
      "verse_number": 20,
      "page_number": 585,
      "verse_key": "80:20",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ ٱلسَّبِيلَ يَسَّرَهُۥ",
      "translation": "20. Sima alakisi ye nzela.",
      "footnotes": ""
    },
    {
      "id": 21,
      "verse_number": 21,
      "page_number": 585,
      "verse_key": "80:21",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ أَمَاتَهُۥ فَأَقۡبَرَهُۥ",
      "translation": "21. Sima abomi ye mpe akotisi ye na lilita.",
      "footnotes": ""
    },
    {
      "id": 22,
      "verse_number": 22,
      "page_number": 585,
      "verse_key": "80:22",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ إِذَا شَآءَ أَنشَرَهُۥ",
      "translation": "22. Mpe akosekwisa ye na ntango alingi.",
      "footnotes": ""
    },
    {
      "id": 23,
      "verse_number": 23,
      "page_number": 585,
      "verse_key": "80:23",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "كَلَّا لَمَّا يَقۡضِ مَآ أَمَرَهُۥ",
      "translation": "23. Moto akokisi te maye atindamaki.",
      "footnotes": ""
    },
    {
      "id": 24,
      "verse_number": 24,
      "page_number": 585,
      "verse_key": "80:24",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَلۡيَنظُرِ ٱلۡإِنسَٰنُ إِلَىٰ طَعَامِهِۦٓ",
      "translation": "24. Esengeli na moto atala bileyi naye.",
      "footnotes": ""
    },
    {
      "id": 25,
      "verse_number": 25,
      "page_number": 585,
      "verse_key": "80:25",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أَنَّا صَبَبۡنَا ٱلۡمَآءَ صَبّٗا",
      "translation": "25. Ya soló, biso nde tozali kokitisa mayi.",
      "footnotes": ""
    },
    {
      "id": 26,
      "verse_number": 26,
      "page_number": 585,
      "verse_key": "80:26",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ثُمَّ شَقَقۡنَا ٱلۡأَرۡضَ شَقّٗا",
      "translation": "26. Sima topasoli mabele na nzela,",
      "footnotes": ""
    },
    {
      "id": 27,
      "verse_number": 27,
      "page_number": 585,
      "verse_key": "80:27",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَأَنۢبَتۡنَا فِيهَا حَبّٗا",
      "translation": "27. Mpe na nzela wana tobimisi bambuma,",
      "footnotes": ""
    },
    {
      "id": 28,
      "verse_number": 28,
      "page_number": 585,
      "verse_key": "80:28",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَعِنَبٗا وَقَضۡبٗا",
      "translation": "28. Ba inab(1) mpe bandunda.",
      "footnotes": "1 Inab: Bótala suratu Al-An’ám mokapo: 99."
    },
    {
      "id": 29,
      "verse_number": 29,
      "page_number": 585,
      "verse_key": "80:29",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَزَيۡتُونٗا وَنَخۡلٗا",
      "translation": "29. Na nzete ya zaítún(2) na nzete ya nagh’l(3),",
      "footnotes": "2 Bótala Al-An’ám: 99.\n3 Bótala Al-An’ám: 99."
    },
    {
      "id": 30,
      "verse_number": 30,
      "page_number": 585,
      "verse_key": "80:30",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَحَدَآئِقَ غُلۡبٗا",
      "translation": "ЗО. Мре bilanga ebele,",
      "footnotes": ""
    },
    {
      "id": 31,
      "verse_number": 31,
      "page_number": 585,
      "verse_key": "80:31",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَفَٰكِهَةٗ وَأَبّٗا",
      "translation": "31. Мре tobimisi mbuma mpe matiti.",
      "footnotes": ""
    },
    {
      "id": 32,
      "verse_number": 32,
      "page_number": 585,
      "verse_key": "80:32",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "مَّتَٰعٗا لَّكُمۡ وَلِأَنۡعَٰمِكُمۡ",
      "translation": "32. Bileyi тропа bino na An’âm(1) na bino.",
      "footnotes": "1 An’âm: Bótala Imrân: 14."
    },
    {
      "id": 33,
      "verse_number": 33,
      "page_number": 585,
      "verse_key": "80:33",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "فَإِذَا جَآءَتِ ٱلصَّآخَّةُ",
      "translation": "33. Kasi tango bobulungani ekoya,",
      "footnotes": ""
    },
    {
      "id": 34,
      "verse_number": 34,
      "page_number": 585,
      "verse_key": "80:34",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "يَوۡمَ يَفِرُّ ٱلۡمَرۡءُ مِنۡ أَخِيهِ",
      "translation": "34. Mokolo moye moto akokima ndeko naye,",
      "footnotes": ""
    },
    {
      "id": 35,
      "verse_number": 35,
      "page_number": 585,
      "verse_key": "80:35",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَأُمِّهِۦ وَأَبِيهِ",
      "translation": "35. Mpe akokima mama naye mpe tata naye,",
      "footnotes": ""
    },
    {
      "id": 36,
      "verse_number": 36,
      "page_number": 585,
      "verse_key": "80:36",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَصَٰحِبَتِهِۦ وَبَنِيهِ",
      "translation": "36. Akokima mwasi naye mpe bana baye.",
      "footnotes": ""
    },
    {
      "id": 37,
      "verse_number": 37,
      "page_number": 585,
      "verse_key": "80:37",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "لِكُلِّ ٱمۡرِيٕٖ مِّنۡهُمۡ يَوۡمَئِذٖ شَأۡنٞ يُغۡنِيهِ",
      "translation": "37. Mpamba te mokolo wana moto na moto akotala sé makambo matali ye-",
      "footnotes": ""
    },
    {
      "id": 38,
      "verse_number": 38,
      "page_number": 585,
      "verse_key": "80:38",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وُجُوهٞ يَوۡمَئِذٖ مُّسۡفِرَةٞ",
      "translation": "38. Mokolo wana bilongi bisusu bikongenga,",
      "footnotes": ""
    },
    {
      "id": 39,
      "verse_number": 39,
      "page_number": 585,
      "verse_key": "80:39",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "ضَاحِكَةٞ مُّسۡتَبۡشِرَةٞ",
      "translation": "39. Bakoseka mpe bakosepela.",
      "footnotes": ""
    },
    {
      "id": 40,
      "verse_number": 40,
      "page_number": 585,
      "verse_key": "80:40",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "وَوُجُوهٞ يَوۡمَئِذٍ عَلَيۡهَا غَبَرَةٞ",
      "translation": "40. Kasi bilongi bisusu mokolo yango bikozala na putulu,",
      "footnotes": ""
    },
    {
      "id": 41,
      "verse_number": 41,
      "page_number": 585,
      "verse_key": "80:41",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "تَرۡهَقُهَا قَتَرَةٌ",
      "translation": "41. Bikozipama na molili.",
      "footnotes": ""
    },
    {
      "id": 42,
      "verse_number": 42,
      "page_number": 585,
      "verse_key": "80:42",
      "juz_number": 30,
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "sajdah_type": null,
      "sajdah_number": null,
      "text": "أُوْلَٰٓئِكَ هُمُ ٱلۡكَفَرَةُ ٱلۡفَجَرَةُ",
      "translation": "42. Baye wana nde bazali bapengwi mpe bato ya mabe.",
      "footnotes": ""
    }
  ]
}