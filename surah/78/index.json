{
  "verses": [
    {
      "id": 5673,
      "number": 1,
      "verse_key": "78:1",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 582,
      "juz_number": 30,
      "text": "عَمَّ يَتَسَآءَلُونَ"
    },
    {
      "id": 5682,
      "number": 10,
      "verse_key": "78:10",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 582,
      "juz_number": 30,
      "text": "وَجَعَلۡنَا ٱلَّيۡلَ لِبَاسٗا"
    },
    {
      "id": 5683,
      "number": 11,
      "verse_key": "78:11",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 582,
      "juz_number": 30,
      "text": "وَجَعَلۡنَا ٱلنَّهَارَ مَعَاشٗا"
    },
    {
      "id": 5684,
      "number": 12,
      "verse_key": "78:12",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 582,
      "juz_number": 30,
      "text": "وَبَنَيۡنَا فَوۡقَكُمۡ سَبۡعٗا شِدَادٗا"
    },
    {
      "id": 5685,
      "number": 13,
      "verse_key": "78:13",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 582,
      "juz_number": 30,
      "text": "وَجَعَلۡنَا سِرَاجٗا وَهَّاجٗا"
    },
    {
      "id": 5686,
      "number": 14,
      "verse_key": "78:14",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 582,
      "juz_number": 30,
      "text": "وَأَنزَلۡنَا مِنَ ٱلۡمُعۡصِرَٰتِ مَآءٗ ثَجَّاجٗا"
    },
    {
      "id": 5687,
      "number": 15,
      "verse_key": "78:15",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 582,
      "juz_number": 30,
      "text": "لِّنُخۡرِجَ بِهِۦ حَبّٗا وَنَبَاتٗا"
    },
    {
      "id": 5688,
      "number": 16,
      "verse_key": "78:16",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 582,
      "juz_number": 30,
      "text": "وَجَنَّٰتٍ أَلۡفَافًا"
    },
    {
      "id": 5689,
      "number": 17,
      "verse_key": "78:17",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 582,
      "juz_number": 30,
      "text": "إِنَّ يَوۡمَ ٱلۡفَصۡلِ كَانَ مِيقَٰتٗا"
    },
    {
      "id": 5690,
      "number": 18,
      "verse_key": "78:18",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 582,
      "juz_number": 30,
      "text": "يَوۡمَ يُنفَخُ فِي ٱلصُّورِ فَتَأۡتُونَ أَفۡوَاجٗا"
    },
    {
      "id": 5691,
      "number": 19,
      "verse_key": "78:19",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 582,
      "juz_number": 30,
      "text": "وَفُتِحَتِ ٱلسَّمَآءُ فَكَانَتۡ أَبۡوَٰبٗا"
    },
    {
      "id": 5674,
      "number": 2,
      "verse_key": "78:2",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 582,
      "juz_number": 30,
      "text": "عَنِ ٱلنَّبَإِ ٱلۡعَظِيمِ"
    },
    {
      "id": 5692,
      "number": 20,
      "verse_key": "78:20",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 582,
      "juz_number": 30,
      "text": "وَسُيِّرَتِ ٱلۡجِبَالُ فَكَانَتۡ سَرَابًا"
    },
    {
      "id": 5693,
      "number": 21,
      "verse_key": "78:21",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 582,
      "juz_number": 30,
      "text": "إِنَّ جَهَنَّمَ كَانَتۡ مِرۡصَادٗا"
    },
    {
      "id": 5694,
      "number": 22,
      "verse_key": "78:22",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 582,
      "juz_number": 30,
      "text": "لِّلطَّٰغِينَ مَـَٔابٗا"
    },
    {
      "id": 5695,
      "number": 23,
      "verse_key": "78:23",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 582,
      "juz_number": 30,
      "text": "لَّٰبِثِينَ فِيهَآ أَحۡقَابٗا"
    },
    {
      "id": 5696,
      "number": 24,
      "verse_key": "78:24",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 582,
      "juz_number": 30,
      "text": "لَّا يَذُوقُونَ فِيهَا بَرۡدٗا وَلَا شَرَابًا"
    },
    {
      "id": 5697,
      "number": 25,
      "verse_key": "78:25",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 582,
      "juz_number": 30,
      "text": "إِلَّا حَمِيمٗا وَغَسَّاقٗا"
    },
    {
      "id": 5698,
      "number": 26,
      "verse_key": "78:26",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 582,
      "juz_number": 30,
      "text": "جَزَآءٗ وِفَاقًا"
    },
    {
      "id": 5699,
      "number": 27,
      "verse_key": "78:27",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 582,
      "juz_number": 30,
      "text": "إِنَّهُمۡ كَانُواْ لَا يَرۡجُونَ حِسَابٗا"
    },
    {
      "id": 5700,
      "number": 28,
      "verse_key": "78:28",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 582,
      "juz_number": 30,
      "text": "وَكَذَّبُواْ بِـَٔايَٰتِنَا كِذَّابٗا"
    },
    {
      "id": 5701,
      "number": 29,
      "verse_key": "78:29",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 582,
      "juz_number": 30,
      "text": "وَكُلَّ شَيۡءٍ أَحۡصَيۡنَٰهُ كِتَٰبٗا"
    },
    {
      "id": 5675,
      "number": 3,
      "verse_key": "78:3",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 582,
      "juz_number": 30,
      "text": "ٱلَّذِي هُمۡ فِيهِ مُخۡتَلِفُونَ"
    },
    {
      "id": 5702,
      "number": 30,
      "verse_key": "78:30",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 582,
      "juz_number": 30,
      "text": "فَذُوقُواْ فَلَن نَّزِيدَكُمۡ إِلَّا عَذَابًا"
    },
    {
      "id": 5703,
      "number": 31,
      "verse_key": "78:31",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 521,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 583,
      "juz_number": 30,
      "text": "إِنَّ لِلۡمُتَّقِينَ مَفَازًا"
    },
    {
      "id": 5704,
      "number": 32,
      "verse_key": "78:32",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 521,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 583,
      "juz_number": 30,
      "text": "حَدَآئِقَ وَأَعۡنَٰبٗا"
    },
    {
      "id": 5705,
      "number": 33,
      "verse_key": "78:33",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 521,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 583,
      "juz_number": 30,
      "text": "وَكَوَاعِبَ أَتۡرَابٗا"
    },
    {
      "id": 5706,
      "number": 34,
      "verse_key": "78:34",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 521,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 583,
      "juz_number": 30,
      "text": "وَكَأۡسٗا دِهَاقٗا"
    },
    {
      "id": 5707,
      "number": 35,
      "verse_key": "78:35",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 521,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 583,
      "juz_number": 30,
      "text": "لَّا يَسۡمَعُونَ فِيهَا لَغۡوٗا وَلَا كِذَّٰبٗا"
    },
    {
      "id": 5708,
      "number": 36,
      "verse_key": "78:36",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 521,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 583,
      "juz_number": 30,
      "text": "جَزَآءٗ مِّن رَّبِّكَ عَطَآءً حِسَابٗا"
    },
    {
      "id": 5709,
      "number": 37,
      "verse_key": "78:37",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 521,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 583,
      "juz_number": 30,
      "text": "رَّبِّ ٱلسَّمَٰوَٰتِ وَٱلۡأَرۡضِ وَمَا بَيۡنَهُمَا ٱلرَّحۡمَٰنِۖ لَا يَمۡلِكُونَ مِنۡهُ خِطَابٗا"
    },
    {
      "id": 5710,
      "number": 38,
      "verse_key": "78:38",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 521,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 583,
      "juz_number": 30,
      "text": "يَوۡمَ يَقُومُ ٱلرُّوحُ وَٱلۡمَلَٰٓئِكَةُ صَفّٗاۖ لَّا يَتَكَلَّمُونَ إِلَّا مَنۡ أَذِنَ لَهُ ٱلرَّحۡمَٰنُ وَقَالَ صَوَابٗا"
    },
    {
      "id": 5711,
      "number": 39,
      "verse_key": "78:39",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 521,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 583,
      "juz_number": 30,
      "text": "ذَٰلِكَ ٱلۡيَوۡمُ ٱلۡحَقُّۖ فَمَن شَآءَ ٱتَّخَذَ إِلَىٰ رَبِّهِۦ مَـَٔابًا"
    },
    {
      "id": 5676,
      "number": 4,
      "verse_key": "78:4",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 582,
      "juz_number": 30,
      "text": "كَلَّا سَيَعۡلَمُونَ"
    },
    {
      "id": 5712,
      "number": 40,
      "verse_key": "78:40",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 521,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 583,
      "juz_number": 30,
      "text": "إِنَّآ أَنذَرۡنَٰكُمۡ عَذَابٗا قَرِيبٗا يَوۡمَ يَنظُرُ ٱلۡمَرۡءُ مَا قَدَّمَتۡ يَدَاهُ وَيَقُولُ ٱلۡكَافِرُ يَٰلَيۡتَنِي كُنتُ تُرَٰبَۢا"
    },
    {
      "id": 5677,
      "number": 5,
      "verse_key": "78:5",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 582,
      "juz_number": 30,
      "text": "ثُمَّ كَلَّا سَيَعۡلَمُونَ"
    },
    {
      "id": 5678,
      "number": 6,
      "verse_key": "78:6",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 582,
      "juz_number": 30,
      "text": "أَلَمۡ نَجۡعَلِ ٱلۡأَرۡضَ مِهَٰدٗا"
    },
    {
      "id": 5679,
      "number": 7,
      "verse_key": "78:7",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 582,
      "juz_number": 30,
      "text": "وَٱلۡجِبَالَ أَوۡتَادٗا"
    },
    {
      "id": 5680,
      "number": 8,
      "verse_key": "78:8",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 582,
      "juz_number": 30,
      "text": "وَخَلَقۡنَٰكُمۡ أَزۡوَٰجٗا"
    },
    {
      "id": 5681,
      "number": 9,
      "verse_key": "78:9",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 582,
      "juz_number": 30,
      "text": "وَجَعَلۡنَا نَوۡمَكُمۡ سُبَاتٗا"
    }
  ]
}