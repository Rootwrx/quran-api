{
  "verses": [
    {
      "id": 5713,
      "number": 1,
      "verse_key": "79:1",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 583,
      "juz_number": 30,
      "text": "وَٱلنَّٰزِعَٰتِ غَرۡقٗا"
    },
    {
      "id": 5722,
      "number": 10,
      "verse_key": "79:10",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 583,
      "juz_number": 30,
      "text": "يَقُولُونَ أَءِنَّا لَمَرۡدُودُونَ فِي ٱلۡحَافِرَةِ"
    },
    {
      "id": 5723,
      "number": 11,
      "verse_key": "79:11",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 583,
      "juz_number": 30,
      "text": "أَءِذَا كُنَّا عِظَٰمٗا نَّخِرَةٗ"
    },
    {
      "id": 5724,
      "number": 12,
      "verse_key": "79:12",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 583,
      "juz_number": 30,
      "text": "قَالُواْ تِلۡكَ إِذٗا كَرَّةٌ خَاسِرَةٞ"
    },
    {
      "id": 5725,
      "number": 13,
      "verse_key": "79:13",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 583,
      "juz_number": 30,
      "text": "فَإِنَّمَا هِيَ زَجۡرَةٞ وَٰحِدَةٞ"
    },
    {
      "id": 5726,
      "number": 14,
      "verse_key": "79:14",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 583,
      "juz_number": 30,
      "text": "فَإِذَا هُم بِٱلسَّاهِرَةِ"
    },
    {
      "id": 5727,
      "number": 15,
      "verse_key": "79:15",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 583,
      "juz_number": 30,
      "text": "هَلۡ أَتَىٰكَ حَدِيثُ مُوسَىٰٓ"
    },
    {
      "id": 5728,
      "number": 16,
      "verse_key": "79:16",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "text": "إِذۡ نَادَىٰهُ رَبُّهُۥ بِٱلۡوَادِ ٱلۡمُقَدَّسِ طُوًى"
    },
    {
      "id": 5729,
      "number": 17,
      "verse_key": "79:17",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "text": "ٱذۡهَبۡ إِلَىٰ فِرۡعَوۡنَ إِنَّهُۥ طَغَىٰ"
    },
    {
      "id": 5730,
      "number": 18,
      "verse_key": "79:18",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "text": "فَقُلۡ هَل لَّكَ إِلَىٰٓ أَن تَزَكَّىٰ"
    },
    {
      "id": 5731,
      "number": 19,
      "verse_key": "79:19",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "text": "وَأَهۡدِيَكَ إِلَىٰ رَبِّكَ فَتَخۡشَىٰ"
    },
    {
      "id": 5714,
      "number": 2,
      "verse_key": "79:2",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 583,
      "juz_number": 30,
      "text": "وَٱلنَّٰشِطَٰتِ نَشۡطٗا"
    },
    {
      "id": 5732,
      "number": 20,
      "verse_key": "79:20",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "text": "فَأَرَىٰهُ ٱلۡأٓيَةَ ٱلۡكُبۡرَىٰ"
    },
    {
      "id": 5733,
      "number": 21,
      "verse_key": "79:21",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "text": "فَكَذَّبَ وَعَصَىٰ"
    },
    {
      "id": 5734,
      "number": 22,
      "verse_key": "79:22",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "text": "ثُمَّ أَدۡبَرَ يَسۡعَىٰ"
    },
    {
      "id": 5735,
      "number": 23,
      "verse_key": "79:23",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "text": "فَحَشَرَ فَنَادَىٰ"
    },
    {
      "id": 5736,
      "number": 24,
      "verse_key": "79:24",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "text": "فَقَالَ أَنَا۠ رَبُّكُمُ ٱلۡأَعۡلَىٰ"
    },
    {
      "id": 5737,
      "number": 25,
      "verse_key": "79:25",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "text": "فَأَخَذَهُ ٱللَّهُ نَكَالَ ٱلۡأٓخِرَةِ وَٱلۡأُولَىٰٓ"
    },
    {
      "id": 5738,
      "number": 26,
      "verse_key": "79:26",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "text": "إِنَّ فِي ذَٰلِكَ لَعِبۡرَةٗ لِّمَن يَخۡشَىٰٓ"
    },
    {
      "id": 5739,
      "number": 27,
      "verse_key": "79:27",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "text": "ءَأَنتُمۡ أَشَدُّ خَلۡقًا أَمِ ٱلسَّمَآءُۚ بَنَىٰهَا"
    },
    {
      "id": 5740,
      "number": 28,
      "verse_key": "79:28",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "text": "رَفَعَ سَمۡكَهَا فَسَوَّىٰهَا"
    },
    {
      "id": 5741,
      "number": 29,
      "verse_key": "79:29",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "text": "وَأَغۡطَشَ لَيۡلَهَا وَأَخۡرَجَ ضُحَىٰهَا"
    },
    {
      "id": 5715,
      "number": 3,
      "verse_key": "79:3",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 583,
      "juz_number": 30,
      "text": "وَٱلسَّٰبِحَٰتِ سَبۡحٗا"
    },
    {
      "id": 5742,
      "number": 30,
      "verse_key": "79:30",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "text": "وَٱلۡأَرۡضَ بَعۡدَ ذَٰلِكَ دَحَىٰهَآ"
    },
    {
      "id": 5743,
      "number": 31,
      "verse_key": "79:31",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "text": "أَخۡرَجَ مِنۡهَا مَآءَهَا وَمَرۡعَىٰهَا"
    },
    {
      "id": 5744,
      "number": 32,
      "verse_key": "79:32",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "text": "وَٱلۡجِبَالَ أَرۡسَىٰهَا"
    },
    {
      "id": 5745,
      "number": 33,
      "verse_key": "79:33",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "text": "مَتَٰعٗا لَّكُمۡ وَلِأَنۡعَٰمِكُمۡ"
    },
    {
      "id": 5746,
      "number": 34,
      "verse_key": "79:34",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "text": "فَإِذَا جَآءَتِ ٱلطَّآمَّةُ ٱلۡكُبۡرَىٰ"
    },
    {
      "id": 5747,
      "number": 35,
      "verse_key": "79:35",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "text": "يَوۡمَ يَتَذَكَّرُ ٱلۡإِنسَٰنُ مَا سَعَىٰ"
    },
    {
      "id": 5748,
      "number": 36,
      "verse_key": "79:36",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "text": "وَبُرِّزَتِ ٱلۡجَحِيمُ لِمَن يَرَىٰ"
    },
    {
      "id": 5749,
      "number": 37,
      "verse_key": "79:37",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "text": "فَأَمَّا مَن طَغَىٰ"
    },
    {
      "id": 5750,
      "number": 38,
      "verse_key": "79:38",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "text": "وَءَاثَرَ ٱلۡحَيَوٰةَ ٱلدُّنۡيَا"
    },
    {
      "id": 5751,
      "number": 39,
      "verse_key": "79:39",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "text": "فَإِنَّ ٱلۡجَحِيمَ هِيَ ٱلۡمَأۡوَىٰ"
    },
    {
      "id": 5716,
      "number": 4,
      "verse_key": "79:4",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 583,
      "juz_number": 30,
      "text": "فَٱلسَّٰبِقَٰتِ سَبۡقٗا"
    },
    {
      "id": 5752,
      "number": 40,
      "verse_key": "79:40",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "text": "وَأَمَّا مَنۡ خَافَ مَقَامَ رَبِّهِۦ وَنَهَى ٱلنَّفۡسَ عَنِ ٱلۡهَوَىٰ"
    },
    {
      "id": 5753,
      "number": 41,
      "verse_key": "79:41",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "text": "فَإِنَّ ٱلۡجَنَّةَ هِيَ ٱلۡمَأۡوَىٰ"
    },
    {
      "id": 5754,
      "number": 42,
      "verse_key": "79:42",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "text": "يَسۡـَٔلُونَكَ عَنِ ٱلسَّاعَةِ أَيَّانَ مُرۡسَىٰهَا"
    },
    {
      "id": 5755,
      "number": 43,
      "verse_key": "79:43",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "text": "فِيمَ أَنتَ مِن ذِكۡرَىٰهَآ"
    },
    {
      "id": 5756,
      "number": 44,
      "verse_key": "79:44",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "text": "إِلَىٰ رَبِّكَ مُنتَهَىٰهَآ"
    },
    {
      "id": 5757,
      "number": 45,
      "verse_key": "79:45",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "text": "إِنَّمَآ أَنتَ مُنذِرُ مَن يَخۡشَىٰهَا"
    },
    {
      "id": 5758,
      "number": 46,
      "verse_key": "79:46",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "text": "كَأَنَّهُمۡ يَوۡمَ يَرَوۡنَهَا لَمۡ يَلۡبَثُوٓاْ إِلَّا عَشِيَّةً أَوۡ ضُحَىٰهَا"
    },
    {
      "id": 5717,
      "number": 5,
      "verse_key": "79:5",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 583,
      "juz_number": 30,
      "text": "فَٱلۡمُدَبِّرَٰتِ أَمۡرٗا"
    },
    {
      "id": 5718,
      "number": 6,
      "verse_key": "79:6",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 583,
      "juz_number": 30,
      "text": "يَوۡمَ تَرۡجُفُ ٱلرَّاجِفَةُ"
    },
    {
      "id": 5719,
      "number": 7,
      "verse_key": "79:7",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 583,
      "juz_number": 30,
      "text": "تَتۡبَعُهَا ٱلرَّادِفَةُ"
    },
    {
      "id": 5720,
      "number": 8,
      "verse_key": "79:8",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 583,
      "juz_number": 30,
      "text": "قُلُوبٞ يَوۡمَئِذٖ وَاجِفَةٌ"
    },
    {
      "id": 5721,
      "number": 9,
      "verse_key": "79:9",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 583,
      "juz_number": 30,
      "text": "أَبۡصَٰرُهَا خَٰشِعَةٞ"
    }
  ]
}