{
  "verses": [
    {
      "id": 5949,
      "number": 1,
      "verse_key": "87:1",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 531,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 591,
      "juz_number": 30,
      "text": "سَبِّحِ ٱسۡمَ رَبِّكَ ٱلۡأَعۡلَى"
    },
    {
      "id": 5958,
      "number": 10,
      "verse_key": "87:10",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 531,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 591,
      "juz_number": 30,
      "text": "سَيَذَّكَّرُ مَن يَخۡشَىٰ"
    },
    {
      "id": 5959,
      "number": 11,
      "verse_key": "87:11",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 531,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 591,
      "juz_number": 30,
      "text": "وَيَتَجَنَّبُهَا ٱلۡأَشۡقَى"
    },
    {
      "id": 5960,
      "number": 12,
      "verse_key": "87:12",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 531,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 591,
      "juz_number": 30,
      "text": "ٱلَّذِي يَصۡلَى ٱلنَّارَ ٱلۡكُبۡرَىٰ"
    },
    {
      "id": 5961,
      "number": 13,
      "verse_key": "87:13",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 531,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 591,
      "juz_number": 30,
      "text": "ثُمَّ لَا يَمُوتُ فِيهَا وَلَا يَحۡيَىٰ"
    },
    {
      "id": 5962,
      "number": 14,
      "verse_key": "87:14",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 531,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 591,
      "juz_number": 30,
      "text": "قَدۡ أَفۡلَحَ مَن تَزَكَّىٰ"
    },
    {
      "id": 5963,
      "number": 15,
      "verse_key": "87:15",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 531,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 591,
      "juz_number": 30,
      "text": "وَذَكَرَ ٱسۡمَ رَبِّهِۦ فَصَلَّىٰ"
    },
    {
      "id": 5964,
      "number": 16,
      "verse_key": "87:16",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 531,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "text": "بَلۡ تُؤۡثِرُونَ ٱلۡحَيَوٰةَ ٱلدُّنۡيَا"
    },
    {
      "id": 5965,
      "number": 17,
      "verse_key": "87:17",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 531,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "text": "وَٱلۡأٓخِرَةُ خَيۡرٞ وَأَبۡقَىٰٓ"
    },
    {
      "id": 5966,
      "number": 18,
      "verse_key": "87:18",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 531,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "text": "إِنَّ هَٰذَا لَفِي ٱلصُّحُفِ ٱلۡأُولَىٰ"
    },
    {
      "id": 5967,
      "number": 19,
      "verse_key": "87:19",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 531,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "text": "صُحُفِ إِبۡرَٰهِيمَ وَمُوسَىٰ"
    },
    {
      "id": 5950,
      "number": 2,
      "verse_key": "87:2",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 531,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 591,
      "juz_number": 30,
      "text": "ٱلَّذِي خَلَقَ فَسَوَّىٰ"
    },
    {
      "id": 5951,
      "number": 3,
      "verse_key": "87:3",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 531,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 591,
      "juz_number": 30,
      "text": "وَٱلَّذِي قَدَّرَ فَهَدَىٰ"
    },
    {
      "id": 5952,
      "number": 4,
      "verse_key": "87:4",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 531,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 591,
      "juz_number": 30,
      "text": "وَٱلَّذِيٓ أَخۡرَجَ ٱلۡمَرۡعَىٰ"
    },
    {
      "id": 5953,
      "number": 5,
      "verse_key": "87:5",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 531,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 591,
      "juz_number": 30,
      "text": "فَجَعَلَهُۥ غُثَآءً أَحۡوَىٰ"
    },
    {
      "id": 5954,
      "number": 6,
      "verse_key": "87:6",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 531,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 591,
      "juz_number": 30,
      "text": "سَنُقۡرِئُكَ فَلَا تَنسَىٰٓ"
    },
    {
      "id": 5955,
      "number": 7,
      "verse_key": "87:7",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 531,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 591,
      "juz_number": 30,
      "text": "إِلَّا مَا شَآءَ ٱللَّهُۚ إِنَّهُۥ يَعۡلَمُ ٱلۡجَهۡرَ وَمَا يَخۡفَىٰ"
    },
    {
      "id": 5956,
      "number": 8,
      "verse_key": "87:8",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 531,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 591,
      "juz_number": 30,
      "text": "وَنُيَسِّرُكَ لِلۡيُسۡرَىٰ"
    },
    {
      "id": 5957,
      "number": 9,
      "verse_key": "87:9",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 531,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 591,
      "juz_number": 30,
      "text": "فَذَكِّرۡ إِن نَّفَعَتِ ٱلذِّكۡرَىٰ"
    }
  ]
}