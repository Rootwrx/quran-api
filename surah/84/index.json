{
  "verses": [
    {
      "id": 5885,
      "number": 1,
      "verse_key": "84:1",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 589,
      "juz_number": 30,
      "text": "إِذَا ٱلسَّمَآءُ ٱنشَقَّتۡ"
    },
    {
      "id": 5886,
      "number": 2,
      "verse_key": "84:2",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 589,
      "juz_number": 30,
      "text": "وَأَذِنَتۡ لِرَبِّهَا وَحُقَّتۡ"
    },
    {
      "id": 5887,
      "number": 3,
      "verse_key": "84:3",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 589,
      "juz_number": 30,
      "text": "وَإِذَا ٱلۡأَرۡضُ مُدَّتۡ"
    },
    {
      "id": 5888,
      "number": 4,
      "verse_key": "84:4",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 589,
      "juz_number": 30,
      "text": "وَأَلۡقَتۡ مَا فِيهَا وَتَخَلَّتۡ"
    },
    {
      "id": 5889,
      "number": 5,
      "verse_key": "84:5",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 589,
      "juz_number": 30,
      "text": "وَأَذِنَتۡ لِرَبِّهَا وَحُقَّتۡ"
    },
    {
      "id": 5890,
      "number": 6,
      "verse_key": "84:6",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 589,
      "juz_number": 30,
      "text": "يَٰٓأَيُّهَا ٱلۡإِنسَٰنُ إِنَّكَ كَادِحٌ إِلَىٰ رَبِّكَ كَدۡحٗا فَمُلَٰقِيهِ"
    },
    {
      "id": 5891,
      "number": 7,
      "verse_key": "84:7",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 589,
      "juz_number": 30,
      "text": "فَأَمَّا مَنۡ أُوتِيَ كِتَٰبَهُۥ بِيَمِينِهِۦ"
    },
    {
      "id": 5892,
      "number": 8,
      "verse_key": "84:8",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 589,
      "juz_number": 30,
      "text": "فَسَوۡفَ يُحَاسَبُ حِسَابٗا يَسِيرٗا"
    },
    {
      "id": 5893,
      "number": 9,
      "verse_key": "84:9",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 589,
      "juz_number": 30,
      "text": "وَيَنقَلِبُ إِلَىٰٓ أَهۡلِهِۦ مَسۡرُورٗا"
    },
    {
      "id": 5894,
      "number": 10,
      "verse_key": "84:10",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 589,
      "juz_number": 30,
      "text": "وَأَمَّا مَنۡ أُوتِيَ كِتَٰبَهُۥ وَرَآءَ ظَهۡرِهِۦ"
    },
    {
      "id": 5895,
      "number": 11,
      "verse_key": "84:11",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 589,
      "juz_number": 30,
      "text": "فَسَوۡفَ يَدۡعُواْ ثُبُورٗا"
    },
    {
      "id": 5896,
      "number": 12,
      "verse_key": "84:12",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 589,
      "juz_number": 30,
      "text": "وَيَصۡلَىٰ سَعِيرًا"
    },
    {
      "id": 5897,
      "number": 13,
      "verse_key": "84:13",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 589,
      "juz_number": 30,
      "text": "إِنَّهُۥ كَانَ فِيٓ أَهۡلِهِۦ مَسۡرُورًا"
    },
    {
      "id": 5898,
      "number": 14,
      "verse_key": "84:14",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 589,
      "juz_number": 30,
      "text": "إِنَّهُۥ ظَنَّ أَن لَّن يَحُورَ"
    },
    {
      "id": 5899,
      "number": 15,
      "verse_key": "84:15",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 589,
      "juz_number": 30,
      "text": "بَلَىٰٓۚ إِنَّ رَبَّهُۥ كَانَ بِهِۦ بَصِيرٗا"
    },
    {
      "id": 5900,
      "number": 16,
      "verse_key": "84:16",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 589,
      "juz_number": 30,
      "text": "فَلَآ أُقۡسِمُ بِٱلشَّفَقِ"
    },
    {
      "id": 5901,
      "number": 17,
      "verse_key": "84:17",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 589,
      "juz_number": 30,
      "text": "وَٱلَّيۡلِ وَمَا وَسَقَ"
    },
    {
      "id": 5902,
      "number": 18,
      "verse_key": "84:18",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 589,
      "juz_number": 30,
      "text": "وَٱلۡقَمَرِ إِذَا ٱتَّسَقَ"
    },
    {
      "id": 5903,
      "number": 19,
      "verse_key": "84:19",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 589,
      "juz_number": 30,
      "text": "لَتَرۡكَبُنَّ طَبَقًا عَن طَبَقٖ"
    },
    {
      "id": 5904,
      "number": 20,
      "verse_key": "84:20",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 589,
      "juz_number": 30,
      "text": "فَمَا لَهُمۡ لَا يُؤۡمِنُونَ"
    },
    {
      "id": 5905,
      "number": 21,
      "verse_key": "84:21",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": 13,
      "page_number": 589,
      "juz_number": 30,
      "text": "وَإِذَا قُرِئَ عَلَيۡهِمُ ٱلۡقُرۡءَانُ لَا يَسۡجُدُونَۤ۩"
    },
    {
      "id": 5906,
      "number": 22,
      "verse_key": "84:22",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 589,
      "juz_number": 30,
      "text": "بَلِ ٱلَّذِينَ كَفَرُواْ يُكَذِّبُونَ"
    },
    {
      "id": 5907,
      "number": 23,
      "verse_key": "84:23",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 589,
      "juz_number": 30,
      "text": "وَٱللَّهُ أَعۡلَمُ بِمَا يُوعُونَ"
    },
    {
      "id": 5908,
      "number": 24,
      "verse_key": "84:24",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 589,
      "juz_number": 30,
      "text": "فَبَشِّرۡهُم بِعَذَابٍ أَلِيمٍ"
    },
    {
      "id": 5909,
      "number": 25,
      "verse_key": "84:25",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 589,
      "juz_number": 30,
      "text": "إِلَّا ٱلَّذِينَ ءَامَنُواْ وَعَمِلُواْ ٱلصَّٰلِحَٰتِ لَهُمۡ أَجۡرٌ غَيۡرُ مَمۡنُونِۭ"
    }
  ]
}