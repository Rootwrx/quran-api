{
  "verses": [
    {
      "id": 5376,
      "number": 1,
      "verse_key": "70:1",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 568,
      "juz_number": 29,
      "text": "سَأَلَ سَآئِلُۢ بِعَذَابٖ وَاقِعٖ"
    },
    {
      "id": 5385,
      "number": 10,
      "verse_key": "70:10",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 568,
      "juz_number": 29,
      "text": "وَلَا يَسۡـَٔلُ حَمِيمٌ حَمِيمٗا"
    },
    {
      "id": 5386,
      "number": 11,
      "verse_key": "70:11",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 569,
      "juz_number": 29,
      "text": "يُبَصَّرُونَهُمۡۚ يَوَدُّ ٱلۡمُجۡرِمُ لَوۡ يَفۡتَدِي مِنۡ عَذَابِ يَوۡمِئِذِۭ بِبَنِيهِ"
    },
    {
      "id": 5387,
      "number": 12,
      "verse_key": "70:12",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 569,
      "juz_number": 29,
      "text": "وَصَٰحِبَتِهِۦ وَأَخِيهِ"
    },
    {
      "id": 5388,
      "number": 13,
      "verse_key": "70:13",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 569,
      "juz_number": 29,
      "text": "وَفَصِيلَتِهِ ٱلَّتِي تُـٔۡوِيهِ"
    },
    {
      "id": 5389,
      "number": 14,
      "verse_key": "70:14",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 569,
      "juz_number": 29,
      "text": "وَمَن فِي ٱلۡأَرۡضِ جَمِيعٗا ثُمَّ يُنجِيهِ"
    },
    {
      "id": 5390,
      "number": 15,
      "verse_key": "70:15",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 569,
      "juz_number": 29,
      "text": "كَلَّآۖ إِنَّهَا لَظَىٰ"
    },
    {
      "id": 5391,
      "number": 16,
      "verse_key": "70:16",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 569,
      "juz_number": 29,
      "text": "نَزَّاعَةٗ لِّلشَّوَىٰ"
    },
    {
      "id": 5392,
      "number": 17,
      "verse_key": "70:17",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 569,
      "juz_number": 29,
      "text": "تَدۡعُواْ مَنۡ أَدۡبَرَ وَتَوَلَّىٰ"
    },
    {
      "id": 5393,
      "number": 18,
      "verse_key": "70:18",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 569,
      "juz_number": 29,
      "text": "وَجَمَعَ فَأَوۡعَىٰٓ"
    },
    {
      "id": 5394,
      "number": 19,
      "verse_key": "70:19",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 569,
      "juz_number": 29,
      "text": "۞ إِنَّ ٱلۡإِنسَٰنَ خُلِقَ هَلُوعًا"
    },
    {
      "id": 5377,
      "number": 2,
      "verse_key": "70:2",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 568,
      "juz_number": 29,
      "text": "لِّلۡكَٰفِرِينَ لَيۡسَ لَهُۥ دَافِعٞ"
    },
    {
      "id": 5395,
      "number": 20,
      "verse_key": "70:20",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 569,
      "juz_number": 29,
      "text": "إِذَا مَسَّهُ ٱلشَّرُّ جَزُوعٗا"
    },
    {
      "id": 5396,
      "number": 21,
      "verse_key": "70:21",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 569,
      "juz_number": 29,
      "text": "وَإِذَا مَسَّهُ ٱلۡخَيۡرُ مَنُوعًا"
    },
    {
      "id": 5397,
      "number": 22,
      "verse_key": "70:22",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 569,
      "juz_number": 29,
      "text": "إِلَّا ٱلۡمُصَلِّينَ"
    },
    {
      "id": 5398,
      "number": 23,
      "verse_key": "70:23",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 569,
      "juz_number": 29,
      "text": "ٱلَّذِينَ هُمۡ عَلَىٰ صَلَاتِهِمۡ دَآئِمُونَ"
    },
    {
      "id": 5399,
      "number": 24,
      "verse_key": "70:24",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 569,
      "juz_number": 29,
      "text": "وَٱلَّذِينَ فِيٓ أَمۡوَٰلِهِمۡ حَقّٞ مَّعۡلُومٞ"
    },
    {
      "id": 5400,
      "number": 25,
      "verse_key": "70:25",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 569,
      "juz_number": 29,
      "text": "لِّلسَّآئِلِ وَٱلۡمَحۡرُومِ"
    },
    {
      "id": 5401,
      "number": 26,
      "verse_key": "70:26",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 569,
      "juz_number": 29,
      "text": "وَٱلَّذِينَ يُصَدِّقُونَ بِيَوۡمِ ٱلدِّينِ"
    },
    {
      "id": 5402,
      "number": 27,
      "verse_key": "70:27",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 569,
      "juz_number": 29,
      "text": "وَٱلَّذِينَ هُم مِّنۡ عَذَابِ رَبِّهِم مُّشۡفِقُونَ"
    },
    {
      "id": 5403,
      "number": 28,
      "verse_key": "70:28",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 569,
      "juz_number": 29,
      "text": "إِنَّ عَذَابَ رَبِّهِمۡ غَيۡرُ مَأۡمُونٖ"
    },
    {
      "id": 5404,
      "number": 29,
      "verse_key": "70:29",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 569,
      "juz_number": 29,
      "text": "وَٱلَّذِينَ هُمۡ لِفُرُوجِهِمۡ حَٰفِظُونَ"
    },
    {
      "id": 5378,
      "number": 3,
      "verse_key": "70:3",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 568,
      "juz_number": 29,
      "text": "مِّنَ ٱللَّهِ ذِي ٱلۡمَعَارِجِ"
    },
    {
      "id": 5405,
      "number": 30,
      "verse_key": "70:30",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 569,
      "juz_number": 29,
      "text": "إِلَّا عَلَىٰٓ أَزۡوَٰجِهِمۡ أَوۡ مَا مَلَكَتۡ أَيۡمَٰنُهُمۡ فَإِنَّهُمۡ غَيۡرُ مَلُومِينَ"
    },
    {
      "id": 5406,
      "number": 31,
      "verse_key": "70:31",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 569,
      "juz_number": 29,
      "text": "فَمَنِ ٱبۡتَغَىٰ وَرَآءَ ذَٰلِكَ فَأُوْلَٰٓئِكَ هُمُ ٱلۡعَادُونَ"
    },
    {
      "id": 5407,
      "number": 32,
      "verse_key": "70:32",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 569,
      "juz_number": 29,
      "text": "وَٱلَّذِينَ هُمۡ لِأَمَٰنَٰتِهِمۡ وَعَهۡدِهِمۡ رَٰعُونَ"
    },
    {
      "id": 5408,
      "number": 33,
      "verse_key": "70:33",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 569,
      "juz_number": 29,
      "text": "وَٱلَّذِينَ هُم بِشَهَٰدَٰتِهِمۡ قَآئِمُونَ"
    },
    {
      "id": 5409,
      "number": 34,
      "verse_key": "70:34",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 569,
      "juz_number": 29,
      "text": "وَٱلَّذِينَ هُمۡ عَلَىٰ صَلَاتِهِمۡ يُحَافِظُونَ"
    },
    {
      "id": 5410,
      "number": 35,
      "verse_key": "70:35",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 569,
      "juz_number": 29,
      "text": "أُوْلَٰٓئِكَ فِي جَنَّٰتٖ مُّكۡرَمُونَ"
    },
    {
      "id": 5411,
      "number": 36,
      "verse_key": "70:36",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 505,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 569,
      "juz_number": 29,
      "text": "فَمَالِ ٱلَّذِينَ كَفَرُواْ قِبَلَكَ مُهۡطِعِينَ"
    },
    {
      "id": 5412,
      "number": 37,
      "verse_key": "70:37",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 505,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 569,
      "juz_number": 29,
      "text": "عَنِ ٱلۡيَمِينِ وَعَنِ ٱلشِّمَالِ عِزِينَ"
    },
    {
      "id": 5413,
      "number": 38,
      "verse_key": "70:38",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 505,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 569,
      "juz_number": 29,
      "text": "أَيَطۡمَعُ كُلُّ ٱمۡرِيٕٖ مِّنۡهُمۡ أَن يُدۡخَلَ جَنَّةَ نَعِيمٖ"
    },
    {
      "id": 5414,
      "number": 39,
      "verse_key": "70:39",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 505,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 569,
      "juz_number": 29,
      "text": "كَلَّآۖ إِنَّا خَلَقۡنَٰهُم مِّمَّا يَعۡلَمُونَ"
    },
    {
      "id": 5379,
      "number": 4,
      "verse_key": "70:4",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 568,
      "juz_number": 29,
      "text": "تَعۡرُجُ ٱلۡمَلَٰٓئِكَةُ وَٱلرُّوحُ إِلَيۡهِ فِي يَوۡمٖ كَانَ مِقۡدَارُهُۥ خَمۡسِينَ أَلۡفَ سَنَةٖ"
    },
    {
      "id": 5415,
      "number": 40,
      "verse_key": "70:40",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 505,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 570,
      "juz_number": 29,
      "text": "فَلَآ أُقۡسِمُ بِرَبِّ ٱلۡمَشَٰرِقِ وَٱلۡمَغَٰرِبِ إِنَّا لَقَٰدِرُونَ"
    },
    {
      "id": 5416,
      "number": 41,
      "verse_key": "70:41",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 505,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 570,
      "juz_number": 29,
      "text": "عَلَىٰٓ أَن نُّبَدِّلَ خَيۡرٗا مِّنۡهُمۡ وَمَا نَحۡنُ بِمَسۡبُوقِينَ"
    },
    {
      "id": 5417,
      "number": 42,
      "verse_key": "70:42",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 505,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 570,
      "juz_number": 29,
      "text": "فَذَرۡهُمۡ يَخُوضُواْ وَيَلۡعَبُواْ حَتَّىٰ يُلَٰقُواْ يَوۡمَهُمُ ٱلَّذِي يُوعَدُونَ"
    },
    {
      "id": 5418,
      "number": 43,
      "verse_key": "70:43",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 505,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 570,
      "juz_number": 29,
      "text": "يَوۡمَ يَخۡرُجُونَ مِنَ ٱلۡأَجۡدَاثِ سِرَاعٗا كَأَنَّهُمۡ إِلَىٰ نُصُبٖ يُوفِضُونَ"
    },
    {
      "id": 5419,
      "number": 44,
      "verse_key": "70:44",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 505,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 570,
      "juz_number": 29,
      "text": "خَٰشِعَةً أَبۡصَٰرُهُمۡ تَرۡهَقُهُمۡ ذِلَّةٞۚ ذَٰلِكَ ٱلۡيَوۡمُ ٱلَّذِي كَانُواْ يُوعَدُونَ"
    },
    {
      "id": 5380,
      "number": 5,
      "verse_key": "70:5",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 568,
      "juz_number": 29,
      "text": "فَٱصۡبِرۡ صَبۡرٗا جَمِيلًا"
    },
    {
      "id": 5381,
      "number": 6,
      "verse_key": "70:6",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 568,
      "juz_number": 29,
      "text": "إِنَّهُمۡ يَرَوۡنَهُۥ بَعِيدٗا"
    },
    {
      "id": 5382,
      "number": 7,
      "verse_key": "70:7",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 568,
      "juz_number": 29,
      "text": "وَنَرَىٰهُ قَرِيبٗا"
    },
    {
      "id": 5383,
      "number": 8,
      "verse_key": "70:8",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 568,
      "juz_number": 29,
      "text": "يَوۡمَ تَكُونُ ٱلسَّمَآءُ كَٱلۡمُهۡلِ"
    },
    {
      "id": 5384,
      "number": 9,
      "verse_key": "70:9",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 568,
      "juz_number": 29,
      "text": "وَتَكُونُ ٱلۡجِبَالُ كَٱلۡعِهۡنِ"
