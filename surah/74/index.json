{
  "verses": [
    {
      "id": 5496,
      "number": 1,
      "verse_key": "74:1",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 512,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 575,
      "juz_number": 29,
      "text": "يَٰٓأَيُّهَا ٱلۡمُدَّثِّرُ"
    },
    {
      "id": 5505,
      "number": 10,
      "verse_key": "74:10",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 512,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 575,
      "juz_number": 29,
      "text": "عَلَى ٱلۡكَٰفِرِينَ غَيۡرُ يَسِيرٖ"
    },
    {
      "id": 5506,
      "number": 11,
      "verse_key": "74:11",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 512,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 575,
      "juz_number": 29,
      "text": "ذَرۡنِي وَمَنۡ خَلَقۡتُ وَحِيدٗا"
    },
    {
      "id": 5507,
      "number": 12,
      "verse_key": "74:12",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 512,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 575,
      "juz_number": 29,
      "text": "وَجَعَلۡتُ لَهُۥ مَالٗا مَّمۡدُودٗا"
    },
    {
      "id": 5508,
      "number": 13,
      "verse_key": "74:13",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 512,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 575,
      "juz_number": 29,
      "text": "وَبَنِينَ شُهُودٗا"
    },
    {
      "id": 5509,
      "number": 14,
      "verse_key": "74:14",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 512,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 575,
      "juz_number": 29,
      "text": "وَمَهَّدتُّ لَهُۥ تَمۡهِيدٗا"
    },
    {
      "id": 5510,
      "number": 15,
      "verse_key": "74:15",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 512,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 575,
      "juz_number": 29,
      "text": "ثُمَّ يَطۡمَعُ أَنۡ أَزِيدَ"
    },
    {
      "id": 5511,
      "number": 16,
      "verse_key": "74:16",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 512,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 575,
      "juz_number": 29,
      "text": "كَلَّآۖ إِنَّهُۥ كَانَ لِأٓيَٰتِنَا عَنِيدٗا"
    },
    {
      "id": 5512,
      "number": 17,
      "verse_key": "74:17",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 512,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 575,
      "juz_number": 29,
      "text": "سَأُرۡهِقُهُۥ صَعُودًا"
    },
    {
      "id": 5513,
      "number": 18,
      "verse_key": "74:18",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 512,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 576,
      "juz_number": 29,
      "text": "إِنَّهُۥ فَكَّرَ وَقَدَّرَ"
    },
    {
      "id": 5514,
      "number": 19,
      "verse_key": "74:19",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 512,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 576,
      "juz_number": 29,
      "text": "فَقُتِلَ كَيۡفَ قَدَّرَ"
    },
    {
      "id": 5497,
      "number": 2,
      "verse_key": "74:2",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 512,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 575,
      "juz_number": 29,
      "text": "قُمۡ فَأَنذِرۡ"
    },
    {
      "id": 5515,
      "number": 20,
      "verse_key": "74:20",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 512,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 576,
      "juz_number": 29,
      "text": "ثُمَّ قُتِلَ كَيۡفَ قَدَّرَ"
    },
    {
      "id": 5516,
      "number": 21,
      "verse_key": "74:21",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 512,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 576,
      "juz_number": 29,
      "text": "ثُمَّ نَظَرَ"
    },
    {
      "id": 5517,
      "number": 22,
      "verse_key": "74:22",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 512,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 576,
      "juz_number": 29,
      "text": "ثُمَّ عَبَسَ وَبَسَرَ"
    },
    {
      "id": 5518,
      "number": 23,
      "verse_key": "74:23",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 512,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 576,
      "juz_number": 29,
      "text": "ثُمَّ أَدۡبَرَ وَٱسۡتَكۡبَرَ"
    },
    {
      "id": 5519,
      "number": 24,
      "verse_key": "74:24",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 512,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 576,
      "juz_number": 29,
      "text": "فَقَالَ إِنۡ هَٰذَآ إِلَّا سِحۡرٞ يُؤۡثَرُ"
    },
    {
      "id": 5520,
      "number": 25,
      "verse_key": "74:25",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 512,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 576,
      "juz_number": 29,
      "text": "إِنۡ هَٰذَآ إِلَّا قَوۡلُ ٱلۡبَشَرِ"
    },
    {
      "id": 5521,
      "number": 26,
      "verse_key": "74:26",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 512,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 576,
      "juz_number": 29,
      "text": "سَأُصۡلِيهِ سَقَرَ"
    },
    {
      "id": 5522,
      "number": 27,
      "verse_key": "74:27",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 512,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 576,
      "juz_number": 29,
      "text": "وَمَآ أَدۡرَىٰكَ مَا سَقَرُ"
    },
    {
      "id": 5523,
      "number": 28,
      "verse_key": "74:28",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 512,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 576,
      "juz_number": 29,
      "text": "لَا تُبۡقِي وَلَا تَذَرُ"
    },
    {
      "id": 5524,
      "number": 29,
      "verse_key": "74:29",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 512,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 576,
      "juz_number": 29,
      "text": "لَوَّاحَةٞ لِّلۡبَشَرِ"
    },
    {
      "id": 5498,
      "number": 3,
      "verse_key": "74:3",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 512,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 575,
      "juz_number": 29,
      "text": "وَرَبَّكَ فَكَبِّرۡ"
    },
    {
      "id": 5525,
      "number": 30,
      "verse_key": "74:30",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 512,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 576,
      "juz_number": 29,
      "text": "عَلَيۡهَا تِسۡعَةَ عَشَرَ"
    },
    {
      "id": 5526,
      "number": 31,
      "verse_key": "74:31",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 512,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 576,
      "juz_number": 29,
      "text": "وَمَا جَعَلۡنَآ أَصۡحَٰبَ ٱلنَّارِ إِلَّا مَلَٰٓئِكَةٗۖ وَمَا جَعَلۡنَا عِدَّتَهُمۡ إِلَّا فِتۡنَةٗ لِّلَّذِينَ كَفَرُواْ لِيَسۡتَيۡقِنَ ٱلَّذِينَ أُوتُواْ ٱلۡكِتَٰبَ وَيَزۡدَادَ ٱلَّذِينَ ءَامَنُوٓاْ إِيمَٰنٗا وَلَا يَرۡتَابَ ٱلَّذِينَ أُوتُواْ ٱلۡكِتَٰبَ وَٱلۡمُؤۡمِنُونَ وَلِيَقُولَ ٱلَّذِينَ فِي قُلُوبِهِم مَّرَضٞ وَٱلۡكَٰفِرُونَ مَاذَآ أَرَادَ ٱللَّهُ بِهَٰذَا مَثَلٗاۚ كَذَٰلِكَ يُضِلُّ ٱللَّهُ مَن يَشَآءُ وَيَهۡدِي مَن يَشَآءُۚ وَمَا يَعۡلَمُ جُنُودَ رَبِّكَ إِلَّا هُوَۚ وَمَا هِيَ إِلَّا ذِكۡرَىٰ لِلۡبَشَرِ"
    },
    {
      "id": 5527,
      "number": 32,
      "verse_key": "74:32",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 513,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 576,
      "juz_number": 29,
      "text": "كَلَّا وَٱلۡقَمَرِ"
    },
    {
      "id": 5528,
      "number": 33,
      "verse_key": "74:33",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 513,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 576,
      "juz_number": 29,
      "text": "وَٱلَّيۡلِ إِذۡ أَدۡبَرَ"
    },
    {
      "id": 5529,
      "number": 34,
      "verse_key": "74:34",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 513,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 576,
      "juz_number": 29,
      "text": "وَٱلصُّبۡحِ إِذَآ أَسۡفَرَ"
    },
    {
      "id": 5530,
      "number": 35,
      "verse_key": "74:35",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 513,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 576,
      "juz_number": 29,
      "text": "إِنَّهَا لَإِحۡدَى ٱلۡكُبَرِ"
    },
    {
      "id": 5531,
      "number": 36,
      "verse_key": "74:36",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 513,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 576,
      "juz_number": 29,
      "text": "نَذِيرٗا لِّلۡبَشَرِ"
    },
    {
      "id": 5532,
      "number": 37,
      "verse_key": "74:37",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 513,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 576,
      "juz_number": 29,
      "text": "لِمَن شَآءَ مِنكُمۡ أَن يَتَقَدَّمَ أَوۡ يَتَأَخَّرَ"
    },
    {
      "id": 5533,
      "number": 38,
      "verse_key": "74:38",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 513,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 576,
      "juz_number": 29,
      "text": "كُلُّ نَفۡسِۭ بِمَا كَسَبَتۡ رَهِينَةٌ"
    },
    {
      "id": 5534,
      "number": 39,
      "verse_key": "74:39",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 513,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 576,
      "juz_number": 29,
      "text": "إِلَّآ أَصۡحَٰبَ ٱلۡيَمِينِ"
    },
    {
      "id": 5499,
      "number": 4,
      "verse_key": "74:4",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 512,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 575,
      "juz_number": 29,
      "text": "وَثِيَابَكَ فَطَهِّرۡ"
    },
    {
      "id": 5535,
      "number": 40,
      "verse_key": "74:40",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 513,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 576,
      "juz_number": 29,
      "text": "فِي جَنَّٰتٖ يَتَسَآءَلُونَ"
    },
    {
      "id": 5536,
      "number": 41,
      "verse_key": "74:41",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 513,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 576,
      "juz_number": 29,
      "text": "عَنِ ٱلۡمُجۡرِمِينَ"
    },
    {
      "id": 5537,
      "number": 42,
      "verse_key": "74:42",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 513,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 576,
      "juz_number": 29,
      "text": "مَا سَلَكَكُمۡ فِي سَقَرَ"
    },
    {
      "id": 5538,
      "number": 43,
      "verse_key": "74:43",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 513,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 576,
      "juz_number": 29,
      "text": "قَالُواْ لَمۡ نَكُ مِنَ ٱلۡمُصَلِّينَ"
    },
    {
      "id": 5539,
      "number": 44,
      "verse_key": "74:44",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 513,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 576,
      "juz_number": 29,
      "text": "وَلَمۡ نَكُ نُطۡعِمُ ٱلۡمِسۡكِينَ"
    },
    {
      "id": 5540,
      "number": 45,
      "verse_key": "74:45",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 513,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 576,
      "juz_number": 29,
      "text": "وَكُنَّا نَخُوضُ مَعَ ٱلۡخَآئِضِينَ"
    },
    {
      "id": 5541,
      "number": 46,
      "verse_key": "74:46",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 513,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 576,
      "juz_number": 29,
      "text": "وَكُنَّا نُكَذِّبُ بِيَوۡمِ ٱلدِّينِ"
    },
    {
      "id": 5542,
      "number": 47,
      "verse_key": "74:47",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 513,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 576,
      "juz_number": 29,
      "text": "حَتَّىٰٓ أَتَىٰنَا ٱلۡيَقِينُ"
    },
    {
      "id": 5543,
      "number": 48,
      "verse_key": "74:48",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 513,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 577,
      "juz_number": 29,
      "text": "فَمَا تَنفَعُهُمۡ شَفَٰعَةُ ٱلشَّٰفِعِينَ"
    },
    {
      "id": 5544,
      "number": 49,
      "verse_key": "74:49",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 513,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 577,
      "juz_number": 29,
      "text": "فَمَا لَهُمۡ عَنِ ٱلتَّذۡكِرَةِ مُعۡرِضِينَ"
    },
    {
      "id": 5500,
      "number": 5,
      "verse_key": "74:5",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 512,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 575,
      "juz_number": 29,
      "text": "وَٱلرُّجۡزَ فَٱهۡجُرۡ"
    },
    {
      "id": 5545,
      "number": 50,
      "verse_key": "74:50",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 513,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 577,
      "juz_number": 29,
      "text": "كَأَنَّهُمۡ حُمُرٞ مُّسۡتَنفِرَةٞ"
    },
    {
      "id": 5546,
      "number": 51,
      "verse_key": "74:51",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 513,
