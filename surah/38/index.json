{
  "verses": [
    {
      "id": 3971,
      "number": 1,
      "verse_key": "38:1",
      "hizb_number": 46,
      "rub_el_hizb_number": 181,
      "ruku_number": 393,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 453,
      "juz_number": 23,
      "text": "صٓۚ وَٱلۡقُرۡءَانِ ذِي ٱلذِّكۡرِ"
    },
    {
      "id": 3980,
      "number": 10,
      "verse_key": "38:10",
      "hizb_number": 46,
      "rub_el_hizb_number": 181,
      "ruku_number": 393,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 453,
      "juz_number": 23,
      "text": "أَمۡ لَهُم مُّلۡكُ ٱلسَّمَٰوَٰتِ وَٱلۡأَرۡضِ وَمَا بَيۡنَهُمَاۖ فَلۡيَرۡتَقُواْ فِي ٱلۡأَسۡبَٰبِ"
    },
    {
      "id": 3981,
      "number": 11,
      "verse_key": "38:11",
      "hizb_number": 46,
      "rub_el_hizb_number": 181,
      "ruku_number": 393,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 453,
      "juz_number": 23,
      "text": "جُندٞ مَّا هُنَالِكَ مَهۡزُومٞ مِّنَ ٱلۡأَحۡزَابِ"
    },
    {
      "id": 3982,
      "number": 12,
      "verse_key": "38:12",
      "hizb_number": 46,
      "rub_el_hizb_number": 181,
      "ruku_number": 393,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 453,
      "juz_number": 23,
      "text": "كَذَّبَتۡ قَبۡلَهُمۡ قَوۡمُ نُوحٖ وَعَادٞ وَفِرۡعَوۡنُ ذُو ٱلۡأَوۡتَادِ"
    },
    {
      "id": 3983,
      "number": 13,
      "verse_key": "38:13",
      "hizb_number": 46,
      "rub_el_hizb_number": 181,
      "ruku_number": 393,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 453,
      "juz_number": 23,
      "text": "وَثَمُودُ وَقَوۡمُ لُوطٖ وَأَصۡحَٰبُ لۡـَٔيۡكَةِۚ أُوْلَٰٓئِكَ ٱلۡأَحۡزَابُ"
    },
    {
      "id": 3984,
      "number": 14,
      "verse_key": "38:14",
      "hizb_number": 46,
      "rub_el_hizb_number": 181,
      "ruku_number": 393,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 453,
      "juz_number": 23,
      "text": "إِن كُلٌّ إِلَّا كَذَّبَ ٱلرُّسُلَ فَحَقَّ عِقَابِ"
    },
    {
      "id": 3985,
      "number": 15,
      "verse_key": "38:15",
      "hizb_number": 46,
      "rub_el_hizb_number": 181,
      "ruku_number": 394,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 453,
      "juz_number": 23,
      "text": "وَمَا يَنظُرُ هَٰٓؤُلَآءِ إِلَّا صَيۡحَةٗ وَٰحِدَةٗ مَّا لَهَا مِن فَوَاقٖ"
    },
    {
      "id": 3986,
      "number": 16,
      "verse_key": "38:16",
      "hizb_number": 46,
      "rub_el_hizb_number": 181,
      "ruku_number": 394,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 453,
      "juz_number": 23,
      "text": "وَقَالُواْ رَبَّنَا عَجِّل لَّنَا قِطَّنَا قَبۡلَ يَوۡمِ ٱلۡحِسَابِ"
    },
    {
      "id": 3987,
      "number": 17,
      "verse_key": "38:17",
      "hizb_number": 46,
      "rub_el_hizb_number": 181,
      "ruku_number": 394,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 454,
      "juz_number": 23,
      "text": "ٱصۡبِرۡ عَلَىٰ مَا يَقُولُونَ وَٱذۡكُرۡ عَبۡدَنَا دَاوُۥدَ ذَا ٱلۡأَيۡدِۖ إِنَّهُۥٓ أَوَّابٌ"
    },
    {
      "id": 3988,
      "number": 18,
      "verse_key": "38:18",
      "hizb_number": 46,
      "rub_el_hizb_number": 181,
      "ruku_number": 394,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 454,
      "juz_number": 23,
      "text": "إِنَّا سَخَّرۡنَا ٱلۡجِبَالَ مَعَهُۥ يُسَبِّحۡنَ بِٱلۡعَشِيِّ وَٱلۡإِشۡرَاقِ"
    },
    {
      "id": 3989,
      "number": 19,
      "verse_key": "38:19",
      "hizb_number": 46,
      "rub_el_hizb_number": 181,
      "ruku_number": 394,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 454,
      "juz_number": 23,
      "text": "وَٱلطَّيۡرَ مَحۡشُورَةٗۖ كُلّٞ لَّهُۥٓ أَوَّابٞ"
    },
    {
      "id": 3972,
      "number": 2,
      "verse_key": "38:2",
      "hizb_number": 46,
      "rub_el_hizb_number": 181,
      "ruku_number": 393,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 453,
      "juz_number": 23,
      "text": "بَلِ ٱلَّذِينَ كَفَرُواْ فِي عِزَّةٖ وَشِقَاقٖ"
    },
    {
      "id": 3990,
      "number": 20,
      "verse_key": "38:20",
      "hizb_number": 46,
      "rub_el_hizb_number": 181,
      "ruku_number": 394,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 454,
      "juz_number": 23,
      "text": "وَشَدَدۡنَا مُلۡكَهُۥ وَءَاتَيۡنَٰهُ ٱلۡحِكۡمَةَ وَفَصۡلَ ٱلۡخِطَابِ"
    },
    {
      "id": 3991,
      "number": 21,
      "verse_key": "38:21",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 394,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 454,
      "juz_number": 23,
      "text": "۞ وَهَلۡ أَتَىٰكَ نَبَؤُاْ ٱلۡخَصۡمِ إِذۡ تَسَوَّرُواْ ٱلۡمِحۡرَابَ"
    },
    {
      "id": 3992,
      "number": 22,
      "verse_key": "38:22",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 394,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 454,
      "juz_number": 23,
      "text": "إِذۡ دَخَلُواْ عَلَىٰ دَاوُۥدَ فَفَزِعَ مِنۡهُمۡۖ قَالُواْ لَا تَخَفۡۖ خَصۡمَانِ بَغَىٰ بَعۡضُنَا عَلَىٰ بَعۡضٖ فَٱحۡكُم بَيۡنَنَا بِٱلۡحَقِّ وَلَا تُشۡطِطۡ وَٱهۡدِنَآ إِلَىٰ سَوَآءِ ٱلصِّرَٰطِ"
    },
    {
      "id": 3993,
      "number": 23,
      "verse_key": "38:23",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 394,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 454,
      "juz_number": 23,
      "text": "إِنَّ هَٰذَآ أَخِي لَهُۥ تِسۡعٞ وَتِسۡعُونَ نَعۡجَةٗ وَلِيَ نَعۡجَةٞ وَٰحِدَةٞ فَقَالَ أَكۡفِلۡنِيهَا وَعَزَّنِي فِي ٱلۡخِطَابِ"
    },
    {
      "id": 3994,
      "number": 24,
      "verse_key": "38:24",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 394,
      "manzil_number": 6,
      "sajdah_number": 10,
      "page_number": 454,
      "juz_number": 23,
      "text": "قَالَ لَقَدۡ ظَلَمَكَ بِسُؤَالِ نَعۡجَتِكَ إِلَىٰ نِعَاجِهِۦۖ وَإِنَّ كَثِيرٗا مِّنَ ٱلۡخُلَطَآءِ لَيَبۡغِي بَعۡضُهُمۡ عَلَىٰ بَعۡضٍ إِلَّا ٱلَّذِينَ ءَامَنُواْ وَعَمِلُواْ ٱلصَّٰلِحَٰتِ وَقَلِيلٞ مَّا هُمۡۗ وَظَنَّ دَاوُۥدُ أَنَّمَا فَتَنَّٰهُ فَٱسۡتَغۡفَرَ رَبَّهُۥ وَخَرَّۤ رَاكِعٗاۤ وَأَنَابَ۩"
    },
    {
      "id": 3995,
      "number": 25,
      "verse_key": "38:25",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 394,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 454,
      "juz_number": 23,
      "text": "فَغَفَرۡنَا لَهُۥ ذَٰلِكَۖ وَإِنَّ لَهُۥ عِندَنَا لَزُلۡفَىٰ وَحُسۡنَ مَـَٔابٖ"
    },
    {
      "id": 3996,
      "number": 26,
      "verse_key": "38:26",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 394,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 454,
      "juz_number": 23,
      "text": "يَٰدَاوُۥدُ إِنَّا جَعَلۡنَٰكَ خَلِيفَةٗ فِي ٱلۡأَرۡضِ فَٱحۡكُم بَيۡنَ ٱلنَّاسِ بِٱلۡحَقِّ وَلَا تَتَّبِعِ ٱلۡهَوَىٰ فَيُضِلَّكَ عَن سَبِيلِ ٱللَّهِۚ إِنَّ ٱلَّذِينَ يَضِلُّونَ عَن سَبِيلِ ٱللَّهِ لَهُمۡ عَذَابٞ شَدِيدُۢ بِمَا نَسُواْ يَوۡمَ ٱلۡحِسَابِ"
    },
    {
      "id": 3997,
      "number": 27,
      "verse_key": "38:27",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 395,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 455,
      "juz_number": 23,
      "text": "وَمَا خَلَقۡنَا ٱلسَّمَآءَ وَٱلۡأَرۡضَ وَمَا بَيۡنَهُمَا بَٰطِلٗاۚ ذَٰلِكَ ظَنُّ ٱلَّذِينَ كَفَرُواْۚ فَوَيۡلٞ لِّلَّذِينَ كَفَرُواْ مِنَ ٱلنَّارِ"
    },
    {
      "id": 3998,
      "number": 28,
      "verse_key": "38:28",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 395,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 455,
      "juz_number": 23,
      "text": "أَمۡ نَجۡعَلُ ٱلَّذِينَ ءَامَنُواْ وَعَمِلُواْ ٱلصَّٰلِحَٰتِ كَٱلۡمُفۡسِدِينَ فِي ٱلۡأَرۡضِ أَمۡ نَجۡعَلُ ٱلۡمُتَّقِينَ كَٱلۡفُجَّارِ"
    },
    {
      "id": 3999,
      "number": 29,
      "verse_key": "38:29",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 395,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 455,
      "juz_number": 23,
      "text": "كِتَٰبٌ أَنزَلۡنَٰهُ إِلَيۡكَ مُبَٰرَكٞ لِّيَدَّبَّرُوٓاْ ءَايَٰتِهِۦ وَلِيَتَذَكَّرَ أُوْلُواْ ٱلۡأَلۡبَٰبِ"
    },
    {
      "id": 3973,
      "number": 3,
      "verse_key": "38:3",
      "hizb_number": 46,
      "rub_el_hizb_number": 181,
      "ruku_number": 393,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 453,
      "juz_number": 23,
      "text": "كَمۡ أَهۡلَكۡنَا مِن قَبۡلِهِم مِّن قَرۡنٖ فَنَادَواْ وَّلَاتَ حِينَ مَنَاصٖ"
    },
    {
      "id": 4000,
      "number": 30,
      "verse_key": "38:30",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 395,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 455,
      "juz_number": 23,
      "text": "وَوَهَبۡنَا لِدَاوُۥدَ سُلَيۡمَٰنَۚ نِعۡمَ ٱلۡعَبۡدُ إِنَّهُۥٓ أَوَّابٌ"
    },
    {
      "id": 4001,
      "number": 31,
      "verse_key": "38:31",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 395,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 455,
      "juz_number": 23,
      "text": "إِذۡ عُرِضَ عَلَيۡهِ بِٱلۡعَشِيِّ ٱلصَّٰفِنَٰتُ ٱلۡجِيَادُ"
    },
    {
      "id": 4002,
      "number": 32,
      "verse_key": "38:32",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 395,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 455,
      "juz_number": 23,
      "text": "فَقَالَ إِنِّيٓ أَحۡبَبۡتُ حُبَّ ٱلۡخَيۡرِ عَن ذِكۡرِ رَبِّي حَتَّىٰ تَوَارَتۡ بِٱلۡحِجَابِ"
    },
    {
      "id": 4003,
      "number": 33,
      "verse_key": "38:33",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 395,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 455,
      "juz_number": 23,
      "text": "رُدُّوهَا عَلَيَّۖ فَطَفِقَ مَسۡحَۢا بِٱلسُّوقِ وَٱلۡأَعۡنَاقِ"
    },
    {
      "id": 4004,
      "number": 34,
      "verse_key": "38:34",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 395,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 455,
      "juz_number": 23,
      "text": "وَلَقَدۡ فَتَنَّا سُلَيۡمَٰنَ وَأَلۡقَيۡنَا عَلَىٰ كُرۡسِيِّهِۦ جَسَدٗا ثُمَّ أَنَابَ"
    },
    {
      "id": 4005,
      "number": 35,
      "verse_key": "38:35",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 395,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 455,
      "juz_number": 23,
      "text": "قَالَ رَبِّ ٱغۡفِرۡ لِي وَهَبۡ لِي مُلۡكٗا لَّا يَنۢبَغِي لِأَحَدٖ مِّنۢ بَعۡدِيٓۖ إِنَّكَ أَنتَ ٱلۡوَهَّابُ"
    },
    {
      "id": 4006,
      "number": 36,
      "verse_key": "38:36",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 395,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 455,
      "juz_number": 23,
      "text": "فَسَخَّرۡنَا لَهُ ٱلرِّيحَ تَجۡرِي بِأَمۡرِهِۦ رُخَآءً حَيۡثُ أَصَابَ"
    },
    {
      "id": 4007,
      "number": 37,
      "verse_key": "38:37",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 395,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 455,
      "juz_number": 23,
      "text": "وَٱلشَّيَٰطِينَ كُلَّ بَنَّآءٖ وَغَوَّاصٖ"
    },
    {
      "id": 4008,
      "number": 38,
      "verse_key": "38:38",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 395,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 455,
      "juz_number": 23,
      "text": "وَءَاخَرِينَ مُقَرَّنِينَ فِي ٱلۡأَصۡفَادِ"
    },
    {
      "id": 4009,
      "number": 39,
      "verse_key": "38:39",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 395,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 455,
      "juz_number": 23,
      "text": "هَٰذَا عَطَآؤُنَا فَٱمۡنُنۡ أَوۡ أَمۡسِكۡ بِغَيۡرِ حِسَابٖ"
    },
    {
      "id": 3974,
      "number": 4,
      "verse_key": "38:4",
      "hizb_number": 46,
      "rub_el_hizb_number": 181,
      "ruku_number": 393,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 453,
      "juz_number": 23,
      "text": "وَعَجِبُوٓاْ أَن جَآءَهُم مُّنذِرٞ مِّنۡهُمۡۖ وَقَالَ ٱلۡكَٰفِرُونَ هَٰذَا سَٰحِرٞ كَذَّابٌ"
    },
    {
      "id": 4010,
      "number": 40,
      "verse_key": "38:40",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 395,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 455,
      "juz_number": 23,
      "text": "وَإِنَّ لَهُۥ عِندَنَا لَزُلۡفَىٰ وَحُسۡنَ مَـَٔابٖ"
    },
    {
      "id": 4011,
      "number": 41,
      "verse_key": "38:41",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 455,
      "juz_number": 23,
      "text": "وَٱذۡكُرۡ عَبۡدَنَآ أَيُّوبَ إِذۡ نَادَىٰ رَبَّهُۥٓ أَنِّي مَسَّنِيَ ٱلشَّيۡطَٰنُ بِنُصۡبٖ وَعَذَابٍ"
    },
    {
      "id": 4012,
      "number": 42,
      "verse_key": "38:42",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 455,
      "juz_number": 23,
      "text": "ٱرۡكُضۡ بِرِجۡلِكَۖ هَٰذَا مُغۡتَسَلُۢ بَارِدٞ وَشَرَابٞ"
    },
    {
      "id": 4013,
      "number": 43,
      "verse_key": "38:43",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 456,
      "juz_number": 23,
      "text": "وَوَهَبۡنَا لَهُۥٓ أَهۡلَهُۥ وَمِثۡلَهُم مَّعَهُمۡ رَحۡمَةٗ مِّنَّا وَذِكۡرَىٰ لِأُوْلِي ٱلۡأَلۡبَٰبِ"
    },
    {
      "id": 4014,
      "number": 44,
      "verse_key": "38:44",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 456,
      "juz_number": 23,
      "text": "وَخُذۡ بِيَدِكَ ضِغۡثٗا فَٱضۡرِب بِّهِۦ وَلَا تَحۡنَثۡۗ إِنَّا وَجَدۡنَٰهُ صَابِرٗاۚ نِّعۡمَ ٱلۡعَبۡدُ إِنَّهُۥٓ أَوَّابٞ"
    },
    {
      "id": 4015,
      "number": 45,
      "verse_key": "38:45",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 456,
      "juz_number": 23,
      "text": "وَٱذۡكُرۡ عِبَٰدَنَآ إِبۡرَٰهِيمَ وَإِسۡحَٰقَ وَيَعۡقُوبَ أُوْلِي ٱلۡأَيۡدِي وَٱلۡأَبۡصَٰرِ"
    },
    {
      "id": 4016,
      "number": 46,
      "verse_key": "38:46",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 456,
      "juz_number": 23,
      "text": "إِنَّآ أَخۡلَصۡنَٰهُم بِخَالِصَةٖ ذِكۡرَى ٱلدَّارِ"
    },
    {
      "id": 4017,
      "number": 47,
      "verse_key": "38:47",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 456,
      "juz_number": 23,
      "text": "وَإِنَّهُمۡ عِندَنَا لَمِنَ ٱلۡمُصۡطَفَيۡنَ ٱلۡأَخۡيَارِ"
    },
    {
      "id": 4018,
      "number": 48,
      "verse_key": "38:48",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 456,
      "juz_number": 23,
      "text": "وَٱذۡكُرۡ إِسۡمَٰعِيلَ وَٱلۡيَسَعَ وَذَا ٱلۡكِفۡلِۖ وَكُلّٞ مِّنَ ٱلۡأَخۡيَارِ"
    },
    {
      "id": 4019,
      "number": 49,
      "verse_key": "38:49",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 456,
      "juz_number": 23,
      "text": "هَٰذَا ذِكۡرٞۚ وَإِنَّ لِلۡمُتَّقِينَ لَحُسۡنَ مَـَٔابٖ"
    },
    {
      "id": 3975,
      "number": 5,
      "verse_key": "38:5",
      "hizb_number": 46,
      "rub_el_hizb_number": 181,
      "ruku_number": 393,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 453,
      "juz_number": 23,
      "text": "أَجَعَلَ ٱلۡأٓلِهَةَ إِلَٰهٗا وَٰحِدًاۖ إِنَّ هَٰذَا لَشَيۡءٌ عُجَابٞ"
    },
    {
      "id": 4020,
      "number": 50,
      "verse_key": "38:50",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 456,
      "juz_number": 23,
      "text": "جَنَّٰتِ عَدۡنٖ مُّفَتَّحَةٗ لَّهُمُ ٱلۡأَبۡوَٰبُ"
    },
    {
      "id": 4021,
      "number": 51,
      "verse_key": "38:51",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 456,
      "juz_number": 23,
      "text": "مُتَّكِـِٔينَ فِيهَا يَدۡعُونَ فِيهَا بِفَٰكِهَةٖ كَثِيرَةٖ وَشَرَابٖ"
    },
    {
      "id": 4022,
      "number": 52,
      "verse_key": "38:52",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 456,
      "juz_number": 23,
      "text": "۞ وَعِندَهُمۡ قَٰصِرَٰتُ ٱلطَّرۡفِ أَتۡرَابٌ"
    },
    {
      "id": 4023,
      "number": 53,
      "verse_key": "38:53",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 456,
      "juz_number": 23,
      "text": "هَٰذَا مَا تُوعَدُونَ لِيَوۡمِ ٱلۡحِسَابِ"
    },
    {
      "id": 4024,
      "number": 54,
      "verse_key": "38:54",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 456,
      "juz_number": 23,
      "text": "إِنَّ هَٰذَا لَرِزۡقُنَا مَا لَهُۥ مِن نَّفَادٍ"
    },
    {
      "id": 4025,
      "number": 55,
      "verse_key": "38:55",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 456,
      "juz_number": 23,
      "text": "هَٰذَاۚ وَإِنَّ لِلطَّٰغِينَ لَشَرَّ مَـَٔابٖ"
    },
    {
      "id": 4026,
      "number": 56,
      "verse_key": "38:56",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 456,
      "juz_number": 23,
      "text": "جَهَنَّمَ يَصۡلَوۡنَهَا فَبِئۡسَ ٱلۡمِهَادُ"
    },
    {
      "id": 4027,
      "number": 57,
      "verse_key": "38:57",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 456,
      "juz_number": 23,
      "text": "هَٰذَا فَلۡيَذُوقُوهُ حَمِيمٞ وَغَسَّاقٞ"
    },
    {
      "id": 4028,
      "number": 58,
      "verse_key": "38:58",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 456,
      "juz_number": 23,
      "text": "وَءَاخَرُ مِن شَكۡلِهِۦٓ أَزۡوَٰجٌ"
    },
    {
      "id": 4029,
      "number": 59,
      "verse_key": "38:59",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 456,
      "juz_number": 23,
      "text": "هَٰذَا فَوۡجٞ مُّقۡتَحِمٞ مَّعَكُمۡ لَا مَرۡحَبَۢا بِهِمۡۚ إِنَّهُمۡ صَالُواْ ٱلنَّارِ"
    },
    {
      "id": 3976,
      "number": 6,
      "verse_key": "38:6",
      "hizb_number": 46,
      "rub_el_hizb_number": 181,
      "ruku_number": 393,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 453,
      "juz_number": 23,
      "text": "وَٱنطَلَقَ ٱلۡمَلَأُ مِنۡهُمۡ أَنِ ٱمۡشُواْ وَٱصۡبِرُواْ عَلَىٰٓ ءَالِهَتِكُمۡۖ إِنَّ هَٰذَا لَشَيۡءٞ يُرَادُ"
    },
    {
      "id": 4030,
      "number": 60,
      "verse_key": "38:60",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 456,
      "juz_number": 23,
      "text": "قَالُواْ بَلۡ أَنتُمۡ لَا مَرۡحَبَۢا بِكُمۡۖ أَنتُمۡ قَدَّمۡتُمُوهُ لَنَاۖ فَبِئۡسَ ٱلۡقَرَارُ"
    },
    {
      "id": 4031,
      "number": 61,
      "verse_key": "38:61",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 456,
      "juz_number": 23,
      "text": "قَالُواْ رَبَّنَا مَن قَدَّمَ لَنَا هَٰذَا فَزِدۡهُ عَذَابٗا ضِعۡفٗا فِي ٱلنَّارِ"
    },
    {
      "id": 4032,
      "number": 62,
      "verse_key": "38:62",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 457,
      "juz_number": 23,
      "text": "وَقَالُواْ مَا لَنَا لَا نَرَىٰ رِجَالٗا كُنَّا نَعُدُّهُم مِّنَ ٱلۡأَشۡرَارِ"
    },
    {
      "id": 4033,
      "number": 63,
      "verse_key": "38:63",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 457,
      "juz_number": 23,
      "text": "أَتَّخَذۡنَٰهُمۡ سِخۡرِيًّا أَمۡ زَاغَتۡ عَنۡهُمُ ٱلۡأَبۡصَٰرُ"
    },
    {
      "id": 4034,
      "number": 64,
      "verse_key": "38:64",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 457,
      "juz_number": 23,
      "text": "إِنَّ ذَٰلِكَ لَحَقّٞ تَخَاصُمُ أَهۡلِ ٱلنَّارِ"
    },
    {
      "id": 4035,
      "number": 65,
      "verse_key": "38:65",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 457,
      "juz_number": 23,
      "text": "قُلۡ إِنَّمَآ أَنَا۠ مُنذِرٞۖ وَمَا مِنۡ إِلَٰهٍ إِلَّا ٱللَّهُ ٱلۡوَٰحِدُ ٱلۡقَهَّارُ"
    },
    {
      "id": 4036,
      "number": 66,
      "verse_key": "38:66",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 457,
      "juz_number": 23,
      "text": "رَبُّ ٱلسَّمَٰوَٰتِ وَٱلۡأَرۡضِ وَمَا بَيۡنَهُمَا ٱلۡعَزِيزُ ٱلۡغَفَّٰرُ"
    },
    {
      "id": 4037,
      "number": 67,
      "verse_key": "38:67",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 457,
      "juz_number": 23,
      "text": "قُلۡ هُوَ نَبَؤٌاْ عَظِيمٌ"
    },
    {
      "id": 4038,
      "number": 68,
      "verse_key": "38:68",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 457,
      "juz_number": 23,
      "text": "أَنتُمۡ عَنۡهُ مُعۡرِضُونَ"
    },
    {
      "id": 4039,
      "number": 69,
      "verse_key": "38:69",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 457,
      "juz_number": 23,
      "text": "مَا كَانَ لِيَ مِنۡ عِلۡمِۭ بِٱلۡمَلَإِ ٱلۡأَعۡلَىٰٓ إِذۡ يَخۡتَصِمُونَ"
    },
    {
      "id": 3977,
      "number": 7,
      "verse_key": "38:7",
      "hizb_number": 46,
      "rub_el_hizb_number": 181,
      "ruku_number": 393,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 453,
      "juz_number": 23,
      "text": "مَا سَمِعۡنَا بِهَٰذَا فِي ٱلۡمِلَّةِ ٱلۡأٓخِرَةِ إِنۡ هَٰذَآ إِلَّا ٱخۡتِلَٰقٌ"
    },
    {
      "id": 4040,
      "number": 70,
      "verse_key": "38:70",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 457,
      "juz_number": 23,
      "text": "إِن يُوحَىٰٓ إِلَيَّ إِلَّآ أَنَّمَآ أَنَا۠ نَذِيرٞ مُّبِينٌ"
    },
    {
      "id": 4041,
      "number": 71,
      "verse_key": "38:71",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 457,
      "juz_number": 23,
      "text": "إِذۡ قَالَ رَبُّكَ لِلۡمَلَٰٓئِكَةِ إِنِّي خَٰلِقُۢ بَشَرٗا مِّن طِينٖ"
    },
    {
      "id": 4042,
      "number": 72,
      "verse_key": "38:72",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 457,
      "juz_number": 23,
      "text": "فَإِذَا سَوَّيۡتُهُۥ وَنَفَخۡتُ فِيهِ مِن رُّوحِي فَقَعُواْ لَهُۥ سَٰجِدِينَ"
    },
    {
      "id": 4043,
      "number": 73,
      "verse_key": "38:73",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 457,
      "juz_number": 23,
      "text": "فَسَجَدَ ٱلۡمَلَٰٓئِكَةُ كُلُّهُمۡ أَجۡمَعُونَ"
    },
    {
      "id": 4044,
      "number": 74,
      "verse_key": "38:74",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 457,
      "juz_number": 23,
      "text": "إِلَّآ إِبۡلِيسَ ٱسۡتَكۡبَرَ وَكَانَ مِنَ ٱلۡكَٰفِرِينَ"
    },
    {
      "id": 4045,
      "number": 75,
      "verse_key": "38:75",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 457,
      "juz_number": 23,
      "text": "قَالَ يَٰٓإِبۡلِيسُ مَا مَنَعَكَ أَن تَسۡجُدَ لِمَا خَلَقۡتُ بِيَدَيَّۖ أَسۡتَكۡبَرۡتَ أَمۡ كُنتَ مِنَ ٱلۡعَالِينَ"
    },
    {
      "id": 4046,
      "number": 76,
      "verse_key": "38:76",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 457,
      "juz_number": 23,
      "text": "قَالَ أَنَا۠ خَيۡرٞ مِّنۡهُ خَلَقۡتَنِي مِن نَّارٖ وَخَلَقۡتَهُۥ مِن طِينٖ"
    },
    {
      "id": 4047,
      "number": 77,
      "verse_key": "38:77",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 457,
      "juz_number": 23,
      "text": "قَالَ فَٱخۡرُجۡ مِنۡهَا فَإِنَّكَ رَجِيمٞ"
    },
    {
      "id": 4048,
      "number": 78,
      "verse_key": "38:78",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 457,
      "juz_number": 23,
      "text": "وَإِنَّ عَلَيۡكَ لَعۡنَتِيٓ إِلَىٰ يَوۡمِ ٱلدِّينِ"
    },
    {
      "id": 4049,
      "number": 79,
      "verse_key": "38:79",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 457,
      "juz_number": 23,
      "text": "قَالَ رَبِّ فَأَنظِرۡنِيٓ إِلَىٰ يَوۡمِ يُبۡعَثُونَ"
    },
    {
      "id": 3978,
      "number": 8,
      "verse_key": "38:8",
      "hizb_number": 46,
      "rub_el_hizb_number": 181,
      "ruku_number": 393,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 453,
      "juz_number": 23,
      "text": "أَءُنزِلَ عَلَيۡهِ ٱلذِّكۡرُ مِنۢ بَيۡنِنَاۚ بَلۡ هُمۡ فِي شَكّٖ مِّن ذِكۡرِيۚ بَل لَّمَّا يَذُوقُواْ عَذَابِ"
    },
    {
      "id": 4050,
      "number": 80,
      "verse_key": "38:80",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 457,
      "juz_number": 23,
      "text": "قَالَ فَإِنَّكَ مِنَ ٱلۡمُنظَرِينَ"
    },
    {
      "id": 4051,
      "number": 81,
      "verse_key": "38:81",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 457,
      "juz_number": 23,
      "text": "إِلَىٰ يَوۡمِ ٱلۡوَقۡتِ ٱلۡمَعۡلُومِ"
    },
    {
      "id": 4052,
      "number": 82,
      "verse_key": "38:82",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 457,
      "juz_number": 23,
      "text": "قَالَ فَبِعِزَّتِكَ لَأُغۡوِيَنَّهُمۡ أَجۡمَعِينَ"
    },
    {
      "id": 4053,
      "number": 83,
      "verse_key": "38:83",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 457,
      "juz_number": 23,
      "text": "إِلَّا عِبَادَكَ مِنۡهُمُ ٱلۡمُخۡلَصِينَ"
    },
    {
      "id": 4054,
      "number": 84,
      "verse_key": "38:84",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 458,
      "juz_number": 23,
      "text": "قَالَ فَٱلۡحَقُّ وَٱلۡحَقَّ أَقُولُ"
    },
    {
      "id": 4055,
      "number": 85,
      "verse_key": "38:85",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 458,
      "juz_number": 23,
      "text": "لَأَمۡلَأَنَّ جَهَنَّمَ مِنكَ وَمِمَّن تَبِعَكَ مِنۡهُمۡ أَجۡمَعِينَ"
    },
    {
      "id": 4056,
      "number": 86,
      "verse_key": "38:86",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 458,
      "juz_number": 23,
      "text": "قُلۡ مَآ أَسۡـَٔلُكُمۡ عَلَيۡهِ مِنۡ أَجۡرٖ وَمَآ أَنَا۠ مِنَ ٱلۡمُتَكَلِّفِينَ"
    },
    {
      "id": 4057,
      "number": 87,
      "verse_key": "38:87",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 458,
      "juz_number": 23,
      "text": "إِنۡ هُوَ إِلَّا ذِكۡرٞ لِّلۡعَٰلَمِينَ"
    },
    {
      "id": 4058,
      "number": 88,
      "verse_key": "38:88",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 458,
      "juz_number": 23,
      "text": "وَلَتَعۡلَمُنَّ نَبَأَهُۥ بَعۡدَ حِينِۭ"
    },
    {
      "id": 3979,
      "number": 9,
      "verse_key": "38:9",
      "hizb_number": 46,
      "rub_el_hizb_number": 181,
      "ruku_number": 393,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 453,
      "juz_number": 23,
      "text": "أَمۡ عِندَهُمۡ خَزَآئِنُ رَحۡمَةِ رَبِّكَ ٱلۡعَزِيزِ ٱلۡوَهَّابِ"
    }
  ]
}