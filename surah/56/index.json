{
  "verses": [
    {
      "id": 4980,
      "number": 1,
      "verse_key": "56:1",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 471,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 534,
      "juz_number": 27,
      "text": "إِذَا وَقَعَتِ ٱلۡوَاقِعَةُ"
    },
    {
      "id": 4989,
      "number": 10,
      "verse_key": "56:10",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 471,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 534,
      "juz_number": 27,
      "text": "وَٱلسَّٰبِقُونَ ٱلسَّٰبِقُونَ"
    },
    {
      "id": 4990,
      "number": 11,
      "verse_key": "56:11",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 471,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 534,
      "juz_number": 27,
      "text": "أُوْلَٰٓئِكَ ٱلۡمُقَرَّبُونَ"
    },
    {
      "id": 4991,
      "number": 12,
      "verse_key": "56:12",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 471,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 534,
      "juz_number": 27,
      "text": "فِي جَنَّٰتِ ٱلنَّعِيمِ"
    },
    {
      "id": 4992,
      "number": 13,
      "verse_key": "56:13",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 471,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 534,
      "juz_number": 27,
      "text": "ثُلَّةٞ مِّنَ ٱلۡأَوَّلِينَ"
    },
    {
      "id": 4993,
      "number": 14,
      "verse_key": "56:14",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 471,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 534,
      "juz_number": 27,
      "text": "وَقَلِيلٞ مِّنَ ٱلۡأٓخِرِينَ"
    },
    {
      "id": 4994,
      "number": 15,
      "verse_key": "56:15",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 471,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 534,
      "juz_number": 27,
      "text": "عَلَىٰ سُرُرٖ مَّوۡضُونَةٖ"
    },
    {
      "id": 4995,
      "number": 16,
      "verse_key": "56:16",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 471,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 534,
      "juz_number": 27,
      "text": "مُّتَّكِـِٔينَ عَلَيۡهَا مُتَقَٰبِلِينَ"
    },
    {
      "id": 4996,
      "number": 17,
      "verse_key": "56:17",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 471,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 535,
      "juz_number": 27,
      "text": "يَطُوفُ عَلَيۡهِمۡ وِلۡدَٰنٞ مُّخَلَّدُونَ"
    },
    {
      "id": 4997,
      "number": 18,
      "verse_key": "56:18",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 471,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 535,
      "juz_number": 27,
      "text": "بِأَكۡوَابٖ وَأَبَارِيقَ وَكَأۡسٖ مِّن مَّعِينٖ"
    },
    {
      "id": 4998,
      "number": 19,
      "verse_key": "56:19",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 471,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 535,
      "juz_number": 27,
      "text": "لَّا يُصَدَّعُونَ عَنۡهَا وَلَا يُنزِفُونَ"
    },
    {
      "id": 4981,
      "number": 2,
      "verse_key": "56:2",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 471,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 534,
      "juz_number": 27,
      "text": "لَيۡسَ لِوَقۡعَتِهَا كَاذِبَةٌ"
    },
    {
      "id": 4999,
      "number": 20,
      "verse_key": "56:20",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 471,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 535,
      "juz_number": 27,
      "text": "وَفَٰكِهَةٖ مِّمَّا يَتَخَيَّرُونَ"
    },
    {
      "id": 5000,
      "number": 21,
      "verse_key": "56:21",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 471,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 535,
      "juz_number": 27,
      "text": "وَلَحۡمِ طَيۡرٖ مِّمَّا يَشۡتَهُونَ"
    },
    {
      "id": 5001,
      "number": 22,
      "verse_key": "56:22",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 471,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 535,
      "juz_number": 27,
      "text": "وَحُورٌ عِينٞ"
    },
    {
      "id": 5002,
      "number": 23,
      "verse_key": "56:23",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 471,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 535,
      "juz_number": 27,
      "text": "كَأَمۡثَٰلِ ٱللُّؤۡلُوِٕ ٱلۡمَكۡنُونِ"
    },
    {
      "id": 5003,
      "number": 24,
      "verse_key": "56:24",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 471,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 535,
      "juz_number": 27,
      "text": "جَزَآءَۢ بِمَا كَانُواْ يَعۡمَلُونَ"
    },
    {
      "id": 5004,
      "number": 25,
      "verse_key": "56:25",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 471,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 535,
      "juz_number": 27,
      "text": "لَا يَسۡمَعُونَ فِيهَا لَغۡوٗا وَلَا تَأۡثِيمًا"
    },
    {
      "id": 5005,
      "number": 26,
      "verse_key": "56:26",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 471,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 535,
      "juz_number": 27,
      "text": "إِلَّا قِيلٗا سَلَٰمٗا سَلَٰمٗا"
    },
    {
      "id": 5006,
      "number": 27,
      "verse_key": "56:27",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 471,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 535,
      "juz_number": 27,
      "text": "وَأَصۡحَٰبُ ٱلۡيَمِينِ مَآ أَصۡحَٰبُ ٱلۡيَمِينِ"
    },
    {
      "id": 5007,
      "number": 28,
      "verse_key": "56:28",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 471,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 535,
      "juz_number": 27,
      "text": "فِي سِدۡرٖ مَّخۡضُودٖ"
    },
    {
      "id": 5008,
      "number": 29,
      "verse_key": "56:29",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 471,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 535,
      "juz_number": 27,
      "text": "وَطَلۡحٖ مَّنضُودٖ"
    },
    {
      "id": 4982,
      "number": 3,
      "verse_key": "56:3",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 471,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 534,
      "juz_number": 27,
      "text": "خَافِضَةٞ رَّافِعَةٌ"
    },
    {
      "id": 5009,
      "number": 30,
      "verse_key": "56:30",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 471,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 535,
      "juz_number": 27,
      "text": "وَظِلّٖ مَّمۡدُودٖ"
    },
    {
      "id": 5010,
      "number": 31,
      "verse_key": "56:31",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 471,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 535,
      "juz_number": 27,
      "text": "وَمَآءٖ مَّسۡكُوبٖ"
    },
    {
      "id": 5011,
      "number": 32,
      "verse_key": "56:32",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 471,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 535,
      "juz_number": 27,
      "text": "وَفَٰكِهَةٖ كَثِيرَةٖ"
    },
    {
      "id": 5012,
      "number": 33,
      "verse_key": "56:33",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 471,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 535,
      "juz_number": 27,
      "text": "لَّا مَقۡطُوعَةٖ وَلَا مَمۡنُوعَةٖ"
    },
    {
      "id": 5013,
      "number": 34,
      "verse_key": "56:34",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 471,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 535,
      "juz_number": 27,
      "text": "وَفُرُشٖ مَّرۡفُوعَةٍ"
    },
    {
      "id": 5014,
      "number": 35,
      "verse_key": "56:35",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 471,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 535,
      "juz_number": 27,
      "text": "إِنَّآ أَنشَأۡنَٰهُنَّ إِنشَآءٗ"
    },
    {
      "id": 5015,
      "number": 36,
      "verse_key": "56:36",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 471,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 535,
      "juz_number": 27,
      "text": "فَجَعَلۡنَٰهُنَّ أَبۡكَارًا"
    },
    {
      "id": 5016,
      "number": 37,
      "verse_key": "56:37",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 471,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 535,
      "juz_number": 27,
      "text": "عُرُبًا أَتۡرَابٗا"
    },
    {
      "id": 5017,
      "number": 38,
      "verse_key": "56:38",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 471,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 535,
      "juz_number": 27,
      "text": "لِّأَصۡحَٰبِ ٱلۡيَمِينِ"
    },
    {
      "id": 5018,
      "number": 39,
      "verse_key": "56:39",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 472,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 535,
      "juz_number": 27,
      "text": "ثُلَّةٞ مِّنَ ٱلۡأَوَّلِينَ"
    },
    {
      "id": 4983,
      "number": 4,
      "verse_key": "56:4",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 471,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 534,
      "juz_number": 27,
      "text": "إِذَا رُجَّتِ ٱلۡأَرۡضُ رَجّٗا"
    },
    {
      "id": 5019,
      "number": 40,
      "verse_key": "56:40",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 472,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 535,
      "juz_number": 27,
      "text": "وَثُلَّةٞ مِّنَ ٱلۡأٓخِرِينَ"
    },
    {
      "id": 5020,
      "number": 41,
      "verse_key": "56:41",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 472,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 535,
      "juz_number": 27,
      "text": "وَأَصۡحَٰبُ ٱلشِّمَالِ مَآ أَصۡحَٰبُ ٱلشِّمَالِ"
    },
    {
      "id": 5021,
      "number": 42,
      "verse_key": "56:42",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 472,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 535,
      "juz_number": 27,
      "text": "فِي سَمُومٖ وَحَمِيمٖ"
    },
    {
      "id": 5022,
      "number": 43,
      "verse_key": "56:43",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 472,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 535,
      "juz_number": 27,
      "text": "وَظِلّٖ مِّن يَحۡمُومٖ"
    },
    {
      "id": 5023,
      "number": 44,
      "verse_key": "56:44",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 472,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 535,
      "juz_number": 27,
      "text": "لَّا بَارِدٖ وَلَا كَرِيمٍ"
    },
    {
      "id": 5024,
      "number": 45,
      "verse_key": "56:45",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 472,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 535,
      "juz_number": 27,
      "text": "إِنَّهُمۡ كَانُواْ قَبۡلَ ذَٰلِكَ مُتۡرَفِينَ"
    },
    {
      "id": 5025,
      "number": 46,
      "verse_key": "56:46",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 472,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 535,
      "juz_number": 27,
      "text": "وَكَانُواْ يُصِرُّونَ عَلَى ٱلۡحِنثِ ٱلۡعَظِيمِ"
    },
    {
      "id": 5026,
      "number": 47,
      "verse_key": "56:47",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 472,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 535,
      "juz_number": 27,
      "text": "وَكَانُواْ يَقُولُونَ أَئِذَا مِتۡنَا وَكُنَّا تُرَابٗا وَعِظَٰمًا أَءِنَّا لَمَبۡعُوثُونَ"
    },
    {
      "id": 5027,
      "number": 48,
      "verse_key": "56:48",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 472,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 535,
      "juz_number": 27,
      "text": "أَوَءَابَآؤُنَا ٱلۡأَوَّلُونَ"
    },
    {
      "id": 5028,
      "number": 49,
      "verse_key": "56:49",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 472,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 535,
      "juz_number": 27,
      "text": "قُلۡ إِنَّ ٱلۡأَوَّلِينَ وَٱلۡأٓخِرِينَ"
    },
    {
      "id": 4984,
      "number": 5,
      "verse_key": "56:5",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 471,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 534,
      "juz_number": 27,
      "text": "وَبُسَّتِ ٱلۡجِبَالُ بَسّٗا"
    },
    {
      "id": 5029,
      "number": 50,
      "verse_key": "56:50",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 472,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 535,
      "juz_number": 27,
      "text": "لَمَجۡمُوعُونَ إِلَىٰ مِيقَٰتِ يَوۡمٖ مَّعۡلُومٖ"
    },
    {
      "id": 5030,
      "number": 51,
      "verse_key": "56:51",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 472,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 536,
      "juz_number": 27,
      "text": "ثُمَّ إِنَّكُمۡ أَيُّهَا ٱلضَّآلُّونَ ٱلۡمُكَذِّبُونَ"
    },
    {
      "id": 5031,
      "number": 52,
      "verse_key": "56:52",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 472,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 536,
      "juz_number": 27,
      "text": "لَأٓكِلُونَ مِن شَجَرٖ مِّن زَقُّومٖ"
    },
    {
      "id": 5032,
      "number": 53,
      "verse_key": "56:53",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 472,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 536,
      "juz_number": 27,
      "text": "فَمَالِـُٔونَ مِنۡهَا ٱلۡبُطُونَ"
    },
    {
      "id": 5033,
      "number": 54,
      "verse_key": "56:54",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 472,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 536,
      "juz_number": 27,
      "text": "فَشَٰرِبُونَ عَلَيۡهِ مِنَ ٱلۡحَمِيمِ"
    },
    {
      "id": 5034,
      "number": 55,
      "verse_key": "56:55",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 472,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 536,
      "juz_number": 27,
      "text": "فَشَٰرِبُونَ شُرۡبَ ٱلۡهِيمِ"
    },
    {
      "id": 5035,
      "number": 56,
      "verse_key": "56:56",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 472,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 536,
      "juz_number": 27,
      "text": "هَٰذَا نُزُلُهُمۡ يَوۡمَ ٱلدِّينِ"
    },
    {
      "id": 5036,
      "number": 57,
      "verse_key": "56:57",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 472,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 536,
      "juz_number": 27,
      "text": "نَحۡنُ خَلَقۡنَٰكُمۡ فَلَوۡلَا تُصَدِّقُونَ"
    },
    {
      "id": 5037,
      "number": 58,
      "verse_key": "56:58",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 472,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 536,
      "juz_number": 27,
      "text": "أَفَرَءَيۡتُم مَّا تُمۡنُونَ"
    },
    {
      "id": 5038,
      "number": 59,
      "verse_key": "56:59",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 472,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 536,
      "juz_number": 27,
      "text": "ءَأَنتُمۡ تَخۡلُقُونَهُۥٓ أَمۡ نَحۡنُ ٱلۡخَٰلِقُونَ"
    },
    {
      "id": 4985,
      "number": 6,
      "verse_key": "56:6",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 471,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 534,
      "juz_number": 27,
      "text": "فَكَانَتۡ هَبَآءٗ مُّنۢبَثّٗا"
    },
    {
      "id": 5039,
      "number": 60,
      "verse_key": "56:60",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 472,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 536,
      "juz_number": 27,
      "text": "نَحۡنُ قَدَّرۡنَا بَيۡنَكُمُ ٱلۡمَوۡتَ وَمَا نَحۡنُ بِمَسۡبُوقِينَ"
    },
    {
      "id": 5040,
      "number": 61,
      "verse_key": "56:61",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 472,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 536,
      "juz_number": 27,
      "text": "عَلَىٰٓ أَن نُّبَدِّلَ أَمۡثَٰلَكُمۡ وَنُنشِئَكُمۡ فِي مَا لَا تَعۡلَمُونَ"
    },
    {
      "id": 5041,
      "number": 62,
      "verse_key": "56:62",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 472,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 536,
      "juz_number": 27,
      "text": "وَلَقَدۡ عَلِمۡتُمُ ٱلنَّشۡأَةَ ٱلۡأُولَىٰ فَلَوۡلَا تَذَكَّرُونَ"
    },
    {
      "id": 5042,
      "number": 63,
      "verse_key": "56:63",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 472,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 536,
      "juz_number": 27,
      "text": "أَفَرَءَيۡتُم مَّا تَحۡرُثُونَ"
    },
    {
      "id": 5043,
      "number": 64,
      "verse_key": "56:64",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 472,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 536,
      "juz_number": 27,
      "text": "ءَأَنتُمۡ تَزۡرَعُونَهُۥٓ أَمۡ نَحۡنُ ٱلزَّٰرِعُونَ"
    },
    {
      "id": 5044,
      "number": 65,
      "verse_key": "56:65",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 472,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 536,
      "juz_number": 27,
      "text": "لَوۡ نَشَآءُ لَجَعَلۡنَٰهُ حُطَٰمٗا فَظَلۡتُمۡ تَفَكَّهُونَ"
    },
    {
      "id": 5045,
      "number": 66,
      "verse_key": "56:66",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 472,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 536,
      "juz_number": 27,
      "text": "إِنَّا لَمُغۡرَمُونَ"
    },
    {
      "id": 5046,
      "number": 67,
      "verse_key": "56:67",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 472,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 536,
      "juz_number": 27,
      "text": "بَلۡ نَحۡنُ مَحۡرُومُونَ"
    },
    {
      "id": 5047,
      "number": 68,
      "verse_key": "56:68",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 472,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 536,
      "juz_number": 27,
      "text": "أَفَرَءَيۡتُمُ ٱلۡمَآءَ ٱلَّذِي تَشۡرَبُونَ"
    },
    {
      "id": 5048,
      "number": 69,
      "verse_key": "56:69",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 472,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 536,
      "juz_number": 27,
      "text": "ءَأَنتُمۡ أَنزَلۡتُمُوهُ مِنَ ٱلۡمُزۡنِ أَمۡ نَحۡنُ ٱلۡمُنزِلُونَ"
    },
    {
      "id": 4986,
      "number": 7,
      "verse_key": "56:7",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 471,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 534,
      "juz_number": 27,
      "text": "وَكُنتُمۡ أَزۡوَٰجٗا ثَلَٰثَةٗ"
    },
    {
      "id": 5049,
      "number": 70,
      "verse_key": "56:70",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 472,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 536,
      "juz_number": 27,
      "text": "لَوۡ نَشَآءُ جَعَلۡنَٰهُ أُجَاجٗا فَلَوۡلَا تَشۡكُرُونَ"
    },
    {
      "id": 5050,
      "number": 71,
      "verse_key": "56:71",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 472,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 536,
      "juz_number": 27,
      "text": "أَفَرَءَيۡتُمُ ٱلنَّارَ ٱلَّتِي تُورُونَ"
    },
    {
      "id": 5051,
      "number": 72,
      "verse_key": "56:72",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 472,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 536,
      "juz_number": 27,
      "text": "ءَأَنتُمۡ أَنشَأۡتُمۡ شَجَرَتَهَآ أَمۡ نَحۡنُ ٱلۡمُنشِـُٔونَ"
    },
    {
      "id": 5052,
      "number": 73,
      "verse_key": "56:73",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 472,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 536,
      "juz_number": 27,
      "text": "نَحۡنُ جَعَلۡنَٰهَا تَذۡكِرَةٗ وَمَتَٰعٗا لِّلۡمُقۡوِينَ"
    },
    {
      "id": 5053,
      "number": 74,
      "verse_key": "56:74",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 472,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 536,
      "juz_number": 27,
      "text": "فَسَبِّحۡ بِٱسۡمِ رَبِّكَ ٱلۡعَظِيمِ"
    },
    {
      "id": 5054,
      "number": 75,
      "verse_key": "56:75",
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "ruku_number": 473,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 536,
      "juz_number": 27,
      "text": "۞ فَلَآ أُقۡسِمُ بِمَوَٰقِعِ ٱلنُّجُومِ"
    },
    {
      "id": 5055,
      "number": 76,
      "verse_key": "56:76",
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "ruku_number": 473,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 536,
      "juz_number": 27,
      "text": "وَإِنَّهُۥ لَقَسَمٞ لَّوۡ تَعۡلَمُونَ عَظِيمٌ"
    },
    {
      "id": 5056,
      "number": 77,
      "verse_key": "56:77",
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "ruku_number": 473,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 537,
      "juz_number": 27,
      "text": "إِنَّهُۥ لَقُرۡءَانٞ كَرِيمٞ"
    },
    {
      "id": 5057,
      "number": 78,
      "verse_key": "56:78",
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "ruku_number": 473,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 537,
      "juz_number": 27,
      "text": "فِي كِتَٰبٖ مَّكۡنُونٖ"
    },
    {
      "id": 5058,
      "number": 79,
      "verse_key": "56:79",
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "ruku_number": 473,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 537,
      "juz_number": 27,
      "text": "لَّا يَمَسُّهُۥٓ إِلَّا ٱلۡمُطَهَّرُونَ"
    },
    {
      "id": 4987,
      "number": 8,
      "verse_key": "56:8",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 471,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 534,
      "juz_number": 27,
      "text": "فَأَصۡحَٰبُ ٱلۡمَيۡمَنَةِ مَآ أَصۡحَٰبُ ٱلۡمَيۡمَنَةِ"
    },
    {
      "id": 5059,
      "number": 80,
      "verse_key": "56:80",
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "ruku_number": 473,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 537,
      "juz_number": 27,
      "text": "تَنزِيلٞ مِّن رَّبِّ ٱلۡعَٰلَمِينَ"
    },
    {
      "id": 5060,
      "number": 81,
      "verse_key": "56:81",
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "ruku_number": 473,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 537,
      "juz_number": 27,
      "text": "أَفَبِهَٰذَا ٱلۡحَدِيثِ أَنتُم مُّدۡهِنُونَ"
    },
    {
      "id": 5061,
      "number": 82,
      "verse_key": "56:82",
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "ruku_number": 473,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 537,
      "juz_number": 27,
      "text": "وَتَجۡعَلُونَ رِزۡقَكُمۡ أَنَّكُمۡ تُكَذِّبُونَ"
    },
    {
      "id": 5062,
      "number": 83,
      "verse_key": "56:83",
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "ruku_number": 473,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 537,
      "juz_number": 27,
      "text": "فَلَوۡلَآ إِذَا بَلَغَتِ ٱلۡحُلۡقُومَ"
    },
    {
      "id": 5063,
      "number": 84,
      "verse_key": "56:84",
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "ruku_number": 473,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 537,
      "juz_number": 27,
      "text": "وَأَنتُمۡ حِينَئِذٖ تَنظُرُونَ"
    },
    {
      "id": 5064,
      "number": 85,
      "verse_key": "56:85",
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "ruku_number": 473,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 537,
      "juz_number": 27,
      "text": "وَنَحۡنُ أَقۡرَبُ إِلَيۡهِ مِنكُمۡ وَلَٰكِن لَّا تُبۡصِرُونَ"
    },
    {
      "id": 5065,
      "number": 86,
      "verse_key": "56:86",
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "ruku_number": 473,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 537,
      "juz_number": 27,
      "text": "فَلَوۡلَآ إِن كُنتُمۡ غَيۡرَ مَدِينِينَ"
    },
    {
      "id": 5066,
      "number": 87,
      "verse_key": "56:87",
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "ruku_number": 473,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 537,
      "juz_number": 27,
      "text": "تَرۡجِعُونَهَآ إِن كُنتُمۡ صَٰدِقِينَ"
    },
    {
      "id": 5067,
      "number": 88,
      "verse_key": "56:88",
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "ruku_number": 473,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 537,
      "juz_number": 27,
      "text": "فَأَمَّآ إِن كَانَ مِنَ ٱلۡمُقَرَّبِينَ"
    },
    {
      "id": 5068,
      "number": 89,
      "verse_key": "56:89",
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "ruku_number": 473,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 537,
      "juz_number": 27,
      "text": "فَرَوۡحٞ وَرَيۡحَانٞ وَجَنَّتُ نَعِيمٖ"
    },
    {
      "id": 4988,
      "number": 9,
      "verse_key": "56:9",
      "hizb_number": 54,
      "rub_el_hizb_number": 214,
      "ruku_number": 471,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 534,
      "juz_number": 27,
      "text": "وَأَصۡحَٰبُ ٱلۡمَشۡـَٔمَةِ مَآ أَصۡحَٰبُ ٱلۡمَشۡـَٔمَةِ"
    },
    {
      "id": 5069,
      "number": 90,
      "verse_key": "56:90",
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "ruku_number": 473,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 537,
      "juz_number": 27,
      "text": "وَأَمَّآ إِن كَانَ مِنۡ أَصۡحَٰبِ ٱلۡيَمِينِ"
    },
    {
      "id": 5070,
      "number": 91,
      "verse_key": "56:91",
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "ruku_number": 473,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 537,
      "juz_number": 27,
      "text": "فَسَلَٰمٞ لَّكَ مِنۡ أَصۡحَٰبِ ٱلۡيَمِينِ"
    },
    {
      "id": 5071,
      "number": 92,
      "verse_key": "56:92",
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "ruku_number": 473,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 537,
      "juz_number": 27,
      "text": "وَأَمَّآ إِن كَانَ مِنَ ٱلۡمُكَذِّبِينَ ٱلضَّآلِّينَ"
    },
    {
      "id": 5072,
      "number": 93,
      "verse_key": "56:93",
      "hizb_number": 54,
      "rub_el_hizb_number": 215,
      "ruku_number": 473,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 537,
      "juz_number": 27,
      "text": "فَنُزُلٞ مِّنۡ حَمِيمٖ"
    },
    {
      "id": 5073,
