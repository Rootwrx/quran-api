{
  "verses": [
    {
      "id": 5476,
      "number": 1,
      "verse_key": "73:1",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 510,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 574,
      "juz_number": 29,
      "text": "يَٰٓأَيُّهَا ٱلۡمُزَّمِّلُ"
    },
    {
      "id": 5477,
      "number": 2,
      "verse_key": "73:2",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 510,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 574,
      "juz_number": 29,
      "text": "قُمِ ٱلَّيۡلَ إِلَّا قَلِيلٗا"
    },
    {
      "id": 5478,
      "number": 3,
      "verse_key": "73:3",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 510,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 574,
      "juz_number": 29,
      "text": "نِّصۡفَهُۥٓ أَوِ ٱنقُصۡ مِنۡهُ قَلِيلًا"
    },
    {
      "id": 5479,
      "number": 4,
      "verse_key": "73:4",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 510,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 574,
      "juz_number": 29,
      "text": "أَوۡ زِدۡ عَلَيۡهِ وَرَتِّلِ ٱلۡقُرۡءَانَ تَرۡتِيلًا"
    },
    {
      "id": 5480,
      "number": 5,
      "verse_key": "73:5",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 510,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 574,
      "juz_number": 29,
      "text": "إِنَّا سَنُلۡقِي عَلَيۡكَ قَوۡلٗا ثَقِيلًا"
    },
    {
      "id": 5481,
      "number": 6,
      "verse_key": "73:6",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 510,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 574,
      "juz_number": 29,
      "text": "إِنَّ نَاشِئَةَ ٱلَّيۡلِ هِيَ أَشَدُّ وَطۡـٔٗا وَأَقۡوَمُ قِيلًا"
    },
    {
      "id": 5482,
      "number": 7,
      "verse_key": "73:7",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 510,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 574,
      "juz_number": 29,
      "text": "إِنَّ لَكَ فِي ٱلنَّهَارِ سَبۡحٗا طَوِيلٗا"
    },
    {
      "id": 5483,
      "number": 8,
      "verse_key": "73:8",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 510,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 574,
      "juz_number": 29,
      "text": "وَٱذۡكُرِ ٱسۡمَ رَبِّكَ وَتَبَتَّلۡ إِلَيۡهِ تَبۡتِيلٗا"
    },
    {
      "id": 5484,
      "number": 9,
      "verse_key": "73:9",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 510,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 574,
      "juz_number": 29,
      "text": "رَّبُّ ٱلۡمَشۡرِقِ وَٱلۡمَغۡرِبِ لَآ إِلَٰهَ إِلَّا هُوَ فَٱتَّخِذۡهُ وَكِيلٗا"
    },
    {
      "id": 5485,
      "number": 10,
      "verse_key": "73:10",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 510,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 574,
      "juz_number": 29,
      "text": "وَٱصۡبِرۡ عَلَىٰ مَا يَقُولُونَ وَٱهۡجُرۡهُمۡ هَجۡرٗا جَمِيلٗا"
    },
    {
      "id": 5486,
      "number": 11,
      "verse_key": "73:11",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 510,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 574,
      "juz_number": 29,
      "text": "وَذَرۡنِي وَٱلۡمُكَذِّبِينَ أُوْلِي ٱلنَّعۡمَةِ وَمَهِّلۡهُمۡ قَلِيلًا"
    },
    {
      "id": 5487,
      "number": 12,
      "verse_key": "73:12",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 510,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 574,
      "juz_number": 29,
      "text": "إِنَّ لَدَيۡنَآ أَنكَالٗا وَجَحِيمٗا"
    },
    {
      "id": 5488,
      "number": 13,
      "verse_key": "73:13",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 510,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 574,
      "juz_number": 29,
      "text": "وَطَعَامٗا ذَا غُصَّةٖ وَعَذَابًا أَلِيمٗا"
    },
    {
      "id": 5489,
      "number": 14,
      "verse_key": "73:14",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 510,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 574,
      "juz_number": 29,
      "text": "يَوۡمَ تَرۡجُفُ ٱلۡأَرۡضُ وَٱلۡجِبَالُ وَكَانَتِ ٱلۡجِبَالُ كَثِيبٗا مَّهِيلًا"
    },
    {
      "id": 5490,
      "number": 15,
      "verse_key": "73:15",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 510,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 574,
      "juz_number": 29,
      "text": "إِنَّآ أَرۡسَلۡنَآ إِلَيۡكُمۡ رَسُولٗا شَٰهِدًا عَلَيۡكُمۡ كَمَآ أَرۡسَلۡنَآ إِلَىٰ فِرۡعَوۡنَ رَسُولٗا"
    },
    {
      "id": 5491,
      "number": 16,
      "verse_key": "73:16",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 510,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 574,
      "juz_number": 29,
      "text": "فَعَصَىٰ فِرۡعَوۡنُ ٱلرَّسُولَ فَأَخَذۡنَٰهُ أَخۡذٗا وَبِيلٗا"
    },
    {
      "id": 5492,
      "number": 17,
      "verse_key": "73:17",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 510,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 574,
      "juz_number": 29,
      "text": "فَكَيۡفَ تَتَّقُونَ إِن كَفَرۡتُمۡ يَوۡمٗا يَجۡعَلُ ٱلۡوِلۡدَٰنَ شِيبًا"
    },
    {
      "id": 5493,
      "number": 18,
      "verse_key": "73:18",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 510,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 574,
      "juz_number": 29,
      "text": "ٱلسَّمَآءُ مُنفَطِرُۢ بِهِۦۚ كَانَ وَعۡدُهُۥ مَفۡعُولًا"
    },
    {
      "id": 5494,
      "number": 19,
      "verse_key": "73:19",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 510,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 574,
      "juz_number": 29,
      "text": "إِنَّ هَٰذِهِۦ تَذۡكِرَةٞۖ فَمَن شَآءَ ٱتَّخَذَ إِلَىٰ رَبِّهِۦ سَبِيلًا"
    },
    {
      "id": 5495,
      "number": 20,
      "verse_key": "73:20",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 511,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 575,
      "juz_number": 29,
      "text": "۞ إِنَّ رَبَّكَ يَعۡلَمُ أَنَّكَ تَقُومُ أَدۡنَىٰ مِن ثُلُثَيِ ٱلَّيۡلِ وَنِصۡفَهُۥ وَثُلُثَهُۥ وَطَآئِفَةٞ مِّنَ ٱلَّذِينَ مَعَكَۚ وَٱللَّهُ يُقَدِّرُ ٱلَّيۡلَ وَٱلنَّهَارَۚ عَلِمَ أَن لَّن تُحۡصُوهُ فَتَابَ عَلَيۡكُمۡۖ فَٱقۡرَءُواْ مَا تَيَسَّرَ مِنَ ٱلۡقُرۡءَانِۚ عَلِمَ أَن سَيَكُونُ مِنكُم مَّرۡضَىٰ وَءَاخَرُونَ يَضۡرِبُونَ فِي ٱلۡأَرۡضِ يَبۡتَغُونَ مِن فَضۡلِ ٱللَّهِ وَءَاخَرُونَ يُقَٰتِلُونَ فِي سَبِيلِ ٱللَّهِۖ فَٱقۡرَءُواْ مَا تَيَسَّرَ مِنۡهُۚ وَأَقِيمُواْ ٱلصَّلَوٰةَ وَءَاتُواْ ٱلزَّكَوٰةَ وَأَقۡرِضُواْ ٱللَّهَ قَرۡضًا حَسَنٗاۚ وَمَا تُقَدِّمُواْ لِأَنفُسِكُم مِّنۡ خَيۡرٖ تَجِدُوهُ عِندَ ٱللَّهِ هُوَ خَيۡرٗا وَأَعۡظَمَ أَجۡرٗاۚ وَٱسۡتَغۡفِرُواْ ٱللَّهَۖ إِنَّ ٱللَّهَ غَفُورٞ رَّحِيمُۢ"
    }
  ]
}