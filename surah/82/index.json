{
  "verses": [
    {
      "id": 5830,
      "number": 1,
      "verse_key": "82:1",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 526,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 587,
      "juz_number": 30,
      "text": "إِذَا ٱلسَّمَآءُ ٱنفَطَرَتۡ"
    },
    {
      "id": 5839,
      "number": 10,
      "verse_key": "82:10",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 526,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 587,
      "juz_number": 30,
      "text": "وَإِنَّ عَلَيۡكُمۡ لَحَٰفِظِينَ"
    },
    {
      "id": 5840,
      "number": 11,
      "verse_key": "82:11",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 526,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 587,
      "juz_number": 30,
      "text": "كِرَامٗا كَٰتِبِينَ"
    },
    {
      "id": 5841,
      "number": 12,
      "verse_key": "82:12",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 526,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 587,
      "juz_number": 30,
      "text": "يَعۡلَمُونَ مَا تَفۡعَلُونَ"
    },
    {
      "id": 5842,
      "number": 13,
      "verse_key": "82:13",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 526,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 587,
      "juz_number": 30,
      "text": "إِنَّ ٱلۡأَبۡرَارَ لَفِي نَعِيمٖ"
    },
    {
      "id": 5843,
      "number": 14,
      "verse_key": "82:14",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 526,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 587,
      "juz_number": 30,
      "text": "وَإِنَّ ٱلۡفُجَّارَ لَفِي جَحِيمٖ"
    },
    {
      "id": 5844,
      "number": 15,
      "verse_key": "82:15",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 526,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 587,
      "juz_number": 30,
      "text": "يَصۡلَوۡنَهَا يَوۡمَ ٱلدِّينِ"
    },
    {
      "id": 5845,
      "number": 16,
      "verse_key": "82:16",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 526,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 587,
      "juz_number": 30,
      "text": "وَمَا هُمۡ عَنۡهَا بِغَآئِبِينَ"
    },
    {
      "id": 5846,
      "number": 17,
      "verse_key": "82:17",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 526,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 587,
      "juz_number": 30,
      "text": "وَمَآ أَدۡرَىٰكَ مَا يَوۡمُ ٱلدِّينِ"
    },
    {
      "id": 5847,
      "number": 18,
      "verse_key": "82:18",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 526,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 587,
      "juz_number": 30,
      "text": "ثُمَّ مَآ أَدۡرَىٰكَ مَا يَوۡمُ ٱلدِّينِ"
    },
    {
      "id": 5848,
      "number": 19,
      "verse_key": "82:19",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 526,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 587,
      "juz_number": 30,
      "text": "يَوۡمَ لَا تَمۡلِكُ نَفۡسٞ لِّنَفۡسٖ شَيۡـٔٗاۖ وَٱلۡأَمۡرُ يَوۡمَئِذٖ لِّلَّهِ"
    },
    {
      "id": 5831,
      "number": 2,
      "verse_key": "82:2",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 526,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 587,
      "juz_number": 30,
      "text": "وَإِذَا ٱلۡكَوَاكِبُ ٱنتَثَرَتۡ"
    },
    {
      "id": 5832,
      "number": 3,
      "verse_key": "82:3",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 526,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 587,
      "juz_number": 30,
      "text": "وَإِذَا ٱلۡبِحَارُ فُجِّرَتۡ"
    },
    {
      "id": 5833,
      "number": 4,
      "verse_key": "82:4",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 526,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 587,
      "juz_number": 30,
      "text": "وَإِذَا ٱلۡقُبُورُ بُعۡثِرَتۡ"
    },
    {
      "id": 5834,
      "number": 5,
      "verse_key": "82:5",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 526,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 587,
      "juz_number": 30,
      "text": "عَلِمَتۡ نَفۡسٞ مَّا قَدَّمَتۡ وَأَخَّرَتۡ"
    },
    {
      "id": 5835,
      "number": 6,
      "verse_key": "82:6",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 526,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 587,
      "juz_number": 30,
      "text": "يَٰٓأَيُّهَا ٱلۡإِنسَٰنُ مَا غَرَّكَ بِرَبِّكَ ٱلۡكَرِيمِ"
    },
    {
      "id": 5836,
      "number": 7,
      "verse_key": "82:7",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 526,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 587,
      "juz_number": 30,
      "text": "ٱلَّذِي خَلَقَكَ فَسَوَّىٰكَ فَعَدَلَكَ"
    },
    {
      "id": 5837,
      "number": 8,
      "verse_key": "82:8",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 526,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 587,
      "juz_number": 30,
      "text": "فِيٓ أَيِّ صُورَةٖ مَّا شَآءَ رَكَّبَكَ"
    },
    {
      "id": 5838,
      "number": 9,
      "verse_key": "82:9",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 526,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 587,
      "juz_number": 30,
      "text": "كَلَّا بَلۡ تُكَذِّبُونَ بِٱلدِّينِ"
    }
  ]
}