{
  "verses": [
    {
      "id": 5910,
      "number": 1,
      "verse_key": "85:1",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 590,
      "juz_number": 30,
      "text": "وَٱلسَّمَآءِ ذَاتِ ٱلۡبُرُوجِ"
    },
    {
      "id": 5911,
      "number": 2,
      "verse_key": "85:2",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 590,
      "juz_number": 30,
      "text": "وَٱلۡيَوۡمِ ٱلۡمَوۡعُودِ"
    },
    {
      "id": 5912,
      "number": 3,
      "verse_key": "85:3",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 590,
      "juz_number": 30,
      "text": "وَشَاهِدٖ وَمَشۡهُودٖ"
    },
    {
      "id": 5913,
      "number": 4,
      "verse_key": "85:4",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 590,
      "juz_number": 30,
      "text": "قُتِلَ أَصۡحَٰبُ ٱلۡأُخۡدُودِ"
    },
    {
      "id": 5914,
      "number": 5,
      "verse_key": "85:5",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 590,
      "juz_number": 30,
      "text": "ٱلنَّارِ ذَاتِ ٱلۡوَقُودِ"
    },
    {
      "id": 5915,
      "number": 6,
      "verse_key": "85:6",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 590,
      "juz_number": 30,
      "text": "إِذۡ هُمۡ عَلَيۡهَا قُعُودٞ"
    },
    {
      "id": 5916,
      "number": 7,
      "verse_key": "85:7",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 590,
      "juz_number": 30,
      "text": "وَهُمۡ عَلَىٰ مَا يَفۡعَلُونَ بِٱلۡمُؤۡمِنِينَ شُهُودٞ"
    },
    {
      "id": 5917,
      "number": 8,
      "verse_key": "85:8",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 590,
      "juz_number": 30,
      "text": "وَمَا نَقَمُواْ مِنۡهُمۡ إِلَّآ أَن يُؤۡمِنُواْ بِٱللَّهِ ٱلۡعَزِيزِ ٱلۡحَمِيدِ"
    },
    {
      "id": 5918,
      "number": 9,
      "verse_key": "85:9",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 590,
      "juz_number": 30,
      "text": "ٱلَّذِي لَهُۥ مُلۡكُ ٱلسَّمَٰوَٰتِ وَٱلۡأَرۡضِۚ وَٱللَّهُ عَلَىٰ كُلِّ شَيۡءٖ شَهِيدٌ"
    },
    {
      "id": 5919,
      "number": 10,
      "verse_key": "85:10",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 590,
      "juz_number": 30,
      "text": "إِنَّ ٱلَّذِينَ فَتَنُواْ ٱلۡمُؤۡمِنِينَ وَٱلۡمُؤۡمِنَٰتِ ثُمَّ لَمۡ يَتُوبُواْ فَلَهُمۡ عَذَابُ جَهَنَّمَ وَلَهُمۡ عَذَابُ ٱلۡحَرِيقِ"
    },
    {
      "id": 5920,
      "number": 11,
      "verse_key": "85:11",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 590,
      "juz_number": 30,
      "text": "إِنَّ ٱلَّذِينَ ءَامَنُواْ وَعَمِلُواْ ٱلصَّٰلِحَٰتِ لَهُمۡ جَنَّٰتٞ تَجۡرِي مِن تَحۡتِهَا ٱلۡأَنۡهَٰرُۚ ذَٰلِكَ ٱلۡفَوۡزُ ٱلۡكَبِيرُ"
    },
    {
      "id": 5921,
      "number": 12,
      "verse_key": "85:12",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 590,
      "juz_number": 30,
      "text": "إِنَّ بَطۡشَ رَبِّكَ لَشَدِيدٌ"
    },
    {
      "id": 5922,
      "number": 13,
      "verse_key": "85:13",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 590,
      "juz_number": 30,
      "text": "إِنَّهُۥ هُوَ يُبۡدِئُ وَيُعِيدُ"
    },
    {
      "id": 5923,
      "number": 14,
      "verse_key": "85:14",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 590,
      "juz_number": 30,
      "text": "وَهُوَ ٱلۡغَفُورُ ٱلۡوَدُودُ"
    },
    {
      "id": 5924,
      "number": 15,
      "verse_key": "85:15",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 590,
      "juz_number": 30,
      "text": "ذُو ٱلۡعَرۡشِ ٱلۡمَجِيدُ"
    },
    {
      "id": 5925,
      "number": 16,
      "verse_key": "85:16",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 590,
      "juz_number": 30,
      "text": "فَعَّالٞ لِّمَا يُرِيدُ"
    },
    {
      "id": 5926,
      "number": 17,
      "verse_key": "85:17",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 590,
      "juz_number": 30,
      "text": "هَلۡ أَتَىٰكَ حَدِيثُ ٱلۡجُنُودِ"
    },
    {
      "id": 5927,
      "number": 18,
      "verse_key": "85:18",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 590,
      "juz_number": 30,
      "text": "فِرۡعَوۡنَ وَثَمُودَ"
    },
    {
      "id": 5928,
      "number": 19,
      "verse_key": "85:19",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 590,
      "juz_number": 30,
      "text": "بَلِ ٱلَّذِينَ كَفَرُواْ فِي تَكۡذِيبٖ"
    },
    {
      "id": 5929,
      "number": 20,
      "verse_key": "85:20",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 590,
      "juz_number": 30,
      "text": "وَٱللَّهُ مِن وَرَآئِهِم مُّحِيطُۢ"
    },
    {
      "id": 5930,
      "number": 21,
      "verse_key": "85:21",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 590,
      "juz_number": 30,
      "text": "بَلۡ هُوَ قُرۡءَانٞ مَّجِيدٞ"
    },
    {
      "id": 5931,
      "number": 22,
      "verse_key": "85:22",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 590,
      "juz_number": 30,
      "text": "فِي لَوۡحٖ مَّحۡفُوظِۭ"
    }
  ]
}