{
  "verses": [
    {
      "id": 5759,
      "number": 1,
      "verse_key": "80:1",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 585,
      "juz_number": 30,
      "text": "عَبَسَ وَتَوَلَّىٰٓ"
    },
    {
      "id": 5768,
      "number": 10,
      "verse_key": "80:10",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 585,
      "juz_number": 30,
      "text": "فَأَنتَ عَنۡهُ تَلَهَّىٰ"
    },
    {
      "id": 5769,
      "number": 11,
      "verse_key": "80:11",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 585,
      "juz_number": 30,
      "text": "كَلَّآ إِنَّهَا تَذۡكِرَةٞ"
    },
    {
      "id": 5770,
      "number": 12,
      "verse_key": "80:12",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 585,
      "juz_number": 30,
      "text": "فَمَن شَآءَ ذَكَرَهُۥ"
    },
    {
      "id": 5771,
      "number": 13,
      "verse_key": "80:13",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 585,
      "juz_number": 30,
      "text": "فِي صُحُفٖ مُّكَرَّمَةٖ"
    },
    {
      "id": 5772,
      "number": 14,
      "verse_key": "80:14",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 585,
      "juz_number": 30,
      "text": "مَّرۡفُوعَةٖ مُّطَهَّرَةِۭ"
    },
    {
      "id": 5773,
      "number": 15,
      "verse_key": "80:15",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 585,
      "juz_number": 30,
      "text": "بِأَيۡدِي سَفَرَةٖ"
    },
    {
      "id": 5774,
      "number": 16,
      "verse_key": "80:16",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 585,
      "juz_number": 30,
      "text": "كِرَامِۭ بَرَرَةٖ"
    },
    {
      "id": 5775,
      "number": 17,
      "verse_key": "80:17",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 585,
      "juz_number": 30,
      "text": "قُتِلَ ٱلۡإِنسَٰنُ مَآ أَكۡفَرَهُۥ"
    },
    {
      "id": 5776,
      "number": 18,
      "verse_key": "80:18",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 585,
      "juz_number": 30,
      "text": "مِنۡ أَيِّ شَيۡءٍ خَلَقَهُۥ"
    },
    {
      "id": 5777,
      "number": 19,
      "verse_key": "80:19",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 585,
      "juz_number": 30,
      "text": "مِن نُّطۡفَةٍ خَلَقَهُۥ فَقَدَّرَهُۥ"
    },
    {
      "id": 5760,
      "number": 2,
      "verse_key": "80:2",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 585,
      "juz_number": 30,
      "text": "أَن جَآءَهُ ٱلۡأَعۡمَىٰ"
    },
    {
      "id": 5778,
      "number": 20,
      "verse_key": "80:20",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 585,
      "juz_number": 30,
      "text": "ثُمَّ ٱلسَّبِيلَ يَسَّرَهُۥ"
    },
    {
      "id": 5779,
      "number": 21,
      "verse_key": "80:21",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 585,
      "juz_number": 30,
      "text": "ثُمَّ أَمَاتَهُۥ فَأَقۡبَرَهُۥ"
    },
    {
      "id": 5780,
      "number": 22,
      "verse_key": "80:22",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 585,
      "juz_number": 30,
      "text": "ثُمَّ إِذَا شَآءَ أَنشَرَهُۥ"
    },
    {
      "id": 5781,
      "number": 23,
      "verse_key": "80:23",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 585,
      "juz_number": 30,
      "text": "كَلَّا لَمَّا يَقۡضِ مَآ أَمَرَهُۥ"
    },
    {
      "id": 5782,
      "number": 24,
      "verse_key": "80:24",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 585,
      "juz_number": 30,
      "text": "فَلۡيَنظُرِ ٱلۡإِنسَٰنُ إِلَىٰ طَعَامِهِۦٓ"
    },
    {
      "id": 5783,
      "number": 25,
      "verse_key": "80:25",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 585,
      "juz_number": 30,
      "text": "أَنَّا صَبَبۡنَا ٱلۡمَآءَ صَبّٗا"
    },
    {
      "id": 5784,
      "number": 26,
      "verse_key": "80:26",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 585,
      "juz_number": 30,
      "text": "ثُمَّ شَقَقۡنَا ٱلۡأَرۡضَ شَقّٗا"
    },
    {
      "id": 5785,
      "number": 27,
      "verse_key": "80:27",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 585,
      "juz_number": 30,
      "text": "فَأَنۢبَتۡنَا فِيهَا حَبّٗا"
    },
    {
      "id": 5786,
      "number": 28,
      "verse_key": "80:28",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 585,
      "juz_number": 30,
      "text": "وَعِنَبٗا وَقَضۡبٗا"
    },
    {
      "id": 5787,
      "number": 29,
      "verse_key": "80:29",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 585,
      "juz_number": 30,
      "text": "وَزَيۡتُونٗا وَنَخۡلٗا"
    },
    {
      "id": 5761,
      "number": 3,
      "verse_key": "80:3",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 585,
      "juz_number": 30,
      "text": "وَمَا يُدۡرِيكَ لَعَلَّهُۥ يَزَّكَّىٰٓ"
    },
    {
      "id": 5788,
      "number": 30,
      "verse_key": "80:30",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 585,
      "juz_number": 30,
      "text": "وَحَدَآئِقَ غُلۡبٗا"
    },
    {
      "id": 5789,
      "number": 31,
      "verse_key": "80:31",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 585,
      "juz_number": 30,
      "text": "وَفَٰكِهَةٗ وَأَبّٗا"
    },
    {
      "id": 5790,
      "number": 32,
      "verse_key": "80:32",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 585,
      "juz_number": 30,
      "text": "مَّتَٰعٗا لَّكُمۡ وَلِأَنۡعَٰمِكُمۡ"
    },
    {
      "id": 5791,
      "number": 33,
      "verse_key": "80:33",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 585,
      "juz_number": 30,
      "text": "فَإِذَا جَآءَتِ ٱلصَّآخَّةُ"
    },
    {
      "id": 5792,
      "number": 34,
      "verse_key": "80:34",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 585,
      "juz_number": 30,
      "text": "يَوۡمَ يَفِرُّ ٱلۡمَرۡءُ مِنۡ أَخِيهِ"
    },
    {
      "id": 5793,
      "number": 35,
      "verse_key": "80:35",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 585,
      "juz_number": 30,
      "text": "وَأُمِّهِۦ وَأَبِيهِ"
    },
    {
      "id": 5794,
      "number": 36,
      "verse_key": "80:36",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 585,
      "juz_number": 30,
      "text": "وَصَٰحِبَتِهِۦ وَبَنِيهِ"
    },
    {
      "id": 5795,
      "number": 37,
      "verse_key": "80:37",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 585,
      "juz_number": 30,
      "text": "لِكُلِّ ٱمۡرِيٕٖ مِّنۡهُمۡ يَوۡمَئِذٖ شَأۡنٞ يُغۡنِيهِ"
    },
    {
      "id": 5796,
      "number": 38,
      "verse_key": "80:38",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 585,
      "juz_number": 30,
      "text": "وُجُوهٞ يَوۡمَئِذٖ مُّسۡفِرَةٞ"
    },
    {
      "id": 5797,
      "number": 39,
      "verse_key": "80:39",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 585,
      "juz_number": 30,
      "text": "ضَاحِكَةٞ مُّسۡتَبۡشِرَةٞ"
    },
    {
      "id": 5762,
      "number": 4,
      "verse_key": "80:4",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 585,
      "juz_number": 30,
      "text": "أَوۡ يَذَّكَّرُ فَتَنفَعَهُ ٱلذِّكۡرَىٰٓ"
    },
    {
      "id": 5798,
      "number": 40,
      "verse_key": "80:40",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 585,
      "juz_number": 30,
      "text": "وَوُجُوهٞ يَوۡمَئِذٍ عَلَيۡهَا غَبَرَةٞ"
    },
    {
      "id": 5799,
      "number": 41,
      "verse_key": "80:41",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 585,
      "juz_number": 30,
      "text": "تَرۡهَقُهَا قَتَرَةٌ"
    },
    {
      "id": 5800,
      "number": 42,
      "verse_key": "80:42",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 585,
      "juz_number": 30,
      "text": "أُوْلَٰٓئِكَ هُمُ ٱلۡكَفَرَةُ ٱلۡفَجَرَةُ"
    },
    {
      "id": 5763,
      "number": 5,
      "verse_key": "80:5",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 585,
      "juz_number": 30,
      "text": "أَمَّا مَنِ ٱسۡتَغۡنَىٰ"
    },
    {
      "id": 5764,
      "number": 6,
      "verse_key": "80:6",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 585,
      "juz_number": 30,
      "text": "فَأَنتَ لَهُۥ تَصَدَّىٰ"
    },
    {
      "id": 5765,
      "number": 7,
      "verse_key": "80:7",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 585,
      "juz_number": 30,
      "text": "وَمَا عَلَيۡكَ أَلَّا يَزَّكَّىٰ"
    },
    {
      "id": 5766,
      "number": 8,
      "verse_key": "80:8",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 585,
      "juz_number": 30,
      "text": "وَأَمَّا مَن جَآءَكَ يَسۡعَىٰ"
    },
    {
      "id": 5767,
      "number": 9,
      "verse_key": "80:9",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 585,
      "juz_number": 30,
      "text": "وَهُوَ يَخۡشَىٰ"
    }
  ]
}