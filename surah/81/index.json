{
  "verses": [
    {
      "id": 5801,
      "number": 1,
      "verse_key": "81:1",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 586,
      "juz_number": 30,
      "text": "إِذَا ٱلشَّمۡسُ كُوِّرَتۡ"
    },
    {
      "id": 5810,
      "number": 10,
      "verse_key": "81:10",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 586,
      "juz_number": 30,
      "text": "وَإِذَا ٱلصُّحُفُ نُشِرَتۡ"
    },
    {
      "id": 5811,
      "number": 11,
      "verse_key": "81:11",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 586,
      "juz_number": 30,
      "text": "وَإِذَا ٱلسَّمَآءُ كُشِطَتۡ"
    },
    {
      "id": 5812,
      "number": 12,
      "verse_key": "81:12",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 586,
      "juz_number": 30,
      "text": "وَإِذَا ٱلۡجَحِيمُ سُعِّرَتۡ"
    },
    {
      "id": 5813,
      "number": 13,
      "verse_key": "81:13",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 586,
      "juz_number": 30,
      "text": "وَإِذَا ٱلۡجَنَّةُ أُزۡلِفَتۡ"
    },
    {
      "id": 5814,
      "number": 14,
      "verse_key": "81:14",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 586,
      "juz_number": 30,
      "text": "عَلِمَتۡ نَفۡسٞ مَّآ أَحۡضَرَتۡ"
    },
    {
      "id": 5815,
      "number": 15,
      "verse_key": "81:15",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 586,
      "juz_number": 30,
      "text": "فَلَآ أُقۡسِمُ بِٱلۡخُنَّسِ"
    },
    {
      "id": 5816,
      "number": 16,
      "verse_key": "81:16",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 586,
      "juz_number": 30,
      "text": "ٱلۡجَوَارِ ٱلۡكُنَّسِ"
    },
    {
      "id": 5817,
      "number": 17,
      "verse_key": "81:17",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 586,
      "juz_number": 30,
      "text": "وَٱلَّيۡلِ إِذَا عَسۡعَسَ"
    },
    {
      "id": 5818,
      "number": 18,
      "verse_key": "81:18",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 586,
      "juz_number": 30,
      "text": "وَٱلصُّبۡحِ إِذَا تَنَفَّسَ"
    },
    {
      "id": 5819,
      "number": 19,
      "verse_key": "81:19",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 586,
      "juz_number": 30,
      "text": "إِنَّهُۥ لَقَوۡلُ رَسُولٖ كَرِيمٖ"
    },
    {
      "id": 5802,
      "number": 2,
      "verse_key": "81:2",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 586,
      "juz_number": 30,
      "text": "وَإِذَا ٱلنُّجُومُ ٱنكَدَرَتۡ"
    },
    {
      "id": 5820,
      "number": 20,
      "verse_key": "81:20",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 586,
      "juz_number": 30,
      "text": "ذِي قُوَّةٍ عِندَ ذِي ٱلۡعَرۡشِ مَكِينٖ"
    },
    {
      "id": 5821,
      "number": 21,
      "verse_key": "81:21",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 586,
      "juz_number": 30,
      "text": "مُّطَاعٖ ثَمَّ أَمِينٖ"
    },
    {
      "id": 5822,
      "number": 22,
      "verse_key": "81:22",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 586,
      "juz_number": 30,
      "text": "وَمَا صَاحِبُكُم بِمَجۡنُونٖ"
    },
    {
      "id": 5823,
      "number": 23,
      "verse_key": "81:23",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 586,
      "juz_number": 30,
      "text": "وَلَقَدۡ رَءَاهُ بِٱلۡأُفُقِ ٱلۡمُبِينِ"
    },
    {
      "id": 5824,
      "number": 24,
      "verse_key": "81:24",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 586,
      "juz_number": 30,
      "text": "وَمَا هُوَ عَلَى ٱلۡغَيۡبِ بِضَنِينٖ"
    },
    {
      "id": 5825,
      "number": 25,
      "verse_key": "81:25",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 586,
      "juz_number": 30,
      "text": "وَمَا هُوَ بِقَوۡلِ شَيۡطَٰنٖ رَّجِيمٖ"
    },
    {
      "id": 5826,
      "number": 26,
      "verse_key": "81:26",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 586,
      "juz_number": 30,
      "text": "فَأَيۡنَ تَذۡهَبُونَ"
    },
    {
      "id": 5827,
      "number": 27,
      "verse_key": "81:27",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 586,
      "juz_number": 30,
      "text": "إِنۡ هُوَ إِلَّا ذِكۡرٞ لِّلۡعَٰلَمِينَ"
    },
    {
      "id": 5828,
      "number": 28,
      "verse_key": "81:28",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 586,
      "juz_number": 30,
      "text": "لِمَن شَآءَ مِنكُمۡ أَن يَسۡتَقِيمَ"
    },
    {
      "id": 5829,
      "number": 29,
      "verse_key": "81:29",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 586,
      "juz_number": 30,
      "text": "وَمَا تَشَآءُونَ إِلَّآ أَن يَشَآءَ ٱللَّهُ رَبُّ ٱلۡعَٰلَمِينَ"
    },
    {
      "id": 5803,
      "number": 3,
      "verse_key": "81:3",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 586,
      "juz_number": 30,
      "text": "وَإِذَا ٱلۡجِبَالُ سُيِّرَتۡ"
    },
    {
      "id": 5804,
      "number": 4,
      "verse_key": "81:4",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 586,
      "juz_number": 30,
      "text": "وَإِذَا ٱلۡعِشَارُ عُطِّلَتۡ"
    },
    {
      "id": 5805,
      "number": 5,
      "verse_key": "81:5",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 586,
      "juz_number": 30,
      "text": "وَإِذَا ٱلۡوُحُوشُ حُشِرَتۡ"
    },
    {
      "id": 5806,
      "number": 6,
      "verse_key": "81:6",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 586,
      "juz_number": 30,
      "text": "وَإِذَا ٱلۡبِحَارُ سُجِّرَتۡ"
    },
    {
      "id": 5807,
      "number": 7,
      "verse_key": "81:7",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 586,
      "juz_number": 30,
      "text": "وَإِذَا ٱلنُّفُوسُ زُوِّجَتۡ"
    },
    {
      "id": 5808,
      "number": 8,
      "verse_key": "81:8",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 586,
      "juz_number": 30,
      "text": "وَإِذَا ٱلۡمَوۡءُۥدَةُ سُئِلَتۡ"
    },
    {
      "id": 5809,
      "number": 9,
      "verse_key": "81:9",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 586,
      "juz_number": 30,
      "text": "بِأَيِّ ذَنۢبٖ قُتِلَتۡ"
    }
  ]
}