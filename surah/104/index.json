      "ruku_number": 548,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 601,
      "juz_number": 30,
      "text": "ٱلَّذِي جَمَعَ مَالٗا وَعَدَّدَهُۥ"
