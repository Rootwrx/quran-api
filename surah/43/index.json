{
  "verses": [
    {
      "id": 4326,
      "number": 1,
      "verse_key": "43:1",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 489,
      "juz_number": 25,
      "text": "حمٓ"
    },
    {
      "id": 4327,
      "number": 2,
      "verse_key": "43:2",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 489,
      "juz_number": 25,
      "text": "وَٱلۡكِتَٰبِ ٱلۡمُبِينِ"
    },
    {
      "id": 4328,
      "number": 3,
      "verse_key": "43:3",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 489,
      "juz_number": 25,
      "text": "إِنَّا جَعَلۡنَٰهُ قُرۡءَٰنًا عَرَبِيّٗا لَّعَلَّكُمۡ تَعۡقِلُونَ"
    },
    {
      "id": 4329,
      "number": 4,
      "verse_key": "43:4",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 489,
      "juz_number": 25,
      "text": "وَإِنَّهُۥ فِيٓ أُمِّ ٱلۡكِتَٰبِ لَدَيۡنَا لَعَلِيٌّ حَكِيمٌ"
    },
    {
      "id": 4330,
      "number": 5,
      "verse_key": "43:5",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 489,
      "juz_number": 25,
      "text": "أَفَنَضۡرِبُ عَنكُمُ ٱلذِّكۡرَ صَفۡحًا أَن كُنتُمۡ قَوۡمٗا مُّسۡرِفِينَ"
    },
    {
      "id": 4331,
      "number": 6,
      "verse_key": "43:6",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 489,
      "juz_number": 25,
      "text": "وَكَمۡ أَرۡسَلۡنَا مِن نَّبِيّٖ فِي ٱلۡأَوَّلِينَ"
    },
    {
      "id": 4332,
      "number": 7,
      "verse_key": "43:7",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 489,
      "juz_number": 25,
      "text": "وَمَا يَأۡتِيهِم مِّن نَّبِيٍّ إِلَّا كَانُواْ بِهِۦ يَسۡتَهۡزِءُونَ"
    },
    {
      "id": 4333,
      "number": 8,
      "verse_key": "43:8",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 489,
      "juz_number": 25,
      "text": "فَأَهۡلَكۡنَآ أَشَدَّ مِنۡهُم بَطۡشٗا وَمَضَىٰ مَثَلُ ٱلۡأَوَّلِينَ"
    },
    {
      "id": 4334,
      "number": 9,
      "verse_key": "43:9",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 489,
      "juz_number": 25,
      "text": "وَلَئِن سَأَلۡتَهُم مَّنۡ خَلَقَ ٱلسَّمَٰوَٰتِ وَٱلۡأَرۡضَ لَيَقُولُنَّ خَلَقَهُنَّ ٱلۡعَزِيزُ ٱلۡعَلِيمُ"
    },
    {
      "id": 4335,
      "number": 10,
      "verse_key": "43:10",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 489,
      "juz_number": 25,
      "text": "ٱلَّذِي جَعَلَ لَكُمُ ٱلۡأَرۡضَ مَهۡدٗا وَجَعَلَ لَكُمۡ فِيهَا سُبُلٗا لَّعَلَّكُمۡ تَهۡتَدُونَ"
    },
    {
      "id": 4336,
      "number": 11,
      "verse_key": "43:11",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 490,
      "juz_number": 25,
      "text": "وَٱلَّذِي نَزَّلَ مِنَ ٱلسَّمَآءِ مَآءَۢ بِقَدَرٖ فَأَنشَرۡنَا بِهِۦ بَلۡدَةٗ مَّيۡتٗاۚ كَذَٰلِكَ تُخۡرَجُونَ"
    },
    {
      "id": 4337,
      "number": 12,
      "verse_key": "43:12",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 490,
      "juz_number": 25,
      "text": "وَٱلَّذِي خَلَقَ ٱلۡأَزۡوَٰجَ كُلَّهَا وَجَعَلَ لَكُم مِّنَ ٱلۡفُلۡكِ وَٱلۡأَنۡعَٰمِ مَا تَرۡكَبُونَ"
    },
    {
      "id": 4338,
      "number": 13,
      "verse_key": "43:13",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 490,
      "juz_number": 25,
      "text": "لِتَسۡتَوُۥاْ عَلَىٰ ظُهُورِهِۦ ثُمَّ تَذۡكُرُواْ نِعۡمَةَ رَبِّكُمۡ إِذَا ٱسۡتَوَيۡتُمۡ عَلَيۡهِ وَتَقُولُواْ سُبۡحَٰنَ ٱلَّذِي سَخَّرَ لَنَا هَٰذَا وَمَا كُنَّا لَهُۥ مُقۡرِنِينَ"
    },
    {
      "id": 4339,
      "number": 14,
      "verse_key": "43:14",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 490,
      "juz_number": 25,
      "text": "وَإِنَّآ إِلَىٰ رَبِّنَا لَمُنقَلِبُونَ"
    },
    {
      "id": 4340,
      "number": 15,
      "verse_key": "43:15",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 490,
      "juz_number": 25,
      "text": "وَجَعَلُواْ لَهُۥ مِنۡ عِبَادِهِۦ جُزۡءًاۚ إِنَّ ٱلۡإِنسَٰنَ لَكَفُورٞ مُّبِينٌ"
    },
    {
      "id": 4341,
      "number": 16,
      "verse_key": "43:16",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 427,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 490,
      "juz_number": 25,
      "text": "أَمِ ٱتَّخَذَ مِمَّا يَخۡلُقُ بَنَاتٖ وَأَصۡفَىٰكُم بِٱلۡبَنِينَ"
    },
    {
      "id": 4342,
      "number": 17,
      "verse_key": "43:17",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 427,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 490,
      "juz_number": 25,
      "text": "وَإِذَا بُشِّرَ أَحَدُهُم بِمَا ضَرَبَ لِلرَّحۡمَٰنِ مَثَلٗا ظَلَّ وَجۡهُهُۥ مُسۡوَدّٗا وَهُوَ كَظِيمٌ"
    },
    {
      "id": 4343,
      "number": 18,
      "verse_key": "43:18",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 427,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 490,
      "juz_number": 25,
      "text": "أَوَمَن يُنَشَّؤُاْ فِي ٱلۡحِلۡيَةِ وَهُوَ فِي ٱلۡخِصَامِ غَيۡرُ مُبِينٖ"
    },
    {
      "id": 4344,
      "number": 19,
      "verse_key": "43:19",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 427,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 490,
      "juz_number": 25,
      "text": "وَجَعَلُواْ ٱلۡمَلَٰٓئِكَةَ ٱلَّذِينَ هُمۡ عِبَٰدُ ٱلرَّحۡمَٰنِ إِنَٰثًاۚ أَشَهِدُواْ خَلۡقَهُمۡۚ سَتُكۡتَبُ شَهَٰدَتُهُمۡ وَيُسۡـَٔلُونَ"
    },
    {
      "id": 4345,
      "number": 20,
      "verse_key": "43:20",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 427,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 490,
      "juz_number": 25,
      "text": "وَقَالُواْ لَوۡ شَآءَ ٱلرَّحۡمَٰنُ مَا عَبَدۡنَٰهُمۗ مَّا لَهُم بِذَٰلِكَ مِنۡ عِلۡمٍۖ إِنۡ هُمۡ إِلَّا يَخۡرُصُونَ"
    },
    {
      "id": 4346,
      "number": 21,
      "verse_key": "43:21",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 427,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 490,
      "juz_number": 25,
      "text": "أَمۡ ءَاتَيۡنَٰهُمۡ كِتَٰبٗا مِّن قَبۡلِهِۦ فَهُم بِهِۦ مُسۡتَمۡسِكُونَ"
    },
    {
      "id": 4347,
      "number": 22,
      "verse_key": "43:22",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 427,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 490,
      "juz_number": 25,
      "text": "بَلۡ قَالُوٓاْ إِنَّا وَجَدۡنَآ ءَابَآءَنَا عَلَىٰٓ أُمَّةٖ وَإِنَّا عَلَىٰٓ ءَاثَٰرِهِم مُّهۡتَدُونَ"
    },
    {
      "id": 4348,
      "number": 23,
      "verse_key": "43:23",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 427,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 491,
      "juz_number": 25,
      "text": "وَكَذَٰلِكَ مَآ أَرۡسَلۡنَا مِن قَبۡلِكَ فِي قَرۡيَةٖ مِّن نَّذِيرٍ إِلَّا قَالَ مُتۡرَفُوهَآ إِنَّا وَجَدۡنَآ ءَابَآءَنَا عَلَىٰٓ أُمَّةٖ وَإِنَّا عَلَىٰٓ ءَاثَٰرِهِم مُّقۡتَدُونَ"
    },
    {
      "id": 4349,
      "number": 24,
      "verse_key": "43:24",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 427,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 491,
      "juz_number": 25,
      "text": "۞ قَٰلَ أَوَلَوۡ جِئۡتُكُم بِأَهۡدَىٰ مِمَّا وَجَدتُّمۡ عَلَيۡهِ ءَابَآءَكُمۡۖ قَالُوٓاْ إِنَّا بِمَآ أُرۡسِلۡتُم بِهِۦ كَٰفِرُونَ"
    },
    {
      "id": 4350,
      "number": 25,
      "verse_key": "43:25",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 427,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 491,
      "juz_number": 25,
      "text": "فَٱنتَقَمۡنَا مِنۡهُمۡۖ فَٱنظُرۡ كَيۡفَ كَانَ عَٰقِبَةُ ٱلۡمُكَذِّبِينَ"
    },
    {
      "id": 4351,
      "number": 26,
      "verse_key": "43:26",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 428,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 491,
      "juz_number": 25,
      "text": "وَإِذۡ قَالَ إِبۡرَٰهِيمُ لِأَبِيهِ وَقَوۡمِهِۦٓ إِنَّنِي بَرَآءٞ مِّمَّا تَعۡبُدُونَ"
    },
    {
      "id": 4352,
      "number": 27,
      "verse_key": "43:27",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 428,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 491,
      "juz_number": 25,
      "text": "إِلَّا ٱلَّذِي فَطَرَنِي فَإِنَّهُۥ سَيَهۡدِينِ"
    },
    {
      "id": 4353,
      "number": 28,
      "verse_key": "43:28",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 428,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 491,
      "juz_number": 25,
      "text": "وَجَعَلَهَا كَلِمَةَۢ بَاقِيَةٗ فِي عَقِبِهِۦ لَعَلَّهُمۡ يَرۡجِعُونَ"
    },
    {
      "id": 4354,
      "number": 29,
      "verse_key": "43:29",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 428,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 491,
      "juz_number": 25,
      "text": "بَلۡ مَتَّعۡتُ هَٰٓؤُلَآءِ وَءَابَآءَهُمۡ حَتَّىٰ جَآءَهُمُ ٱلۡحَقُّ وَرَسُولٞ مُّبِينٞ"
    },
    {
      "id": 4355,
      "number": 30,
      "verse_key": "43:30",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 428,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 491,
      "juz_number": 25,
      "text": "وَلَمَّا جَآءَهُمُ ٱلۡحَقُّ قَالُواْ هَٰذَا سِحۡرٞ وَإِنَّا بِهِۦ كَٰفِرُونَ"
    },
    {
      "id": 4356,
      "number": 31,
      "verse_key": "43:31",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 428,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 491,
      "juz_number": 25,
      "text": "وَقَالُواْ لَوۡلَا نُزِّلَ هَٰذَا ٱلۡقُرۡءَانُ عَلَىٰ رَجُلٖ مِّنَ ٱلۡقَرۡيَتَيۡنِ عَظِيمٍ"
    },
    {
      "id": 4357,
      "number": 32,
      "verse_key": "43:32",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 428,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 491,
      "juz_number": 25,
      "text": "أَهُمۡ يَقۡسِمُونَ رَحۡمَتَ رَبِّكَۚ نَحۡنُ قَسَمۡنَا بَيۡنَهُم مَّعِيشَتَهُمۡ فِي ٱلۡحَيَوٰةِ ٱلدُّنۡيَاۚ وَرَفَعۡنَا بَعۡضَهُمۡ فَوۡقَ بَعۡضٖ دَرَجَٰتٖ لِّيَتَّخِذَ بَعۡضُهُم بَعۡضٗا سُخۡرِيّٗاۗ وَرَحۡمَتُ رَبِّكَ خَيۡرٞ مِّمَّا يَجۡمَعُونَ"
    },
    {
      "id": 4358,
      "number": 33,
      "verse_key": "43:33",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 428,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 491,
      "juz_number": 25,
      "text": "وَلَوۡلَآ أَن يَكُونَ ٱلنَّاسُ أُمَّةٗ وَٰحِدَةٗ لَّجَعَلۡنَا لِمَن يَكۡفُرُ بِٱلرَّحۡمَٰنِ لِبُيُوتِهِمۡ سُقُفٗا مِّن فِضَّةٖ وَمَعَارِجَ عَلَيۡهَا يَظۡهَرُونَ"
    },
    {
      "id": 4359,
      "number": 34,
      "verse_key": "43:34",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 428,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 492,
      "juz_number": 25,
      "text": "وَلِبُيُوتِهِمۡ أَبۡوَٰبٗا وَسُرُرًا عَلَيۡهَا يَتَّكِـُٔونَ"
    },
    {
      "id": 4360,
      "number": 35,
      "verse_key": "43:35",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 428,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 492,
      "juz_number": 25,
      "text": "وَزُخۡرُفٗاۚ وَإِن كُلُّ ذَٰلِكَ لَمَّا مَتَٰعُ ٱلۡحَيَوٰةِ ٱلدُّنۡيَاۚ وَٱلۡأٓخِرَةُ عِندَ رَبِّكَ لِلۡمُتَّقِينَ"
    },
    {
      "id": 4361,
      "number": 36,
      "verse_key": "43:36",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 429,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 492,
      "juz_number": 25,
      "text": "وَمَن يَعۡشُ عَن ذِكۡرِ ٱلرَّحۡمَٰنِ نُقَيِّضۡ لَهُۥ شَيۡطَٰنٗا فَهُوَ لَهُۥ قَرِينٞ"
    },
    {
      "id": 4362,
      "number": 37,
      "verse_key": "43:37",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 429,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 492,
      "juz_number": 25,
      "text": "وَإِنَّهُمۡ لَيَصُدُّونَهُمۡ عَنِ ٱلسَّبِيلِ وَيَحۡسَبُونَ أَنَّهُم مُّهۡتَدُونَ"
    },
    {
      "id": 4363,
      "number": 38,
      "verse_key": "43:38",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 429,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 492,
      "juz_number": 25,
      "text": "حَتَّىٰٓ إِذَا جَآءَنَا قَالَ يَٰلَيۡتَ بَيۡنِي وَبَيۡنَكَ بُعۡدَ ٱلۡمَشۡرِقَيۡنِ فَبِئۡسَ ٱلۡقَرِينُ"
    },
    {
      "id": 4364,
      "number": 39,
      "verse_key": "43:39",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 429,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 492,
      "juz_number": 25,
      "text": "وَلَن يَنفَعَكُمُ ٱلۡيَوۡمَ إِذ ظَّلَمۡتُمۡ أَنَّكُمۡ فِي ٱلۡعَذَابِ مُشۡتَرِكُونَ"
    },
    {
      "id": 4365,
      "number": 40,
      "verse_key": "43:40",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 429,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 492,
      "juz_number": 25,
      "text": "أَفَأَنتَ تُسۡمِعُ ٱلصُّمَّ أَوۡ تَهۡدِي ٱلۡعُمۡيَ وَمَن كَانَ فِي ضَلَٰلٖ مُّبِينٖ"
    },
    {
      "id": 4366,
      "number": 41,
      "verse_key": "43:41",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 429,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 492,
      "juz_number": 25,
      "text": "فَإِمَّا نَذۡهَبَنَّ بِكَ فَإِنَّا مِنۡهُم مُّنتَقِمُونَ"
    },
    {
      "id": 4367,
      "number": 42,
      "verse_key": "43:42",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 429,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 492,
      "juz_number": 25,
      "text": "أَوۡ نُرِيَنَّكَ ٱلَّذِي وَعَدۡنَٰهُمۡ فَإِنَّا عَلَيۡهِم مُّقۡتَدِرُونَ"
    },
    {
      "id": 4368,
      "number": 43,
      "verse_key": "43:43",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 429,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 492,
      "juz_number": 25,
      "text": "فَٱسۡتَمۡسِكۡ بِٱلَّذِيٓ أُوحِيَ إِلَيۡكَۖ إِنَّكَ عَلَىٰ صِرَٰطٖ مُّسۡتَقِيمٖ"
    },
    {
      "id": 4369,
      "number": 44,
      "verse_key": "43:44",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 429,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 492,
      "juz_number": 25,
      "text": "وَإِنَّهُۥ لَذِكۡرٞ لَّكَ وَلِقَوۡمِكَۖ وَسَوۡفَ تُسۡـَٔلُونَ"
    },
    {
      "id": 4370,
      "number": 45,
      "verse_key": "43:45",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 429,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 492,
      "juz_number": 25,
      "text": "وَسۡـَٔلۡ مَنۡ أَرۡسَلۡنَا مِن قَبۡلِكَ مِن رُّسُلِنَآ أَجَعَلۡنَا مِن دُونِ ٱلرَّحۡمَٰنِ ءَالِهَةٗ يُعۡبَدُونَ"
    },
    {
      "id": 4371,
      "number": 46,
      "verse_key": "43:46",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 430,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 492,
      "juz_number": 25,
      "text": "وَلَقَدۡ أَرۡسَلۡنَا مُوسَىٰ بِـَٔايَٰتِنَآ إِلَىٰ فِرۡعَوۡنَ وَمَلَإِيْهِۦ فَقَالَ إِنِّي رَسُولُ رَبِّ ٱلۡعَٰلَمِينَ"
    },
    {
      "id": 4372,
      "number": 47,
      "verse_key": "43:47",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 430,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 492,
      "juz_number": 25,
      "text": "فَلَمَّا جَآءَهُم بِـَٔايَٰتِنَآ إِذَا هُم مِّنۡهَا يَضۡحَكُونَ"
    },
    {
      "id": 4373,
      "number": 48,
      "verse_key": "43:48",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 430,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 493,
      "juz_number": 25,
      "text": "وَمَا نُرِيهِم مِّنۡ ءَايَةٍ إِلَّا هِيَ أَكۡبَرُ مِنۡ أُخۡتِهَاۖ وَأَخَذۡنَٰهُم بِٱلۡعَذَابِ لَعَلَّهُمۡ يَرۡجِعُونَ"
    },
    {
      "id": 4374,
      "number": 49,
      "verse_key": "43:49",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 430,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 493,
      "juz_number": 25,
      "text": "وَقَالُواْ يَٰٓأَيُّهَ ٱلسَّاحِرُ ٱدۡعُ لَنَا رَبَّكَ بِمَا عَهِدَ عِندَكَ إِنَّنَا لَمُهۡتَدُونَ"
    },
    {
      "id": 4375,
      "number": 50,
      "verse_key": "43:50",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 430,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 493,
      "juz_number": 25,
      "text": "فَلَمَّا كَشَفۡنَا عَنۡهُمُ ٱلۡعَذَابَ إِذَا هُمۡ يَنكُثُونَ"
    },
    {
      "id": 4376,
      "number": 51,
      "verse_key": "43:51",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 430,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 493,
      "juz_number": 25,
      "text": "وَنَادَىٰ فِرۡعَوۡنُ فِي قَوۡمِهِۦ قَالَ يَٰقَوۡمِ أَلَيۡسَ لِي مُلۡكُ مِصۡرَ وَهَٰذِهِ ٱلۡأَنۡهَٰرُ تَجۡرِي مِن تَحۡتِيٓۚ أَفَلَا تُبۡصِرُونَ"
    },
    {
      "id": 4377,
      "number": 52,
      "verse_key": "43:52",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 430,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 493,
      "juz_number": 25,
      "text": "أَمۡ أَنَا۠ خَيۡرٞ مِّنۡ هَٰذَا ٱلَّذِي هُوَ مَهِينٞ وَلَا يَكَادُ يُبِينُ"
    },
    {
      "id": 4378,
      "number": 53,
      "verse_key": "43:53",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 430,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 493,
      "juz_number": 25,
      "text": "فَلَوۡلَآ أُلۡقِيَ عَلَيۡهِ أَسۡوِرَةٞ مِّن ذَهَبٍ أَوۡ جَآءَ مَعَهُ ٱلۡمَلَٰٓئِكَةُ مُقۡتَرِنِينَ"
    },
    {
      "id": 4379,
      "number": 54,
      "verse_key": "43:54",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 430,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 493,
      "juz_number": 25,
      "text": "فَٱسۡتَخَفَّ قَوۡمَهُۥ فَأَطَاعُوهُۚ إِنَّهُمۡ كَانُواْ قَوۡمٗا فَٰسِقِينَ"
    },
    {
      "id": 4380,
      "number": 55,
      "verse_key": "43:55",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 430,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 493,
      "juz_number": 25,
      "text": "فَلَمَّآ ءَاسَفُونَا ٱنتَقَمۡنَا مِنۡهُمۡ فَأَغۡرَقۡنَٰهُمۡ أَجۡمَعِينَ"
    },
    {
      "id": 4381,
      "number": 56,
      "verse_key": "43:56",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 430,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 493,
      "juz_number": 25,
      "text": "فَجَعَلۡنَٰهُمۡ سَلَفٗا وَمَثَلٗا لِّلۡأٓخِرِينَ"
    },
    {
      "id": 4382,
      "number": 57,
      "verse_key": "43:57",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 431,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 493,
      "juz_number": 25,
      "text": "۞ وَلَمَّا ضُرِبَ ٱبۡنُ مَرۡيَمَ مَثَلًا إِذَا قَوۡمُكَ مِنۡهُ يَصِدُّونَ"
    },
    {
      "id": 4383,
      "number": 58,
      "verse_key": "43:58",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 431,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 493,
      "juz_number": 25,
      "text": "وَقَالُوٓاْ ءَأَٰلِهَتُنَا خَيۡرٌ أَمۡ هُوَۚ مَا ضَرَبُوهُ لَكَ إِلَّا جَدَلَۢاۚ بَلۡ هُمۡ قَوۡمٌ خَصِمُونَ"
    },
    {
      "id": 4384,
      "number": 59,
      "verse_key": "43:59",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 431,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 493,
      "juz_number": 25,
      "text": "إِنۡ هُوَ إِلَّا عَبۡدٌ أَنۡعَمۡنَا عَلَيۡهِ وَجَعَلۡنَٰهُ مَثَلٗا لِّبَنِيٓ إِسۡرَٰٓءِيلَ"
    },
    {
      "id": 4385,
      "number": 60,
      "verse_key": "43:60",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 431,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 493,
      "juz_number": 25,
      "text": "وَلَوۡ نَشَآءُ لَجَعَلۡنَا مِنكُم مَّلَٰٓئِكَةٗ فِي ٱلۡأَرۡضِ يَخۡلُفُونَ"
    },
    {
      "id": 4386,
      "number": 61,
      "verse_key": "43:61",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 431,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 494,
      "juz_number": 25,
      "text": "وَإِنَّهُۥ لَعِلۡمٞ لِّلسَّاعَةِ فَلَا تَمۡتَرُنَّ بِهَا وَٱتَّبِعُونِۚ هَٰذَا صِرَٰطٞ مُّسۡتَقِيمٞ"
    },
    {
      "id": 4387,
      "number": 62,
      "verse_key": "43:62",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 431,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 494,
      "juz_number": 25,
      "text": "وَلَا يَصُدَّنَّكُمُ ٱلشَّيۡطَٰنُۖ إِنَّهُۥ لَكُمۡ عَدُوّٞ مُّبِينٞ"
    },
    {
      "id": 4388,
      "number": 63,
      "verse_key": "43:63",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 431,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 494,
      "juz_number": 25,
      "text": "وَلَمَّا جَآءَ عِيسَىٰ بِٱلۡبَيِّنَٰتِ قَالَ قَدۡ جِئۡتُكُم بِٱلۡحِكۡمَةِ وَلِأُبَيِّنَ لَكُم بَعۡضَ ٱلَّذِي تَخۡتَلِفُونَ فِيهِۖ فَٱتَّقُواْ ٱللَّهَ وَأَطِيعُونِ"
    },
    {
      "id": 4389,
      "number": 64,
      "verse_key": "43:64",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 431,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 494,
      "juz_number": 25,
      "text": "إِنَّ ٱللَّهَ هُوَ رَبِّي وَرَبُّكُمۡ فَٱعۡبُدُوهُۚ هَٰذَا صِرَٰطٞ مُّسۡتَقِيمٞ"
    },
    {
      "id": 4390,
      "number": 65,
      "verse_key": "43:65",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 431,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 494,
      "juz_number": 25,
      "text": "فَٱخۡتَلَفَ ٱلۡأَحۡزَابُ مِنۢ بَيۡنِهِمۡۖ فَوَيۡلٞ لِّلَّذِينَ ظَلَمُواْ مِنۡ عَذَابِ يَوۡمٍ أَلِيمٍ"
    },
    {
      "id": 4391,
      "number": 66,
      "verse_key": "43:66",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 431,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 494,
      "juz_number": 25,
      "text": "هَلۡ يَنظُرُونَ إِلَّا ٱلسَّاعَةَ أَن تَأۡتِيَهُم بَغۡتَةٗ وَهُمۡ لَا يَشۡعُرُونَ"
    },
    {
      "id": 4392,
      "number": 67,
      "verse_key": "43:67",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 431,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 494,
      "juz_number": 25,
      "text": "ٱلۡأَخِلَّآءُ يَوۡمَئِذِۭ بَعۡضُهُمۡ لِبَعۡضٍ عَدُوٌّ إِلَّا ٱلۡمُتَّقِينَ"
    },
    {
      "id": 4393,
      "number": 68,
      "verse_key": "43:68",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 494,
      "juz_number": 25,
      "text": "يَٰعِبَادِ لَا خَوۡفٌ عَلَيۡكُمُ ٱلۡيَوۡمَ وَلَآ أَنتُمۡ تَحۡزَنُونَ"
    },
    {
      "id": 4394,
      "number": 69,
      "verse_key": "43:69",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 494,
      "juz_number": 25,
      "text": "ٱلَّذِينَ ءَامَنُواْ بِـَٔايَٰتِنَا وَكَانُواْ مُسۡلِمِينَ"
    },
    {
      "id": 4395,
      "number": 70,
      "verse_key": "43:70",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 494,
      "juz_number": 25,
      "text": "ٱدۡخُلُواْ ٱلۡجَنَّةَ أَنتُمۡ وَأَزۡوَٰجُكُمۡ تُحۡبَرُونَ"
    },
    {
      "id": 4396,
      "number": 71,
      "verse_key": "43:71",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 494,
      "juz_number": 25,
      "text": "يُطَافُ عَلَيۡهِم بِصِحَافٖ مِّن ذَهَبٖ وَأَكۡوَابٖۖ وَفِيهَا مَا تَشۡتَهِيهِ ٱلۡأَنفُسُ وَتَلَذُّ ٱلۡأَعۡيُنُۖ وَأَنتُمۡ فِيهَا خَٰلِدُونَ"
    },
    {
      "id": 4397,
      "number": 72,
      "verse_key": "43:72",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 494,
      "juz_number": 25,
      "text": "وَتِلۡكَ ٱلۡجَنَّةُ ٱلَّتِيٓ أُورِثۡتُمُوهَا بِمَا كُنتُمۡ تَعۡمَلُونَ"
    },
    {
      "id": 4398,
      "number": 73,
      "verse_key": "43:73",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 494,
      "juz_number": 25,
      "text": "لَكُمۡ فِيهَا فَٰكِهَةٞ كَثِيرَةٞ مِّنۡهَا تَأۡكُلُونَ"
    },
    {
      "id": 4399,
      "number": 74,
      "verse_key": "43:74",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 495,
      "juz_number": 25,
      "text": "إِنَّ ٱلۡمُجۡرِمِينَ فِي عَذَابِ جَهَنَّمَ خَٰلِدُونَ"
    },
    {
      "id": 4400,
      "number": 75,
      "verse_key": "43:75",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 495,
      "juz_number": 25,
      "text": "لَا يُفَتَّرُ عَنۡهُمۡ وَهُمۡ فِيهِ مُبۡلِسُونَ"
    },
    {
      "id": 4401,
      "number": 76,
      "verse_key": "43:76",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 495,
      "juz_number": 25,
      "text": "وَمَا ظَلَمۡنَٰهُمۡ وَلَٰكِن كَانُواْ هُمُ ٱلظَّٰلِمِينَ"
    },
    {
      "id": 4402,
      "number": 77,
      "verse_key": "43:77",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 495,
      "juz_number": 25,
      "text": "وَنَادَوۡاْ يَٰمَٰلِكُ لِيَقۡضِ عَلَيۡنَا رَبُّكَۖ قَالَ إِنَّكُم مَّٰكِثُونَ"
    },
    {
      "id": 4403,
      "number": 78,
      "verse_key": "43:78",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 495,
      "juz_number": 25,
      "text": "لَقَدۡ جِئۡنَٰكُم بِٱلۡحَقِّ وَلَٰكِنَّ أَكۡثَرَكُمۡ لِلۡحَقِّ كَٰرِهُونَ"
    },
    {
      "id": 4404,
      "number": 79,
      "verse_key": "43:79",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 495,
      "juz_number": 25,
      "text": "أَمۡ أَبۡرَمُوٓاْ أَمۡرٗا فَإِنَّا مُبۡرِمُونَ"
    },
    {
      "id": 4405,
      "number": 80,
      "verse_key": "43:80",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 495,
      "juz_number": 25,
      "text": "أَمۡ يَحۡسَبُونَ أَنَّا لَا نَسۡمَعُ سِرَّهُمۡ وَنَجۡوَىٰهُمۚ بَلَىٰ وَرُسُلُنَا لَدَيۡهِمۡ يَكۡتُبُونَ"
    },
    {
      "id": 4406,
      "number": 81,
      "verse_key": "43:81",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 495,
      "juz_number": 25,
      "text": "قُلۡ إِن كَانَ لِلرَّحۡمَٰنِ وَلَدٞ فَأَنَا۠ أَوَّلُ ٱلۡعَٰبِدِينَ"
    },
    {
      "id": 4407,
      "number": 82,
      "verse_key": "43:82",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 495,
      "juz_number": 25,
      "text": "سُبۡحَٰنَ رَبِّ ٱلسَّمَٰوَٰتِ وَٱلۡأَرۡضِ رَبِّ ٱلۡعَرۡشِ عَمَّا يَصِفُونَ"
    },
    {
      "id": 4408,
      "number": 83,
      "verse_key": "43:83",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 495,
      "juz_number": 25,
      "text": "فَذَرۡهُمۡ يَخُوضُواْ وَيَلۡعَبُواْ حَتَّىٰ يُلَٰقُواْ يَوۡمَهُمُ ٱلَّذِي يُوعَدُونَ"
    },
    {
      "id": 4409,
      "number": 84,
      "verse_key": "43:84",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 495,
      "juz_number": 25,
      "text": "وَهُوَ ٱلَّذِي فِي ٱلسَّمَآءِ إِلَٰهٞ وَفِي ٱلۡأَرۡضِ إِلَٰهٞۚ وَهُوَ ٱلۡحَكِيمُ ٱلۡعَلِيمُ"
    },
    {
      "id": 4410,
      "number": 85,
      "verse_key": "43:85",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 495,
      "juz_number": 25,
      "text": "وَتَبَارَكَ ٱلَّذِي لَهُۥ مُلۡكُ ٱلسَّمَٰوَٰتِ وَٱلۡأَرۡضِ وَمَا بَيۡنَهُمَا وَعِندَهُۥ عِلۡمُ ٱلسَّاعَةِ وَإِلَيۡهِ تُرۡجَعُونَ"
    },
    {
      "id": 4411,
      "number": 86,
      "verse_key": "43:86",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 495,
      "juz_number": 25,
      "text": "وَلَا يَمۡلِكُ ٱلَّذِينَ يَدۡعُونَ مِن دُونِهِ ٱلشَّفَٰعَةَ إِلَّا مَن شَهِدَ بِٱلۡحَقِّ وَهُمۡ يَعۡلَمُونَ"
    },
    {
      "id": 4412,
      "number": 87,
      "verse_key": "43:87",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 495,
      "juz_number": 25,
      "text": "وَلَئِن سَأَلۡتَهُم مَّنۡ خَلَقَهُمۡ لَيَقُولُنَّ ٱللَّهُۖ فَأَنَّىٰ يُؤۡفَكُونَ"
    },
    {
      "id": 4413,
      "number": 88,
      "verse_key": "43:88",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 495,
      "juz_number": 25,
      "text": "وَقِيلِهِۦ يَٰرَبِّ إِنَّ هَٰٓؤُلَآءِ قَوۡمٞ لَّا يُؤۡمِنُونَ"
    },
    {
      "id": 4414,
      "number": 89,
      "verse_key": "43:89",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 495,
      "juz_number": 25,
      "text": "فَٱصۡفَحۡ عَنۡهُمۡ وَقُلۡ سَلَٰمٞۚ فَسَوۡفَ يَعۡلَمُونَ"
    }
  ]
}