{
  "verses": [
    {
      "id": 4736,
      "number": 1,
      "verse_key": "52:1",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 523,
      "juz_number": 27,
      "text": "وَٱلطُّورِ"
    },
    {
      "id": 4745,
      "number": 10,
      "verse_key": "52:10",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 523,
      "juz_number": 27,
      "text": "وَتَسِيرُ ٱلۡجِبَالُ سَيۡرٗا"
    },
    {
      "id": 4746,
      "number": 11,
      "verse_key": "52:11",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 523,
      "juz_number": 27,
      "text": "فَوَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ"
    },
    {
      "id": 4747,
      "number": 12,
      "verse_key": "52:12",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 523,
      "juz_number": 27,
      "text": "ٱلَّذِينَ هُمۡ فِي خَوۡضٖ يَلۡعَبُونَ"
    },
    {
      "id": 4748,
      "number": 13,
      "verse_key": "52:13",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 523,
      "juz_number": 27,
      "text": "يَوۡمَ يُدَعُّونَ إِلَىٰ نَارِ جَهَنَّمَ دَعًّا"
    },
    {
      "id": 4749,
      "number": 14,
      "verse_key": "52:14",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 523,
      "juz_number": 27,
      "text": "هَٰذِهِ ٱلنَّارُ ٱلَّتِي كُنتُم بِهَا تُكَذِّبُونَ"
    },
    {
      "id": 4750,
      "number": 15,
      "verse_key": "52:15",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 524,
      "juz_number": 27,
      "text": "أَفَسِحۡرٌ هَٰذَآ أَمۡ أَنتُمۡ لَا تُبۡصِرُونَ"
    },
    {
      "id": 4751,
      "number": 16,
      "verse_key": "52:16",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 524,
      "juz_number": 27,
      "text": "ٱصۡلَوۡهَا فَٱصۡبِرُوٓاْ أَوۡ لَا تَصۡبِرُواْ سَوَآءٌ عَلَيۡكُمۡۖ إِنَّمَا تُجۡزَوۡنَ مَا كُنتُمۡ تَعۡمَلُونَ"
    },
    {
      "id": 4752,
      "number": 17,
      "verse_key": "52:17",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 524,
      "juz_number": 27,
      "text": "إِنَّ ٱلۡمُتَّقِينَ فِي جَنَّٰتٖ وَنَعِيمٖ"
    },
    {
      "id": 4753,
      "number": 18,
      "verse_key": "52:18",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 524,
      "juz_number": 27,
      "text": "فَٰكِهِينَ بِمَآ ءَاتَىٰهُمۡ رَبُّهُمۡ وَوَقَىٰهُمۡ رَبُّهُمۡ عَذَابَ ٱلۡجَحِيمِ"
    },
    {
      "id": 4754,
      "number": 19,
      "verse_key": "52:19",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 524,
      "juz_number": 27,
      "text": "كُلُواْ وَٱشۡرَبُواْ هَنِيٓـَٔۢا بِمَا كُنتُمۡ تَعۡمَلُونَ"
    },
    {
      "id": 4737,
      "number": 2,
      "verse_key": "52:2",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 523,
      "juz_number": 27,
      "text": "وَكِتَٰبٖ مَّسۡطُورٖ"
    },
    {
      "id": 4755,
      "number": 20,
      "verse_key": "52:20",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 524,
      "juz_number": 27,
      "text": "مُتَّكِـِٔينَ عَلَىٰ سُرُرٖ مَّصۡفُوفَةٖۖ وَزَوَّجۡنَٰهُم بِحُورٍ عِينٖ"
    },
    {
      "id": 4756,
      "number": 21,
      "verse_key": "52:21",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 524,
      "juz_number": 27,
      "text": "وَٱلَّذِينَ ءَامَنُواْ وَٱتَّبَعَتۡهُمۡ ذُرِّيَّتُهُم بِإِيمَٰنٍ أَلۡحَقۡنَا بِهِمۡ ذُرِّيَّتَهُمۡ وَمَآ أَلَتۡنَٰهُم مِّنۡ عَمَلِهِم مِّن شَيۡءٖۚ كُلُّ ٱمۡرِيِٕۭ بِمَا كَسَبَ رَهِينٞ"
    },
    {
      "id": 4757,
      "number": 22,
      "verse_key": "52:22",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 524,
      "juz_number": 27,
      "text": "وَأَمۡدَدۡنَٰهُم بِفَٰكِهَةٖ وَلَحۡمٖ مِّمَّا يَشۡتَهُونَ"
    },
    {
      "id": 4758,
      "number": 23,
      "verse_key": "52:23",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 524,
      "juz_number": 27,
      "text": "يَتَنَٰزَعُونَ فِيهَا كَأۡسٗا لَّا لَغۡوٞ فِيهَا وَلَا تَأۡثِيمٞ"
    },
    {
      "id": 4759,
      "number": 24,
      "verse_key": "52:24",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 524,
      "juz_number": 27,
      "text": "۞ وَيَطُوفُ عَلَيۡهِمۡ غِلۡمَانٞ لَّهُمۡ كَأَنَّهُمۡ لُؤۡلُؤٞ مَّكۡنُونٞ"
    },
    {
      "id": 4760,
      "number": 25,
      "verse_key": "52:25",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 524,
      "juz_number": 27,
      "text": "وَأَقۡبَلَ بَعۡضُهُمۡ عَلَىٰ بَعۡضٖ يَتَسَآءَلُونَ"
    },
    {
      "id": 4761,
      "number": 26,
      "verse_key": "52:26",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 524,
      "juz_number": 27,
      "text": "قَالُوٓاْ إِنَّا كُنَّا قَبۡلُ فِيٓ أَهۡلِنَا مُشۡفِقِينَ"
    },
    {
      "id": 4762,
      "number": 27,
      "verse_key": "52:27",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 524,
      "juz_number": 27,
      "text": "فَمَنَّ ٱللَّهُ عَلَيۡنَا وَوَقَىٰنَا عَذَابَ ٱلسَّمُومِ"
    },
    {
      "id": 4763,
      "number": 28,
      "verse_key": "52:28",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 524,
      "juz_number": 27,
      "text": "إِنَّا كُنَّا مِن قَبۡلُ نَدۡعُوهُۖ إِنَّهُۥ هُوَ ٱلۡبَرُّ ٱلرَّحِيمُ"
    },
    {
      "id": 4764,
      "number": 29,
      "verse_key": "52:29",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 524,
      "juz_number": 27,
      "text": "فَذَكِّرۡ فَمَآ أَنتَ بِنِعۡمَتِ رَبِّكَ بِكَاهِنٖ وَلَا مَجۡنُونٍ"
    },
    {
      "id": 4738,
      "number": 3,
      "verse_key": "52:3",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 523,
      "juz_number": 27,
      "text": "فِي رَقّٖ مَّنشُورٖ"
    },
    {
      "id": 4765,
      "number": 30,
      "verse_key": "52:30",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 524,
      "juz_number": 27,
      "text": "أَمۡ يَقُولُونَ شَاعِرٞ نَّتَرَبَّصُ بِهِۦ رَيۡبَ ٱلۡمَنُونِ"
    },
    {
      "id": 4766,
      "number": 31,
      "verse_key": "52:31",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 524,
      "juz_number": 27,
      "text": "قُلۡ تَرَبَّصُواْ فَإِنِّي مَعَكُم مِّنَ ٱلۡمُتَرَبِّصِينَ"
    },
    {
      "id": 4767,
      "number": 32,
      "verse_key": "52:32",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 525,
      "juz_number": 27,
      "text": "أَمۡ تَأۡمُرُهُمۡ أَحۡلَٰمُهُم بِهَٰذَآۚ أَمۡ هُمۡ قَوۡمٞ طَاغُونَ"
    },
    {
      "id": 4768,
      "number": 33,
      "verse_key": "52:33",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 525,
      "juz_number": 27,
      "text": "أَمۡ يَقُولُونَ تَقَوَّلَهُۥۚ بَل لَّا يُؤۡمِنُونَ"
    },
    {
      "id": 4769,
      "number": 34,
      "verse_key": "52:34",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 525,
      "juz_number": 27,
      "text": "فَلۡيَأۡتُواْ بِحَدِيثٖ مِّثۡلِهِۦٓ إِن كَانُواْ صَٰدِقِينَ"
    },
    {
      "id": 4770,
      "number": 35,
      "verse_key": "52:35",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 525,
      "juz_number": 27,
      "text": "أَمۡ خُلِقُواْ مِنۡ غَيۡرِ شَيۡءٍ أَمۡ هُمُ ٱلۡخَٰلِقُونَ"
    },
    {
      "id": 4771,
      "number": 36,
      "verse_key": "52:36",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 525,
      "juz_number": 27,
      "text": "أَمۡ خَلَقُواْ ٱلسَّمَٰوَٰتِ وَٱلۡأَرۡضَۚ بَل لَّا يُوقِنُونَ"
    },
    {
      "id": 4772,
      "number": 37,
      "verse_key": "52:37",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 525,
      "juz_number": 27,
      "text": "أَمۡ عِندَهُمۡ خَزَآئِنُ رَبِّكَ أَمۡ هُمُ ٱلۡمُصَۜيۡطِرُونَ"
    },
    {
      "id": 4773,
      "number": 38,
      "verse_key": "52:38",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 525,
      "juz_number": 27,
      "text": "أَمۡ لَهُمۡ سُلَّمٞ يَسۡتَمِعُونَ فِيهِۖ فَلۡيَأۡتِ مُسۡتَمِعُهُم بِسُلۡطَٰنٖ مُّبِينٍ"
    },
    {
      "id": 4774,
      "number": 39,
      "verse_key": "52:39",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 525,
      "juz_number": 27,
      "text": "أَمۡ لَهُ ٱلۡبَنَٰتُ وَلَكُمُ ٱلۡبَنُونَ"
    },
    {
      "id": 4739,
      "number": 4,
      "verse_key": "52:4",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 523,
      "juz_number": 27,
      "text": "وَٱلۡبَيۡتِ ٱلۡمَعۡمُورِ"
    },
    {
      "id": 4775,
      "number": 40,
      "verse_key": "52:40",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 525,
      "juz_number": 27,
      "text": "أَمۡ تَسۡـَٔلُهُمۡ أَجۡرٗا فَهُم مِّن مَّغۡرَمٖ مُّثۡقَلُونَ"
    },
    {
      "id": 4776,
      "number": 41,
      "verse_key": "52:41",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 525,
      "juz_number": 27,
      "text": "أَمۡ عِندَهُمُ ٱلۡغَيۡبُ فَهُمۡ يَكۡتُبُونَ"
    },
    {
      "id": 4777,
      "number": 42,
      "verse_key": "52:42",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 525,
      "juz_number": 27,
      "text": "أَمۡ يُرِيدُونَ كَيۡدٗاۖ فَٱلَّذِينَ كَفَرُواْ هُمُ ٱلۡمَكِيدُونَ"
    },
    {
      "id": 4778,
      "number": 43,
      "verse_key": "52:43",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 525,
      "juz_number": 27,
      "text": "أَمۡ لَهُمۡ إِلَٰهٌ غَيۡرُ ٱللَّهِۚ سُبۡحَٰنَ ٱللَّهِ عَمَّا يُشۡرِكُونَ"
    },
    {
      "id": 4779,
      "number": 44,
      "verse_key": "52:44",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 525,
      "juz_number": 27,
      "text": "وَإِن يَرَوۡاْ كِسۡفٗا مِّنَ ٱلسَّمَآءِ سَاقِطٗا يَقُولُواْ سَحَابٞ مَّرۡكُومٞ"
    },
    {
      "id": 4780,
      "number": 45,
      "verse_key": "52:45",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 525,
      "juz_number": 27,
      "text": "فَذَرۡهُمۡ حَتَّىٰ يُلَٰقُواْ يَوۡمَهُمُ ٱلَّذِي فِيهِ يُصۡعَقُونَ"
    },
    {
      "id": 4781,
      "number": 46,
      "verse_key": "52:46",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 525,
      "juz_number": 27,
