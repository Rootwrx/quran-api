{
  "verses": [
    {
      "id": 4631,
      "number": 1,
      "verse_key": "50:1",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 518,
      "juz_number": 26,
      "text": "قٓۚ وَٱلۡقُرۡءَانِ ٱلۡمَجِيدِ"
    },
    {
      "id": 4640,
      "number": 10,
      "verse_key": "50:10",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 518,
      "juz_number": 26,
      "text": "وَٱلنَّخۡلَ بَاسِقَٰتٖ لَّهَا طَلۡعٞ نَّضِيدٞ"
    },
    {
      "id": 4641,
      "number": 11,
      "verse_key": "50:11",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 518,
      "juz_number": 26,
      "text": "رِّزۡقٗا لِّلۡعِبَادِۖ وَأَحۡيَيۡنَا بِهِۦ بَلۡدَةٗ مَّيۡتٗاۚ كَذَٰلِكَ ٱلۡخُرُوجُ"
    },
    {
      "id": 4642,
      "number": 12,
      "verse_key": "50:12",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 518,
      "juz_number": 26,
      "text": "كَذَّبَتۡ قَبۡلَهُمۡ قَوۡمُ نُوحٖ وَأَصۡحَٰبُ ٱلرَّسِّ وَثَمُودُ"
    },
    {
      "id": 4643,
      "number": 13,
      "verse_key": "50:13",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 518,
      "juz_number": 26,
      "text": "وَعَادٞ وَفِرۡعَوۡنُ وَإِخۡوَٰنُ لُوطٖ"
    },
    {
      "id": 4644,
      "number": 14,
      "verse_key": "50:14",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 518,
      "juz_number": 26,
      "text": "وَأَصۡحَٰبُ ٱلۡأَيۡكَةِ وَقَوۡمُ تُبَّعٖۚ كُلّٞ كَذَّبَ ٱلرُّسُلَ فَحَقَّ وَعِيدِ"
    },
    {
      "id": 4645,
      "number": 15,
      "verse_key": "50:15",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 518,
      "juz_number": 26,
      "text": "أَفَعَيِينَا بِٱلۡخَلۡقِ ٱلۡأَوَّلِۚ بَلۡ هُمۡ فِي لَبۡسٖ مِّنۡ خَلۡقٖ جَدِيدٖ"
    },
    {
      "id": 4646,
      "number": 16,
      "verse_key": "50:16",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 519,
      "juz_number": 26,
      "text": "وَلَقَدۡ خَلَقۡنَا ٱلۡإِنسَٰنَ وَنَعۡلَمُ مَا تُوَسۡوِسُ بِهِۦ نَفۡسُهُۥۖ وَنَحۡنُ أَقۡرَبُ إِلَيۡهِ مِنۡ حَبۡلِ ٱلۡوَرِيدِ"
    },
    {
      "id": 4647,
      "number": 17,
      "verse_key": "50:17",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 519,
      "juz_number": 26,
      "text": "إِذۡ يَتَلَقَّى ٱلۡمُتَلَقِّيَانِ عَنِ ٱلۡيَمِينِ وَعَنِ ٱلشِّمَالِ قَعِيدٞ"
    },
    {
      "id": 4648,
      "number": 18,
      "verse_key": "50:18",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 519,
      "juz_number": 26,
      "text": "مَّا يَلۡفِظُ مِن قَوۡلٍ إِلَّا لَدَيۡهِ رَقِيبٌ عَتِيدٞ"
    },
    {
      "id": 4649,
      "number": 19,
      "verse_key": "50:19",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 519,
      "juz_number": 26,
      "text": "وَجَآءَتۡ سَكۡرَةُ ٱلۡمَوۡتِ بِٱلۡحَقِّۖ ذَٰلِكَ مَا كُنتَ مِنۡهُ تَحِيدُ"
    },
    {
      "id": 4632,
      "number": 2,
      "verse_key": "50:2",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 518,
      "juz_number": 26,
      "text": "بَلۡ عَجِبُوٓاْ أَن جَآءَهُم مُّنذِرٞ مِّنۡهُمۡ فَقَالَ ٱلۡكَٰفِرُونَ هَٰذَا شَيۡءٌ عَجِيبٌ"
    },
    {
      "id": 4650,
      "number": 20,
      "verse_key": "50:20",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 519,
      "juz_number": 26,
      "text": "وَنُفِخَ فِي ٱلصُّورِۚ ذَٰلِكَ يَوۡمُ ٱلۡوَعِيدِ"
    },
    {
      "id": 4651,
      "number": 21,
      "verse_key": "50:21",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 519,
      "juz_number": 26,
      "text": "وَجَآءَتۡ كُلُّ نَفۡسٖ مَّعَهَا سَآئِقٞ وَشَهِيدٞ"
    },
    {
      "id": 4652,
      "number": 22,
      "verse_key": "50:22",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 519,
      "juz_number": 26,
      "text": "لَّقَدۡ كُنتَ فِي غَفۡلَةٖ مِّنۡ هَٰذَا فَكَشَفۡنَا عَنكَ غِطَآءَكَ فَبَصَرُكَ ٱلۡيَوۡمَ حَدِيدٞ"
    },
    {
      "id": 4653,
      "number": 23,
      "verse_key": "50:23",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 519,
      "juz_number": 26,
      "text": "وَقَالَ قَرِينُهُۥ هَٰذَا مَا لَدَيَّ عَتِيدٌ"
    },
    {
      "id": 4654,
      "number": 24,
      "verse_key": "50:24",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 519,
      "juz_number": 26,
      "text": "أَلۡقِيَا فِي جَهَنَّمَ كُلَّ كَفَّارٍ عَنِيدٖ"
    },
    {
      "id": 4655,
      "number": 25,
      "verse_key": "50:25",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 519,
      "juz_number": 26,
      "text": "مَّنَّاعٖ لِّلۡخَيۡرِ مُعۡتَدٖ مُّرِيبٍ"
    },
    {
      "id": 4656,
      "number": 26,
      "verse_key": "50:26",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 519,
      "juz_number": 26,
      "text": "ٱلَّذِي جَعَلَ مَعَ ٱللَّهِ إِلَٰهًا ءَاخَرَ فَأَلۡقِيَاهُ فِي ٱلۡعَذَابِ ٱلشَّدِيدِ"
    },
    {
      "id": 4657,
      "number": 27,
      "verse_key": "50:27",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 519,
      "juz_number": 26,
      "text": "۞ قَالَ قَرِينُهُۥ رَبَّنَا مَآ أَطۡغَيۡتُهُۥ وَلَٰكِن كَانَ فِي ضَلَٰلِۭ بَعِيدٖ"
    },
    {
      "id": 4658,
      "number": 28,
      "verse_key": "50:28",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 519,
      "juz_number": 26,
      "text": "قَالَ لَا تَخۡتَصِمُواْ لَدَيَّ وَقَدۡ قَدَّمۡتُ إِلَيۡكُم بِٱلۡوَعِيدِ"
    },
    {
      "id": 4659,
      "number": 29,
      "verse_key": "50:29",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 519,
      "juz_number": 26,
      "text": "مَا يُبَدَّلُ ٱلۡقَوۡلُ لَدَيَّ وَمَآ أَنَا۠ بِظَلَّٰمٖ لِّلۡعَبِيدِ"
    },
    {
      "id": 4633,
      "number": 3,
      "verse_key": "50:3",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 518,
      "juz_number": 26,
      "text": "أَءِذَا مِتۡنَا وَكُنَّا تُرَابٗاۖ ذَٰلِكَ رَجۡعُۢ بَعِيدٞ"
    },
    {
      "id": 4660,
      "number": 30,
      "verse_key": "50:30",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 519,
      "juz_number": 26,
      "text": "يَوۡمَ نَقُولُ لِجَهَنَّمَ هَلِ ٱمۡتَلَأۡتِ وَتَقُولُ هَلۡ مِن مَّزِيدٖ"
    },
    {
      "id": 4661,
      "number": 31,
      "verse_key": "50:31",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 519,
      "juz_number": 26,
      "text": "وَأُزۡلِفَتِ ٱلۡجَنَّةُ لِلۡمُتَّقِينَ غَيۡرَ بَعِيدٍ"
    },
    {
      "id": 4662,
      "number": 32,
      "verse_key": "50:32",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 519,
      "juz_number": 26,
      "text": "هَٰذَا مَا تُوعَدُونَ لِكُلِّ أَوَّابٍ حَفِيظٖ"
    },
    {
      "id": 4663,
      "number": 33,
      "verse_key": "50:33",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 519,
      "juz_number": 26,
      "text": "مَّنۡ خَشِيَ ٱلرَّحۡمَٰنَ بِٱلۡغَيۡبِ وَجَآءَ بِقَلۡبٖ مُّنِيبٍ"
    },
    {
      "id": 4664,
      "number": 34,
      "verse_key": "50:34",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 519,
      "juz_number": 26,
      "text": "ٱدۡخُلُوهَا بِسَلَٰمٖۖ ذَٰلِكَ يَوۡمُ ٱلۡخُلُودِ"
    },
    {
      "id": 4665,
      "number": 35,
      "verse_key": "50:35",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 519,
      "juz_number": 26,
      "text": "لَهُم مَّا يَشَآءُونَ فِيهَا وَلَدَيۡنَا مَزِيدٞ"
    },
    {
      "id": 4666,
      "number": 36,
      "verse_key": "50:36",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 520,
      "juz_number": 26,
      "text": "وَكَمۡ أَهۡلَكۡنَا قَبۡلَهُم مِّن قَرۡنٍ هُمۡ أَشَدُّ مِنۡهُم بَطۡشٗا فَنَقَّبُواْ فِي ٱلۡبِلَٰدِ هَلۡ مِن مَّحِيصٍ"
    },
    {
      "id": 4667,
      "number": 37,
      "verse_key": "50:37",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 520,
      "juz_number": 26,
      "text": "إِنَّ فِي ذَٰلِكَ لَذِكۡرَىٰ لِمَن كَانَ لَهُۥ قَلۡبٌ أَوۡ أَلۡقَى ٱلسَّمۡعَ وَهُوَ شَهِيدٞ"
    },
    {
      "id": 4668,
      "number": 38,
      "verse_key": "50:38",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 520,
      "juz_number": 26,
      "text": "وَلَقَدۡ خَلَقۡنَا ٱلسَّمَٰوَٰتِ وَٱلۡأَرۡضَ وَمَا بَيۡنَهُمَا فِي سِتَّةِ أَيَّامٖ وَمَا مَسَّنَا مِن لُّغُوبٖ"
    },
    {
      "id": 4669,
      "number": 39,
      "verse_key": "50:39",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 520,
      "juz_number": 26,
      "text": "فَٱصۡبِرۡ عَلَىٰ مَا يَقُولُونَ وَسَبِّحۡ بِحَمۡدِ رَبِّكَ قَبۡلَ طُلُوعِ ٱلشَّمۡسِ وَقَبۡلَ ٱلۡغُرُوبِ"
    },
    {
      "id": 4634,
      "number": 4,
      "verse_key": "50:4",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 518,
      "juz_number": 26,
      "text": "قَدۡ عَلِمۡنَا مَا تَنقُصُ ٱلۡأَرۡضُ مِنۡهُمۡۖ وَعِندَنَا كِتَٰبٌ حَفِيظُۢ"
    },
    {
      "id": 4670,
      "number": 40,
      "verse_key": "50:40",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 520,
      "juz_number": 26,
      "text": "وَمِنَ ٱلَّيۡلِ فَسَبِّحۡهُ وَأَدۡبَٰرَ ٱلسُّجُودِ"
    },
    {
      "id": 4671,
      "number": 41,
      "verse_key": "50:41",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 520,
      "juz_number": 26,
      "text": "وَٱسۡتَمِعۡ يَوۡمَ يُنَادِ ٱلۡمُنَادِ مِن مَّكَانٖ قَرِيبٖ"
    },
    {
      "id": 4672,
      "number": 42,
      "verse_key": "50:42",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 520,
      "juz_number": 26,
      "text": "يَوۡمَ يَسۡمَعُونَ ٱلصَّيۡحَةَ بِٱلۡحَقِّۚ ذَٰلِكَ يَوۡمُ ٱلۡخُرُوجِ"
    },
    {
      "id": 4673,
      "number": 43,
      "verse_key": "50:43",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 520,
      "juz_number": 26,
      "text": "إِنَّا نَحۡنُ نُحۡيِۦ وَنُمِيتُ وَإِلَيۡنَا ٱلۡمَصِيرُ"
    },
    {
      "id": 4674,
      "number": 44,
      "verse_key": "50:44",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 520,
      "juz_number": 26,
