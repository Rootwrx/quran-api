{
  "verses": [
    {
      "id": 2674,
      "number": 1,
      "verse_key": "23:1",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 295,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 342,
      "juz_number": 18,
      "text": "قَدۡ أَفۡلَحَ ٱلۡمُؤۡمِنُونَ"
    },
    {
      "id": 2683,
      "number": 10,
      "verse_key": "23:10",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 295,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 342,
      "juz_number": 18,
      "text": "أُوْلَٰٓئِكَ هُمُ ٱلۡوَٰرِثُونَ"
    },
    {
      "id": 2773,
      "number": 100,
      "verse_key": "23:100",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 300,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 348,
      "juz_number": 18,
      "text": "لَعَلِّيٓ أَعۡمَلُ صَٰلِحٗا فِيمَا تَرَكۡتُۚ كَلَّآۚ إِنَّهَا كَلِمَةٌ هُوَ قَآئِلُهَاۖ وَمِن وَرَآئِهِم بَرۡزَخٌ إِلَىٰ يَوۡمِ يُبۡعَثُونَ"
    },
    {
      "id": 2774,
      "number": 101,
      "verse_key": "23:101",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 300,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 348,
      "juz_number": 18,
      "text": "فَإِذَا نُفِخَ فِي ٱلصُّورِ فَلَآ أَنسَابَ بَيۡنَهُمۡ يَوۡمَئِذٖ وَلَا يَتَسَآءَلُونَ"
    },
    {
      "id": 2775,
      "number": 102,
      "verse_key": "23:102",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 300,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 348,
      "juz_number": 18,
      "text": "فَمَن ثَقُلَتۡ مَوَٰزِينُهُۥ فَأُوْلَٰٓئِكَ هُمُ ٱلۡمُفۡلِحُونَ"
    },
    {
      "id": 2776,
      "number": 103,
      "verse_key": "23:103",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 300,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 348,
      "juz_number": 18,
      "text": "وَمَنۡ خَفَّتۡ مَوَٰزِينُهُۥ فَأُوْلَٰٓئِكَ ٱلَّذِينَ خَسِرُوٓاْ أَنفُسَهُمۡ فِي جَهَنَّمَ خَٰلِدُونَ"
    },
    {
      "id": 2777,
      "number": 104,
      "verse_key": "23:104",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 300,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 348,
      "juz_number": 18,
      "text": "تَلۡفَحُ وُجُوهَهُمُ ٱلنَّارُ وَهُمۡ فِيهَا كَٰلِحُونَ"
    },
    {
      "id": 2778,
      "number": 105,
      "verse_key": "23:105",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 300,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 349,
      "juz_number": 18,
      "text": "أَلَمۡ تَكُنۡ ءَايَٰتِي تُتۡلَىٰ عَلَيۡكُمۡ فَكُنتُم بِهَا تُكَذِّبُونَ"
    },
    {
      "id": 2779,
      "number": 106,
      "verse_key": "23:106",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 300,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 349,
      "juz_number": 18,
      "text": "قَالُواْ رَبَّنَا غَلَبَتۡ عَلَيۡنَا شِقۡوَتُنَا وَكُنَّا قَوۡمٗا ضَآلِّينَ"
    },
    {
      "id": 2780,
      "number": 107,
      "verse_key": "23:107",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 300,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 349,
      "juz_number": 18,
      "text": "رَبَّنَآ أَخۡرِجۡنَا مِنۡهَا فَإِنۡ عُدۡنَا فَإِنَّا ظَٰلِمُونَ"
    },
    {
      "id": 2781,
      "number": 108,
      "verse_key": "23:108",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 300,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 349,
      "juz_number": 18,
      "text": "قَالَ ٱخۡسَـُٔواْ فِيهَا وَلَا تُكَلِّمُونِ"
    },
    {
      "id": 2782,
      "number": 109,
      "verse_key": "23:109",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 300,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 349,
      "juz_number": 18,
      "text": "إِنَّهُۥ كَانَ فَرِيقٞ مِّنۡ عِبَادِي يَقُولُونَ رَبَّنَآ ءَامَنَّا فَٱغۡفِرۡ لَنَا وَٱرۡحَمۡنَا وَأَنتَ خَيۡرُ ٱلرَّٰحِمِينَ"
    },
    {
      "id": 2684,
      "number": 11,
      "verse_key": "23:11",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 295,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 342,
      "juz_number": 18,
      "text": "ٱلَّذِينَ يَرِثُونَ ٱلۡفِرۡدَوۡسَ هُمۡ فِيهَا خَٰلِدُونَ"
    },
    {
      "id": 2783,
      "number": 110,
      "verse_key": "23:110",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 300,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 349,
      "juz_number": 18,
      "text": "فَٱتَّخَذۡتُمُوهُمۡ سِخۡرِيًّا حَتَّىٰٓ أَنسَوۡكُمۡ ذِكۡرِي وَكُنتُم مِّنۡهُمۡ تَضۡحَكُونَ"
    },
    {
      "id": 2784,
      "number": 111,
      "verse_key": "23:111",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 300,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 349,
      "juz_number": 18,
      "text": "إِنِّي جَزَيۡتُهُمُ ٱلۡيَوۡمَ بِمَا صَبَرُوٓاْ أَنَّهُمۡ هُمُ ٱلۡفَآئِزُونَ"
    },
    {
      "id": 2785,
      "number": 112,
      "verse_key": "23:112",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 300,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 349,
      "juz_number": 18,
      "text": "قَٰلَ كَمۡ لَبِثۡتُمۡ فِي ٱلۡأَرۡضِ عَدَدَ سِنِينَ"
    },
    {
      "id": 2786,
      "number": 113,
      "verse_key": "23:113",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 300,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 349,
      "juz_number": 18,
      "text": "قَالُواْ لَبِثۡنَا يَوۡمًا أَوۡ بَعۡضَ يَوۡمٖ فَسۡـَٔلِ ٱلۡعَآدِّينَ"
    },
    {
      "id": 2787,
      "number": 114,
      "verse_key": "23:114",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 300,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 349,
      "juz_number": 18,
      "text": "قَٰلَ إِن لَّبِثۡتُمۡ إِلَّا قَلِيلٗاۖ لَّوۡ أَنَّكُمۡ كُنتُمۡ تَعۡلَمُونَ"
    },
    {
      "id": 2788,
      "number": 115,
      "verse_key": "23:115",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 300,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 349,
      "juz_number": 18,
      "text": "أَفَحَسِبۡتُمۡ أَنَّمَا خَلَقۡنَٰكُمۡ عَبَثٗا وَأَنَّكُمۡ إِلَيۡنَا لَا تُرۡجَعُونَ"
    },
    {
      "id": 2789,
      "number": 116,
      "verse_key": "23:116",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 300,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 349,
      "juz_number": 18,
      "text": "فَتَعَٰلَى ٱللَّهُ ٱلۡمَلِكُ ٱلۡحَقُّۖ لَآ إِلَٰهَ إِلَّا هُوَ رَبُّ ٱلۡعَرۡشِ ٱلۡكَرِيمِ"
    },
    {
      "id": 2790,
      "number": 117,
      "verse_key": "23:117",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 300,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 349,
      "juz_number": 18,
      "text": "وَمَن يَدۡعُ مَعَ ٱللَّهِ إِلَٰهًا ءَاخَرَ لَا بُرۡهَٰنَ لَهُۥ بِهِۦ فَإِنَّمَا حِسَابُهُۥ عِندَ رَبِّهِۦٓۚ إِنَّهُۥ لَا يُفۡلِحُ ٱلۡكَٰفِرُونَ"
    },
    {
      "id": 2791,
      "number": 118,
      "verse_key": "23:118",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 300,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 349,
      "juz_number": 18,
      "text": "وَقُل رَّبِّ ٱغۡفِرۡ وَٱرۡحَمۡ وَأَنتَ خَيۡرُ ٱلرَّٰحِمِينَ"
    },
    {
      "id": 2685,
      "number": 12,
      "verse_key": "23:12",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 295,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 342,
      "juz_number": 18,
      "text": "وَلَقَدۡ خَلَقۡنَا ٱلۡإِنسَٰنَ مِن سُلَٰلَةٖ مِّن طِينٖ"
    },
    {
      "id": 2686,
      "number": 13,
      "verse_key": "23:13",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 295,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 342,
      "juz_number": 18,
      "text": "ثُمَّ جَعَلۡنَٰهُ نُطۡفَةٗ فِي قَرَارٖ مَّكِينٖ"
    },
    {
      "id": 2687,
      "number": 14,
      "verse_key": "23:14",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 295,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 342,
      "juz_number": 18,
      "text": "ثُمَّ خَلَقۡنَا ٱلنُّطۡفَةَ عَلَقَةٗ فَخَلَقۡنَا ٱلۡعَلَقَةَ مُضۡغَةٗ فَخَلَقۡنَا ٱلۡمُضۡغَةَ عِظَٰمٗا فَكَسَوۡنَا ٱلۡعِظَٰمَ لَحۡمٗا ثُمَّ أَنشَأۡنَٰهُ خَلۡقًا ءَاخَرَۚ فَتَبَارَكَ ٱللَّهُ أَحۡسَنُ ٱلۡخَٰلِقِينَ"
    },
    {
      "id": 2688,
      "number": 15,
      "verse_key": "23:15",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 295,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 342,
      "juz_number": 18,
      "text": "ثُمَّ إِنَّكُم بَعۡدَ ذَٰلِكَ لَمَيِّتُونَ"
    },
    {
      "id": 2689,
      "number": 16,
      "verse_key": "23:16",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 295,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 342,
      "juz_number": 18,
      "text": "ثُمَّ إِنَّكُمۡ يَوۡمَ ٱلۡقِيَٰمَةِ تُبۡعَثُونَ"
    },
    {
      "id": 2690,
      "number": 17,
      "verse_key": "23:17",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 295,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 342,
      "juz_number": 18,
      "text": "وَلَقَدۡ خَلَقۡنَا فَوۡقَكُمۡ سَبۡعَ طَرَآئِقَ وَمَا كُنَّا عَنِ ٱلۡخَلۡقِ غَٰفِلِينَ"
    },
    {
      "id": 2691,
      "number": 18,
      "verse_key": "23:18",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 295,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 343,
      "juz_number": 18,
      "text": "وَأَنزَلۡنَا مِنَ ٱلسَّمَآءِ مَآءَۢ بِقَدَرٖ فَأَسۡكَنَّٰهُ فِي ٱلۡأَرۡضِۖ وَإِنَّا عَلَىٰ ذَهَابِۭ بِهِۦ لَقَٰدِرُونَ"
    },
    {
      "id": 2692,
      "number": 19,
      "verse_key": "23:19",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 295,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 343,
      "juz_number": 18,
      "text": "فَأَنشَأۡنَا لَكُم بِهِۦ جَنَّٰتٖ مِّن نَّخِيلٖ وَأَعۡنَٰبٖ لَّكُمۡ فِيهَا فَوَٰكِهُ كَثِيرَةٞ وَمِنۡهَا تَأۡكُلُونَ"
    },
    {
      "id": 2675,
      "number": 2,
      "verse_key": "23:2",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 295,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 342,
      "juz_number": 18,
      "text": "ٱلَّذِينَ هُمۡ فِي صَلَاتِهِمۡ خَٰشِعُونَ"
    },
    {
      "id": 2693,
      "number": 20,
      "verse_key": "23:20",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 295,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 343,
      "juz_number": 18,
      "text": "وَشَجَرَةٗ تَخۡرُجُ مِن طُورِ سَيۡنَآءَ تَنۢبُتُ بِٱلدُّهۡنِ وَصِبۡغٖ لِّلۡأٓكِلِينَ"
    },
    {
      "id": 2694,
      "number": 21,
      "verse_key": "23:21",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 295,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 343,
      "juz_number": 18,
      "text": "وَإِنَّ لَكُمۡ فِي ٱلۡأَنۡعَٰمِ لَعِبۡرَةٗۖ نُّسۡقِيكُم مِّمَّا فِي بُطُونِهَا وَلَكُمۡ فِيهَا مَنَٰفِعُ كَثِيرَةٞ وَمِنۡهَا تَأۡكُلُونَ"
    },
    {
      "id": 2695,
      "number": 22,
      "verse_key": "23:22",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 295,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 343,
      "juz_number": 18,
      "text": "وَعَلَيۡهَا وَعَلَى ٱلۡفُلۡكِ تُحۡمَلُونَ"
    },
    {
      "id": 2696,
      "number": 23,
      "verse_key": "23:23",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 296,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 343,
      "juz_number": 18,
      "text": "وَلَقَدۡ أَرۡسَلۡنَا نُوحًا إِلَىٰ قَوۡمِهِۦ فَقَالَ يَٰقَوۡمِ ٱعۡبُدُواْ ٱللَّهَ مَا لَكُم مِّنۡ إِلَٰهٍ غَيۡرُهُۥٓۚ أَفَلَا تَتَّقُونَ"
    },
    {
      "id": 2697,
      "number": 24,
      "verse_key": "23:24",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 296,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 343,
      "juz_number": 18,
      "text": "فَقَالَ ٱلۡمَلَؤُاْ ٱلَّذِينَ كَفَرُواْ مِن قَوۡمِهِۦ مَا هَٰذَآ إِلَّا بَشَرٞ مِّثۡلُكُمۡ يُرِيدُ أَن يَتَفَضَّلَ عَلَيۡكُمۡ وَلَوۡ شَآءَ ٱللَّهُ لَأَنزَلَ مَلَٰٓئِكَةٗ مَّا سَمِعۡنَا بِهَٰذَا فِيٓ ءَابَآئِنَا ٱلۡأَوَّلِينَ"
    },
    {
      "id": 2698,
      "number": 25,
      "verse_key": "23:25",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 296,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 343,
      "juz_number": 18,
      "text": "إِنۡ هُوَ إِلَّا رَجُلُۢ بِهِۦ جِنَّةٞ فَتَرَبَّصُواْ بِهِۦ حَتَّىٰ حِينٖ"
    },
    {
      "id": 2699,
      "number": 26,
      "verse_key": "23:26",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 296,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 343,
      "juz_number": 18,
      "text": "قَالَ رَبِّ ٱنصُرۡنِي بِمَا كَذَّبُونِ"
    },
    {
      "id": 2700,
      "number": 27,
      "verse_key": "23:27",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 296,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 343,
      "juz_number": 18,
      "text": "فَأَوۡحَيۡنَآ إِلَيۡهِ أَنِ ٱصۡنَعِ ٱلۡفُلۡكَ بِأَعۡيُنِنَا وَوَحۡيِنَا فَإِذَا جَآءَ أَمۡرُنَا وَفَارَ ٱلتَّنُّورُ فَٱسۡلُكۡ فِيهَا مِن كُلّٖ زَوۡجَيۡنِ ٱثۡنَيۡنِ وَأَهۡلَكَ إِلَّا مَن سَبَقَ عَلَيۡهِ ٱلۡقَوۡلُ مِنۡهُمۡۖ وَلَا تُخَٰطِبۡنِي فِي ٱلَّذِينَ ظَلَمُوٓاْ إِنَّهُم مُّغۡرَقُونَ"
    },
    {
      "id": 2701,
      "number": 28,
      "verse_key": "23:28",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 296,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 344,
      "juz_number": 18,
      "text": "فَإِذَا ٱسۡتَوَيۡتَ أَنتَ وَمَن مَّعَكَ عَلَى ٱلۡفُلۡكِ فَقُلِ ٱلۡحَمۡدُ لِلَّهِ ٱلَّذِي نَجَّىٰنَا مِنَ ٱلۡقَوۡمِ ٱلظَّٰلِمِينَ"
    },
    {
      "id": 2702,
      "number": 29,
      "verse_key": "23:29",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 296,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 344,
      "juz_number": 18,
      "text": "وَقُل رَّبِّ أَنزِلۡنِي مُنزَلٗا مُّبَارَكٗا وَأَنتَ خَيۡرُ ٱلۡمُنزِلِينَ"
    },
    {
      "id": 2676,
      "number": 3,
      "verse_key": "23:3",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 295,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 342,
      "juz_number": 18,
      "text": "وَٱلَّذِينَ هُمۡ عَنِ ٱللَّغۡوِ مُعۡرِضُونَ"
    },
    {
      "id": 2703,
      "number": 30,
      "verse_key": "23:30",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 296,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 344,
      "juz_number": 18,
      "text": "إِنَّ فِي ذَٰلِكَ لَأٓيَٰتٖ وَإِن كُنَّا لَمُبۡتَلِينَ"
    },
    {
      "id": 2704,
      "number": 31,
      "verse_key": "23:31",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 296,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 344,
      "juz_number": 18,
      "text": "ثُمَّ أَنشَأۡنَا مِنۢ بَعۡدِهِمۡ قَرۡنًا ءَاخَرِينَ"
    },
    {
      "id": 2705,
      "number": 32,
      "verse_key": "23:32",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 296,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 344,
      "juz_number": 18,
      "text": "فَأَرۡسَلۡنَا فِيهِمۡ رَسُولٗا مِّنۡهُمۡ أَنِ ٱعۡبُدُواْ ٱللَّهَ مَا لَكُم مِّنۡ إِلَٰهٍ غَيۡرُهُۥٓۚ أَفَلَا تَتَّقُونَ"
    },
    {
      "id": 2706,
      "number": 33,
      "verse_key": "23:33",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 297,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 344,
      "juz_number": 18,
      "text": "وَقَالَ ٱلۡمَلَأُ مِن قَوۡمِهِ ٱلَّذِينَ كَفَرُواْ وَكَذَّبُواْ بِلِقَآءِ ٱلۡأٓخِرَةِ وَأَتۡرَفۡنَٰهُمۡ فِي ٱلۡحَيَوٰةِ ٱلدُّنۡيَا مَا هَٰذَآ إِلَّا بَشَرٞ مِّثۡلُكُمۡ يَأۡكُلُ مِمَّا تَأۡكُلُونَ مِنۡهُ وَيَشۡرَبُ مِمَّا تَشۡرَبُونَ"
    },
    {
      "id": 2707,
      "number": 34,
      "verse_key": "23:34",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 297,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 344,
      "juz_number": 18,
      "text": "وَلَئِنۡ أَطَعۡتُم بَشَرٗا مِّثۡلَكُمۡ إِنَّكُمۡ إِذٗا لَّخَٰسِرُونَ"
    },
    {
      "id": 2708,
      "number": 35,
      "verse_key": "23:35",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 297,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 344,
      "juz_number": 18,
      "text": "أَيَعِدُكُمۡ أَنَّكُمۡ إِذَا مِتُّمۡ وَكُنتُمۡ تُرَابٗا وَعِظَٰمًا أَنَّكُم مُّخۡرَجُونَ"
    },
    {
      "id": 2709,
      "number": 36,
      "verse_key": "23:36",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 297,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 344,
      "juz_number": 18,
      "text": "۞ هَيۡهَاتَ هَيۡهَاتَ لِمَا تُوعَدُونَ"
    },
    {
      "id": 2710,
      "number": 37,
      "verse_key": "23:37",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 297,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 344,
      "juz_number": 18,
      "text": "إِنۡ هِيَ إِلَّا حَيَاتُنَا ٱلدُّنۡيَا نَمُوتُ وَنَحۡيَا وَمَا نَحۡنُ بِمَبۡعُوثِينَ"
    },
    {
      "id": 2711,
      "number": 38,
      "verse_key": "23:38",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 297,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 344,
      "juz_number": 18,
      "text": "إِنۡ هُوَ إِلَّا رَجُلٌ ٱفۡتَرَىٰ عَلَى ٱللَّهِ كَذِبٗا وَمَا نَحۡنُ لَهُۥ بِمُؤۡمِنِينَ"
    },
    {
      "id": 2712,
      "number": 39,
      "verse_key": "23:39",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 297,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 344,
      "juz_number": 18,
      "text": "قَالَ رَبِّ ٱنصُرۡنِي بِمَا كَذَّبُونِ"
    },
    {
      "id": 2677,
      "number": 4,
      "verse_key": "23:4",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 295,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 342,
      "juz_number": 18,
      "text": "وَٱلَّذِينَ هُمۡ لِلزَّكَوٰةِ فَٰعِلُونَ"
    },
    {
      "id": 2713,
      "number": 40,
      "verse_key": "23:40",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 297,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 344,
      "juz_number": 18,
      "text": "قَالَ عَمَّا قَلِيلٖ لَّيُصۡبِحُنَّ نَٰدِمِينَ"
    },
    {
      "id": 2714,
      "number": 41,
      "verse_key": "23:41",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 297,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 344,
      "juz_number": 18,
      "text": "فَأَخَذَتۡهُمُ ٱلصَّيۡحَةُ بِٱلۡحَقِّ فَجَعَلۡنَٰهُمۡ غُثَآءٗۚ فَبُعۡدٗا لِّلۡقَوۡمِ ٱلظَّٰلِمِينَ"
    },
    {
      "id": 2715,
      "number": 42,
      "verse_key": "23:42",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 297,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 344,
      "juz_number": 18,
      "text": "ثُمَّ أَنشَأۡنَا مِنۢ بَعۡدِهِمۡ قُرُونًا ءَاخَرِينَ"
    },
    {
      "id": 2716,
      "number": 43,
      "verse_key": "23:43",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 297,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 345,
      "juz_number": 18,
      "text": "مَا تَسۡبِقُ مِنۡ أُمَّةٍ أَجَلَهَا وَمَا يَسۡتَـٔۡخِرُونَ"
    },
    {
      "id": 2717,
      "number": 44,
      "verse_key": "23:44",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 297,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 345,
      "juz_number": 18,
      "text": "ثُمَّ أَرۡسَلۡنَا رُسُلَنَا تَتۡرَاۖ كُلَّ مَا جَآءَ أُمَّةٗ رَّسُولُهَا كَذَّبُوهُۖ فَأَتۡبَعۡنَا بَعۡضَهُم بَعۡضٗا وَجَعَلۡنَٰهُمۡ أَحَادِيثَۚ فَبُعۡدٗا لِّقَوۡمٖ لَّا يُؤۡمِنُونَ"
    },
    {
      "id": 2718,
      "number": 45,
      "verse_key": "23:45",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 297,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 345,
      "juz_number": 18,
      "text": "ثُمَّ أَرۡسَلۡنَا مُوسَىٰ وَأَخَاهُ هَٰرُونَ بِـَٔايَٰتِنَا وَسُلۡطَٰنٖ مُّبِينٍ"
    },
    {
      "id": 2719,
      "number": 46,
      "verse_key": "23:46",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 297,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 345,
      "juz_number": 18,
      "text": "إِلَىٰ فِرۡعَوۡنَ وَمَلَإِيْهِۦ فَٱسۡتَكۡبَرُواْ وَكَانُواْ قَوۡمًا عَالِينَ"
    },
    {
      "id": 2720,
      "number": 47,
      "verse_key": "23:47",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 297,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 345,
      "juz_number": 18,
      "text": "فَقَالُوٓاْ أَنُؤۡمِنُ لِبَشَرَيۡنِ مِثۡلِنَا وَقَوۡمُهُمَا لَنَا عَٰبِدُونَ"
    },
    {
      "id": 2721,
      "number": 48,
      "verse_key": "23:48",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 297,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 345,
      "juz_number": 18,
      "text": "فَكَذَّبُوهُمَا فَكَانُواْ مِنَ ٱلۡمُهۡلَكِينَ"
    },
    {
      "id": 2722,
      "number": 49,
      "verse_key": "23:49",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 297,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 345,
      "juz_number": 18,
      "text": "وَلَقَدۡ ءَاتَيۡنَا مُوسَى ٱلۡكِتَٰبَ لَعَلَّهُمۡ يَهۡتَدُونَ"
    },
    {
      "id": 2678,
      "number": 5,
      "verse_key": "23:5",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 295,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 342,
      "juz_number": 18,
      "text": "وَٱلَّذِينَ هُمۡ لِفُرُوجِهِمۡ حَٰفِظُونَ"
    },
    {
      "id": 2723,
      "number": 50,
      "verse_key": "23:50",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 297,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 345,
      "juz_number": 18,
      "text": "وَجَعَلۡنَا ٱبۡنَ مَرۡيَمَ وَأُمَّهُۥٓ ءَايَةٗ وَءَاوَيۡنَٰهُمَآ إِلَىٰ رَبۡوَةٖ ذَاتِ قَرَارٖ وَمَعِينٖ"
    },
    {
      "id": 2724,
      "number": 51,
      "verse_key": "23:51",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 345,
      "juz_number": 18,
      "text": "يَٰٓأَيُّهَا ٱلرُّسُلُ كُلُواْ مِنَ ٱلطَّيِّبَٰتِ وَٱعۡمَلُواْ صَٰلِحًاۖ إِنِّي بِمَا تَعۡمَلُونَ عَلِيمٞ"
    },
    {
      "id": 2725,
      "number": 52,
      "verse_key": "23:52",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 345,
      "juz_number": 18,
      "text": "وَإِنَّ هَٰذِهِۦٓ أُمَّتُكُمۡ أُمَّةٗ وَٰحِدَةٗ وَأَنَا۠ رَبُّكُمۡ فَٱتَّقُونِ"
    },
    {
      "id": 2726,
      "number": 53,
      "verse_key": "23:53",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 345,
      "juz_number": 18,
      "text": "فَتَقَطَّعُوٓاْ أَمۡرَهُم بَيۡنَهُمۡ زُبُرٗاۖ كُلُّ حِزۡبِۭ بِمَا لَدَيۡهِمۡ فَرِحُونَ"
    },
    {
      "id": 2727,
      "number": 54,
      "verse_key": "23:54",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 345,
      "juz_number": 18,
      "text": "فَذَرۡهُمۡ فِي غَمۡرَتِهِمۡ حَتَّىٰ حِينٍ"
    },
    {
      "id": 2728,
      "number": 55,
      "verse_key": "23:55",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 345,
      "juz_number": 18,
      "text": "أَيَحۡسَبُونَ أَنَّمَا نُمِدُّهُم بِهِۦ مِن مَّالٖ وَبَنِينَ"
    },
    {
      "id": 2729,
      "number": 56,
      "verse_key": "23:56",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 345,
      "juz_number": 18,
      "text": "نُسَارِعُ لَهُمۡ فِي ٱلۡخَيۡرَٰتِۚ بَل لَّا يَشۡعُرُونَ"
    },
    {
      "id": 2730,
      "number": 57,
      "verse_key": "23:57",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 345,
      "juz_number": 18,
      "text": "إِنَّ ٱلَّذِينَ هُم مِّنۡ خَشۡيَةِ رَبِّهِم مُّشۡفِقُونَ"
    },
    {
      "id": 2731,
      "number": 58,
      "verse_key": "23:58",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 345,
      "juz_number": 18,
      "text": "وَٱلَّذِينَ هُم بِـَٔايَٰتِ رَبِّهِمۡ يُؤۡمِنُونَ"
    },
    {
      "id": 2732,
      "number": 59,
      "verse_key": "23:59",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 345,
      "juz_number": 18,
      "text": "وَٱلَّذِينَ هُم بِرَبِّهِمۡ لَا يُشۡرِكُونَ"
    },
    {
      "id": 2679,
      "number": 6,
      "verse_key": "23:6",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 295,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 342,
      "juz_number": 18,
      "text": "إِلَّا عَلَىٰٓ أَزۡوَٰجِهِمۡ أَوۡ مَا مَلَكَتۡ أَيۡمَٰنُهُمۡ فَإِنَّهُمۡ غَيۡرُ مَلُومِينَ"
    },
    {
      "id": 2733,
      "number": 60,
      "verse_key": "23:60",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 346,
      "juz_number": 18,
      "text": "وَٱلَّذِينَ يُؤۡتُونَ مَآ ءَاتَواْ وَّقُلُوبُهُمۡ وَجِلَةٌ أَنَّهُمۡ إِلَىٰ رَبِّهِمۡ رَٰجِعُونَ"
    },
    {
      "id": 2734,
      "number": 61,
      "verse_key": "23:61",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 346,
      "juz_number": 18,
      "text": "أُوْلَٰٓئِكَ يُسَٰرِعُونَ فِي ٱلۡخَيۡرَٰتِ وَهُمۡ لَهَا سَٰبِقُونَ"
    },
    {
      "id": 2735,
      "number": 62,
      "verse_key": "23:62",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 346,
      "juz_number": 18,
      "text": "وَلَا نُكَلِّفُ نَفۡسًا إِلَّا وُسۡعَهَاۚ وَلَدَيۡنَا كِتَٰبٞ يَنطِقُ بِٱلۡحَقِّ وَهُمۡ لَا يُظۡلَمُونَ"
    },
    {
      "id": 2736,
      "number": 63,
      "verse_key": "23:63",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 346,
      "juz_number": 18,
      "text": "بَلۡ قُلُوبُهُمۡ فِي غَمۡرَةٖ مِّنۡ هَٰذَا وَلَهُمۡ أَعۡمَٰلٞ مِّن دُونِ ذَٰلِكَ هُمۡ لَهَا عَٰمِلُونَ"
    },
    {
      "id": 2737,
      "number": 64,
      "verse_key": "23:64",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 346,
      "juz_number": 18,
      "text": "حَتَّىٰٓ إِذَآ أَخَذۡنَا مُتۡرَفِيهِم بِٱلۡعَذَابِ إِذَا هُمۡ يَجۡـَٔرُونَ"
    },
    {
      "id": 2738,
      "number": 65,
      "verse_key": "23:65",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 346,
      "juz_number": 18,
      "text": "لَا تَجۡـَٔرُواْ ٱلۡيَوۡمَۖ إِنَّكُم مِّنَّا لَا تُنصَرُونَ"
    },
    {
      "id": 2739,
      "number": 66,
      "verse_key": "23:66",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 346,
      "juz_number": 18,
      "text": "قَدۡ كَانَتۡ ءَايَٰتِي تُتۡلَىٰ عَلَيۡكُمۡ فَكُنتُمۡ عَلَىٰٓ أَعۡقَٰبِكُمۡ تَنكِصُونَ"
    },
    {
      "id": 2740,
      "number": 67,
      "verse_key": "23:67",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 346,
      "juz_number": 18,
      "text": "مُسۡتَكۡبِرِينَ بِهِۦ سَٰمِرٗا تَهۡجُرُونَ"
    },
    {
      "id": 2741,
      "number": 68,
      "verse_key": "23:68",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 346,
      "juz_number": 18,
      "text": "أَفَلَمۡ يَدَّبَّرُواْ ٱلۡقَوۡلَ أَمۡ جَآءَهُم مَّا لَمۡ يَأۡتِ ءَابَآءَهُمُ ٱلۡأَوَّلِينَ"
    },
    {
      "id": 2742,
      "number": 69,
      "verse_key": "23:69",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 346,
      "juz_number": 18,
      "text": "أَمۡ لَمۡ يَعۡرِفُواْ رَسُولَهُمۡ فَهُمۡ لَهُۥ مُنكِرُونَ"
    },
    {
      "id": 2680,
      "number": 7,
      "verse_key": "23:7",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 295,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 342,
      "juz_number": 18,
      "text": "فَمَنِ ٱبۡتَغَىٰ وَرَآءَ ذَٰلِكَ فَأُوْلَٰٓئِكَ هُمُ ٱلۡعَادُونَ"
    },
    {
      "id": 2743,
      "number": 70,
      "verse_key": "23:70",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 346,
      "juz_number": 18,
      "text": "أَمۡ يَقُولُونَ بِهِۦ جِنَّةُۢۚ بَلۡ جَآءَهُم بِٱلۡحَقِّ وَأَكۡثَرُهُمۡ لِلۡحَقِّ كَٰرِهُونَ"
    },
    {
      "id": 2744,
      "number": 71,
      "verse_key": "23:71",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 346,
      "juz_number": 18,
      "text": "وَلَوِ ٱتَّبَعَ ٱلۡحَقُّ أَهۡوَآءَهُمۡ لَفَسَدَتِ ٱلسَّمَٰوَٰتُ وَٱلۡأَرۡضُ وَمَن فِيهِنَّۚ بَلۡ أَتَيۡنَٰهُم بِذِكۡرِهِمۡ فَهُمۡ عَن ذِكۡرِهِم مُّعۡرِضُونَ"
    },
    {
      "id": 2745,
      "number": 72,
      "verse_key": "23:72",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 346,
      "juz_number": 18,
      "text": "أَمۡ تَسۡـَٔلُهُمۡ خَرۡجٗا فَخَرَاجُ رَبِّكَ خَيۡرٞۖ وَهُوَ خَيۡرُ ٱلرَّٰزِقِينَ"
    },
    {
      "id": 2746,
      "number": 73,
      "verse_key": "23:73",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 346,
      "juz_number": 18,
      "text": "وَإِنَّكَ لَتَدۡعُوهُمۡ إِلَىٰ صِرَٰطٖ مُّسۡتَقِيمٖ"
    },
    {
      "id": 2747,
      "number": 74,
      "verse_key": "23:74",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 346,
      "juz_number": 18,
      "text": "وَإِنَّ ٱلَّذِينَ لَا يُؤۡمِنُونَ بِٱلۡأٓخِرَةِ عَنِ ٱلصِّرَٰطِ لَنَٰكِبُونَ"
    },
    {
      "id": 2748,
      "number": 75,
      "verse_key": "23:75",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 347,
      "juz_number": 18,
      "text": "۞ وَلَوۡ رَحِمۡنَٰهُمۡ وَكَشَفۡنَا مَا بِهِم مِّن ضُرّٖ لَّلَجُّواْ فِي طُغۡيَٰنِهِمۡ يَعۡمَهُونَ"
    },
    {
      "id": 2749,
      "number": 76,
      "verse_key": "23:76",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 347,
      "juz_number": 18,
      "text": "وَلَقَدۡ أَخَذۡنَٰهُم بِٱلۡعَذَابِ فَمَا ٱسۡتَكَانُواْ لِرَبِّهِمۡ وَمَا يَتَضَرَّعُونَ"
    },
    {
      "id": 2750,
      "number": 77,
      "verse_key": "23:77",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 347,
      "juz_number": 18,
      "text": "حَتَّىٰٓ إِذَا فَتَحۡنَا عَلَيۡهِم بَابٗا ذَا عَذَابٖ شَدِيدٍ إِذَا هُمۡ فِيهِ مُبۡلِسُونَ"
    },
    {
      "id": 2751,
      "number": 78,
      "verse_key": "23:78",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 299,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 347,
      "juz_number": 18,
      "text": "وَهُوَ ٱلَّذِيٓ أَنشَأَ لَكُمُ ٱلسَّمۡعَ وَٱلۡأَبۡصَٰرَ وَٱلۡأَفۡـِٔدَةَۚ قَلِيلٗا مَّا تَشۡكُرُونَ"
    },
    {
      "id": 2752,
      "number": 79,
      "verse_key": "23:79",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 299,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 347,
      "juz_number": 18,
      "text": "وَهُوَ ٱلَّذِي ذَرَأَكُمۡ فِي ٱلۡأَرۡضِ وَإِلَيۡهِ تُحۡشَرُونَ"
    },
    {
      "id": 2681,
      "number": 8,
      "verse_key": "23:8",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 295,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 342,
      "juz_number": 18,
      "text": "وَٱلَّذِينَ هُمۡ لِأَمَٰنَٰتِهِمۡ وَعَهۡدِهِمۡ رَٰعُونَ"
    },
    {
      "id": 2753,
      "number": 80,
      "verse_key": "23:80",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 299,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 347,
      "juz_number": 18,
      "text": "وَهُوَ ٱلَّذِي يُحۡيِۦ وَيُمِيتُ وَلَهُ ٱخۡتِلَٰفُ ٱلَّيۡلِ وَٱلنَّهَارِۚ أَفَلَا تَعۡقِلُونَ"
    },
    {
      "id": 2754,
      "number": 81,
      "verse_key": "23:81",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 299,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 347,
      "juz_number": 18,
      "text": "بَلۡ قَالُواْ مِثۡلَ مَا قَالَ ٱلۡأَوَّلُونَ"
    },
    {
      "id": 2755,
      "number": 82,
      "verse_key": "23:82",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 299,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 347,
      "juz_number": 18,
      "text": "قَالُوٓاْ أَءِذَا مِتۡنَا وَكُنَّا تُرَابٗا وَعِظَٰمًا أَءِنَّا لَمَبۡعُوثُونَ"
    },
    {
      "id": 2756,
      "number": 83,
      "verse_key": "23:83",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 299,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 347,
      "juz_number": 18,
      "text": "لَقَدۡ وُعِدۡنَا نَحۡنُ وَءَابَآؤُنَا هَٰذَا مِن قَبۡلُ إِنۡ هَٰذَآ إِلَّآ أَسَٰطِيرُ ٱلۡأَوَّلِينَ"
    },
    {
      "id": 2757,
      "number": 84,
      "verse_key": "23:84",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 299,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 347,
      "juz_number": 18,
      "text": "قُل لِّمَنِ ٱلۡأَرۡضُ وَمَن فِيهَآ إِن كُنتُمۡ تَعۡلَمُونَ"
    },
    {
      "id": 2758,
      "number": 85,
      "verse_key": "23:85",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 299,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 347,
      "juz_number": 18,
      "text": "سَيَقُولُونَ لِلَّهِۚ قُلۡ أَفَلَا تَذَكَّرُونَ"
    },
    {
      "id": 2759,
      "number": 86,
      "verse_key": "23:86",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 299,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 347,
      "juz_number": 18,
      "text": "قُلۡ مَن رَّبُّ ٱلسَّمَٰوَٰتِ ٱلسَّبۡعِ وَرَبُّ ٱلۡعَرۡشِ ٱلۡعَظِيمِ"
    },
    {
      "id": 2760,
      "number": 87,
      "verse_key": "23:87",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 299,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 347,
      "juz_number": 18,
      "text": "سَيَقُولُونَ لِلَّهِۚ قُلۡ أَفَلَا تَتَّقُونَ"
    },
    {
      "id": 2761,
      "number": 88,
      "verse_key": "23:88",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 299,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 347,
      "juz_number": 18,
      "text": "قُلۡ مَنۢ بِيَدِهِۦ مَلَكُوتُ كُلِّ شَيۡءٖ وَهُوَ يُجِيرُ وَلَا يُجَارُ عَلَيۡهِ إِن كُنتُمۡ تَعۡلَمُونَ"
    },
    {
      "id": 2762,
      "number": 89,
      "verse_key": "23:89",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 299,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 347,
      "juz_number": 18,
      "text": "سَيَقُولُونَ لِلَّهِۚ قُلۡ فَأَنَّىٰ تُسۡحَرُونَ"
    },
    {
      "id": 2682,
      "number": 9,
      "verse_key": "23:9",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 295,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 342,
      "juz_number": 18,
      "text": "وَٱلَّذِينَ هُمۡ عَلَىٰ صَلَوَٰتِهِمۡ يُحَافِظُونَ"
    },
    {
