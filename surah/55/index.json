      "rub_el_hizb_number": 213,
      "ruku_number": 468,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 531,
      "juz_number": 27,
      "text": "وَٱلۡأَرۡضَ وَضَعَهَا لِلۡأَنَامِ"
