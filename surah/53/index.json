{
  "verses": [
    {
      "id": 4785,
      "number": 1,
      "verse_key": "53:1",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "text": "وَٱلنَّجۡمِ إِذَا هَوَىٰ"
    },
    {
      "id": 4794,
      "number": 10,
      "verse_key": "53:10",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "text": "فَأَوۡحَىٰٓ إِلَىٰ عَبۡدِهِۦ مَآ أَوۡحَىٰ"
    },
    {
      "id": 4795,
      "number": 11,
      "verse_key": "53:11",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "text": "مَا كَذَبَ ٱلۡفُؤَادُ مَا رَأَىٰٓ"
    },
    {
      "id": 4796,
      "number": 12,
      "verse_key": "53:12",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "text": "أَفَتُمَٰرُونَهُۥ عَلَىٰ مَا يَرَىٰ"
    },
    {
      "id": 4797,
      "number": 13,
      "verse_key": "53:13",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "text": "وَلَقَدۡ رَءَاهُ نَزۡلَةً أُخۡرَىٰ"
    },
    {
      "id": 4798,
      "number": 14,
      "verse_key": "53:14",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "text": "عِندَ سِدۡرَةِ ٱلۡمُنتَهَىٰ"
    },
    {
      "id": 4799,
      "number": 15,
      "verse_key": "53:15",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "text": "عِندَهَا جَنَّةُ ٱلۡمَأۡوَىٰٓ"
    },
    {
      "id": 4800,
      "number": 16,
      "verse_key": "53:16",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "text": "إِذۡ يَغۡشَى ٱلسِّدۡرَةَ مَا يَغۡشَىٰ"
    },
    {
      "id": 4801,
      "number": 17,
      "verse_key": "53:17",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "text": "مَا زَاغَ ٱلۡبَصَرُ وَمَا طَغَىٰ"
    },
    {
      "id": 4802,
      "number": 18,
      "verse_key": "53:18",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "text": "لَقَدۡ رَأَىٰ مِنۡ ءَايَٰتِ رَبِّهِ ٱلۡكُبۡرَىٰٓ"
    },
    {
      "id": 4803,
      "number": 19,
      "verse_key": "53:19",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "text": "أَفَرَءَيۡتُمُ ٱللَّٰتَ وَٱلۡعُزَّىٰ"
    },
    {
      "id": 4786,
      "number": 2,
      "verse_key": "53:2",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "text": "مَا ضَلَّ صَاحِبُكُمۡ وَمَا غَوَىٰ"
    },
    {
      "id": 4804,
      "number": 20,
      "verse_key": "53:20",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "text": "وَمَنَوٰةَ ٱلثَّالِثَةَ ٱلۡأُخۡرَىٰٓ"
    },
    {
      "id": 4805,
      "number": 21,
      "verse_key": "53:21",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "text": "أَلَكُمُ ٱلذَّكَرُ وَلَهُ ٱلۡأُنثَىٰ"
    },
    {
      "id": 4806,
      "number": 22,
      "verse_key": "53:22",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "text": "تِلۡكَ إِذٗا قِسۡمَةٞ ضِيزَىٰٓ"
    },
    {
      "id": 4807,
      "number": 23,
      "verse_key": "53:23",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "text": "إِنۡ هِيَ إِلَّآ أَسۡمَآءٞ سَمَّيۡتُمُوهَآ أَنتُمۡ وَءَابَآؤُكُم مَّآ أَنزَلَ ٱللَّهُ بِهَا مِن سُلۡطَٰنٍۚ إِن يَتَّبِعُونَ إِلَّا ٱلظَّنَّ وَمَا تَهۡوَى ٱلۡأَنفُسُۖ وَلَقَدۡ جَآءَهُم مِّن رَّبِّهِمُ ٱلۡهُدَىٰٓ"
    },
    {
      "id": 4808,
      "number": 24,
      "verse_key": "53:24",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "text": "أَمۡ لِلۡإِنسَٰنِ مَا تَمَنَّىٰ"
    },
    {
      "id": 4809,
      "number": 25,
      "verse_key": "53:25",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "text": "فَلِلَّهِ ٱلۡأٓخِرَةُ وَٱلۡأُولَىٰ"
    },
    {
      "id": 4810,
      "number": 26,
      "verse_key": "53:26",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 463,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "text": "۞ وَكَم مِّن مَّلَكٖ فِي ٱلسَّمَٰوَٰتِ لَا تُغۡنِي شَفَٰعَتُهُمۡ شَيۡـًٔا إِلَّا مِنۢ بَعۡدِ أَن يَأۡذَنَ ٱللَّهُ لِمَن يَشَآءُ وَيَرۡضَىٰٓ"
    },
    {
      "id": 4811,
      "number": 27,
      "verse_key": "53:27",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 463,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 527,
      "juz_number": 27,
      "text": "إِنَّ ٱلَّذِينَ لَا يُؤۡمِنُونَ بِٱلۡأٓخِرَةِ لَيُسَمُّونَ ٱلۡمَلَٰٓئِكَةَ تَسۡمِيَةَ ٱلۡأُنثَىٰ"
    },
    {
      "id": 4812,
      "number": 28,
      "verse_key": "53:28",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 463,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 527,
      "juz_number": 27,
      "text": "وَمَا لَهُم بِهِۦ مِنۡ عِلۡمٍۖ إِن يَتَّبِعُونَ إِلَّا ٱلظَّنَّۖ وَإِنَّ ٱلظَّنَّ لَا يُغۡنِي مِنَ ٱلۡحَقِّ شَيۡـٔٗا"
    },
    {
      "id": 4813,
      "number": 29,
      "verse_key": "53:29",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 463,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 527,
      "juz_number": 27,
      "text": "فَأَعۡرِضۡ عَن مَّن تَوَلَّىٰ عَن ذِكۡرِنَا وَلَمۡ يُرِدۡ إِلَّا ٱلۡحَيَوٰةَ ٱلدُّنۡيَا"
    },
    {
      "id": 4787,
      "number": 3,
      "verse_key": "53:3",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "text": "وَمَا يَنطِقُ عَنِ ٱلۡهَوَىٰٓ"
    },
    {
      "id": 4814,
      "number": 30,
      "verse_key": "53:30",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 463,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 527,
      "juz_number": 27,
      "text": "ذَٰلِكَ مَبۡلَغُهُم مِّنَ ٱلۡعِلۡمِۚ إِنَّ رَبَّكَ هُوَ أَعۡلَمُ بِمَن ضَلَّ عَن سَبِيلِهِۦ وَهُوَ أَعۡلَمُ بِمَنِ ٱهۡتَدَىٰ"
    },
    {
      "id": 4815,
      "number": 31,
      "verse_key": "53:31",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 463,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 527,
      "juz_number": 27,
      "text": "وَلِلَّهِ مَا فِي ٱلسَّمَٰوَٰتِ وَمَا فِي ٱلۡأَرۡضِ لِيَجۡزِيَ ٱلَّذِينَ أَسَٰٓـُٔواْ بِمَا عَمِلُواْ وَيَجۡزِيَ ٱلَّذِينَ أَحۡسَنُواْ بِٱلۡحُسۡنَى"
    },
    {
      "id": 4816,
      "number": 32,
      "verse_key": "53:32",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 463,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 527,
      "juz_number": 27,
      "text": "ٱلَّذِينَ يَجۡتَنِبُونَ كَبَٰٓئِرَ ٱلۡإِثۡمِ وَٱلۡفَوَٰحِشَ إِلَّا ٱللَّمَمَۚ إِنَّ رَبَّكَ وَٰسِعُ ٱلۡمَغۡفِرَةِۚ هُوَ أَعۡلَمُ بِكُمۡ إِذۡ أَنشَأَكُم مِّنَ ٱلۡأَرۡضِ وَإِذۡ أَنتُمۡ أَجِنَّةٞ فِي بُطُونِ أُمَّهَٰتِكُمۡۖ فَلَا تُزَكُّوٓاْ أَنفُسَكُمۡۖ هُوَ أَعۡلَمُ بِمَنِ ٱتَّقَىٰٓ"
    },
    {
      "id": 4817,
      "number": 33,
      "verse_key": "53:33",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 527,
      "juz_number": 27,
      "text": "أَفَرَءَيۡتَ ٱلَّذِي تَوَلَّىٰ"
    },
    {
      "id": 4818,
      "number": 34,
      "verse_key": "53:34",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 527,
      "juz_number": 27,
      "text": "وَأَعۡطَىٰ قَلِيلٗا وَأَكۡدَىٰٓ"
    },
    {
      "id": 4819,
      "number": 35,
      "verse_key": "53:35",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 527,
      "juz_number": 27,
      "text": "أَعِندَهُۥ عِلۡمُ ٱلۡغَيۡبِ فَهُوَ يَرَىٰٓ"
    },
    {
      "id": 4820,
      "number": 36,
      "verse_key": "53:36",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 527,
      "juz_number": 27,
      "text": "أَمۡ لَمۡ يُنَبَّأۡ بِمَا فِي صُحُفِ مُوسَىٰ"
    },
    {
      "id": 4821,
      "number": 37,
      "verse_key": "53:37",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 527,
      "juz_number": 27,
      "text": "وَإِبۡرَٰهِيمَ ٱلَّذِي وَفَّىٰٓ"
    },
    {
      "id": 4822,
      "number": 38,
      "verse_key": "53:38",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 527,
      "juz_number": 27,
      "text": "أَلَّا تَزِرُ وَازِرَةٞ وِزۡرَ أُخۡرَىٰ"
    },
    {
      "id": 4823,
      "number": 39,
      "verse_key": "53:39",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 527,
      "juz_number": 27,
      "text": "وَأَن لَّيۡسَ لِلۡإِنسَٰنِ إِلَّا مَا سَعَىٰ"
    },
    {
      "id": 4788,
      "number": 4,
      "verse_key": "53:4",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "text": "إِنۡ هُوَ إِلَّا وَحۡيٞ يُوحَىٰ"
    },
    {
      "id": 4824,
      "number": 40,
      "verse_key": "53:40",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 527,
      "juz_number": 27,
      "text": "وَأَنَّ سَعۡيَهُۥ سَوۡفَ يُرَىٰ"
    },
    {
      "id": 4825,
      "number": 41,
      "verse_key": "53:41",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 527,
      "juz_number": 27,
      "text": "ثُمَّ يُجۡزَىٰهُ ٱلۡجَزَآءَ ٱلۡأَوۡفَىٰ"
    },
    {
      "id": 4826,
      "number": 42,
      "verse_key": "53:42",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 527,
      "juz_number": 27,
      "text": "وَأَنَّ إِلَىٰ رَبِّكَ ٱلۡمُنتَهَىٰ"
    },
    {
      "id": 4827,
      "number": 43,
      "verse_key": "53:43",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 527,
      "juz_number": 27,
      "text": "وَأَنَّهُۥ هُوَ أَضۡحَكَ وَأَبۡكَىٰ"
    },
    {
      "id": 4828,
      "number": 44,
      "verse_key": "53:44",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 527,
      "juz_number": 27,
      "text": "وَأَنَّهُۥ هُوَ أَمَاتَ وَأَحۡيَا"
    },
    {
      "id": 4829,
      "number": 45,
      "verse_key": "53:45",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 528,
      "juz_number": 27,
      "text": "وَأَنَّهُۥ خَلَقَ ٱلزَّوۡجَيۡنِ ٱلذَّكَرَ وَٱلۡأُنثَىٰ"
    },
    {
      "id": 4830,
      "number": 46,
      "verse_key": "53:46",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 528,
      "juz_number": 27,
