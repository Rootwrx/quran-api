{
  "verses": [
    {
      "id": 6080,
      "number": 1,
      "verse_key": "93:1",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 537,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 596,
      "juz_number": 30,
      "text": "وَٱلضُّحَىٰ"
    },
    {
      "id": 6081,
      "number": 2,
      "verse_key": "93:2",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 537,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 596,
      "juz_number": 30,
      "text": "وَٱلَّيۡلِ إِذَا سَجَىٰ"
    },
    {
      "id": 6082,
      "number": 3,
      "verse_key": "93:3",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 537,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 596,
      "juz_number": 30,
      "text": "مَا وَدَّعَكَ رَبُّكَ وَمَا قَلَىٰ"
    },
    {
      "id": 6083,
      "number": 4,
      "verse_key": "93:4",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 537,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 596,
      "juz_number": 30,
      "text": "وَلَلۡأٓخِرَةُ خَيۡرٞ لَّكَ مِنَ ٱلۡأُولَىٰ"
    },
    {
      "id": 6084,
      "number": 5,
      "verse_key": "93:5",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 537,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 596,
      "juz_number": 30,
      "text": "وَلَسَوۡفَ يُعۡطِيكَ رَبُّكَ فَتَرۡضَىٰٓ"
    },
    {
      "id": 6085,
      "number": 6,
      "verse_key": "93:6",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 537,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 596,
      "juz_number": 30,
      "text": "أَلَمۡ يَجِدۡكَ يَتِيمٗا فَـَٔاوَىٰ"
    },
    {
      "id": 6086,
      "number": 7,
      "verse_key": "93:7",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 537,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 596,
      "juz_number": 30,
      "text": "وَوَجَدَكَ ضَآلّٗا فَهَدَىٰ"
    },
    {
      "id": 6087,
      "number": 8,
      "verse_key": "93:8",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 537,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 596,
      "juz_number": 30,
      "text": "وَوَجَدَكَ عَآئِلٗا فَأَغۡنَىٰ"
    },
    {
      "id": 6088,
      "number": 9,
      "verse_key": "93:9",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 537,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 596,
      "juz_number": 30,
      "text": "فَأَمَّا ٱلۡيَتِيمَ فَلَا تَقۡهَرۡ"
    },
    {
      "id": 6089,
      "number": 10,
      "verse_key": "93:10",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 537,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 596,
      "juz_number": 30,
      "text": "وَأَمَّا ٱلسَّآئِلَ فَلَا تَنۡهَرۡ"
    },
    {
      "id": 6090,
      "number": 11,
      "verse_key": "93:11",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 537,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 596,
      "juz_number": 30,
      "text": "وَأَمَّا بِنِعۡمَةِ رَبِّكَ فَحَدِّثۡ"
    }
  ]
}