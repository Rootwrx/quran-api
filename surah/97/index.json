      "text": "إِنَّآ أَنزَلۡنَٰهُ فِي لَيۡلَةِ ٱلۡقَدۡرِ"
    },
    {
      "id": 6127,
      "number": 2,
      "verse_key": "97:2",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 541,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 598,
      "juz_number": 30,
      "text": "وَمَآ أَدۡرَىٰكَ مَا لَيۡلَةُ ٱلۡقَدۡرِ"
    },
    {
      "id": 6128,
      "number": 3,
      "verse_key": "97:3",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 541,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 598,
      "juz_number": 30,
      "text": "لَيۡلَةُ ٱلۡقَدۡرِ خَيۡرٞ مِّنۡ أَلۡفِ شَهۡرٖ"
    },
    {
      "id": 6129,
      "number": 4,
      "verse_key": "97:4",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 541,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 598,
      "juz_number": 30,
      "text": "تَنَزَّلُ ٱلۡمَلَٰٓئِكَةُ وَٱلرُّوحُ فِيهَا بِإِذۡنِ رَبِّهِم مِّن كُلِّ أَمۡرٖ"
    },
    {
      "id": 6130,
      "number": 5,
      "verse_key": "97:5",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 541,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 598,
      "juz_number": 30,
      "text": "سَلَٰمٌ هِيَ حَتَّىٰ مَطۡلَعِ ٱلۡفَجۡرِ"
    }
