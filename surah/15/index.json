{
  "verses": [
    {
      "id": 1803,
      "number": 1,
      "verse_key": "15:1",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 218,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 262,
      "juz_number": 14,
      "text": "الٓرۚ تِلۡكَ ءَايَٰتُ ٱلۡكِتَٰبِ وَقُرۡءَانٖ مُّبِينٖ"
    },
    {
      "id": 1812,
      "number": 10,
      "verse_key": "15:10",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 218,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 262,
      "juz_number": 14,
      "text": "وَلَقَدۡ أَرۡسَلۡنَا مِن قَبۡلِكَ فِي شِيَعِ ٱلۡأَوَّلِينَ"
    },
    {
      "id": 1813,
      "number": 11,
      "verse_key": "15:11",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 218,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 262,
      "juz_number": 14,
      "text": "وَمَا يَأۡتِيهِم مِّن رَّسُولٍ إِلَّا كَانُواْ بِهِۦ يَسۡتَهۡزِءُونَ"
    },
    {
      "id": 1814,
      "number": 12,
      "verse_key": "15:12",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 218,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 262,
      "juz_number": 14,
      "text": "كَذَٰلِكَ نَسۡلُكُهُۥ فِي قُلُوبِ ٱلۡمُجۡرِمِينَ"
    },
    {
      "id": 1815,
      "number": 13,
      "verse_key": "15:13",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 218,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 262,
      "juz_number": 14,
      "text": "لَا يُؤۡمِنُونَ بِهِۦ وَقَدۡ خَلَتۡ سُنَّةُ ٱلۡأَوَّلِينَ"
    },
    {
      "id": 1816,
      "number": 14,
      "verse_key": "15:14",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 218,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 262,
      "juz_number": 14,
      "text": "وَلَوۡ فَتَحۡنَا عَلَيۡهِم بَابٗا مِّنَ ٱلسَّمَآءِ فَظَلُّواْ فِيهِ يَعۡرُجُونَ"
    },
    {
      "id": 1817,
      "number": 15,
      "verse_key": "15:15",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 218,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 262,
      "juz_number": 14,
      "text": "لَقَالُوٓاْ إِنَّمَا سُكِّرَتۡ أَبۡصَٰرُنَا بَلۡ نَحۡنُ قَوۡمٞ مَّسۡحُورُونَ"
    },
    {
      "id": 1818,
      "number": 16,
      "verse_key": "15:16",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 219,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 263,
      "juz_number": 14,
      "text": "وَلَقَدۡ جَعَلۡنَا فِي ٱلسَّمَآءِ بُرُوجٗا وَزَيَّنَّٰهَا لِلنَّٰظِرِينَ"
    },
    {
      "id": 1819,
      "number": 17,
      "verse_key": "15:17",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 219,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 263,
      "juz_number": 14,
      "text": "وَحَفِظۡنَٰهَا مِن كُلِّ شَيۡطَٰنٖ رَّجِيمٍ"
    },
    {
      "id": 1820,
      "number": 18,
      "verse_key": "15:18",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 219,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 263,
      "juz_number": 14,
      "text": "إِلَّا مَنِ ٱسۡتَرَقَ ٱلسَّمۡعَ فَأَتۡبَعَهُۥ شِهَابٞ مُّبِينٞ"
    },
    {
      "id": 1821,
      "number": 19,
      "verse_key": "15:19",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 219,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 263,
      "juz_number": 14,
      "text": "وَٱلۡأَرۡضَ مَدَدۡنَٰهَا وَأَلۡقَيۡنَا فِيهَا رَوَٰسِيَ وَأَنۢبَتۡنَا فِيهَا مِن كُلِّ شَيۡءٖ مَّوۡزُونٖ"
    },
    {
      "id": 1804,
      "number": 2,
      "verse_key": "15:2",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 218,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 262,
      "juz_number": 14,
      "text": "رُّبَمَا يَوَدُّ ٱلَّذِينَ كَفَرُواْ لَوۡ كَانُواْ مُسۡلِمِينَ"
    },
    {
      "id": 1822,
      "number": 20,
      "verse_key": "15:20",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 219,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 263,
      "juz_number": 14,
      "text": "وَجَعَلۡنَا لَكُمۡ فِيهَا مَعَٰيِشَ وَمَن لَّسۡتُمۡ لَهُۥ بِرَٰزِقِينَ"
    },
    {
      "id": 1823,
      "number": 21,
      "verse_key": "15:21",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 219,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 263,
      "juz_number": 14,
      "text": "وَإِن مِّن شَيۡءٍ إِلَّا عِندَنَا خَزَآئِنُهُۥ وَمَا نُنَزِّلُهُۥٓ إِلَّا بِقَدَرٖ مَّعۡلُومٖ"
    },
    {
      "id": 1824,
      "number": 22,
      "verse_key": "15:22",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 219,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 263,
      "juz_number": 14,
      "text": "وَأَرۡسَلۡنَا ٱلرِّيَٰحَ لَوَٰقِحَ فَأَنزَلۡنَا مِنَ ٱلسَّمَآءِ مَآءٗ فَأَسۡقَيۡنَٰكُمُوهُ وَمَآ أَنتُمۡ لَهُۥ بِخَٰزِنِينَ"
    },
    {
      "id": 1825,
      "number": 23,
      "verse_key": "15:23",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 219,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 263,
      "juz_number": 14,
      "text": "وَإِنَّا لَنَحۡنُ نُحۡيِۦ وَنُمِيتُ وَنَحۡنُ ٱلۡوَٰرِثُونَ"
    },
    {
      "id": 1826,
      "number": 24,
      "verse_key": "15:24",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 219,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 263,
      "juz_number": 14,
      "text": "وَلَقَدۡ عَلِمۡنَا ٱلۡمُسۡتَقۡدِمِينَ مِنكُمۡ وَلَقَدۡ عَلِمۡنَا ٱلۡمُسۡتَـٔۡخِرِينَ"
    },
    {
      "id": 1827,
      "number": 25,
      "verse_key": "15:25",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 219,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 263,
      "juz_number": 14,
      "text": "وَإِنَّ رَبَّكَ هُوَ يَحۡشُرُهُمۡۚ إِنَّهُۥ حَكِيمٌ عَلِيمٞ"
    },
    {
      "id": 1828,
      "number": 26,
      "verse_key": "15:26",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 220,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 263,
      "juz_number": 14,
      "text": "وَلَقَدۡ خَلَقۡنَا ٱلۡإِنسَٰنَ مِن صَلۡصَٰلٖ مِّنۡ حَمَإٖ مَّسۡنُونٖ"
    },
    {
      "id": 1829,
      "number": 27,
      "verse_key": "15:27",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 220,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 263,
      "juz_number": 14,
      "text": "وَٱلۡجَآنَّ خَلَقۡنَٰهُ مِن قَبۡلُ مِن نَّارِ ٱلسَّمُومِ"
    },
    {
      "id": 1830,
      "number": 28,
      "verse_key": "15:28",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 220,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 263,
      "juz_number": 14,
      "text": "وَإِذۡ قَالَ رَبُّكَ لِلۡمَلَٰٓئِكَةِ إِنِّي خَٰلِقُۢ بَشَرٗا مِّن صَلۡصَٰلٖ مِّنۡ حَمَإٖ مَّسۡنُونٖ"
    },
    {
      "id": 1831,
      "number": 29,
      "verse_key": "15:29",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 220,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 263,
      "juz_number": 14,
      "text": "فَإِذَا سَوَّيۡتُهُۥ وَنَفَخۡتُ فِيهِ مِن رُّوحِي فَقَعُواْ لَهُۥ سَٰجِدِينَ"
    },
    {
      "id": 1805,
      "number": 3,
      "verse_key": "15:3",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 218,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 262,
      "juz_number": 14,
      "text": "ذَرۡهُمۡ يَأۡكُلُواْ وَيَتَمَتَّعُواْ وَيُلۡهِهِمُ ٱلۡأَمَلُۖ فَسَوۡفَ يَعۡلَمُونَ"
    },
    {
      "id": 1832,
      "number": 30,
      "verse_key": "15:30",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 220,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 263,
      "juz_number": 14,
      "text": "فَسَجَدَ ٱلۡمَلَٰٓئِكَةُ كُلُّهُمۡ أَجۡمَعُونَ"
    },
    {
      "id": 1833,
      "number": 31,
      "verse_key": "15:31",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 220,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 263,
      "juz_number": 14,
      "text": "إِلَّآ إِبۡلِيسَ أَبَىٰٓ أَن يَكُونَ مَعَ ٱلسَّٰجِدِينَ"
    },
    {
      "id": 1834,
      "number": 32,
      "verse_key": "15:32",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 220,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 264,
      "juz_number": 14,
      "text": "قَالَ يَٰٓإِبۡلِيسُ مَا لَكَ أَلَّا تَكُونَ مَعَ ٱلسَّٰجِدِينَ"
    },
    {
      "id": 1835,
      "number": 33,
      "verse_key": "15:33",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 220,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 264,
      "juz_number": 14,
      "text": "قَالَ لَمۡ أَكُن لِّأَسۡجُدَ لِبَشَرٍ خَلَقۡتَهُۥ مِن صَلۡصَٰلٖ مِّنۡ حَمَإٖ مَّسۡنُونٖ"
    },
    {
      "id": 1836,
      "number": 34,
      "verse_key": "15:34",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 220,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 264,
      "juz_number": 14,
      "text": "قَالَ فَٱخۡرُجۡ مِنۡهَا فَإِنَّكَ رَجِيمٞ"
    },
    {
      "id": 1837,
      "number": 35,
      "verse_key": "15:35",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 220,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 264,
      "juz_number": 14,
      "text": "وَإِنَّ عَلَيۡكَ ٱللَّعۡنَةَ إِلَىٰ يَوۡمِ ٱلدِّينِ"
    },
    {
      "id": 1838,
      "number": 36,
      "verse_key": "15:36",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 220,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 264,
      "juz_number": 14,
      "text": "قَالَ رَبِّ فَأَنظِرۡنِيٓ إِلَىٰ يَوۡمِ يُبۡعَثُونَ"
    },
    {
      "id": 1839,
      "number": 37,
      "verse_key": "15:37",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 220,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 264,
      "juz_number": 14,
      "text": "قَالَ فَإِنَّكَ مِنَ ٱلۡمُنظَرِينَ"
    },
    {
      "id": 1840,
      "number": 38,
      "verse_key": "15:38",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 220,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 264,
      "juz_number": 14,
      "text": "إِلَىٰ يَوۡمِ ٱلۡوَقۡتِ ٱلۡمَعۡلُومِ"
    },
    {
      "id": 1841,
      "number": 39,
      "verse_key": "15:39",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 220,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 264,
      "juz_number": 14,
      "text": "قَالَ رَبِّ بِمَآ أَغۡوَيۡتَنِي لَأُزَيِّنَنَّ لَهُمۡ فِي ٱلۡأَرۡضِ وَلَأُغۡوِيَنَّهُمۡ أَجۡمَعِينَ"
    },
    {
      "id": 1806,
      "number": 4,
      "verse_key": "15:4",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 218,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 262,
      "juz_number": 14,
      "text": "وَمَآ أَهۡلَكۡنَا مِن قَرۡيَةٍ إِلَّا وَلَهَا كِتَابٞ مَّعۡلُومٞ"
    },
    {
      "id": 1842,
      "number": 40,
      "verse_key": "15:40",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 220,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 264,
      "juz_number": 14,
      "text": "إِلَّا عِبَادَكَ مِنۡهُمُ ٱلۡمُخۡلَصِينَ"
    },
    {
      "id": 1843,
      "number": 41,
      "verse_key": "15:41",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 220,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 264,
      "juz_number": 14,
      "text": "قَالَ هَٰذَا صِرَٰطٌ عَلَيَّ مُسۡتَقِيمٌ"
    },
    {
      "id": 1844,
      "number": 42,
      "verse_key": "15:42",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 220,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 264,
      "juz_number": 14,
      "text": "إِنَّ عِبَادِي لَيۡسَ لَكَ عَلَيۡهِمۡ سُلۡطَٰنٌ إِلَّا مَنِ ٱتَّبَعَكَ مِنَ ٱلۡغَاوِينَ"
    },
    {
      "id": 1845,
      "number": 43,
      "verse_key": "15:43",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 220,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 264,
      "juz_number": 14,
      "text": "وَإِنَّ جَهَنَّمَ لَمَوۡعِدُهُمۡ أَجۡمَعِينَ"
    },
    {
      "id": 1846,
      "number": 44,
      "verse_key": "15:44",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 220,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 264,
      "juz_number": 14,
      "text": "لَهَا سَبۡعَةُ أَبۡوَٰبٖ لِّكُلِّ بَابٖ مِّنۡهُمۡ جُزۡءٞ مَّقۡسُومٌ"
    },
    {
      "id": 1847,
      "number": 45,
      "verse_key": "15:45",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 221,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 264,
      "juz_number": 14,
      "text": "إِنَّ ٱلۡمُتَّقِينَ فِي جَنَّٰتٖ وَعُيُونٍ"
    },
    {
      "id": 1848,
      "number": 46,
      "verse_key": "15:46",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 221,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 264,
      "juz_number": 14,
      "text": "ٱدۡخُلُوهَا بِسَلَٰمٍ ءَامِنِينَ"
    },
    {
      "id": 1849,
      "number": 47,
      "verse_key": "15:47",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 221,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 264,
      "juz_number": 14,
      "text": "وَنَزَعۡنَا مَا فِي صُدُورِهِم مِّنۡ غِلٍّ إِخۡوَٰنًا عَلَىٰ سُرُرٖ مُّتَقَٰبِلِينَ"
    },
    {
      "id": 1850,
      "number": 48,
      "verse_key": "15:48",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 221,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 264,
      "juz_number": 14,
      "text": "لَا يَمَسُّهُمۡ فِيهَا نَصَبٞ وَمَا هُم مِّنۡهَا بِمُخۡرَجِينَ"
    },
    {
      "id": 1851,
      "number": 49,
      "verse_key": "15:49",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 221,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 264,
      "juz_number": 14,
      "text": "۞ نَبِّئۡ عِبَادِيٓ أَنِّيٓ أَنَا ٱلۡغَفُورُ ٱلرَّحِيمُ"
    },
    {
      "id": 1807,
      "number": 5,
      "verse_key": "15:5",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 218,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 262,
      "juz_number": 14,
      "text": "مَّا تَسۡبِقُ مِنۡ أُمَّةٍ أَجَلَهَا وَمَا يَسۡتَـٔۡخِرُونَ"
    },
    {
      "id": 1852,
      "number": 50,
      "verse_key": "15:50",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 221,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 264,
      "juz_number": 14,
      "text": "وَأَنَّ عَذَابِي هُوَ ٱلۡعَذَابُ ٱلۡأَلِيمُ"
    },
    {
      "id": 1853,
      "number": 51,
      "verse_key": "15:51",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 221,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 264,
      "juz_number": 14,
      "text": "وَنَبِّئۡهُمۡ عَن ضَيۡفِ إِبۡرَٰهِيمَ"
    },
    {
      "id": 1854,
      "number": 52,
      "verse_key": "15:52",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 221,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 265,
      "juz_number": 14,
      "text": "إِذۡ دَخَلُواْ عَلَيۡهِ فَقَالُواْ سَلَٰمٗا قَالَ إِنَّا مِنكُمۡ وَجِلُونَ"
    },
    {
      "id": 1855,
      "number": 53,
      "verse_key": "15:53",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 221,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 265,
      "juz_number": 14,
      "text": "قَالُواْ لَا تَوۡجَلۡ إِنَّا نُبَشِّرُكَ بِغُلَٰمٍ عَلِيمٖ"
    },
    {
      "id": 1856,
      "number": 54,
      "verse_key": "15:54",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 221,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 265,
      "juz_number": 14,
      "text": "قَالَ أَبَشَّرۡتُمُونِي عَلَىٰٓ أَن مَّسَّنِيَ ٱلۡكِبَرُ فَبِمَ تُبَشِّرُونَ"
    },
    {
      "id": 1857,
      "number": 55,
      "verse_key": "15:55",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 221,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 265,
      "juz_number": 14,
      "text": "قَالُواْ بَشَّرۡنَٰكَ بِٱلۡحَقِّ فَلَا تَكُن مِّنَ ٱلۡقَٰنِطِينَ"
    },
    {
      "id": 1858,
      "number": 56,
      "verse_key": "15:56",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 221,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 265,
      "juz_number": 14,
      "text": "قَالَ وَمَن يَقۡنَطُ مِن رَّحۡمَةِ رَبِّهِۦٓ إِلَّا ٱلضَّآلُّونَ"
    },
    {
      "id": 1859,
      "number": 57,
      "verse_key": "15:57",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 221,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 265,
      "juz_number": 14,
      "text": "قَالَ فَمَا خَطۡبُكُمۡ أَيُّهَا ٱلۡمُرۡسَلُونَ"
    },
    {
      "id": 1860,
      "number": 58,
      "verse_key": "15:58",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 221,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 265,
      "juz_number": 14,
      "text": "قَالُوٓاْ إِنَّآ أُرۡسِلۡنَآ إِلَىٰ قَوۡمٖ مُّجۡرِمِينَ"
    },
    {
      "id": 1861,
      "number": 59,
      "verse_key": "15:59",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 221,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 265,
      "juz_number": 14,
      "text": "إِلَّآ ءَالَ لُوطٍ إِنَّا لَمُنَجُّوهُمۡ أَجۡمَعِينَ"
    },
    {
      "id": 1808,
      "number": 6,
      "verse_key": "15:6",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 218,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 262,
      "juz_number": 14,
      "text": "وَقَالُواْ يَٰٓأَيُّهَا ٱلَّذِي نُزِّلَ عَلَيۡهِ ٱلذِّكۡرُ إِنَّكَ لَمَجۡنُونٞ"
    },
    {
      "id": 1862,
      "number": 60,
      "verse_key": "15:60",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 221,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 265,
      "juz_number": 14,
      "text": "إِلَّا ٱمۡرَأَتَهُۥ قَدَّرۡنَآ إِنَّهَا لَمِنَ ٱلۡغَٰبِرِينَ"
    },
    {
      "id": 1863,
      "number": 61,
      "verse_key": "15:61",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 222,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 265,
      "juz_number": 14,
      "text": "فَلَمَّا جَآءَ ءَالَ لُوطٍ ٱلۡمُرۡسَلُونَ"
    },
    {
      "id": 1864,
      "number": 62,
      "verse_key": "15:62",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 222,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 265,
      "juz_number": 14,
      "text": "قَالَ إِنَّكُمۡ قَوۡمٞ مُّنكَرُونَ"
    },
    {
      "id": 1865,
      "number": 63,
      "verse_key": "15:63",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 222,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 265,
      "juz_number": 14,
      "text": "قَالُواْ بَلۡ جِئۡنَٰكَ بِمَا كَانُواْ فِيهِ يَمۡتَرُونَ"
    },
    {
      "id": 1866,
      "number": 64,
      "verse_key": "15:64",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 222,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 265,
      "juz_number": 14,
      "text": "وَأَتَيۡنَٰكَ بِٱلۡحَقِّ وَإِنَّا لَصَٰدِقُونَ"
    },
    {
      "id": 1867,
      "number": 65,
      "verse_key": "15:65",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 222,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 265,
      "juz_number": 14,
      "text": "فَأَسۡرِ بِأَهۡلِكَ بِقِطۡعٖ مِّنَ ٱلَّيۡلِ وَٱتَّبِعۡ أَدۡبَٰرَهُمۡ وَلَا يَلۡتَفِتۡ مِنكُمۡ أَحَدٞ وَٱمۡضُواْ حَيۡثُ تُؤۡمَرُونَ"
    },
    {
      "id": 1868,
      "number": 66,
      "verse_key": "15:66",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 222,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 265,
      "juz_number": 14,
      "text": "وَقَضَيۡنَآ إِلَيۡهِ ذَٰلِكَ ٱلۡأَمۡرَ أَنَّ دَابِرَ هَٰٓؤُلَآءِ مَقۡطُوعٞ مُّصۡبِحِينَ"
    },
    {
      "id": 1869,
      "number": 67,
      "verse_key": "15:67",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 222,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 265,
      "juz_number": 14,
      "text": "وَجَآءَ أَهۡلُ ٱلۡمَدِينَةِ يَسۡتَبۡشِرُونَ"
    },
    {
      "id": 1870,
      "number": 68,
      "verse_key": "15:68",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 222,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 265,
      "juz_number": 14,
      "text": "قَالَ إِنَّ هَٰٓؤُلَآءِ ضَيۡفِي فَلَا تَفۡضَحُونِ"
    },
    {
      "id": 1871,
      "number": 69,
      "verse_key": "15:69",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 222,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 265,
      "juz_number": 14,
      "text": "وَٱتَّقُواْ ٱللَّهَ وَلَا تُخۡزُونِ"
    },
    {
      "id": 1809,
      "number": 7,
      "verse_key": "15:7",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 218,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 262,
      "juz_number": 14,
      "text": "لَّوۡمَا تَأۡتِينَا بِٱلۡمَلَٰٓئِكَةِ إِن كُنتَ مِنَ ٱلصَّٰدِقِينَ"
    },
    {
      "id": 1872,
      "number": 70,
      "verse_key": "15:70",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 222,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 265,
      "juz_number": 14,
      "text": "قَالُوٓاْ أَوَلَمۡ نَنۡهَكَ عَنِ ٱلۡعَٰلَمِينَ"
    },
    {
      "id": 1873,
      "number": 71,
      "verse_key": "15:71",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 222,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 266,
      "juz_number": 14,
      "text": "قَالَ هَٰٓؤُلَآءِ بَنَاتِيٓ إِن كُنتُمۡ فَٰعِلِينَ"
    },
    {
      "id": 1874,
      "number": 72,
      "verse_key": "15:72",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 222,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 266,
      "juz_number": 14,
      "text": "لَعَمۡرُكَ إِنَّهُمۡ لَفِي سَكۡرَتِهِمۡ يَعۡمَهُونَ"
    },
    {
      "id": 1875,
      "number": 73,
      "verse_key": "15:73",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 222,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 266,
      "juz_number": 14,
      "text": "فَأَخَذَتۡهُمُ ٱلصَّيۡحَةُ مُشۡرِقِينَ"
    },
    {
      "id": 1876,
      "number": 74,
      "verse_key": "15:74",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 222,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 266,
      "juz_number": 14,
      "text": "فَجَعَلۡنَا عَٰلِيَهَا سَافِلَهَا وَأَمۡطَرۡنَا عَلَيۡهِمۡ حِجَارَةٗ مِّن سِجِّيلٍ"
    },
    {
      "id": 1877,
      "number": 75,
      "verse_key": "15:75",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 222,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 266,
      "juz_number": 14,
      "text": "إِنَّ فِي ذَٰلِكَ لَأٓيَٰتٖ لِّلۡمُتَوَسِّمِينَ"
    },
    {
      "id": 1878,
      "number": 76,
      "verse_key": "15:76",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 222,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 266,
      "juz_number": 14,
      "text": "وَإِنَّهَا لَبِسَبِيلٖ مُّقِيمٍ"
    },
    {
      "id": 1879,
      "number": 77,
      "verse_key": "15:77",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 222,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 266,
      "juz_number": 14,
      "text": "إِنَّ فِي ذَٰلِكَ لَأٓيَةٗ لِّلۡمُؤۡمِنِينَ"
    },
    {
      "id": 1880,
      "number": 78,
      "verse_key": "15:78",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 222,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 266,
      "juz_number": 14,
      "text": "وَإِن كَانَ أَصۡحَٰبُ ٱلۡأَيۡكَةِ لَظَٰلِمِينَ"
    },
    {
      "id": 1881,
      "number": 79,
      "verse_key": "15:79",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 222,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 266,
      "juz_number": 14,
      "text": "فَٱنتَقَمۡنَا مِنۡهُمۡ وَإِنَّهُمَا لَبِإِمَامٖ مُّبِينٖ"
    },
    {
      "id": 1810,
      "number": 8,
      "verse_key": "15:8",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 218,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 262,
      "juz_number": 14,
      "text": "مَا نُنَزِّلُ ٱلۡمَلَٰٓئِكَةَ إِلَّا بِٱلۡحَقِّ وَمَا كَانُوٓاْ إِذٗا مُّنظَرِينَ"
    },
    {
      "id": 1882,
      "number": 80,
      "verse_key": "15:80",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 223,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 266,
      "juz_number": 14,
      "text": "وَلَقَدۡ كَذَّبَ أَصۡحَٰبُ ٱلۡحِجۡرِ ٱلۡمُرۡسَلِينَ"
    },
    {
      "id": 1883,
      "number": 81,
      "verse_key": "15:81",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 223,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 266,
      "juz_number": 14,
      "text": "وَءَاتَيۡنَٰهُمۡ ءَايَٰتِنَا فَكَانُواْ عَنۡهَا مُعۡرِضِينَ"
    },
    {
      "id": 1884,
      "number": 82,
      "verse_key": "15:82",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 223,
      "manzil_number": 3,
      "sajdah_number": null,
      "page_number": 266,
      "juz_number": 14,
