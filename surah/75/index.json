{
  "verses": [
    {
      "id": 5552,
      "number": 1,
      "verse_key": "75:1",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 577,
      "juz_number": 29,
      "text": "لَآ أُقۡسِمُ بِيَوۡمِ ٱلۡقِيَٰمَةِ"
    },
    {
      "id": 5561,
      "number": 10,
      "verse_key": "75:10",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 577,
      "juz_number": 29,
      "text": "يَقُولُ ٱلۡإِنسَٰنُ يَوۡمَئِذٍ أَيۡنَ ٱلۡمَفَرُّ"
    },
    {
      "id": 5562,
      "number": 11,
      "verse_key": "75:11",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 577,
      "juz_number": 29,
      "text": "كَلَّا لَا وَزَرَ"
    },
    {
      "id": 5563,
      "number": 12,
      "verse_key": "75:12",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 577,
      "juz_number": 29,
      "text": "إِلَىٰ رَبِّكَ يَوۡمَئِذٍ ٱلۡمُسۡتَقَرُّ"
    },
    {
      "id": 5564,
      "number": 13,
      "verse_key": "75:13",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 577,
      "juz_number": 29,
      "text": "يُنَبَّؤُاْ ٱلۡإِنسَٰنُ يَوۡمَئِذِۭ بِمَا قَدَّمَ وَأَخَّرَ"
    },
    {
      "id": 5565,
      "number": 14,
      "verse_key": "75:14",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 577,
      "juz_number": 29,
      "text": "بَلِ ٱلۡإِنسَٰنُ عَلَىٰ نَفۡسِهِۦ بَصِيرَةٞ"
    },
    {
      "id": 5566,
      "number": 15,
      "verse_key": "75:15",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 577,
      "juz_number": 29,
      "text": "وَلَوۡ أَلۡقَىٰ مَعَاذِيرَهُۥ"
    },
    {
      "id": 5567,
      "number": 16,
      "verse_key": "75:16",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 577,
      "juz_number": 29,
      "text": "لَا تُحَرِّكۡ بِهِۦ لِسَانَكَ لِتَعۡجَلَ بِهِۦٓ"
    },
    {
      "id": 5568,
      "number": 17,
      "verse_key": "75:17",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 577,
      "juz_number": 29,
      "text": "إِنَّ عَلَيۡنَا جَمۡعَهُۥ وَقُرۡءَانَهُۥ"
    },
    {
      "id": 5569,
      "number": 18,
      "verse_key": "75:18",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 577,
      "juz_number": 29,
      "text": "فَإِذَا قَرَأۡنَٰهُ فَٱتَّبِعۡ قُرۡءَانَهُۥ"
    },
    {
      "id": 5570,
      "number": 19,
      "verse_key": "75:19",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 577,
      "juz_number": 29,
      "text": "ثُمَّ إِنَّ عَلَيۡنَا بَيَانَهُۥ"
    },
    {
      "id": 5553,
      "number": 2,
      "verse_key": "75:2",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 577,
      "juz_number": 29,
      "text": "وَلَآ أُقۡسِمُ بِٱلنَّفۡسِ ٱللَّوَّامَةِ"
    },
    {
      "id": 5571,
      "number": 20,
      "verse_key": "75:20",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 578,
      "juz_number": 29,
      "text": "كَلَّا بَلۡ تُحِبُّونَ ٱلۡعَاجِلَةَ"
    },
    {
      "id": 5572,
      "number": 21,
      "verse_key": "75:21",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 578,
      "juz_number": 29,
      "text": "وَتَذَرُونَ ٱلۡأٓخِرَةَ"
    },
    {
      "id": 5573,
      "number": 22,
      "verse_key": "75:22",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 578,
      "juz_number": 29,
      "text": "وُجُوهٞ يَوۡمَئِذٖ نَّاضِرَةٌ"
    },
    {
      "id": 5574,
      "number": 23,
      "verse_key": "75:23",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 578,
      "juz_number": 29,
      "text": "إِلَىٰ رَبِّهَا نَاظِرَةٞ"
    },
    {
      "id": 5575,
      "number": 24,
      "verse_key": "75:24",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 578,
      "juz_number": 29,
      "text": "وَوُجُوهٞ يَوۡمَئِذِۭ بَاسِرَةٞ"
    },
    {
      "id": 5576,
      "number": 25,
      "verse_key": "75:25",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 578,
      "juz_number": 29,
      "text": "تَظُنُّ أَن يُفۡعَلَ بِهَا فَاقِرَةٞ"
    },
    {
      "id": 5577,
      "number": 26,
      "verse_key": "75:26",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 578,
      "juz_number": 29,
      "text": "كَلَّآ إِذَا بَلَغَتِ ٱلتَّرَاقِيَ"
    },
    {
      "id": 5578,
      "number": 27,
      "verse_key": "75:27",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 578,
      "juz_number": 29,
      "text": "وَقِيلَ مَنۡۜ رَاقٖ"
    },
    {
      "id": 5579,
      "number": 28,
      "verse_key": "75:28",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 578,
      "juz_number": 29,
      "text": "وَظَنَّ أَنَّهُ ٱلۡفِرَاقُ"
    },
    {
      "id": 5580,
      "number": 29,
      "verse_key": "75:29",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 578,
      "juz_number": 29,
      "text": "وَٱلۡتَفَّتِ ٱلسَّاقُ بِٱلسَّاقِ"
    },
    {
      "id": 5554,
      "number": 3,
      "verse_key": "75:3",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 577,
      "juz_number": 29,
      "text": "أَيَحۡسَبُ ٱلۡإِنسَٰنُ أَلَّن نَّجۡمَعَ عِظَامَهُۥ"
    },
    {
      "id": 5581,
      "number": 30,
      "verse_key": "75:30",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 578,
      "juz_number": 29,
      "text": "إِلَىٰ رَبِّكَ يَوۡمَئِذٍ ٱلۡمَسَاقُ"
    },
    {
      "id": 5582,
      "number": 31,
      "verse_key": "75:31",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 515,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 578,
      "juz_number": 29,
      "text": "فَلَا صَدَّقَ وَلَا صَلَّىٰ"
    },
    {
      "id": 5583,
      "number": 32,
      "verse_key": "75:32",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 515,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 578,
      "juz_number": 29,
      "text": "وَلَٰكِن كَذَّبَ وَتَوَلَّىٰ"
    },
    {
      "id": 5584,
      "number": 33,
      "verse_key": "75:33",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 515,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 578,
      "juz_number": 29,
      "text": "ثُمَّ ذَهَبَ إِلَىٰٓ أَهۡلِهِۦ يَتَمَطَّىٰٓ"
    },
    {
      "id": 5585,
      "number": 34,
      "verse_key": "75:34",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 515,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 578,
      "juz_number": 29,
      "text": "أَوۡلَىٰ لَكَ فَأَوۡلَىٰ"
    },
    {
      "id": 5586,
      "number": 35,
      "verse_key": "75:35",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 515,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 578,
      "juz_number": 29,
      "text": "ثُمَّ أَوۡلَىٰ لَكَ فَأَوۡلَىٰٓ"
    },
    {
      "id": 5587,
      "number": 36,
      "verse_key": "75:36",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 515,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 578,
      "juz_number": 29,
      "text": "أَيَحۡسَبُ ٱلۡإِنسَٰنُ أَن يُتۡرَكَ سُدًى"
    },
    {
      "id": 5588,
      "number": 37,
      "verse_key": "75:37",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 515,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 578,
      "juz_number": 29,
      "text": "أَلَمۡ يَكُ نُطۡفَةٗ مِّن مَّنِيّٖ يُمۡنَىٰ"
    },
    {
      "id": 5589,
      "number": 38,
      "verse_key": "75:38",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 515,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 578,
      "juz_number": 29,
      "text": "ثُمَّ كَانَ عَلَقَةٗ فَخَلَقَ فَسَوَّىٰ"
    },
    {
      "id": 5590,
      "number": 39,
      "verse_key": "75:39",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 515,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 578,
      "juz_number": 29,
      "text": "فَجَعَلَ مِنۡهُ ٱلزَّوۡجَيۡنِ ٱلذَّكَرَ وَٱلۡأُنثَىٰٓ"
    },
    {
      "id": 5555,
      "number": 4,
      "verse_key": "75:4",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 577,
      "juz_number": 29,
      "text": "بَلَىٰ قَٰدِرِينَ عَلَىٰٓ أَن نُّسَوِّيَ بَنَانَهُۥ"
    },
    {
      "id": 5591,
      "number": 40,
      "verse_key": "75:40",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 515,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 578,
      "juz_number": 29,
      "text": "أَلَيۡسَ ذَٰلِكَ بِقَٰدِرٍ عَلَىٰٓ أَن يُحۡـِۧيَ ٱلۡمَوۡتَىٰ"
    },
    {
      "id": 5556,
      "number": 5,
      "verse_key": "75:5",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 577,
      "juz_number": 29,
      "text": "بَلۡ يُرِيدُ ٱلۡإِنسَٰنُ لِيَفۡجُرَ أَمَامَهُۥ"
    },
    {
      "id": 5557,
      "number": 6,
      "verse_key": "75:6",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 577,
      "juz_number": 29,
      "text": "يَسۡـَٔلُ أَيَّانَ يَوۡمُ ٱلۡقِيَٰمَةِ"
    },
    {
      "id": 5558,
      "number": 7,
      "verse_key": "75:7",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 577,
      "juz_number": 29,
      "text": "فَإِذَا بَرِقَ ٱلۡبَصَرُ"
    },
    {
      "id": 5559,
      "number": 8,
      "verse_key": "75:8",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 577,
      "juz_number": 29,
      "text": "وَخَسَفَ ٱلۡقَمَرُ"
    },
    {
      "id": 5560,
      "number": 9,
      "verse_key": "75:9",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 577,
      "juz_number": 29,
      "text": "وَجُمِعَ ٱلشَّمۡسُ وَٱلۡقَمَرُ"
    }
  ]
}