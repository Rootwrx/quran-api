      "juz_number": 30,
      "text": "وَٱلتِّينِ وَٱلزَّيۡتُونِ"
    },
    {
      "id": 6100,
      "number": 2,
      "verse_key": "95:2",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 539,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 597,
      "juz_number": 30,
      "text": "وَطُورِ سِينِينَ"
    },
    {
      "id": 6101,
      "number": 3,
      "verse_key": "95:3",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 539,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 597,
      "juz_number": 30,
      "text": "وَهَٰذَا ٱلۡبَلَدِ ٱلۡأَمِينِ"
    },
    {
      "id": 6102,
      "number": 4,
      "verse_key": "95:4",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 539,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 597,
      "juz_number": 30,
      "text": "لَقَدۡ خَلَقۡنَا ٱلۡإِنسَٰنَ فِيٓ أَحۡسَنِ تَقۡوِيمٖ"
    },
    {
      "id": 6103,
      "number": 5,
      "verse_key": "95:5",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 539,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 597,
      "juz_number": 30,
      "text": "ثُمَّ رَدَدۡنَٰهُ أَسۡفَلَ سَٰفِلِينَ"
    },
    {
      "id": 6104,
      "number": 6,
      "verse_key": "95:6",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 539,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 597,
      "juz_number": 30,
      "text": "إِلَّا ٱلَّذِينَ ءَامَنُواْ وَعَمِلُواْ ٱلصَّٰلِحَٰتِ فَلَهُمۡ أَجۡرٌ غَيۡرُ مَمۡنُونٖ"
    },
    {
      "id": 6105,
      "number": 7,
      "verse_key": "95:7",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 539,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 597,
      "juz_number": 30,
      "text": "فَمَا يُكَذِّبُكَ بَعۡدُ بِٱلدِّينِ"
    },
    {
      "id": 6106,
      "number": 8,
      "verse_key": "95:8",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 539,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 597,
      "juz_number": 30,
      "text": "أَلَيۡسَ ٱللَّهُ بِأَحۡكَمِ ٱلۡحَٰكِمِينَ"
    }
  ]
