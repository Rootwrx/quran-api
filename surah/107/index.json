      "text": "أَرَءَيۡتَ ٱلَّذِي يُكَذِّبُ بِٱلدِّينِ"
    },
    {
      "id": 6199,
      "number": 2,
      "verse_key": "107:2",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 551,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 602,
      "juz_number": 30,
      "text": "فَذَٰلِكَ ٱلَّذِي يَدُعُّ ٱلۡيَتِيمَ"
    },
    {
      "id": 6200,
      "number": 3,
      "verse_key": "107:3",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 551,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 602,
      "juz_number": 30,
      "text": "وَلَا يَحُضُّ عَلَىٰ طَعَامِ ٱلۡمِسۡكِينِ"
    },
    {
      "id": 6201,
      "number": 4,
      "verse_key": "107:4",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 551,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 602,
      "juz_number": 30,
      "text": "فَوَيۡلٞ لِّلۡمُصَلِّينَ"
