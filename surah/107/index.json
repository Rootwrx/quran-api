      "rub_el_hizb_number": 240,
      "ruku_number": 551,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 602,
      "juz_number": 30,
      "text": "ٱلَّذِينَ هُمۡ يُرَآءُونَ"
    },
    {
      "id": 6204,
      "number": 7,
      "verse_key": "107:7",
      "hizb_number": 60,
