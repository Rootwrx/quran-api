    {
      "id": 6202,
      "number": 5,
      "verse_key": "107:5",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 551,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 602,
      "juz_number": 30,
      "text": "ٱلَّذِينَ هُمۡ عَن صَلَاتِهِمۡ سَاهُونَ"
