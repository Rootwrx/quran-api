{
  "verses": [
    {
      "id": 5272,
      "number": 1,
      "verse_key": "68:1",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 564,
      "juz_number": 29,
      "text": "نٓۚ وَٱلۡقَلَمِ وَمَا يَسۡطُرُونَ"
    },
    {
      "id": 5281,
      "number": 10,
      "verse_key": "68:10",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 564,
      "juz_number": 29,
      "text": "وَلَا تُطِعۡ كُلَّ حَلَّافٖ مَّهِينٍ"
    },
    {
      "id": 5282,
      "number": 11,
      "verse_key": "68:11",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 564,
      "juz_number": 29,
      "text": "هَمَّازٖ مَّشَّآءِۭ بِنَمِيمٖ"
    },
    {
      "id": 5283,
      "number": 12,
      "verse_key": "68:12",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 564,
      "juz_number": 29,
      "text": "مَّنَّاعٖ لِّلۡخَيۡرِ مُعۡتَدٍ أَثِيمٍ"
    },
    {
      "id": 5284,
      "number": 13,
      "verse_key": "68:13",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 564,
      "juz_number": 29,
      "text": "عُتُلِّۭ بَعۡدَ ذَٰلِكَ زَنِيمٍ"
    },
    {
      "id": 5285,
      "number": 14,
      "verse_key": "68:14",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 564,
      "juz_number": 29,
      "text": "أَن كَانَ ذَا مَالٖ وَبَنِينَ"
    },
    {
      "id": 5286,
      "number": 15,
      "verse_key": "68:15",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 564,
      "juz_number": 29,
      "text": "إِذَا تُتۡلَىٰ عَلَيۡهِ ءَايَٰتُنَا قَالَ أَسَٰطِيرُ ٱلۡأَوَّلِينَ"
    },
    {
      "id": 5287,
      "number": 16,
      "verse_key": "68:16",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 565,
      "juz_number": 29,
      "text": "سَنَسِمُهُۥ عَلَى ٱلۡخُرۡطُومِ"
    },
    {
      "id": 5288,
      "number": 17,
      "verse_key": "68:17",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 565,
      "juz_number": 29,
      "text": "إِنَّا بَلَوۡنَٰهُمۡ كَمَا بَلَوۡنَآ أَصۡحَٰبَ ٱلۡجَنَّةِ إِذۡ أَقۡسَمُواْ لَيَصۡرِمُنَّهَا مُصۡبِحِينَ"
    },
    {
      "id": 5289,
      "number": 18,
      "verse_key": "68:18",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 565,
      "juz_number": 29,
      "text": "وَلَا يَسۡتَثۡنُونَ"
    },
    {
      "id": 5290,
      "number": 19,
      "verse_key": "68:19",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 565,
      "juz_number": 29,
      "text": "فَطَافَ عَلَيۡهَا طَآئِفٞ مِّن رَّبِّكَ وَهُمۡ نَآئِمُونَ"
    },
    {
      "id": 5273,
      "number": 2,
      "verse_key": "68:2",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 564,
      "juz_number": 29,
      "text": "مَآ أَنتَ بِنِعۡمَةِ رَبِّكَ بِمَجۡنُونٖ"
    },
    {
      "id": 5291,
      "number": 20,
      "verse_key": "68:20",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 565,
      "juz_number": 29,
      "text": "فَأَصۡبَحَتۡ كَٱلصَّرِيمِ"
    },
    {
      "id": 5292,
      "number": 21,
      "verse_key": "68:21",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 565,
      "juz_number": 29,
      "text": "فَتَنَادَوۡاْ مُصۡبِحِينَ"
    },
    {
      "id": 5293,
      "number": 22,
      "verse_key": "68:22",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 565,
      "juz_number": 29,
      "text": "أَنِ ٱغۡدُواْ عَلَىٰ حَرۡثِكُمۡ إِن كُنتُمۡ صَٰرِمِينَ"
    },
    {
      "id": 5294,
      "number": 23,
      "verse_key": "68:23",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 565,
      "juz_number": 29,
      "text": "فَٱنطَلَقُواْ وَهُمۡ يَتَخَٰفَتُونَ"
    },
    {
      "id": 5295,
      "number": 24,
      "verse_key": "68:24",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 565,
      "juz_number": 29,
      "text": "أَن لَّا يَدۡخُلَنَّهَا ٱلۡيَوۡمَ عَلَيۡكُم مِّسۡكِينٞ"
    },
    {
      "id": 5296,
      "number": 25,
      "verse_key": "68:25",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 565,
      "juz_number": 29,
      "text": "وَغَدَوۡاْ عَلَىٰ حَرۡدٖ قَٰدِرِينَ"
    },
    {
      "id": 5297,
      "number": 26,
      "verse_key": "68:26",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 565,
      "juz_number": 29,
      "text": "فَلَمَّا رَأَوۡهَا قَالُوٓاْ إِنَّا لَضَآلُّونَ"
    },
    {
      "id": 5298,
      "number": 27,
      "verse_key": "68:27",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 565,
      "juz_number": 29,
      "text": "بَلۡ نَحۡنُ مَحۡرُومُونَ"
    },
    {
      "id": 5299,
      "number": 28,
      "verse_key": "68:28",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 565,
      "juz_number": 29,
      "text": "قَالَ أَوۡسَطُهُمۡ أَلَمۡ أَقُل لَّكُمۡ لَوۡلَا تُسَبِّحُونَ"
    },
    {
      "id": 5300,
      "number": 29,
      "verse_key": "68:29",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 565,
      "juz_number": 29,
      "text": "قَالُواْ سُبۡحَٰنَ رَبِّنَآ إِنَّا كُنَّا ظَٰلِمِينَ"
    },
    {
      "id": 5274,
      "number": 3,
      "verse_key": "68:3",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 564,
      "juz_number": 29,
      "text": "وَإِنَّ لَكَ لَأَجۡرًا غَيۡرَ مَمۡنُونٖ"
    },
    {
      "id": 5301,
      "number": 30,
      "verse_key": "68:30",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 565,
      "juz_number": 29,
      "text": "فَأَقۡبَلَ بَعۡضُهُمۡ عَلَىٰ بَعۡضٖ يَتَلَٰوَمُونَ"
    },
    {
      "id": 5302,
      "number": 31,
      "verse_key": "68:31",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 565,
      "juz_number": 29,
      "text": "قَالُواْ يَٰوَيۡلَنَآ إِنَّا كُنَّا طَٰغِينَ"
    },
    {
      "id": 5303,
      "number": 32,
      "verse_key": "68:32",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 565,
      "juz_number": 29,
      "text": "عَسَىٰ رَبُّنَآ أَن يُبۡدِلَنَا خَيۡرٗا مِّنۡهَآ إِنَّآ إِلَىٰ رَبِّنَا رَٰغِبُونَ"
    },
    {
      "id": 5304,
      "number": 33,
      "verse_key": "68:33",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 565,
      "juz_number": 29,
      "text": "كَذَٰلِكَ ٱلۡعَذَابُۖ وَلَعَذَابُ ٱلۡأٓخِرَةِ أَكۡبَرُۚ لَوۡ كَانُواْ يَعۡلَمُونَ"
    },
    {
      "id": 5305,
      "number": 34,
      "verse_key": "68:34",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 501,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 565,
      "juz_number": 29,
      "text": "إِنَّ لِلۡمُتَّقِينَ عِندَ رَبِّهِمۡ جَنَّٰتِ ٱلنَّعِيمِ"
    },
    {
      "id": 5306,
      "number": 35,
      "verse_key": "68:35",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 501,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 565,
      "juz_number": 29,
      "text": "أَفَنَجۡعَلُ ٱلۡمُسۡلِمِينَ كَٱلۡمُجۡرِمِينَ"
    },
    {
      "id": 5307,
      "number": 36,
      "verse_key": "68:36",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 501,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 565,
      "juz_number": 29,
      "text": "مَا لَكُمۡ كَيۡفَ تَحۡكُمُونَ"
    },
    {
      "id": 5308,
      "number": 37,
      "verse_key": "68:37",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 501,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 565,
      "juz_number": 29,
      "text": "أَمۡ لَكُمۡ كِتَٰبٞ فِيهِ تَدۡرُسُونَ"
    },
    {
      "id": 5309,
      "number": 38,
      "verse_key": "68:38",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 501,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 565,
      "juz_number": 29,
      "text": "إِنَّ لَكُمۡ فِيهِ لَمَا تَخَيَّرُونَ"
    },
    {
      "id": 5310,
      "number": 39,
      "verse_key": "68:39",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 501,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 565,
      "juz_number": 29,
      "text": "أَمۡ لَكُمۡ أَيۡمَٰنٌ عَلَيۡنَا بَٰلِغَةٌ إِلَىٰ يَوۡمِ ٱلۡقِيَٰمَةِ إِنَّ لَكُمۡ لَمَا تَحۡكُمُونَ"
    },
    {
      "id": 5275,
      "number": 4,
      "verse_key": "68:4",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 564,
      "juz_number": 29,
      "text": "وَإِنَّكَ لَعَلَىٰ خُلُقٍ عَظِيمٖ"
    },
    {
      "id": 5311,
      "number": 40,
      "verse_key": "68:40",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 501,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 565,
      "juz_number": 29,
      "text": "سَلۡهُمۡ أَيُّهُم بِذَٰلِكَ زَعِيمٌ"
    },
    {
      "id": 5312,
      "number": 41,
      "verse_key": "68:41",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 501,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 565,
      "juz_number": 29,
      "text": "أَمۡ لَهُمۡ شُرَكَآءُ فَلۡيَأۡتُواْ بِشُرَكَآئِهِمۡ إِن كَانُواْ صَٰدِقِينَ"
    },
    {
      "id": 5313,
      "number": 42,
      "verse_key": "68:42",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 501,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 565,
      "juz_number": 29,
      "text": "يَوۡمَ يُكۡشَفُ عَن سَاقٖ وَيُدۡعَوۡنَ إِلَى ٱلسُّجُودِ فَلَا يَسۡتَطِيعُونَ"
    },
    {
      "id": 5314,
      "number": 43,
      "verse_key": "68:43",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 501,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 566,
      "juz_number": 29,
      "text": "خَٰشِعَةً أَبۡصَٰرُهُمۡ تَرۡهَقُهُمۡ ذِلَّةٞۖ وَقَدۡ كَانُواْ يُدۡعَوۡنَ إِلَى ٱلسُّجُودِ وَهُمۡ سَٰلِمُونَ"
    },
    {
      "id": 5315,
      "number": 44,
      "verse_key": "68:44",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 501,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 566,
      "juz_number": 29,
      "text": "فَذَرۡنِي وَمَن يُكَذِّبُ بِهَٰذَا ٱلۡحَدِيثِۖ سَنَسۡتَدۡرِجُهُم مِّنۡ حَيۡثُ لَا يَعۡلَمُونَ"
    },
    {
      "id": 5316,
      "number": 45,
      "verse_key": "68:45",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 501,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 566,
      "juz_number": 29,
      "text": "وَأُمۡلِي لَهُمۡۚ إِنَّ كَيۡدِي مَتِينٌ"
    },
    {
      "id": 5317,
      "number": 46,
      "verse_key": "68:46",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 501,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 566,
      "juz_number": 29,
      "text": "أَمۡ تَسۡـَٔلُهُمۡ أَجۡرٗا فَهُم مِّن مَّغۡرَمٖ مُّثۡقَلُونَ"
    },
    {
      "id": 5318,
      "number": 47,
      "verse_key": "68:47",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 501,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 566,
      "juz_number": 29,
      "text": "أَمۡ عِندَهُمُ ٱلۡغَيۡبُ فَهُمۡ يَكۡتُبُونَ"
    },
    {
      "id": 5319,
      "number": 48,
      "verse_key": "68:48",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 501,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 566,
      "juz_number": 29,
      "text": "فَٱصۡبِرۡ لِحُكۡمِ رَبِّكَ وَلَا تَكُن كَصَاحِبِ ٱلۡحُوتِ إِذۡ نَادَىٰ وَهُوَ مَكۡظُومٞ"
    },
    {
      "id": 5320,
