{
  "verses": [
    {
      "id": 6044,
      "number": 1,
      "verse_key": "91:1",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 535,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 595,
      "juz_number": 30,
      "text": "وَٱلشَّمۡسِ وَضُحَىٰهَا"
    },
    {
      "id": 6053,
      "number": 10,
      "verse_key": "91:10",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 535,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 595,
      "juz_number": 30,
      "text": "وَقَدۡ خَابَ مَن دَسَّىٰهَا"
    },
    {
      "id": 6054,
      "number": 11,
      "verse_key": "91:11",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 535,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 595,
      "juz_number": 30,
      "text": "كَذَّبَتۡ ثَمُودُ بِطَغۡوَىٰهَآ"
    },
    {
      "id": 6055,
      "number": 12,
      "verse_key": "91:12",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 535,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 595,
      "juz_number": 30,
      "text": "إِذِ ٱنۢبَعَثَ أَشۡقَىٰهَا"
    },
    {
      "id": 6056,
      "number": 13,
      "verse_key": "91:13",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 535,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 595,
      "juz_number": 30,
      "text": "فَقَالَ لَهُمۡ رَسُولُ ٱللَّهِ نَاقَةَ ٱللَّهِ وَسُقۡيَٰهَا"
    },
    {
      "id": 6057,
      "number": 14,
      "verse_key": "91:14",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 535,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 595,
      "juz_number": 30,
      "text": "فَكَذَّبُوهُ فَعَقَرُوهَا فَدَمۡدَمَ عَلَيۡهِمۡ رَبُّهُم بِذَنۢبِهِمۡ فَسَوَّىٰهَا"
    },
    {
      "id": 6058,
      "number": 15,
      "verse_key": "91:15",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 535,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 595,
      "juz_number": 30,
      "text": "وَلَا يَخَافُ عُقۡبَٰهَا"
    },
    {
      "id": 6045,
      "number": 2,
      "verse_key": "91:2",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 535,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 595,
      "juz_number": 30,
      "text": "وَٱلۡقَمَرِ إِذَا تَلَىٰهَا"
    },
    {
      "id": 6046,
      "number": 3,
      "verse_key": "91:3",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 535,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 595,
      "juz_number": 30,
      "text": "وَٱلنَّهَارِ إِذَا جَلَّىٰهَا"
    },
    {
      "id": 6047,
      "number": 4,
      "verse_key": "91:4",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 535,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 595,
      "juz_number": 30,
      "text": "وَٱلَّيۡلِ إِذَا يَغۡشَىٰهَا"
    },
    {
      "id": 6048,
      "number": 5,
      "verse_key": "91:5",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 535,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 595,
      "juz_number": 30,
      "text": "وَٱلسَّمَآءِ وَمَا بَنَىٰهَا"
    },
    {
      "id": 6049,
      "number": 6,
      "verse_key": "91:6",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 535,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 595,
      "juz_number": 30,
      "text": "وَٱلۡأَرۡضِ وَمَا طَحَىٰهَا"
    },
    {
      "id": 6050,
      "number": 7,
      "verse_key": "91:7",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 535,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 595,
      "juz_number": 30,
      "text": "وَنَفۡسٖ وَمَا سَوَّىٰهَا"
    },
    {
      "id": 6051,
      "number": 8,
      "verse_key": "91:8",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 535,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 595,
      "juz_number": 30,
      "text": "فَأَلۡهَمَهَا فُجُورَهَا وَتَقۡوَىٰهَا"
    },
    {
      "id": 6052,
      "number": 9,
      "verse_key": "91:9",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 535,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 595,
      "juz_number": 30,
      "text": "قَدۡ أَفۡلَحَ مَن زَكَّىٰهَا"
    }
  ]
}