{
  "verses": [
    {
      "id": 5994,
      "number": 1,
      "verse_key": "89:1",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 593,
      "juz_number": 30,
      "text": "وَٱلۡفَجۡرِ"
    },
    {
      "id": 6003,
      "number": 10,
      "verse_key": "89:10",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 593,
      "juz_number": 30,
      "text": "وَفِرۡعَوۡنَ ذِي ٱلۡأَوۡتَادِ"
    },
    {
      "id": 6004,
      "number": 11,
      "verse_key": "89:11",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 593,
      "juz_number": 30,
      "text": "ٱلَّذِينَ طَغَوۡاْ فِي ٱلۡبِلَٰدِ"
    },
    {
      "id": 6005,
      "number": 12,
      "verse_key": "89:12",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 593,
      "juz_number": 30,
      "text": "فَأَكۡثَرُواْ فِيهَا ٱلۡفَسَادَ"
    },
    {
      "id": 6006,
      "number": 13,
      "verse_key": "89:13",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 593,
      "juz_number": 30,
      "text": "فَصَبَّ عَلَيۡهِمۡ رَبُّكَ سَوۡطَ عَذَابٍ"
    },
    {
      "id": 6007,
      "number": 14,
      "verse_key": "89:14",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 593,
      "juz_number": 30,
      "text": "إِنَّ رَبَّكَ لَبِٱلۡمِرۡصَادِ"
    },
    {
      "id": 6008,
      "number": 15,
      "verse_key": "89:15",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 593,
      "juz_number": 30,
      "text": "فَأَمَّا ٱلۡإِنسَٰنُ إِذَا مَا ٱبۡتَلَىٰهُ رَبُّهُۥ فَأَكۡرَمَهُۥ وَنَعَّمَهُۥ فَيَقُولُ رَبِّيٓ أَكۡرَمَنِ"
    },
    {
      "id": 6009,
      "number": 16,
      "verse_key": "89:16",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 593,
      "juz_number": 30,
      "text": "وَأَمَّآ إِذَا مَا ٱبۡتَلَىٰهُ فَقَدَرَ عَلَيۡهِ رِزۡقَهُۥ فَيَقُولُ رَبِّيٓ أَهَٰنَنِ"
    },
    {
      "id": 6010,
      "number": 17,
      "verse_key": "89:17",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 593,
      "juz_number": 30,
      "text": "كَلَّاۖ بَل لَّا تُكۡرِمُونَ ٱلۡيَتِيمَ"
    },
    {
      "id": 6011,
      "number": 18,
      "verse_key": "89:18",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 593,
      "juz_number": 30,
      "text": "وَلَا تَحَٰٓضُّونَ عَلَىٰ طَعَامِ ٱلۡمِسۡكِينِ"
    },
    {
      "id": 6012,
      "number": 19,
      "verse_key": "89:19",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 593,
      "juz_number": 30,
      "text": "وَتَأۡكُلُونَ ٱلتُّرَاثَ أَكۡلٗا لَّمّٗا"
    },
    {
      "id": 5995,
      "number": 2,
      "verse_key": "89:2",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 593,
      "juz_number": 30,
      "text": "وَلَيَالٍ عَشۡرٖ"
    },
    {
      "id": 6013,
      "number": 20,
      "verse_key": "89:20",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 593,
      "juz_number": 30,
      "text": "وَتُحِبُّونَ ٱلۡمَالَ حُبّٗا جَمّٗا"
    },
    {
      "id": 6014,
      "number": 21,
      "verse_key": "89:21",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 593,
      "juz_number": 30,
      "text": "كَلَّآۖ إِذَا دُكَّتِ ٱلۡأَرۡضُ دَكّٗا دَكّٗا"
    },
    {
      "id": 6015,
      "number": 22,
      "verse_key": "89:22",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 593,
      "juz_number": 30,
      "text": "وَجَآءَ رَبُّكَ وَٱلۡمَلَكُ صَفّٗا صَفّٗا"
    },
    {
      "id": 6016,
      "number": 23,
      "verse_key": "89:23",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 593,
      "juz_number": 30,
      "text": "وَجِاْيٓءَ يَوۡمَئِذِۭ بِجَهَنَّمَۚ يَوۡمَئِذٖ يَتَذَكَّرُ ٱلۡإِنسَٰنُ وَأَنَّىٰ لَهُ ٱلذِّكۡرَىٰ"
    },
    {
      "id": 6017,
      "number": 24,
      "verse_key": "89:24",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 594,
      "juz_number": 30,
      "text": "يَقُولُ يَٰلَيۡتَنِي قَدَّمۡتُ لِحَيَاتِي"
    },
    {
      "id": 6018,
      "number": 25,
      "verse_key": "89:25",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 594,
      "juz_number": 30,
      "text": "فَيَوۡمَئِذٖ لَّا يُعَذِّبُ عَذَابَهُۥٓ أَحَدٞ"
    },
    {
      "id": 6019,
      "number": 26,
      "verse_key": "89:26",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 594,
      "juz_number": 30,
      "text": "وَلَا يُوثِقُ وَثَاقَهُۥٓ أَحَدٞ"
    },
    {
      "id": 6020,
      "number": 27,
      "verse_key": "89:27",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 594,
      "juz_number": 30,
      "text": "يَٰٓأَيَّتُهَا ٱلنَّفۡسُ ٱلۡمُطۡمَئِنَّةُ"
    },
    {
      "id": 6021,
      "number": 28,
      "verse_key": "89:28",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 594,
      "juz_number": 30,
      "text": "ٱرۡجِعِيٓ إِلَىٰ رَبِّكِ رَاضِيَةٗ مَّرۡضِيَّةٗ"
    },
    {
      "id": 6022,
      "number": 29,
      "verse_key": "89:29",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 594,
      "juz_number": 30,
      "text": "فَٱدۡخُلِي فِي عِبَٰدِي"
    },
    {
      "id": 5996,
      "number": 3,
      "verse_key": "89:3",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 593,
      "juz_number": 30,
      "text": "وَٱلشَّفۡعِ وَٱلۡوَتۡرِ"
    },
    {
      "id": 6023,
      "number": 30,
      "verse_key": "89:30",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 594,
      "juz_number": 30,
      "text": "وَٱدۡخُلِي جَنَّتِي"
    },
    {
      "id": 5997,
      "number": 4,
      "verse_key": "89:4",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 593,
      "juz_number": 30,
      "text": "وَٱلَّيۡلِ إِذَا يَسۡرِ"
    },
    {
      "id": 5998,
      "number": 5,
      "verse_key": "89:5",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 593,
      "juz_number": 30,
      "text": "هَلۡ فِي ذَٰلِكَ قَسَمٞ لِّذِي حِجۡرٍ"
    },
    {
      "id": 5999,
      "number": 6,
      "verse_key": "89:6",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 593,
      "juz_number": 30,
      "text": "أَلَمۡ تَرَ كَيۡفَ فَعَلَ رَبُّكَ بِعَادٍ"
    },
    {
      "id": 6000,
      "number": 7,
      "verse_key": "89:7",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 593,
      "juz_number": 30,
      "text": "إِرَمَ ذَاتِ ٱلۡعِمَادِ"
    },
    {
      "id": 6001,
      "number": 8,
      "verse_key": "89:8",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 593,
      "juz_number": 30,
      "text": "ٱلَّتِي لَمۡ يُخۡلَقۡ مِثۡلُهَا فِي ٱلۡبِلَٰدِ"
    },
    {
      "id": 6002,
      "number": 9,
      "verse_key": "89:9",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 593,
      "juz_number": 30,
      "text": "وَثَمُودَ ٱلَّذِينَ جَابُواْ ٱلصَّخۡرَ بِٱلۡوَادِ"
    }
  ]
}