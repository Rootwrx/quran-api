{
  "verses": [
    {
      "id": 5623,
      "number": 1,
      "verse_key": "77:1",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 580,
      "juz_number": 29,
      "text": "وَٱلۡمُرۡسَلَٰتِ عُرۡفٗا"
    },
    {
      "id": 5624,
      "number": 2,
      "verse_key": "77:2",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 580,
      "juz_number": 29,
      "text": "فَٱلۡعَٰصِفَٰتِ عَصۡفٗا"
    },
    {
      "id": 5625,
      "number": 3,
      "verse_key": "77:3",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 580,
      "juz_number": 29,
      "text": "وَٱلنَّٰشِرَٰتِ نَشۡرٗا"
    },
    {
      "id": 5626,
      "number": 4,
      "verse_key": "77:4",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 580,
      "juz_number": 29,
      "text": "فَٱلۡفَٰرِقَٰتِ فَرۡقٗا"
    },
    {
      "id": 5627,
      "number": 5,
      "verse_key": "77:5",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 580,
      "juz_number": 29,
      "text": "فَٱلۡمُلۡقِيَٰتِ ذِكۡرًا"
    },
    {
      "id": 5628,
      "number": 6,
      "verse_key": "77:6",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 580,
      "juz_number": 29,
      "text": "عُذۡرًا أَوۡ نُذۡرًا"
    },
    {
      "id": 5629,
      "number": 7,
      "verse_key": "77:7",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 580,
      "juz_number": 29,
      "text": "إِنَّمَا تُوعَدُونَ لَوَٰقِعٞ"
    },
    {
      "id": 5630,
      "number": 8,
      "verse_key": "77:8",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 580,
      "juz_number": 29,
      "text": "فَإِذَا ٱلنُّجُومُ طُمِسَتۡ"
    },
    {
      "id": 5631,
      "number": 9,
      "verse_key": "77:9",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 580,
      "juz_number": 29,
      "text": "وَإِذَا ٱلسَّمَآءُ فُرِجَتۡ"
    },
    {
      "id": 5632,
      "number": 10,
      "verse_key": "77:10",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 580,
      "juz_number": 29,
      "text": "وَإِذَا ٱلۡجِبَالُ نُسِفَتۡ"
    },
    {
      "id": 5633,
      "number": 11,
      "verse_key": "77:11",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 580,
      "juz_number": 29,
      "text": "وَإِذَا ٱلرُّسُلُ أُقِّتَتۡ"
    },
    {
      "id": 5634,
      "number": 12,
      "verse_key": "77:12",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 580,
      "juz_number": 29,
      "text": "لِأَيِّ يَوۡمٍ أُجِّلَتۡ"
    },
    {
      "id": 5635,
      "number": 13,
      "verse_key": "77:13",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 580,
      "juz_number": 29,
      "text": "لِيَوۡمِ ٱلۡفَصۡلِ"
    },
    {
      "id": 5636,
      "number": 14,
      "verse_key": "77:14",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 580,
      "juz_number": 29,
      "text": "وَمَآ أَدۡرَىٰكَ مَا يَوۡمُ ٱلۡفَصۡلِ"
    },
    {
      "id": 5637,
      "number": 15,
      "verse_key": "77:15",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 580,
      "juz_number": 29,
      "text": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ"
    },
    {
      "id": 5638,
      "number": 16,
      "verse_key": "77:16",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 580,
      "juz_number": 29,
      "text": "أَلَمۡ نُهۡلِكِ ٱلۡأَوَّلِينَ"
    },
    {
      "id": 5639,
      "number": 17,
      "verse_key": "77:17",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 580,
      "juz_number": 29,
      "text": "ثُمَّ نُتۡبِعُهُمُ ٱلۡأٓخِرِينَ"
    },
    {
      "id": 5640,
      "number": 18,
      "verse_key": "77:18",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 580,
      "juz_number": 29,
      "text": "كَذَٰلِكَ نَفۡعَلُ بِٱلۡمُجۡرِمِينَ"
    },
    {
      "id": 5641,
      "number": 19,
      "verse_key": "77:19",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 580,
      "juz_number": 29,
      "text": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ"
    },
    {
      "id": 5642,
      "number": 20,
      "verse_key": "77:20",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "text": "أَلَمۡ نَخۡلُقكُّم مِّن مَّآءٖ مَّهِينٖ"
    },
    {
      "id": 5643,
      "number": 21,
      "verse_key": "77:21",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "text": "فَجَعَلۡنَٰهُ فِي قَرَارٖ مَّكِينٍ"
    },
    {
      "id": 5644,
      "number": 22,
      "verse_key": "77:22",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "text": "إِلَىٰ قَدَرٖ مَّعۡلُومٖ"
    },
    {
      "id": 5645,
      "number": 23,
      "verse_key": "77:23",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "text": "فَقَدَرۡنَا فَنِعۡمَ ٱلۡقَٰدِرُونَ"
    },
    {
      "id": 5646,
      "number": 24,
      "verse_key": "77:24",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "text": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ"
    },
    {
      "id": 5647,
      "number": 25,
      "verse_key": "77:25",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "text": "أَلَمۡ نَجۡعَلِ ٱلۡأَرۡضَ كِفَاتًا"
    },
    {
      "id": 5648,
      "number": 26,
      "verse_key": "77:26",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "text": "أَحۡيَآءٗ وَأَمۡوَٰتٗا"
    },
    {
      "id": 5649,
      "number": 27,
      "verse_key": "77:27",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "text": "وَجَعَلۡنَا فِيهَا رَوَٰسِيَ شَٰمِخَٰتٖ وَأَسۡقَيۡنَٰكُم مَّآءٗ فُرَاتٗا"
    },
    {
      "id": 5650,
      "number": 28,
      "verse_key": "77:28",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "text": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ"
    },
    {
      "id": 5651,
      "number": 29,
      "verse_key": "77:29",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "text": "ٱنطَلِقُوٓاْ إِلَىٰ مَا كُنتُم بِهِۦ تُكَذِّبُونَ"
    },
    {
      "id": 5652,
      "number": 30,
      "verse_key": "77:30",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "text": "ٱنطَلِقُوٓاْ إِلَىٰ ظِلّٖ ذِي ثَلَٰثِ شُعَبٖ"
    },
    {
      "id": 5653,
      "number": 31,
      "verse_key": "77:31",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "text": "لَّا ظَلِيلٖ وَلَا يُغۡنِي مِنَ ٱللَّهَبِ"
    },
    {
      "id": 5654,
      "number": 32,
      "verse_key": "77:32",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "text": "إِنَّهَا تَرۡمِي بِشَرَرٖ كَٱلۡقَصۡرِ"
    },
    {
      "id": 5655,
      "number": 33,
      "verse_key": "77:33",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "text": "كَأَنَّهُۥ جِمَٰلَتٞ صُفۡرٞ"
    },
    {
      "id": 5656,
      "number": 34,
      "verse_key": "77:34",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "text": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ"
    },
    {
      "id": 5657,
      "number": 35,
      "verse_key": "77:35",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "text": "هَٰذَا يَوۡمُ لَا يَنطِقُونَ"
    },
    {
      "id": 5658,
      "number": 36,
      "verse_key": "77:36",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "text": "وَلَا يُؤۡذَنُ لَهُمۡ فَيَعۡتَذِرُونَ"
    },
    {
      "id": 5659,
      "number": 37,
      "verse_key": "77:37",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "text": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ"
    },
    {
      "id": 5660,
      "number": 38,
      "verse_key": "77:38",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "text": "هَٰذَا يَوۡمُ ٱلۡفَصۡلِۖ جَمَعۡنَٰكُمۡ وَٱلۡأَوَّلِينَ"
    },
    {
      "id": 5661,
      "number": 39,
      "verse_key": "77:39",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "text": "فَإِن كَانَ لَكُمۡ كَيۡدٞ فَكِيدُونِ"
    },
    {
      "id": 5662,
      "number": 40,
      "verse_key": "77:40",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "text": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ"
    },
    {
      "id": 5663,
      "number": 41,
      "verse_key": "77:41",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 519,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "text": "إِنَّ ٱلۡمُتَّقِينَ فِي ظِلَٰلٖ وَعُيُونٖ"
    },
    {
      "id": 5664,
      "number": 42,
      "verse_key": "77:42",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 519,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "text": "وَفَوَٰكِهَ مِمَّا يَشۡتَهُونَ"
    },
    {
      "id": 5665,
      "number": 43,
      "verse_key": "77:43",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 519,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "text": "كُلُواْ وَٱشۡرَبُواْ هَنِيٓـَٔۢا بِمَا كُنتُمۡ تَعۡمَلُونَ"
    },
    {
      "id": 5666,
      "number": 44,
      "verse_key": "77:44",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 519,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "text": "إِنَّا كَذَٰلِكَ نَجۡزِي ٱلۡمُحۡسِنِينَ"
    },
    {
      "id": 5667,
      "number": 45,
      "verse_key": "77:45",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 519,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "text": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ"
    },
    {
      "id": 5668,
      "number": 46,
      "verse_key": "77:46",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 519,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "text": "كُلُواْ وَتَمَتَّعُواْ قَلِيلًا إِنَّكُم مُّجۡرِمُونَ"
    },
    {
      "id": 5669,
      "number": 47,
      "verse_key": "77:47",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 519,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "text": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ"
    },
    {
