{
  "verses": [
    {
      "id": 5968,
      "number": 1,
      "verse_key": "88:1",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "text": "هَلۡ أَتَىٰكَ حَدِيثُ ٱلۡغَٰشِيَةِ"
    },
    {
      "id": 5969,
      "number": 2,
      "verse_key": "88:2",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "text": "وُجُوهٞ يَوۡمَئِذٍ خَٰشِعَةٌ"
    },
    {
      "id": 5970,
      "number": 3,
      "verse_key": "88:3",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "text": "عَامِلَةٞ نَّاصِبَةٞ"
    },
    {
      "id": 5971,
      "number": 4,
      "verse_key": "88:4",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "text": "تَصۡلَىٰ نَارًا حَامِيَةٗ"
    },
    {
      "id": 5972,
      "number": 5,
      "verse_key": "88:5",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "text": "تُسۡقَىٰ مِنۡ عَيۡنٍ ءَانِيَةٖ"
    },
    {
      "id": 5973,
      "number": 6,
      "verse_key": "88:6",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "text": "لَّيۡسَ لَهُمۡ طَعَامٌ إِلَّا مِن ضَرِيعٖ"
    },
    {
      "id": 5974,
      "number": 7,
      "verse_key": "88:7",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "text": "لَّا يُسۡمِنُ وَلَا يُغۡنِي مِن جُوعٖ"
    },
    {
      "id": 5975,
      "number": 8,
      "verse_key": "88:8",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "text": "وُجُوهٞ يَوۡمَئِذٖ نَّاعِمَةٞ"
    },
    {
      "id": 5976,
      "number": 9,
      "verse_key": "88:9",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "text": "لِّسَعۡيِهَا رَاضِيَةٞ"
    },
    {
      "id": 5977,
      "number": 10,
      "verse_key": "88:10",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "text": "فِي جَنَّةٍ عَالِيَةٖ"
    },
    {
      "id": 5978,
      "number": 11,
      "verse_key": "88:11",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "text": "لَّا تَسۡمَعُ فِيهَا لَٰغِيَةٗ"
    },
    {
      "id": 5979,
      "number": 12,
      "verse_key": "88:12",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "text": "فِيهَا عَيۡنٞ جَارِيَةٞ"
    },
    {
      "id": 5980,
      "number": 13,
      "verse_key": "88:13",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "text": "فِيهَا سُرُرٞ مَّرۡفُوعَةٞ"
    },
    {
      "id": 5981,
      "number": 14,
      "verse_key": "88:14",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "text": "وَأَكۡوَابٞ مَّوۡضُوعَةٞ"
    },
    {
      "id": 5982,
      "number": 15,
      "verse_key": "88:15",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "text": "وَنَمَارِقُ مَصۡفُوفَةٞ"
    },
    {
      "id": 5983,
      "number": 16,
      "verse_key": "88:16",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "text": "وَزَرَابِيُّ مَبۡثُوثَةٌ"
    },
    {
      "id": 5984,
      "number": 17,
      "verse_key": "88:17",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "text": "أَفَلَا يَنظُرُونَ إِلَى ٱلۡإِبِلِ كَيۡفَ خُلِقَتۡ"
    },
    {
      "id": 5985,
      "number": 18,
      "verse_key": "88:18",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "text": "وَإِلَى ٱلسَّمَآءِ كَيۡفَ رُفِعَتۡ"
    },
    {
      "id": 5986,
      "number": 19,
      "verse_key": "88:19",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "text": "وَإِلَى ٱلۡجِبَالِ كَيۡفَ نُصِبَتۡ"
    },
    {
      "id": 5987,
      "number": 20,
      "verse_key": "88:20",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "text": "وَإِلَى ٱلۡأَرۡضِ كَيۡفَ سُطِحَتۡ"
    },
    {
      "id": 5988,
      "number": 21,
      "verse_key": "88:21",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "text": "فَذَكِّرۡ إِنَّمَآ أَنتَ مُذَكِّرٞ"
    },
    {
      "id": 5989,
      "number": 22,
      "verse_key": "88:22",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "text": "لَّسۡتَ عَلَيۡهِم بِمُصَيۡطِرٍ"
    },
    {
      "id": 5990,
      "number": 23,
      "verse_key": "88:23",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "text": "إِلَّا مَن تَوَلَّىٰ وَكَفَرَ"
    },
    {
      "id": 5991,
      "number": 24,
      "verse_key": "88:24",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "text": "فَيُعَذِّبُهُ ٱللَّهُ ٱلۡعَذَابَ ٱلۡأَكۡبَرَ"
    },
    {
      "id": 5992,
      "number": 25,
      "verse_key": "88:25",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "text": "إِنَّ إِلَيۡنَآ إِيَابَهُمۡ"
    },
    {
      "id": 5993,
      "number": 26,
      "verse_key": "88:26",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "text": "ثُمَّ إِنَّ عَلَيۡنَا حِسَابَهُم"
    }
  ]
}