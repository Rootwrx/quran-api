{
  "verses": [
    {
      "id": 2251,
      "number": 1,
      "verse_key": "19:1",
      "hizb_number": 31,
      "rub_el_hizb_number": 122,
      "ruku_number": 264,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 305,
      "juz_number": 16,
      "text": "كٓهيعٓصٓ"
    },
    {
      "id": 2260,
      "number": 10,
      "verse_key": "19:10",
      "hizb_number": 31,
      "rub_el_hizb_number": 122,
      "ruku_number": 264,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 305,
      "juz_number": 16,
      "text": "قَالَ رَبِّ ٱجۡعَل لِّيٓ ءَايَةٗۖ قَالَ ءَايَتُكَ أَلَّا تُكَلِّمَ ٱلنَّاسَ ثَلَٰثَ لَيَالٖ سَوِيّٗا"
    },
    {
      "id": 2261,
      "number": 11,
      "verse_key": "19:11",
      "hizb_number": 31,
      "rub_el_hizb_number": 122,
      "ruku_number": 264,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 305,
      "juz_number": 16,
      "text": "فَخَرَجَ عَلَىٰ قَوۡمِهِۦ مِنَ ٱلۡمِحۡرَابِ فَأَوۡحَىٰٓ إِلَيۡهِمۡ أَن سَبِّحُواْ بُكۡرَةٗ وَعَشِيّٗا"
    },
    {
      "id": 2262,
      "number": 12,
      "verse_key": "19:12",
      "hizb_number": 31,
      "rub_el_hizb_number": 122,
      "ruku_number": 264,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 306,
      "juz_number": 16,
      "text": "يَٰيَحۡيَىٰ خُذِ ٱلۡكِتَٰبَ بِقُوَّةٖۖ وَءَاتَيۡنَٰهُ ٱلۡحُكۡمَ صَبِيّٗا"
    },
    {
      "id": 2263,
      "number": 13,
      "verse_key": "19:13",
      "hizb_number": 31,
      "rub_el_hizb_number": 122,
      "ruku_number": 264,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 306,
      "juz_number": 16,
      "text": "وَحَنَانٗا مِّن لَّدُنَّا وَزَكَوٰةٗۖ وَكَانَ تَقِيّٗا"
    },
    {
      "id": 2264,
      "number": 14,
      "verse_key": "19:14",
      "hizb_number": 31,
      "rub_el_hizb_number": 122,
      "ruku_number": 264,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 306,
      "juz_number": 16,
      "text": "وَبَرَّۢا بِوَٰلِدَيۡهِ وَلَمۡ يَكُن جَبَّارًا عَصِيّٗا"
    },
    {
      "id": 2265,
      "number": 15,
      "verse_key": "19:15",
      "hizb_number": 31,
      "rub_el_hizb_number": 122,
      "ruku_number": 264,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 306,
      "juz_number": 16,
      "text": "وَسَلَٰمٌ عَلَيۡهِ يَوۡمَ وُلِدَ وَيَوۡمَ يَمُوتُ وَيَوۡمَ يُبۡعَثُ حَيّٗا"
    },
    {
      "id": 2266,
      "number": 16,
      "verse_key": "19:16",
      "hizb_number": 31,
      "rub_el_hizb_number": 122,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 306,
      "juz_number": 16,
      "text": "وَٱذۡكُرۡ فِي ٱلۡكِتَٰبِ مَرۡيَمَ إِذِ ٱنتَبَذَتۡ مِنۡ أَهۡلِهَا مَكَانٗا شَرۡقِيّٗا"
    },
    {
      "id": 2267,
      "number": 17,
      "verse_key": "19:17",
      "hizb_number": 31,
      "rub_el_hizb_number": 122,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 306,
      "juz_number": 16,
      "text": "فَٱتَّخَذَتۡ مِن دُونِهِمۡ حِجَابٗا فَأَرۡسَلۡنَآ إِلَيۡهَا رُوحَنَا فَتَمَثَّلَ لَهَا بَشَرٗا سَوِيّٗا"
    },
    {
      "id": 2268,
      "number": 18,
      "verse_key": "19:18",
      "hizb_number": 31,
      "rub_el_hizb_number": 122,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 306,
      "juz_number": 16,
      "text": "قَالَتۡ إِنِّيٓ أَعُوذُ بِٱلرَّحۡمَٰنِ مِنكَ إِن كُنتَ تَقِيّٗا"
    },
    {
      "id": 2269,
      "number": 19,
      "verse_key": "19:19",
      "hizb_number": 31,
      "rub_el_hizb_number": 122,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 306,
      "juz_number": 16,
      "text": "قَالَ إِنَّمَآ أَنَا۠ رَسُولُ رَبِّكِ لِأَهَبَ لَكِ غُلَٰمٗا زَكِيّٗا"
    },
    {
      "id": 2252,
      "number": 2,
      "verse_key": "19:2",
      "hizb_number": 31,
      "rub_el_hizb_number": 122,
      "ruku_number": 264,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 305,
      "juz_number": 16,
      "text": "ذِكۡرُ رَحۡمَتِ رَبِّكَ عَبۡدَهُۥ زَكَرِيَّآ"
    },
    {
      "id": 2270,
      "number": 20,
      "verse_key": "19:20",
      "hizb_number": 31,
      "rub_el_hizb_number": 122,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 306,
      "juz_number": 16,
      "text": "قَالَتۡ أَنَّىٰ يَكُونُ لِي غُلَٰمٞ وَلَمۡ يَمۡسَسۡنِي بَشَرٞ وَلَمۡ أَكُ بَغِيّٗا"
    },
    {
      "id": 2271,
      "number": 21,
      "verse_key": "19:21",
      "hizb_number": 31,
      "rub_el_hizb_number": 122,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 306,
      "juz_number": 16,
      "text": "قَالَ كَذَٰلِكِ قَالَ رَبُّكِ هُوَ عَلَيَّ هَيِّنٞۖ وَلِنَجۡعَلَهُۥٓ ءَايَةٗ لِّلنَّاسِ وَرَحۡمَةٗ مِّنَّاۚ وَكَانَ أَمۡرٗا مَّقۡضِيّٗا"
    },
    {
      "id": 2272,
      "number": 22,
      "verse_key": "19:22",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 306,
      "juz_number": 16,
      "text": "۞ فَحَمَلَتۡهُ فَٱنتَبَذَتۡ بِهِۦ مَكَانٗا قَصِيّٗا"
    },
    {
      "id": 2273,
      "number": 23,
      "verse_key": "19:23",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 306,
      "juz_number": 16,
      "text": "فَأَجَآءَهَا ٱلۡمَخَاضُ إِلَىٰ جِذۡعِ ٱلنَّخۡلَةِ قَالَتۡ يَٰلَيۡتَنِي مِتُّ قَبۡلَ هَٰذَا وَكُنتُ نَسۡيٗا مَّنسِيّٗا"
    },
    {
      "id": 2274,
      "number": 24,
      "verse_key": "19:24",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 306,
      "juz_number": 16,
      "text": "فَنَادَىٰهَا مِن تَحۡتِهَآ أَلَّا تَحۡزَنِي قَدۡ جَعَلَ رَبُّكِ تَحۡتَكِ سَرِيّٗا"
    },
    {
      "id": 2275,
      "number": 25,
      "verse_key": "19:25",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 306,
      "juz_number": 16,
      "text": "وَهُزِّيٓ إِلَيۡكِ بِجِذۡعِ ٱلنَّخۡلَةِ تُسَٰقِطۡ عَلَيۡكِ رُطَبٗا جَنِيّٗا"
    },
    {
      "id": 2276,
      "number": 26,
      "verse_key": "19:26",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 307,
      "juz_number": 16,
      "text": "فَكُلِي وَٱشۡرَبِي وَقَرِّي عَيۡنٗاۖ فَإِمَّا تَرَيِنَّ مِنَ ٱلۡبَشَرِ أَحَدٗا فَقُولِيٓ إِنِّي نَذَرۡتُ لِلرَّحۡمَٰنِ صَوۡمٗا فَلَنۡ أُكَلِّمَ ٱلۡيَوۡمَ إِنسِيّٗا"
    },
    {
      "id": 2277,
      "number": 27,
      "verse_key": "19:27",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 307,
      "juz_number": 16,
      "text": "فَأَتَتۡ بِهِۦ قَوۡمَهَا تَحۡمِلُهُۥۖ قَالُواْ يَٰمَرۡيَمُ لَقَدۡ جِئۡتِ شَيۡـٔٗا فَرِيّٗا"
    },
    {
      "id": 2278,
      "number": 28,
      "verse_key": "19:28",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 307,
      "juz_number": 16,
      "text": "يَٰٓأُخۡتَ هَٰرُونَ مَا كَانَ أَبُوكِ ٱمۡرَأَ سَوۡءٖ وَمَا كَانَتۡ أُمُّكِ بَغِيّٗا"
    },
    {
      "id": 2279,
      "number": 29,
      "verse_key": "19:29",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 307,
      "juz_number": 16,
      "text": "فَأَشَارَتۡ إِلَيۡهِۖ قَالُواْ كَيۡفَ نُكَلِّمُ مَن كَانَ فِي ٱلۡمَهۡدِ صَبِيّٗا"
    },
    {
      "id": 2253,
      "number": 3,
      "verse_key": "19:3",
      "hizb_number": 31,
      "rub_el_hizb_number": 122,
      "ruku_number": 264,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 305,
      "juz_number": 16,
      "text": "إِذۡ نَادَىٰ رَبَّهُۥ نِدَآءً خَفِيّٗا"
    },
    {
      "id": 2280,
      "number": 30,
      "verse_key": "19:30",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 307,
      "juz_number": 16,
      "text": "قَالَ إِنِّي عَبۡدُ ٱللَّهِ ءَاتَىٰنِيَ ٱلۡكِتَٰبَ وَجَعَلَنِي نَبِيّٗا"
    },
    {
      "id": 2281,
      "number": 31,
      "verse_key": "19:31",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 307,
      "juz_number": 16,
      "text": "وَجَعَلَنِي مُبَارَكًا أَيۡنَ مَا كُنتُ وَأَوۡصَٰنِي بِٱلصَّلَوٰةِ وَٱلزَّكَوٰةِ مَا دُمۡتُ حَيّٗا"
    },
    {
      "id": 2282,
      "number": 32,
      "verse_key": "19:32",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 307,
      "juz_number": 16,
      "text": "وَبَرَّۢا بِوَٰلِدَتِي وَلَمۡ يَجۡعَلۡنِي جَبَّارٗا شَقِيّٗا"
    },
    {
      "id": 2283,
      "number": 33,
      "verse_key": "19:33",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 307,
      "juz_number": 16,
      "text": "وَٱلسَّلَٰمُ عَلَيَّ يَوۡمَ وُلِدتُّ وَيَوۡمَ أَمُوتُ وَيَوۡمَ أُبۡعَثُ حَيّٗا"
    },
    {
      "id": 2284,
      "number": 34,
      "verse_key": "19:34",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 307,
      "juz_number": 16,
      "text": "ذَٰلِكَ عِيسَى ٱبۡنُ مَرۡيَمَۖ قَوۡلَ ٱلۡحَقِّ ٱلَّذِي فِيهِ يَمۡتَرُونَ"
    },
    {
      "id": 2285,
      "number": 35,
      "verse_key": "19:35",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 307,
      "juz_number": 16,
      "text": "مَا كَانَ لِلَّهِ أَن يَتَّخِذَ مِن وَلَدٖۖ سُبۡحَٰنَهُۥٓۚ إِذَا قَضَىٰٓ أَمۡرٗا فَإِنَّمَا يَقُولُ لَهُۥ كُن فَيَكُونُ"
    },
    {
      "id": 2286,
      "number": 36,
      "verse_key": "19:36",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 307,
      "juz_number": 16,
      "text": "وَإِنَّ ٱللَّهَ رَبِّي وَرَبُّكُمۡ فَٱعۡبُدُوهُۚ هَٰذَا صِرَٰطٞ مُّسۡتَقِيمٞ"
    },
    {
      "id": 2287,
      "number": 37,
      "verse_key": "19:37",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 307,
      "juz_number": 16,
      "text": "فَٱخۡتَلَفَ ٱلۡأَحۡزَابُ مِنۢ بَيۡنِهِمۡۖ فَوَيۡلٞ لِّلَّذِينَ كَفَرُواْ مِن مَّشۡهَدِ يَوۡمٍ عَظِيمٍ"
    },
    {
      "id": 2288,
      "number": 38,
      "verse_key": "19:38",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 307,
      "juz_number": 16,
      "text": "أَسۡمِعۡ بِهِمۡ وَأَبۡصِرۡ يَوۡمَ يَأۡتُونَنَا لَٰكِنِ ٱلظَّٰلِمُونَ ٱلۡيَوۡمَ فِي ضَلَٰلٖ مُّبِينٖ"
    },
    {
      "id": 2289,
      "number": 39,
      "verse_key": "19:39",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 308,
      "juz_number": 16,
      "text": "وَأَنذِرۡهُمۡ يَوۡمَ ٱلۡحَسۡرَةِ إِذۡ قُضِيَ ٱلۡأَمۡرُ وَهُمۡ فِي غَفۡلَةٖ وَهُمۡ لَا يُؤۡمِنُونَ"
    },
    {
      "id": 2254,
      "number": 4,
      "verse_key": "19:4",
      "hizb_number": 31,
      "rub_el_hizb_number": 122,
      "ruku_number": 264,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 305,
      "juz_number": 16,
      "text": "قَالَ رَبِّ إِنِّي وَهَنَ ٱلۡعَظۡمُ مِنِّي وَٱشۡتَعَلَ ٱلرَّأۡسُ شَيۡبٗا وَلَمۡ أَكُنۢ بِدُعَآئِكَ رَبِّ شَقِيّٗا"
    },
    {
      "id": 2290,
      "number": 40,
      "verse_key": "19:40",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 308,
      "juz_number": 16,
      "text": "إِنَّا نَحۡنُ نَرِثُ ٱلۡأَرۡضَ وَمَنۡ عَلَيۡهَا وَإِلَيۡنَا يُرۡجَعُونَ"
    },
    {
      "id": 2291,
      "number": 41,
      "verse_key": "19:41",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 266,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 308,
      "juz_number": 16,
      "text": "وَٱذۡكُرۡ فِي ٱلۡكِتَٰبِ إِبۡرَٰهِيمَۚ إِنَّهُۥ كَانَ صِدِّيقٗا نَّبِيًّا"
    },
    {
      "id": 2292,
      "number": 42,
      "verse_key": "19:42",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 266,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 308,
      "juz_number": 16,
      "text": "إِذۡ قَالَ لِأَبِيهِ يَٰٓأَبَتِ لِمَ تَعۡبُدُ مَا لَا يَسۡمَعُ وَلَا يُبۡصِرُ وَلَا يُغۡنِي عَنكَ شَيۡـٔٗا"
    },
    {
      "id": 2293,
      "number": 43,
      "verse_key": "19:43",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 266,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 308,
      "juz_number": 16,
      "text": "يَٰٓأَبَتِ إِنِّي قَدۡ جَآءَنِي مِنَ ٱلۡعِلۡمِ مَا لَمۡ يَأۡتِكَ فَٱتَّبِعۡنِيٓ أَهۡدِكَ صِرَٰطٗا سَوِيّٗا"
    },
    {
      "id": 2294,
      "number": 44,
      "verse_key": "19:44",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 266,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 308,
      "juz_number": 16,
      "text": "يَٰٓأَبَتِ لَا تَعۡبُدِ ٱلشَّيۡطَٰنَۖ إِنَّ ٱلشَّيۡطَٰنَ كَانَ لِلرَّحۡمَٰنِ عَصِيّٗا"
    },
    {
      "id": 2295,
      "number": 45,
      "verse_key": "19:45",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 266,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 308,
      "juz_number": 16,
      "text": "يَٰٓأَبَتِ إِنِّيٓ أَخَافُ أَن يَمَسَّكَ عَذَابٞ مِّنَ ٱلرَّحۡمَٰنِ فَتَكُونَ لِلشَّيۡطَٰنِ وَلِيّٗا"
    },
    {
      "id": 2296,
      "number": 46,
      "verse_key": "19:46",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 266,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 308,
      "juz_number": 16,
      "text": "قَالَ أَرَاغِبٌ أَنتَ عَنۡ ءَالِهَتِي يَٰٓإِبۡرَٰهِيمُۖ لَئِن لَّمۡ تَنتَهِ لَأَرۡجُمَنَّكَۖ وَٱهۡجُرۡنِي مَلِيّٗا"
    },
    {
      "id": 2297,
      "number": 47,
      "verse_key": "19:47",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 266,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 308,
      "juz_number": 16,
      "text": "قَالَ سَلَٰمٌ عَلَيۡكَۖ سَأَسۡتَغۡفِرُ لَكَ رَبِّيٓۖ إِنَّهُۥ كَانَ بِي حَفِيّٗا"
    },
    {
      "id": 2298,
      "number": 48,
      "verse_key": "19:48",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 266,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 308,
      "juz_number": 16,
      "text": "وَأَعۡتَزِلُكُمۡ وَمَا تَدۡعُونَ مِن دُونِ ٱللَّهِ وَأَدۡعُواْ رَبِّي عَسَىٰٓ أَلَّآ أَكُونَ بِدُعَآءِ رَبِّي شَقِيّٗا"
    },
    {
      "id": 2299,
      "number": 49,
      "verse_key": "19:49",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 266,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 308,
      "juz_number": 16,
      "text": "فَلَمَّا ٱعۡتَزَلَهُمۡ وَمَا يَعۡبُدُونَ مِن دُونِ ٱللَّهِ وَهَبۡنَا لَهُۥٓ إِسۡحَٰقَ وَيَعۡقُوبَۖ وَكُلّٗا جَعَلۡنَا نَبِيّٗا"
    },
    {
      "id": 2255,
      "number": 5,
      "verse_key": "19:5",
      "hizb_number": 31,
      "rub_el_hizb_number": 122,
      "ruku_number": 264,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 305,
      "juz_number": 16,
      "text": "وَإِنِّي خِفۡتُ ٱلۡمَوَٰلِيَ مِن وَرَآءِي وَكَانَتِ ٱمۡرَأَتِي عَاقِرٗا فَهَبۡ لِي مِن لَّدُنكَ وَلِيّٗا"
    },
    {
      "id": 2300,
      "number": 50,
      "verse_key": "19:50",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 266,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 308,
      "juz_number": 16,
      "text": "وَوَهَبۡنَا لَهُم مِّن رَّحۡمَتِنَا وَجَعَلۡنَا لَهُمۡ لِسَانَ صِدۡقٍ عَلِيّٗا"
    },
    {
      "id": 2301,
      "number": 51,
      "verse_key": "19:51",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 267,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 308,
      "juz_number": 16,
      "text": "وَٱذۡكُرۡ فِي ٱلۡكِتَٰبِ مُوسَىٰٓۚ إِنَّهُۥ كَانَ مُخۡلَصٗا وَكَانَ رَسُولٗا نَّبِيّٗا"
    },
    {
      "id": 2302,
      "number": 52,
      "verse_key": "19:52",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 267,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 309,
      "juz_number": 16,
      "text": "وَنَٰدَيۡنَٰهُ مِن جَانِبِ ٱلطُّورِ ٱلۡأَيۡمَنِ وَقَرَّبۡنَٰهُ نَجِيّٗا"
    },
    {
      "id": 2303,
      "number": 53,
      "verse_key": "19:53",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 267,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 309,
      "juz_number": 16,
      "text": "وَوَهَبۡنَا لَهُۥ مِن رَّحۡمَتِنَآ أَخَاهُ هَٰرُونَ نَبِيّٗا"
    },
    {
      "id": 2304,
      "number": 54,
      "verse_key": "19:54",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 267,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 309,
      "juz_number": 16,
      "text": "وَٱذۡكُرۡ فِي ٱلۡكِتَٰبِ إِسۡمَٰعِيلَۚ إِنَّهُۥ كَانَ صَادِقَ ٱلۡوَعۡدِ وَكَانَ رَسُولٗا نَّبِيّٗا"
    },
    {
      "id": 2305,
      "number": 55,
      "verse_key": "19:55",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 267,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 309,
      "juz_number": 16,
      "text": "وَكَانَ يَأۡمُرُ أَهۡلَهُۥ بِٱلصَّلَوٰةِ وَٱلزَّكَوٰةِ وَكَانَ عِندَ رَبِّهِۦ مَرۡضِيّٗا"
    },
    {
      "id": 2306,
      "number": 56,
      "verse_key": "19:56",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 267,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 309,
      "juz_number": 16,
      "text": "وَٱذۡكُرۡ فِي ٱلۡكِتَٰبِ إِدۡرِيسَۚ إِنَّهُۥ كَانَ صِدِّيقٗا نَّبِيّٗا"
    },
    {
      "id": 2307,
      "number": 57,
      "verse_key": "19:57",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 267,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 309,
      "juz_number": 16,
      "text": "وَرَفَعۡنَٰهُ مَكَانًا عَلِيًّا"
    },
    {
      "id": 2308,
      "number": 58,
      "verse_key": "19:58",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 267,
      "manzil_number": 4,
      "sajdah_number": 5,
      "page_number": 309,
      "juz_number": 16,
      "text": "أُوْلَٰٓئِكَ ٱلَّذِينَ أَنۡعَمَ ٱللَّهُ عَلَيۡهِم مِّنَ ٱلنَّبِيِّـۧنَ مِن ذُرِّيَّةِ ءَادَمَ وَمِمَّنۡ حَمَلۡنَا مَعَ نُوحٖ وَمِن ذُرِّيَّةِ إِبۡرَٰهِيمَ وَإِسۡرَٰٓءِيلَ وَمِمَّنۡ هَدَيۡنَا وَٱجۡتَبَيۡنَآۚ إِذَا تُتۡلَىٰ عَلَيۡهِمۡ ءَايَٰتُ ٱلرَّحۡمَٰنِ خَرُّواْۤ سُجَّدٗاۤ وَبُكِيّٗا۩"
    },
    {
      "id": 2309,
      "number": 59,
      "verse_key": "19:59",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 267,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 309,
      "juz_number": 16,
      "text": "۞ فَخَلَفَ مِنۢ بَعۡدِهِمۡ خَلۡفٌ أَضَاعُواْ ٱلصَّلَوٰةَ وَٱتَّبَعُواْ ٱلشَّهَوَٰتِۖ فَسَوۡفَ يَلۡقَوۡنَ غَيًّا"
    },
    {
      "id": 2256,
      "number": 6,
      "verse_key": "19:6",
      "hizb_number": 31,
      "rub_el_hizb_number": 122,
      "ruku_number": 264,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 305,
      "juz_number": 16,
      "text": "يَرِثُنِي وَيَرِثُ مِنۡ ءَالِ يَعۡقُوبَۖ وَٱجۡعَلۡهُ رَبِّ رَضِيّٗا"
    },
    {
      "id": 2310,
      "number": 60,
      "verse_key": "19:60",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 267,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 309,
      "juz_number": 16,
      "text": "إِلَّا مَن تَابَ وَءَامَنَ وَعَمِلَ صَٰلِحٗا فَأُوْلَٰٓئِكَ يَدۡخُلُونَ ٱلۡجَنَّةَ وَلَا يُظۡلَمُونَ شَيۡـٔٗا"
    },
    {
      "id": 2311,
      "number": 61,
      "verse_key": "19:61",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 267,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 309,
      "juz_number": 16,
      "text": "جَنَّٰتِ عَدۡنٍ ٱلَّتِي وَعَدَ ٱلرَّحۡمَٰنُ عِبَادَهُۥ بِٱلۡغَيۡبِۚ إِنَّهُۥ كَانَ وَعۡدُهُۥ مَأۡتِيّٗا"
    },
    {
      "id": 2312,
      "number": 62,
      "verse_key": "19:62",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 267,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 309,
      "juz_number": 16,
      "text": "لَّا يَسۡمَعُونَ فِيهَا لَغۡوًا إِلَّا سَلَٰمٗاۖ وَلَهُمۡ رِزۡقُهُمۡ فِيهَا بُكۡرَةٗ وَعَشِيّٗا"
    },
    {
      "id": 2313,
      "number": 63,
      "verse_key": "19:63",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 267,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 309,
      "juz_number": 16,
      "text": "تِلۡكَ ٱلۡجَنَّةُ ٱلَّتِي نُورِثُ مِنۡ عِبَادِنَا مَن كَانَ تَقِيّٗا"
    },
    {
      "id": 2314,
      "number": 64,
      "verse_key": "19:64",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 267,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 309,
      "juz_number": 16,
      "text": "وَمَا نَتَنَزَّلُ إِلَّا بِأَمۡرِ رَبِّكَۖ لَهُۥ مَا بَيۡنَ أَيۡدِينَا وَمَا خَلۡفَنَا وَمَا بَيۡنَ ذَٰلِكَۚ وَمَا كَانَ رَبُّكَ نَسِيّٗا"
    },
    {
      "id": 2315,
      "number": 65,
      "verse_key": "19:65",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 267,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 310,
      "juz_number": 16,
      "text": "رَّبُّ ٱلسَّمَٰوَٰتِ وَٱلۡأَرۡضِ وَمَا بَيۡنَهُمَا فَٱعۡبُدۡهُ وَٱصۡطَبِرۡ لِعِبَٰدَتِهِۦۚ هَلۡ تَعۡلَمُ لَهُۥ سَمِيّٗا"
    },
    {
      "id": 2316,
      "number": 66,
      "verse_key": "19:66",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 268,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 310,
      "juz_number": 16,
      "text": "وَيَقُولُ ٱلۡإِنسَٰنُ أَءِذَا مَا مِتُّ لَسَوۡفَ أُخۡرَجُ حَيًّا"
    },
    {
      "id": 2317,
      "number": 67,
      "verse_key": "19:67",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 268,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 310,
      "juz_number": 16,
      "text": "أَوَلَا يَذۡكُرُ ٱلۡإِنسَٰنُ أَنَّا خَلَقۡنَٰهُ مِن قَبۡلُ وَلَمۡ يَكُ شَيۡـٔٗا"
    },
    {
      "id": 2318,
      "number": 68,
      "verse_key": "19:68",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 268,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 310,
      "juz_number": 16,
      "text": "فَوَرَبِّكَ لَنَحۡشُرَنَّهُمۡ وَٱلشَّيَٰطِينَ ثُمَّ لَنُحۡضِرَنَّهُمۡ حَوۡلَ جَهَنَّمَ جِثِيّٗا"
    },
    {
      "id": 2319,
      "number": 69,
      "verse_key": "19:69",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 268,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 310,
      "juz_number": 16,
      "text": "ثُمَّ لَنَنزِعَنَّ مِن كُلِّ شِيعَةٍ أَيُّهُمۡ أَشَدُّ عَلَى ٱلرَّحۡمَٰنِ عِتِيّٗا"
    },
    {
      "id": 2257,
      "number": 7,
      "verse_key": "19:7",
      "hizb_number": 31,
      "rub_el_hizb_number": 122,
      "ruku_number": 264,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 305,
      "juz_number": 16,
      "text": "يَٰزَكَرِيَّآ إِنَّا نُبَشِّرُكَ بِغُلَٰمٍ ٱسۡمُهُۥ يَحۡيَىٰ لَمۡ نَجۡعَل لَّهُۥ مِن قَبۡلُ سَمِيّٗا"
    },
    {
      "id": 2320,
      "number": 70,
      "verse_key": "19:70",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 268,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 310,
      "juz_number": 16,
      "text": "ثُمَّ لَنَحۡنُ أَعۡلَمُ بِٱلَّذِينَ هُمۡ أَوۡلَىٰ بِهَا صِلِيّٗا"
    },
    {
      "id": 2321,
      "number": 71,
      "verse_key": "19:71",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 268,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 310,
      "juz_number": 16,
      "text": "وَإِن مِّنكُمۡ إِلَّا وَارِدُهَاۚ كَانَ عَلَىٰ رَبِّكَ حَتۡمٗا مَّقۡضِيّٗا"
    },
    {
      "id": 2322,
      "number": 72,
      "verse_key": "19:72",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 268,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 310,
      "juz_number": 16,
      "text": "ثُمَّ نُنَجِّي ٱلَّذِينَ ٱتَّقَواْ وَّنَذَرُ ٱلظَّٰلِمِينَ فِيهَا جِثِيّٗا"
    },
    {
      "id": 2323,
      "number": 73,
      "verse_key": "19:73",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 268,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 310,
      "juz_number": 16,
      "text": "وَإِذَا تُتۡلَىٰ عَلَيۡهِمۡ ءَايَٰتُنَا بَيِّنَٰتٖ قَالَ ٱلَّذِينَ كَفَرُواْ لِلَّذِينَ ءَامَنُوٓاْ أَيُّ ٱلۡفَرِيقَيۡنِ خَيۡرٞ مَّقَامٗا وَأَحۡسَنُ نَدِيّٗا"
    },
    {
      "id": 2324,
      "number": 74,
      "verse_key": "19:74",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 268,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 310,
      "juz_number": 16,
      "text": "وَكَمۡ أَهۡلَكۡنَا قَبۡلَهُم مِّن قَرۡنٍ هُمۡ أَحۡسَنُ أَثَٰثٗا وَرِءۡيٗا"
    },
    {
      "id": 2325,
      "number": 75,
      "verse_key": "19:75",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 268,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 310,
      "juz_number": 16,
      "text": "قُلۡ مَن كَانَ فِي ٱلضَّلَٰلَةِ فَلۡيَمۡدُدۡ لَهُ ٱلرَّحۡمَٰنُ مَدًّاۚ حَتَّىٰٓ إِذَا رَأَوۡاْ مَا يُوعَدُونَ إِمَّا ٱلۡعَذَابَ وَإِمَّا ٱلسَّاعَةَ فَسَيَعۡلَمُونَ مَنۡ هُوَ شَرّٞ مَّكَانٗا وَأَضۡعَفُ جُندٗا"
    },
    {
      "id": 2326,
      "number": 76,
      "verse_key": "19:76",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 268,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 310,
      "juz_number": 16,
      "text": "وَيَزِيدُ ٱللَّهُ ٱلَّذِينَ ٱهۡتَدَوۡاْ هُدٗىۗ وَٱلۡبَٰقِيَٰتُ ٱلصَّٰلِحَٰتُ خَيۡرٌ عِندَ رَبِّكَ ثَوَابٗا وَخَيۡرٞ مَّرَدًّا"
    },
    {
      "id": 2327,
      "number": 77,
      "verse_key": "19:77",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 268,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 311,
      "juz_number": 16,
      "text": "أَفَرَءَيۡتَ ٱلَّذِي كَفَرَ بِـَٔايَٰتِنَا وَقَالَ لَأُوتَيَنَّ مَالٗا وَوَلَدًا"
    },
    {
      "id": 2328,
      "number": 78,
      "verse_key": "19:78",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 268,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 311,
      "juz_number": 16,
      "text": "أَطَّلَعَ ٱلۡغَيۡبَ أَمِ ٱتَّخَذَ عِندَ ٱلرَّحۡمَٰنِ عَهۡدٗا"
    },
    {
      "id": 2329,
      "number": 79,
      "verse_key": "19:79",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 268,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 311,
      "juz_number": 16,
      "text": "كَلَّاۚ سَنَكۡتُبُ مَا يَقُولُ وَنَمُدُّ لَهُۥ مِنَ ٱلۡعَذَابِ مَدّٗا"
    },
    {
      "id": 2258,
      "number": 8,
      "verse_key": "19:8",
      "hizb_number": 31,
      "rub_el_hizb_number": 122,
      "ruku_number": 264,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 305,
      "juz_number": 16,
      "text": "قَالَ رَبِّ أَنَّىٰ يَكُونُ لِي غُلَٰمٞ وَكَانَتِ ٱمۡرَأَتِي عَاقِرٗا وَقَدۡ بَلَغۡتُ مِنَ ٱلۡكِبَرِ عِتِيّٗا"
    },
    {
      "id": 2330,
      "number": 80,
      "verse_key": "19:80",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 268,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 311,
      "juz_number": 16,
      "text": "وَنَرِثُهُۥ مَا يَقُولُ وَيَأۡتِينَا فَرۡدٗا"
    },
    {
      "id": 2331,
      "number": 81,
      "verse_key": "19:81",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 268,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 311,
      "juz_number": 16,
      "text": "وَٱتَّخَذُواْ مِن دُونِ ٱللَّهِ ءَالِهَةٗ لِّيَكُونُواْ لَهُمۡ عِزّٗا"
    },
    {
      "id": 2332,
      "number": 82,
      "verse_key": "19:82",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 268,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 311,
      "juz_number": 16,
      "text": "كَلَّاۚ سَيَكۡفُرُونَ بِعِبَادَتِهِمۡ وَيَكُونُونَ عَلَيۡهِمۡ ضِدًّا"
    },
    {
      "id": 2333,
      "number": 83,
      "verse_key": "19:83",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 269,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 311,
      "juz_number": 16,
      "text": "أَلَمۡ تَرَ أَنَّآ أَرۡسَلۡنَا ٱلشَّيَٰطِينَ عَلَى ٱلۡكَٰفِرِينَ تَؤُزُّهُمۡ أَزّٗا"
    },
    {
      "id": 2334,
      "number": 84,
      "verse_key": "19:84",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 269,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 311,
      "juz_number": 16,
      "text": "فَلَا تَعۡجَلۡ عَلَيۡهِمۡۖ إِنَّمَا نَعُدُّ لَهُمۡ عَدّٗا"
    },
    {
      "id": 2335,
      "number": 85,
      "verse_key": "19:85",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 269,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 311,
      "juz_number": 16,
      "text": "يَوۡمَ نَحۡشُرُ ٱلۡمُتَّقِينَ إِلَى ٱلرَّحۡمَٰنِ وَفۡدٗا"
    },
    {
      "id": 2336,
      "number": 86,
      "verse_key": "19:86",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 269,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 311,
      "juz_number": 16,
      "text": "وَنَسُوقُ ٱلۡمُجۡرِمِينَ إِلَىٰ جَهَنَّمَ وِرۡدٗا"
    },
    {
      "id": 2337,
      "number": 87,
      "verse_key": "19:87",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 269,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 311,
      "juz_number": 16,
      "text": "لَّا يَمۡلِكُونَ ٱلشَّفَٰعَةَ إِلَّا مَنِ ٱتَّخَذَ عِندَ ٱلرَّحۡمَٰنِ عَهۡدٗا"
    },
    {
      "id": 2338,
      "number": 88,
      "verse_key": "19:88",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 269,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 311,
      "juz_number": 16,
      "text": "وَقَالُواْ ٱتَّخَذَ ٱلرَّحۡمَٰنُ وَلَدٗا"
    },
    {
      "id": 2339,
      "number": 89,
      "verse_key": "19:89",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 269,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 311,
      "juz_number": 16,
      "text": "لَّقَدۡ جِئۡتُمۡ شَيۡـًٔا إِدّٗا"
    },
    {
      "id": 2259,
      "number": 9,
      "verse_key": "19:9",
      "hizb_number": 31,
      "rub_el_hizb_number": 122,
      "ruku_number": 264,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 305,
      "juz_number": 16,
      "text": "قَالَ كَذَٰلِكَ قَالَ رَبُّكَ هُوَ عَلَيَّ هَيِّنٞ وَقَدۡ خَلَقۡتُكَ مِن قَبۡلُ وَلَمۡ تَكُ شَيۡـٔٗا"
    },
    {
      "id": 2340,
      "number": 90,
      "verse_key": "19:90",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 269,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 311,
      "juz_number": 16,
      "text": "تَكَادُ ٱلسَّمَٰوَٰتُ يَتَفَطَّرۡنَ مِنۡهُ وَتَنشَقُّ ٱلۡأَرۡضُ وَتَخِرُّ ٱلۡجِبَالُ هَدًّا"
    },
    {
      "id": 2341,
      "number": 91,
      "verse_key": "19:91",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 269,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 311,
      "juz_number": 16,
      "text": "أَن دَعَوۡاْ لِلرَّحۡمَٰنِ وَلَدٗا"
    },
    {
      "id": 2342,
      "number": 92,
      "verse_key": "19:92",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 269,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 311,
      "juz_number": 16,
      "text": "وَمَا يَنۢبَغِي لِلرَّحۡمَٰنِ أَن يَتَّخِذَ وَلَدًا"
    },
    {
      "id": 2343,
      "number": 93,
      "verse_key": "19:93",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 269,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 311,
      "juz_number": 16,
      "text": "إِن كُلُّ مَن فِي ٱلسَّمَٰوَٰتِ وَٱلۡأَرۡضِ إِلَّآ ءَاتِي ٱلرَّحۡمَٰنِ عَبۡدٗا"
    },
    {
      "id": 2344,
      "number": 94,
      "verse_key": "19:94",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 269,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 311,
      "juz_number": 16,
      "text": "لَّقَدۡ أَحۡصَىٰهُمۡ وَعَدَّهُمۡ عَدّٗا"
    },
    {
      "id": 2345,
      "number": 95,
      "verse_key": "19:95",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 269,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 311,
      "juz_number": 16,
      "text": "وَكُلُّهُمۡ ءَاتِيهِ يَوۡمَ ٱلۡقِيَٰمَةِ فَرۡدًا"
    },
    {
      "id": 2346,
      "number": 96,
      "verse_key": "19:96",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 269,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 312,
      "juz_number": 16,
      "text": "إِنَّ ٱلَّذِينَ ءَامَنُواْ وَعَمِلُواْ ٱلصَّٰلِحَٰتِ سَيَجۡعَلُ لَهُمُ ٱلرَّحۡمَٰنُ وُدّٗا"
    },
    {
      "id": 2347,
      "number": 97,
      "verse_key": "19:97",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 269,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 312,
      "juz_number": 16,
      "text": "فَإِنَّمَا يَسَّرۡنَٰهُ بِلِسَانِكَ لِتُبَشِّرَ بِهِ ٱلۡمُتَّقِينَ وَتُنذِرَ بِهِۦ قَوۡمٗا لُّدّٗا"
    },
    {
      "id": 2348,
      "number": 98,
      "verse_key": "19:98",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 269,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 312,
      "juz_number": 16,
      "text": "وَكَمۡ أَهۡلَكۡنَا قَبۡلَهُم مِّن قَرۡنٍ هَلۡ تُحِسُّ مِنۡهُم مِّنۡ أَحَدٍ أَوۡ تَسۡمَعُ لَهُمۡ رِكۡزَۢا"
    }
  ]
}