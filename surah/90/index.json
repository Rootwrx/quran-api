{
  "verses": [
    {
      "id": 6024,
      "number": 1,
      "verse_key": "90:1",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 534,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 594,
      "juz_number": 30,
      "text": "لَآ أُقۡسِمُ بِهَٰذَا ٱلۡبَلَدِ"
    },
    {
      "id": 6025,
      "number": 2,
      "verse_key": "90:2",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 534,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 594,
      "juz_number": 30,
      "text": "وَأَنتَ حِلُّۢ بِهَٰذَا ٱلۡبَلَدِ"
    },
    {
      "id": 6026,
      "number": 3,
      "verse_key": "90:3",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 534,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 594,
      "juz_number": 30,
      "text": "وَوَالِدٖ وَمَا وَلَدَ"
    },
    {
      "id": 6027,
      "number": 4,
      "verse_key": "90:4",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 534,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 594,
      "juz_number": 30,
      "text": "لَقَدۡ خَلَقۡنَا ٱلۡإِنسَٰنَ فِي كَبَدٍ"
    },
    {
      "id": 6028,
      "number": 5,
      "verse_key": "90:5",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 534,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 594,
      "juz_number": 30,
      "text": "أَيَحۡسَبُ أَن لَّن يَقۡدِرَ عَلَيۡهِ أَحَدٞ"
    },
    {
      "id": 6029,
      "number": 6,
      "verse_key": "90:6",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 534,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 594,
      "juz_number": 30,
      "text": "يَقُولُ أَهۡلَكۡتُ مَالٗا لُّبَدًا"
    },
    {
      "id": 6030,
      "number": 7,
      "verse_key": "90:7",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 534,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 594,
      "juz_number": 30,
      "text": "أَيَحۡسَبُ أَن لَّمۡ يَرَهُۥٓ أَحَدٌ"
    },
    {
      "id": 6031,
      "number": 8,
      "verse_key": "90:8",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 534,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 594,
      "juz_number": 30,
      "text": "أَلَمۡ نَجۡعَل لَّهُۥ عَيۡنَيۡنِ"
    },
    {
      "id": 6032,
      "number": 9,
      "verse_key": "90:9",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 534,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 594,
      "juz_number": 30,
      "text": "وَلِسَانٗا وَشَفَتَيۡنِ"
    },
    {
      "id": 6033,
      "number": 10,
      "verse_key": "90:10",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 534,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 594,
      "juz_number": 30,
      "text": "وَهَدَيۡنَٰهُ ٱلنَّجۡدَيۡنِ"
    },
    {
      "id": 6034,
      "number": 11,
      "verse_key": "90:11",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 534,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 594,
      "juz_number": 30,
      "text": "فَلَا ٱقۡتَحَمَ ٱلۡعَقَبَةَ"
    },
    {
      "id": 6035,
      "number": 12,
      "verse_key": "90:12",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 534,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 594,
      "juz_number": 30,
      "text": "وَمَآ أَدۡرَىٰكَ مَا ٱلۡعَقَبَةُ"
    },
    {
      "id": 6036,
      "number": 13,
      "verse_key": "90:13",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 534,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 594,
      "juz_number": 30,
      "text": "فَكُّ رَقَبَةٍ"
    },
    {
      "id": 6037,
      "number": 14,
      "verse_key": "90:14",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 534,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 594,
      "juz_number": 30,
      "text": "أَوۡ إِطۡعَٰمٞ فِي يَوۡمٖ ذِي مَسۡغَبَةٖ"
    },
    {
      "id": 6038,
      "number": 15,
      "verse_key": "90:15",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 534,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 594,
      "juz_number": 30,
      "text": "يَتِيمٗا ذَا مَقۡرَبَةٍ"
    },
    {
      "id": 6039,
      "number": 16,
      "verse_key": "90:16",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 534,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 594,
      "juz_number": 30,
      "text": "أَوۡ مِسۡكِينٗا ذَا مَتۡرَبَةٖ"
    },
    {
      "id": 6040,
      "number": 17,
      "verse_key": "90:17",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 534,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 594,
      "juz_number": 30,
      "text": "ثُمَّ كَانَ مِنَ ٱلَّذِينَ ءَامَنُواْ وَتَوَاصَوۡاْ بِٱلصَّبۡرِ وَتَوَاصَوۡاْ بِٱلۡمَرۡحَمَةِ"
    },
    {
      "id": 6041,
      "number": 18,
      "verse_key": "90:18",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 534,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 594,
      "juz_number": 30,
      "text": "أُوْلَٰٓئِكَ أَصۡحَٰبُ ٱلۡمَيۡمَنَةِ"
    },
    {
      "id": 6042,
      "number": 19,
      "verse_key": "90:19",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 534,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 594,
      "juz_number": 30,
      "text": "وَٱلَّذِينَ كَفَرُواْ بِـَٔايَٰتِنَا هُمۡ أَصۡحَٰبُ ٱلۡمَشۡـَٔمَةِ"
    },
    {
      "id": 6043,
      "number": 20,
      "verse_key": "90:20",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 534,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 594,
      "juz_number": 30,
      "text": "عَلَيۡهِمۡ نَارٞ مُّؤۡصَدَةُۢ"
    }
  ]
}