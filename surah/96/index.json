      "number": 10,
      "verse_key": "96:10",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 540,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 597,
      "juz_number": 30,
