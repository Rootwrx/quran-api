{
  "verses": [
    {
      "id": 3160,
      "number": 1,
      "verse_key": "27:1",
      "hizb_number": 38,
      "rub_el_hizb_number": 151,
      "ruku_number": 327,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 377,
      "juz_number": 19,
      "text": "طسٓۚ تِلۡكَ ءَايَٰتُ ٱلۡقُرۡءَانِ وَكِتَابٖ مُّبِينٍ"
    },
    {
      "id": 3169,
      "number": 10,
      "verse_key": "27:10",
      "hizb_number": 38,
      "rub_el_hizb_number": 151,
      "ruku_number": 327,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 377,
      "juz_number": 19,
      "text": "وَأَلۡقِ عَصَاكَۚ فَلَمَّا رَءَاهَا تَهۡتَزُّ كَأَنَّهَا جَآنّٞ وَلَّىٰ مُدۡبِرٗا وَلَمۡ يُعَقِّبۡۚ يَٰمُوسَىٰ لَا تَخَفۡ إِنِّي لَا يَخَافُ لَدَيَّ ٱلۡمُرۡسَلُونَ"
    },
    {
      "id": 3170,
      "number": 11,
      "verse_key": "27:11",
      "hizb_number": 38,
      "rub_el_hizb_number": 151,
      "ruku_number": 327,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 377,
      "juz_number": 19,
      "text": "إِلَّا مَن ظَلَمَ ثُمَّ بَدَّلَ حُسۡنَۢا بَعۡدَ سُوٓءٖ فَإِنِّي غَفُورٞ رَّحِيمٞ"
    },
    {
      "id": 3171,
      "number": 12,
      "verse_key": "27:12",
      "hizb_number": 38,
      "rub_el_hizb_number": 151,
      "ruku_number": 327,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 377,
      "juz_number": 19,
      "text": "وَأَدۡخِلۡ يَدَكَ فِي جَيۡبِكَ تَخۡرُجۡ بَيۡضَآءَ مِنۡ غَيۡرِ سُوٓءٖۖ فِي تِسۡعِ ءَايَٰتٍ إِلَىٰ فِرۡعَوۡنَ وَقَوۡمِهِۦٓۚ إِنَّهُمۡ كَانُواْ قَوۡمٗا فَٰسِقِينَ"
    },
    {
      "id": 3172,
      "number": 13,
      "verse_key": "27:13",
      "hizb_number": 38,
      "rub_el_hizb_number": 151,
      "ruku_number": 327,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 377,
      "juz_number": 19,
      "text": "فَلَمَّا جَآءَتۡهُمۡ ءَايَٰتُنَا مُبۡصِرَةٗ قَالُواْ هَٰذَا سِحۡرٞ مُّبِينٞ"
    },
    {
      "id": 3173,
      "number": 14,
      "verse_key": "27:14",
      "hizb_number": 38,
      "rub_el_hizb_number": 151,
      "ruku_number": 327,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 378,
      "juz_number": 19,
      "text": "وَجَحَدُواْ بِهَا وَٱسۡتَيۡقَنَتۡهَآ أَنفُسُهُمۡ ظُلۡمٗا وَعُلُوّٗاۚ فَٱنظُرۡ كَيۡفَ كَانَ عَٰقِبَةُ ٱلۡمُفۡسِدِينَ"
    },
    {
      "id": 3174,
      "number": 15,
      "verse_key": "27:15",
      "hizb_number": 38,
      "rub_el_hizb_number": 151,
      "ruku_number": 328,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 378,
      "juz_number": 19,
      "text": "وَلَقَدۡ ءَاتَيۡنَا دَاوُۥدَ وَسُلَيۡمَٰنَ عِلۡمٗاۖ وَقَالَا ٱلۡحَمۡدُ لِلَّهِ ٱلَّذِي فَضَّلَنَا عَلَىٰ كَثِيرٖ مِّنۡ عِبَادِهِ ٱلۡمُؤۡمِنِينَ"
    },
    {
      "id": 3175,
      "number": 16,
      "verse_key": "27:16",
      "hizb_number": 38,
      "rub_el_hizb_number": 151,
      "ruku_number": 328,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 378,
      "juz_number": 19,
      "text": "وَوَرِثَ سُلَيۡمَٰنُ دَاوُۥدَۖ وَقَالَ يَٰٓأَيُّهَا ٱلنَّاسُ عُلِّمۡنَا مَنطِقَ ٱلطَّيۡرِ وَأُوتِينَا مِن كُلِّ شَيۡءٍۖ إِنَّ هَٰذَا لَهُوَ ٱلۡفَضۡلُ ٱلۡمُبِينُ"
    },
    {
      "id": 3176,
      "number": 17,
      "verse_key": "27:17",
      "hizb_number": 38,
      "rub_el_hizb_number": 151,
      "ruku_number": 328,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 378,
      "juz_number": 19,
      "text": "وَحُشِرَ لِسُلَيۡمَٰنَ جُنُودُهُۥ مِنَ ٱلۡجِنِّ وَٱلۡإِنسِ وَٱلطَّيۡرِ فَهُمۡ يُوزَعُونَ"
    },
    {
      "id": 3177,
      "number": 18,
      "verse_key": "27:18",
      "hizb_number": 38,
      "rub_el_hizb_number": 151,
      "ruku_number": 328,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 378,
      "juz_number": 19,
      "text": "حَتَّىٰٓ إِذَآ أَتَوۡاْ عَلَىٰ وَادِ ٱلنَّمۡلِ قَالَتۡ نَمۡلَةٞ يَٰٓأَيُّهَا ٱلنَّمۡلُ ٱدۡخُلُواْ مَسَٰكِنَكُمۡ لَا يَحۡطِمَنَّكُمۡ سُلَيۡمَٰنُ وَجُنُودُهُۥ وَهُمۡ لَا يَشۡعُرُونَ"
    },
    {
      "id": 3178,
      "number": 19,
      "verse_key": "27:19",
      "hizb_number": 38,
      "rub_el_hizb_number": 151,
      "ruku_number": 328,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 378,
      "juz_number": 19,
      "text": "فَتَبَسَّمَ ضَاحِكٗا مِّن قَوۡلِهَا وَقَالَ رَبِّ أَوۡزِعۡنِيٓ أَنۡ أَشۡكُرَ نِعۡمَتَكَ ٱلَّتِيٓ أَنۡعَمۡتَ عَلَيَّ وَعَلَىٰ وَٰلِدَيَّ وَأَنۡ أَعۡمَلَ صَٰلِحٗا تَرۡضَىٰهُ وَأَدۡخِلۡنِي بِرَحۡمَتِكَ فِي عِبَادِكَ ٱلصَّٰلِحِينَ"
    },
    {
      "id": 3161,
      "number": 2,
      "verse_key": "27:2",
      "hizb_number": 38,
      "rub_el_hizb_number": 151,
      "ruku_number": 327,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 377,
      "juz_number": 19,
      "text": "هُدٗى وَبُشۡرَىٰ لِلۡمُؤۡمِنِينَ"
    },
    {
      "id": 3179,
      "number": 20,
      "verse_key": "27:20",
      "hizb_number": 38,
      "rub_el_hizb_number": 151,
      "ruku_number": 328,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 378,
      "juz_number": 19,
      "text": "وَتَفَقَّدَ ٱلطَّيۡرَ فَقَالَ مَالِيَ لَآ أَرَى ٱلۡهُدۡهُدَ أَمۡ كَانَ مِنَ ٱلۡغَآئِبِينَ"
    },
    {
      "id": 3180,
      "number": 21,
      "verse_key": "27:21",
      "hizb_number": 38,
      "rub_el_hizb_number": 151,
      "ruku_number": 328,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 378,
      "juz_number": 19,
      "text": "لَأُعَذِّبَنَّهُۥ عَذَابٗا شَدِيدًا أَوۡ لَأَاْذۡبَحَنَّهُۥٓ أَوۡ لَيَأۡتِيَنِّي بِسُلۡطَٰنٖ مُّبِينٖ"
    },
    {
      "id": 3181,
      "number": 22,
      "verse_key": "27:22",
      "hizb_number": 38,
      "rub_el_hizb_number": 151,
      "ruku_number": 328,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 378,
      "juz_number": 19,
      "text": "فَمَكَثَ غَيۡرَ بَعِيدٖ فَقَالَ أَحَطتُ بِمَا لَمۡ تُحِطۡ بِهِۦ وَجِئۡتُكَ مِن سَبَإِۭ بِنَبَإٖ يَقِينٍ"
    },
    {
      "id": 3182,
      "number": 23,
      "verse_key": "27:23",
      "hizb_number": 38,
      "rub_el_hizb_number": 151,
      "ruku_number": 328,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 379,
      "juz_number": 19,
      "text": "إِنِّي وَجَدتُّ ٱمۡرَأَةٗ تَمۡلِكُهُمۡ وَأُوتِيَتۡ مِن كُلِّ شَيۡءٖ وَلَهَا عَرۡشٌ عَظِيمٞ"
    },
    {
      "id": 3183,
      "number": 24,
      "verse_key": "27:24",
      "hizb_number": 38,
      "rub_el_hizb_number": 151,
      "ruku_number": 328,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 379,
      "juz_number": 19,
      "text": "وَجَدتُّهَا وَقَوۡمَهَا يَسۡجُدُونَ لِلشَّمۡسِ مِن دُونِ ٱللَّهِ وَزَيَّنَ لَهُمُ ٱلشَّيۡطَٰنُ أَعۡمَٰلَهُمۡ فَصَدَّهُمۡ عَنِ ٱلسَّبِيلِ فَهُمۡ لَا يَهۡتَدُونَ"
    },
    {
      "id": 3184,
      "number": 25,
      "verse_key": "27:25",
      "hizb_number": 38,
      "rub_el_hizb_number": 151,
      "ruku_number": 328,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 379,
      "juz_number": 19,
      "text": "أَلَّاۤ يَسۡجُدُواْۤ لِلَّهِ ٱلَّذِي يُخۡرِجُ ٱلۡخَبۡءَ فِي ٱلسَّمَٰوَٰتِ وَٱلۡأَرۡضِ وَيَعۡلَمُ مَا تُخۡفُونَ وَمَا تُعۡلِنُونَ"
    },
    {
      "id": 3185,
      "number": 26,
      "verse_key": "27:26",
      "hizb_number": 38,
      "rub_el_hizb_number": 151,
      "ruku_number": 328,
      "manzil_number": 5,
      "sajdah_number": 8,
      "page_number": 379,
      "juz_number": 19,
      "text": "ٱللَّهُ لَآ إِلَٰهَ إِلَّا هُوَ رَبُّ ٱلۡعَرۡشِ ٱلۡعَظِيمِ۩"
    },
    {
      "id": 3186,
      "number": 27,
      "verse_key": "27:27",
      "hizb_number": 38,
      "rub_el_hizb_number": 152,
      "ruku_number": 328,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 379,
      "juz_number": 19,
      "text": "۞ قَالَ سَنَنظُرُ أَصَدَقۡتَ أَمۡ كُنتَ مِنَ ٱلۡكَٰذِبِينَ"
    },
    {
      "id": 3187,
      "number": 28,
      "verse_key": "27:28",
      "hizb_number": 38,
      "rub_el_hizb_number": 152,
      "ruku_number": 328,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 379,
      "juz_number": 19,
      "text": "ٱذۡهَب بِّكِتَٰبِي هَٰذَا فَأَلۡقِهۡ إِلَيۡهِمۡ ثُمَّ تَوَلَّ عَنۡهُمۡ فَٱنظُرۡ مَاذَا يَرۡجِعُونَ"
    },
    {
      "id": 3188,
      "number": 29,
      "verse_key": "27:29",
      "hizb_number": 38,
      "rub_el_hizb_number": 152,
      "ruku_number": 328,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 379,
      "juz_number": 19,
      "text": "قَالَتۡ يَٰٓأَيُّهَا ٱلۡمَلَؤُاْ إِنِّيٓ أُلۡقِيَ إِلَيَّ كِتَٰبٞ كَرِيمٌ"
    },
    {
      "id": 3162,
      "number": 3,
      "verse_key": "27:3",
      "hizb_number": 38,
      "rub_el_hizb_number": 151,
      "ruku_number": 327,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 377,
      "juz_number": 19,
      "text": "ٱلَّذِينَ يُقِيمُونَ ٱلصَّلَوٰةَ وَيُؤۡتُونَ ٱلزَّكَوٰةَ وَهُم بِٱلۡأٓخِرَةِ هُمۡ يُوقِنُونَ"
    },
    {
      "id": 3189,
      "number": 30,
      "verse_key": "27:30",
      "hizb_number": 38,
      "rub_el_hizb_number": 152,
      "ruku_number": 328,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 379,
      "juz_number": 19,
      "text": "إِنَّهُۥ مِن سُلَيۡمَٰنَ وَإِنَّهُۥ بِسۡمِ ٱللَّهِ ٱلرَّحۡمَٰنِ ٱلرَّحِيمِ"
    },
    {
      "id": 3190,
      "number": 31,
      "verse_key": "27:31",
      "hizb_number": 38,
      "rub_el_hizb_number": 152,
      "ruku_number": 328,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 379,
      "juz_number": 19,
      "text": "أَلَّا تَعۡلُواْ عَلَيَّ وَأۡتُونِي مُسۡلِمِينَ"
    },
    {
      "id": 3191,
      "number": 32,
      "verse_key": "27:32",
      "hizb_number": 38,
      "rub_el_hizb_number": 152,
      "ruku_number": 329,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 379,
      "juz_number": 19,
      "text": "قَالَتۡ يَٰٓأَيُّهَا ٱلۡمَلَؤُاْ أَفۡتُونِي فِيٓ أَمۡرِي مَا كُنتُ قَاطِعَةً أَمۡرًا حَتَّىٰ تَشۡهَدُونِ"
    },
    {
      "id": 3192,
      "number": 33,
      "verse_key": "27:33",
      "hizb_number": 38,
      "rub_el_hizb_number": 152,
      "ruku_number": 329,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 379,
      "juz_number": 19,
      "text": "قَالُواْ نَحۡنُ أُوْلُواْ قُوَّةٖ وَأُوْلُواْ بَأۡسٖ شَدِيدٖ وَٱلۡأَمۡرُ إِلَيۡكِ فَٱنظُرِي مَاذَا تَأۡمُرِينَ"
    },
    {
      "id": 3193,
      "number": 34,
      "verse_key": "27:34",
      "hizb_number": 38,
      "rub_el_hizb_number": 152,
      "ruku_number": 329,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 379,
      "juz_number": 19,
      "text": "قَالَتۡ إِنَّ ٱلۡمُلُوكَ إِذَا دَخَلُواْ قَرۡيَةً أَفۡسَدُوهَا وَجَعَلُوٓاْ أَعِزَّةَ أَهۡلِهَآ أَذِلَّةٗۚ وَكَذَٰلِكَ يَفۡعَلُونَ"
    },
    {
      "id": 3194,
      "number": 35,
      "verse_key": "27:35",
      "hizb_number": 38,
      "rub_el_hizb_number": 152,
      "ruku_number": 329,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 379,
      "juz_number": 19,
      "text": "وَإِنِّي مُرۡسِلَةٌ إِلَيۡهِم بِهَدِيَّةٖ فَنَاظِرَةُۢ بِمَ يَرۡجِعُ ٱلۡمُرۡسَلُونَ"
    },
    {
      "id": 3195,
      "number": 36,
      "verse_key": "27:36",
      "hizb_number": 38,
      "rub_el_hizb_number": 152,
      "ruku_number": 329,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 380,
      "juz_number": 19,
      "text": "فَلَمَّا جَآءَ سُلَيۡمَٰنَ قَالَ أَتُمِدُّونَنِ بِمَالٖ فَمَآ ءَاتَىٰنِۦَ ٱللَّهُ خَيۡرٞ مِّمَّآ ءَاتَىٰكُمۚ بَلۡ أَنتُم بِهَدِيَّتِكُمۡ تَفۡرَحُونَ"
    },
    {
      "id": 3196,
      "number": 37,
      "verse_key": "27:37",
      "hizb_number": 38,
      "rub_el_hizb_number": 152,
      "ruku_number": 329,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 380,
      "juz_number": 19,
      "text": "ٱرۡجِعۡ إِلَيۡهِمۡ فَلَنَأۡتِيَنَّهُم بِجُنُودٖ لَّا قِبَلَ لَهُم بِهَا وَلَنُخۡرِجَنَّهُم مِّنۡهَآ أَذِلَّةٗ وَهُمۡ صَٰغِرُونَ"
    },
    {
      "id": 3197,
      "number": 38,
      "verse_key": "27:38",
      "hizb_number": 38,
      "rub_el_hizb_number": 152,
      "ruku_number": 329,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 380,
      "juz_number": 19,
      "text": "قَالَ يَٰٓأَيُّهَا ٱلۡمَلَؤُاْ أَيُّكُمۡ يَأۡتِينِي بِعَرۡشِهَا قَبۡلَ أَن يَأۡتُونِي مُسۡلِمِينَ"
    },
    {
      "id": 3198,
      "number": 39,
      "verse_key": "27:39",
      "hizb_number": 38,
      "rub_el_hizb_number": 152,
      "ruku_number": 329,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 380,
      "juz_number": 19,
      "text": "قَالَ عِفۡرِيتٞ مِّنَ ٱلۡجِنِّ أَنَا۠ ءَاتِيكَ بِهِۦ قَبۡلَ أَن تَقُومَ مِن مَّقَامِكَۖ وَإِنِّي عَلَيۡهِ لَقَوِيٌّ أَمِينٞ"
    },
    {
      "id": 3163,
      "number": 4,
      "verse_key": "27:4",
      "hizb_number": 38,
      "rub_el_hizb_number": 151,
      "ruku_number": 327,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 377,
      "juz_number": 19,
      "text": "إِنَّ ٱلَّذِينَ لَا يُؤۡمِنُونَ بِٱلۡأٓخِرَةِ زَيَّنَّا لَهُمۡ أَعۡمَٰلَهُمۡ فَهُمۡ يَعۡمَهُونَ"
    },
    {
      "id": 3199,
      "number": 40,
      "verse_key": "27:40",
      "hizb_number": 38,
      "rub_el_hizb_number": 152,
      "ruku_number": 329,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 380,
      "juz_number": 19,
      "text": "قَالَ ٱلَّذِي عِندَهُۥ عِلۡمٞ مِّنَ ٱلۡكِتَٰبِ أَنَا۠ ءَاتِيكَ بِهِۦ قَبۡلَ أَن يَرۡتَدَّ إِلَيۡكَ طَرۡفُكَۚ فَلَمَّا رَءَاهُ مُسۡتَقِرًّا عِندَهُۥ قَالَ هَٰذَا مِن فَضۡلِ رَبِّي لِيَبۡلُوَنِيٓ ءَأَشۡكُرُ أَمۡ أَكۡفُرُۖ وَمَن شَكَرَ فَإِنَّمَا يَشۡكُرُ لِنَفۡسِهِۦۖ وَمَن كَفَرَ فَإِنَّ رَبِّي غَنِيّٞ كَرِيمٞ"
    },
    {
      "id": 3200,
      "number": 41,
      "verse_key": "27:41",
      "hizb_number": 38,
      "rub_el_hizb_number": 152,
      "ruku_number": 329,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 380,
      "juz_number": 19,
      "text": "قَالَ نَكِّرُواْ لَهَا عَرۡشَهَا نَنظُرۡ أَتَهۡتَدِيٓ أَمۡ تَكُونُ مِنَ ٱلَّذِينَ لَا يَهۡتَدُونَ"
    },
    {
      "id": 3201,
      "number": 42,
      "verse_key": "27:42",
      "hizb_number": 38,
      "rub_el_hizb_number": 152,
      "ruku_number": 329,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 380,
      "juz_number": 19,
      "text": "فَلَمَّا جَآءَتۡ قِيلَ أَهَٰكَذَا عَرۡشُكِۖ قَالَتۡ كَأَنَّهُۥ هُوَۚ وَأُوتِينَا ٱلۡعِلۡمَ مِن قَبۡلِهَا وَكُنَّا مُسۡلِمِينَ"
    },
    {
      "id": 3202,
      "number": 43,
      "verse_key": "27:43",
      "hizb_number": 38,
      "rub_el_hizb_number": 152,
      "ruku_number": 329,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 380,
      "juz_number": 19,
      "text": "وَصَدَّهَا مَا كَانَت تَّعۡبُدُ مِن دُونِ ٱللَّهِۖ إِنَّهَا كَانَتۡ مِن قَوۡمٖ كَٰفِرِينَ"
    },
    {
      "id": 3203,
      "number": 44,
      "verse_key": "27:44",
      "hizb_number": 38,
      "rub_el_hizb_number": 152,
      "ruku_number": 329,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 380,
      "juz_number": 19,
      "text": "قِيلَ لَهَا ٱدۡخُلِي ٱلصَّرۡحَۖ فَلَمَّا رَأَتۡهُ حَسِبَتۡهُ لُجَّةٗ وَكَشَفَتۡ عَن سَاقَيۡهَاۚ قَالَ إِنَّهُۥ صَرۡحٞ مُّمَرَّدٞ مِّن قَوَارِيرَۗ قَالَتۡ رَبِّ إِنِّي ظَلَمۡتُ نَفۡسِي وَأَسۡلَمۡتُ مَعَ سُلَيۡمَٰنَ لِلَّهِ رَبِّ ٱلۡعَٰلَمِينَ"
    },
    {
      "id": 3204,
      "number": 45,
      "verse_key": "27:45",
      "hizb_number": 38,
      "rub_el_hizb_number": 152,
      "ruku_number": 330,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 381,
      "juz_number": 19,
      "text": "وَلَقَدۡ أَرۡسَلۡنَآ إِلَىٰ ثَمُودَ أَخَاهُمۡ صَٰلِحًا أَنِ ٱعۡبُدُواْ ٱللَّهَ فَإِذَا هُمۡ فَرِيقَانِ يَخۡتَصِمُونَ"
    },
    {
      "id": 3205,
      "number": 46,
      "verse_key": "27:46",
      "hizb_number": 38,
      "rub_el_hizb_number": 152,
      "ruku_number": 330,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 381,
      "juz_number": 19,
      "text": "قَالَ يَٰقَوۡمِ لِمَ تَسۡتَعۡجِلُونَ بِٱلسَّيِّئَةِ قَبۡلَ ٱلۡحَسَنَةِۖ لَوۡلَا تَسۡتَغۡفِرُونَ ٱللَّهَ لَعَلَّكُمۡ تُرۡحَمُونَ"
    },
    {
      "id": 3206,
      "number": 47,
      "verse_key": "27:47",
      "hizb_number": 38,
      "rub_el_hizb_number": 152,
      "ruku_number": 330,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 381,
      "juz_number": 19,
      "text": "قَالُواْ ٱطَّيَّرۡنَا بِكَ وَبِمَن مَّعَكَۚ قَالَ طَٰٓئِرُكُمۡ عِندَ ٱللَّهِۖ بَلۡ أَنتُمۡ قَوۡمٞ تُفۡتَنُونَ"
    },
    {
      "id": 3207,
      "number": 48,
      "verse_key": "27:48",
      "hizb_number": 38,
      "rub_el_hizb_number": 152,
      "ruku_number": 330,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 381,
      "juz_number": 19,
      "text": "وَكَانَ فِي ٱلۡمَدِينَةِ تِسۡعَةُ رَهۡطٖ يُفۡسِدُونَ فِي ٱلۡأَرۡضِ وَلَا يُصۡلِحُونَ"
    },
    {
      "id": 3208,
      "number": 49,
      "verse_key": "27:49",
      "hizb_number": 38,
      "rub_el_hizb_number": 152,
      "ruku_number": 330,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 381,
      "juz_number": 19,
      "text": "قَالُواْ تَقَاسَمُواْ بِٱللَّهِ لَنُبَيِّتَنَّهُۥ وَأَهۡلَهُۥ ثُمَّ لَنَقُولَنَّ لِوَلِيِّهِۦ مَا شَهِدۡنَا مَهۡلِكَ أَهۡلِهِۦ وَإِنَّا لَصَٰدِقُونَ"
    },
    {
      "id": 3164,
      "number": 5,
      "verse_key": "27:5",
      "hizb_number": 38,
      "rub_el_hizb_number": 151,
      "ruku_number": 327,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 377,
      "juz_number": 19,
      "text": "أُوْلَٰٓئِكَ ٱلَّذِينَ لَهُمۡ سُوٓءُ ٱلۡعَذَابِ وَهُمۡ فِي ٱلۡأٓخِرَةِ هُمُ ٱلۡأَخۡسَرُونَ"
    },
    {
      "id": 3209,
      "number": 50,
      "verse_key": "27:50",
      "hizb_number": 38,
      "rub_el_hizb_number": 152,
      "ruku_number": 330,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 381,
      "juz_number": 19,
      "text": "وَمَكَرُواْ مَكۡرٗا وَمَكَرۡنَا مَكۡرٗا وَهُمۡ لَا يَشۡعُرُونَ"
    },
    {
      "id": 3210,
      "number": 51,
      "verse_key": "27:51",
      "hizb_number": 38,
      "rub_el_hizb_number": 152,
      "ruku_number": 330,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 381,
      "juz_number": 19,
      "text": "فَٱنظُرۡ كَيۡفَ كَانَ عَٰقِبَةُ مَكۡرِهِمۡ أَنَّا دَمَّرۡنَٰهُمۡ وَقَوۡمَهُمۡ أَجۡمَعِينَ"
    },
    {
      "id": 3211,
      "number": 52,
      "verse_key": "27:52",
      "hizb_number": 38,
      "rub_el_hizb_number": 152,
      "ruku_number": 330,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 381,
      "juz_number": 19,
      "text": "فَتِلۡكَ بُيُوتُهُمۡ خَاوِيَةَۢ بِمَا ظَلَمُوٓاْۚ إِنَّ فِي ذَٰلِكَ لَأٓيَةٗ لِّقَوۡمٖ يَعۡلَمُونَ"
    },
    {
      "id": 3212,
      "number": 53,
      "verse_key": "27:53",
      "hizb_number": 38,
      "rub_el_hizb_number": 152,
      "ruku_number": 330,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 381,
      "juz_number": 19,
      "text": "وَأَنجَيۡنَا ٱلَّذِينَ ءَامَنُواْ وَكَانُواْ يَتَّقُونَ"
    },
    {
      "id": 3213,
      "number": 54,
      "verse_key": "27:54",
      "hizb_number": 38,
      "rub_el_hizb_number": 152,
      "ruku_number": 330,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 381,
      "juz_number": 19,
      "text": "وَلُوطًا إِذۡ قَالَ لِقَوۡمِهِۦٓ أَتَأۡتُونَ ٱلۡفَٰحِشَةَ وَأَنتُمۡ تُبۡصِرُونَ"
    },
    {
      "id": 3214,
      "number": 55,
      "verse_key": "27:55",
      "hizb_number": 38,
      "rub_el_hizb_number": 152,
      "ruku_number": 330,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 381,
      "juz_number": 19,
      "text": "أَئِنَّكُمۡ لَتَأۡتُونَ ٱلرِّجَالَ شَهۡوَةٗ مِّن دُونِ ٱلنِّسَآءِۚ بَلۡ أَنتُمۡ قَوۡمٞ تَجۡهَلُونَ"
    },
    {
      "id": 3215,
      "number": 56,
      "verse_key": "27:56",
      "hizb_number": 39,
      "rub_el_hizb_number": 153,
      "ruku_number": 330,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 382,
      "juz_number": 20,
      "text": "۞ فَمَا كَانَ جَوَابَ قَوۡمِهِۦٓ إِلَّآ أَن قَالُوٓاْ أَخۡرِجُوٓاْ ءَالَ لُوطٖ مِّن قَرۡيَتِكُمۡۖ إِنَّهُمۡ أُنَاسٞ يَتَطَهَّرُونَ"
    },
    {
      "id": 3216,
      "number": 57,
      "verse_key": "27:57",
      "hizb_number": 39,
      "rub_el_hizb_number": 153,
      "ruku_number": 330,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 382,
      "juz_number": 20,
      "text": "فَأَنجَيۡنَٰهُ وَأَهۡلَهُۥٓ إِلَّا ٱمۡرَأَتَهُۥ قَدَّرۡنَٰهَا مِنَ ٱلۡغَٰبِرِينَ"
    },
    {
      "id": 3217,
      "number": 58,
      "verse_key": "27:58",
      "hizb_number": 39,
      "rub_el_hizb_number": 153,
      "ruku_number": 330,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 382,
      "juz_number": 20,
      "text": "وَأَمۡطَرۡنَا عَلَيۡهِم مَّطَرٗاۖ فَسَآءَ مَطَرُ ٱلۡمُنذَرِينَ"
    },
    {
      "id": 3218,
      "number": 59,
      "verse_key": "27:59",
      "hizb_number": 39,
      "rub_el_hizb_number": 153,
      "ruku_number": 331,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 382,
      "juz_number": 20,
      "text": "قُلِ ٱلۡحَمۡدُ لِلَّهِ وَسَلَٰمٌ عَلَىٰ عِبَادِهِ ٱلَّذِينَ ٱصۡطَفَىٰٓۗ ءَآللَّهُ خَيۡرٌ أَمَّا يُشۡرِكُونَ"
    },
    {
      "id": 3165,
      "number": 6,
      "verse_key": "27:6",
      "hizb_number": 38,
      "rub_el_hizb_number": 151,
      "ruku_number": 327,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 377,
      "juz_number": 19,
      "text": "وَإِنَّكَ لَتُلَقَّى ٱلۡقُرۡءَانَ مِن لَّدُنۡ حَكِيمٍ عَلِيمٍ"
    },
    {
      "id": 3219,
      "number": 60,
      "verse_key": "27:60",
      "hizb_number": 39,
      "rub_el_hizb_number": 153,
      "ruku_number": 331,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 382,
      "juz_number": 20,
      "text": "أَمَّنۡ خَلَقَ ٱلسَّمَٰوَٰتِ وَٱلۡأَرۡضَ وَأَنزَلَ لَكُم مِّنَ ٱلسَّمَآءِ مَآءٗ فَأَنۢبَتۡنَا بِهِۦ حَدَآئِقَ ذَاتَ بَهۡجَةٖ مَّا كَانَ لَكُمۡ أَن تُنۢبِتُواْ شَجَرَهَآۗ أَءِلَٰهٞ مَّعَ ٱللَّهِۚ بَلۡ هُمۡ قَوۡمٞ يَعۡدِلُونَ"
    },
    {
      "id": 3220,
      "number": 61,
      "verse_key": "27:61",
      "hizb_number": 39,
      "rub_el_hizb_number": 153,
      "ruku_number": 331,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 382,
      "juz_number": 20,
      "text": "أَمَّن جَعَلَ ٱلۡأَرۡضَ قَرَارٗا وَجَعَلَ خِلَٰلَهَآ أَنۡهَٰرٗا وَجَعَلَ لَهَا رَوَٰسِيَ وَجَعَلَ بَيۡنَ ٱلۡبَحۡرَيۡنِ حَاجِزًاۗ أَءِلَٰهٞ مَّعَ ٱللَّهِۚ بَلۡ أَكۡثَرُهُمۡ لَا يَعۡلَمُونَ"
    },
    {
      "id": 3221,
      "number": 62,
      "verse_key": "27:62",
      "hizb_number": 39,
      "rub_el_hizb_number": 153,
      "ruku_number": 331,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 382,
      "juz_number": 20,
      "text": "أَمَّن يُجِيبُ ٱلۡمُضۡطَرَّ إِذَا دَعَاهُ وَيَكۡشِفُ ٱلسُّوٓءَ وَيَجۡعَلُكُمۡ خُلَفَآءَ ٱلۡأَرۡضِۗ أَءِلَٰهٞ مَّعَ ٱللَّهِۚ قَلِيلٗا مَّا تَذَكَّرُونَ"
    },
    {
      "id": 3222,
      "number": 63,
      "verse_key": "27:63",
      "hizb_number": 39,
      "rub_el_hizb_number": 153,
      "ruku_number": 331,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 382,
      "juz_number": 20,
      "text": "أَمَّن يَهۡدِيكُمۡ فِي ظُلُمَٰتِ ٱلۡبَرِّ وَٱلۡبَحۡرِ وَمَن يُرۡسِلُ ٱلرِّيَٰحَ بُشۡرَۢا بَيۡنَ يَدَيۡ رَحۡمَتِهِۦٓۗ أَءِلَٰهٞ مَّعَ ٱللَّهِۚ تَعَٰلَى ٱللَّهُ عَمَّا يُشۡرِكُونَ"
    },
    {
      "id": 3223,
      "number": 64,
      "verse_key": "27:64",
      "hizb_number": 39,
      "rub_el_hizb_number": 153,
      "ruku_number": 331,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 383,
      "juz_number": 20,
      "text": "أَمَّن يَبۡدَؤُاْ ٱلۡخَلۡقَ ثُمَّ يُعِيدُهُۥ وَمَن يَرۡزُقُكُم مِّنَ ٱلسَّمَآءِ وَٱلۡأَرۡضِۗ أَءِلَٰهٞ مَّعَ ٱللَّهِۚ قُلۡ هَاتُواْ بُرۡهَٰنَكُمۡ إِن كُنتُمۡ صَٰدِقِينَ"
    },
    {
      "id": 3224,
      "number": 65,
      "verse_key": "27:65",
      "hizb_number": 39,
      "rub_el_hizb_number": 153,
      "ruku_number": 331,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 383,
      "juz_number": 20,
      "text": "قُل لَّا يَعۡلَمُ مَن فِي ٱلسَّمَٰوَٰتِ وَٱلۡأَرۡضِ ٱلۡغَيۡبَ إِلَّا ٱللَّهُۚ وَمَا يَشۡعُرُونَ أَيَّانَ يُبۡعَثُونَ"
    },
    {
      "id": 3225,
      "number": 66,
      "verse_key": "27:66",
      "hizb_number": 39,
      "rub_el_hizb_number": 153,
      "ruku_number": 331,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 383,
      "juz_number": 20,
      "text": "بَلِ ٱدَّٰرَكَ عِلۡمُهُمۡ فِي ٱلۡأٓخِرَةِۚ بَلۡ هُمۡ فِي شَكّٖ مِّنۡهَاۖ بَلۡ هُم مِّنۡهَا عَمُونَ"
    },
    {
      "id": 3226,
      "number": 67,
      "verse_key": "27:67",
      "hizb_number": 39,
      "rub_el_hizb_number": 153,
      "ruku_number": 332,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 383,
      "juz_number": 20,
      "text": "وَقَالَ ٱلَّذِينَ كَفَرُوٓاْ أَءِذَا كُنَّا تُرَٰبٗا وَءَابَآؤُنَآ أَئِنَّا لَمُخۡرَجُونَ"
    },
    {
      "id": 3227,
      "number": 68,
      "verse_key": "27:68",
      "hizb_number": 39,
      "rub_el_hizb_number": 153,
      "ruku_number": 332,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 383,
      "juz_number": 20,
      "text": "لَقَدۡ وُعِدۡنَا هَٰذَا نَحۡنُ وَءَابَآؤُنَا مِن قَبۡلُ إِنۡ هَٰذَآ إِلَّآ أَسَٰطِيرُ ٱلۡأَوَّلِينَ"
    },
    {
      "id": 3228,
      "number": 69,
      "verse_key": "27:69",
      "hizb_number": 39,
      "rub_el_hizb_number": 153,
      "ruku_number": 332,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 383,
      "juz_number": 20,
      "text": "قُلۡ سِيرُواْ فِي ٱلۡأَرۡضِ فَٱنظُرُواْ كَيۡفَ كَانَ عَٰقِبَةُ ٱلۡمُجۡرِمِينَ"
    },
    {
      "id": 3166,
      "number": 7,
      "verse_key": "27:7",
      "hizb_number": 38,
      "rub_el_hizb_number": 151,
      "ruku_number": 327,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 377,
      "juz_number": 19,
      "text": "إِذۡ قَالَ مُوسَىٰ لِأَهۡلِهِۦٓ إِنِّيٓ ءَانَسۡتُ نَارٗا سَـَٔاتِيكُم مِّنۡهَا بِخَبَرٍ أَوۡ ءَاتِيكُم بِشِهَابٖ قَبَسٖ لَّعَلَّكُمۡ تَصۡطَلُونَ"
    },
    {
      "id": 3229,
      "number": 70,
      "verse_key": "27:70",
      "hizb_number": 39,
      "rub_el_hizb_number": 153,
      "ruku_number": 332,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 383,
      "juz_number": 20,
      "text": "وَلَا تَحۡزَنۡ عَلَيۡهِمۡ وَلَا تَكُن فِي ضَيۡقٖ مِّمَّا يَمۡكُرُونَ"
    },
    {
      "id": 3230,
      "number": 71,
      "verse_key": "27:71",
      "hizb_number": 39,
      "rub_el_hizb_number": 153,
      "ruku_number": 332,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 383,
      "juz_number": 20,
      "text": "وَيَقُولُونَ مَتَىٰ هَٰذَا ٱلۡوَعۡدُ إِن كُنتُمۡ صَٰدِقِينَ"
    },
    {
      "id": 3231,
      "number": 72,
      "verse_key": "27:72",
      "hizb_number": 39,
      "rub_el_hizb_number": 153,
      "ruku_number": 332,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 383,
      "juz_number": 20,
      "text": "قُلۡ عَسَىٰٓ أَن يَكُونَ رَدِفَ لَكُم بَعۡضُ ٱلَّذِي تَسۡتَعۡجِلُونَ"
    },
    {
      "id": 3232,
      "number": 73,
      "verse_key": "27:73",
      "hizb_number": 39,
      "rub_el_hizb_number": 153,
      "ruku_number": 332,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 383,
      "juz_number": 20,
      "text": "وَإِنَّ رَبَّكَ لَذُو فَضۡلٍ عَلَى ٱلنَّاسِ وَلَٰكِنَّ أَكۡثَرَهُمۡ لَا يَشۡكُرُونَ"
    },
    {
      "id": 3233,
      "number": 74,
      "verse_key": "27:74",
      "hizb_number": 39,
      "rub_el_hizb_number": 153,
      "ruku_number": 332,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 383,
      "juz_number": 20,
      "text": "وَإِنَّ رَبَّكَ لَيَعۡلَمُ مَا تُكِنُّ صُدُورُهُمۡ وَمَا يُعۡلِنُونَ"
    },
    {
      "id": 3234,
      "number": 75,
      "verse_key": "27:75",
      "hizb_number": 39,
      "rub_el_hizb_number": 153,
      "ruku_number": 332,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 383,
      "juz_number": 20,
      "text": "وَمَا مِنۡ غَآئِبَةٖ فِي ٱلسَّمَآءِ وَٱلۡأَرۡضِ إِلَّا فِي كِتَٰبٖ مُّبِينٍ"
    },
    {
      "id": 3235,
      "number": 76,
      "verse_key": "27:76",
      "hizb_number": 39,
      "rub_el_hizb_number": 153,
      "ruku_number": 332,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 383,
      "juz_number": 20,
      "text": "إِنَّ هَٰذَا ٱلۡقُرۡءَانَ يَقُصُّ عَلَىٰ بَنِيٓ إِسۡرَٰٓءِيلَ أَكۡثَرَ ٱلَّذِي هُمۡ فِيهِ يَخۡتَلِفُونَ"
    },
    {
      "id": 3236,
      "number": 77,
      "verse_key": "27:77",
      "hizb_number": 39,
      "rub_el_hizb_number": 153,
      "ruku_number": 332,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 384,
      "juz_number": 20,
      "text": "وَإِنَّهُۥ لَهُدٗى وَرَحۡمَةٞ لِّلۡمُؤۡمِنِينَ"
    },
    {
      "id": 3237,
      "number": 78,
      "verse_key": "27:78",
      "hizb_number": 39,
      "rub_el_hizb_number": 153,
      "ruku_number": 332,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 384,
      "juz_number": 20,
      "text": "إِنَّ رَبَّكَ يَقۡضِي بَيۡنَهُم بِحُكۡمِهِۦۚ وَهُوَ ٱلۡعَزِيزُ ٱلۡعَلِيمُ"
    },
    {
      "id": 3238,
      "number": 79,
      "verse_key": "27:79",
      "hizb_number": 39,
      "rub_el_hizb_number": 153,
      "ruku_number": 332,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 384,
      "juz_number": 20,
      "text": "فَتَوَكَّلۡ عَلَى ٱللَّهِۖ إِنَّكَ عَلَى ٱلۡحَقِّ ٱلۡمُبِينِ"
    },
    {
      "id": 3167,
      "number": 8,
      "verse_key": "27:8",
      "hizb_number": 38,
      "rub_el_hizb_number": 151,
      "ruku_number": 327,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 377,
      "juz_number": 19,
      "text": "فَلَمَّا جَآءَهَا نُودِيَ أَنۢ بُورِكَ مَن فِي ٱلنَّارِ وَمَنۡ حَوۡلَهَا وَسُبۡحَٰنَ ٱللَّهِ رَبِّ ٱلۡعَٰلَمِينَ"
    },
    {
      "id": 3239,
      "number": 80,
      "verse_key": "27:80",
      "hizb_number": 39,
      "rub_el_hizb_number": 153,
      "ruku_number": 332,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 384,
      "juz_number": 20,
      "text": "إِنَّكَ لَا تُسۡمِعُ ٱلۡمَوۡتَىٰ وَلَا تُسۡمِعُ ٱلصُّمَّ ٱلدُّعَآءَ إِذَا وَلَّوۡاْ مُدۡبِرِينَ"
    },
    {
      "id": 3240,
      "number": 81,
      "verse_key": "27:81",
      "hizb_number": 39,
      "rub_el_hizb_number": 153,
      "ruku_number": 332,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 384,
      "juz_number": 20,
      "text": "وَمَآ أَنتَ بِهَٰدِي ٱلۡعُمۡيِ عَن ضَلَٰلَتِهِمۡۖ إِن تُسۡمِعُ إِلَّا مَن يُؤۡمِنُ بِـَٔايَٰتِنَا فَهُم مُّسۡلِمُونَ"
    },
    {
      "id": 3241,
      "number": 82,
      "verse_key": "27:82",
      "hizb_number": 39,
      "rub_el_hizb_number": 154,
      "ruku_number": 332,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 384,
      "juz_number": 20,
      "text": "۞ وَإِذَا وَقَعَ ٱلۡقَوۡلُ عَلَيۡهِمۡ أَخۡرَجۡنَا لَهُمۡ دَآبَّةٗ مِّنَ ٱلۡأَرۡضِ تُكَلِّمُهُمۡ أَنَّ ٱلنَّاسَ كَانُواْ بِـَٔايَٰتِنَا لَا يُوقِنُونَ"
    },
    {
      "id": 3242,
      "number": 83,
      "verse_key": "27:83",
      "hizb_number": 39,
      "rub_el_hizb_number": 154,
      "ruku_number": 333,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 384,
      "juz_number": 20,
      "text": "وَيَوۡمَ نَحۡشُرُ مِن كُلِّ أُمَّةٖ فَوۡجٗا مِّمَّن يُكَذِّبُ بِـَٔايَٰتِنَا فَهُمۡ يُوزَعُونَ"
    },
    {
      "id": 3243,
      "number": 84,
      "verse_key": "27:84",
      "hizb_number": 39,
      "rub_el_hizb_number": 154,
      "ruku_number": 333,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 384,
      "juz_number": 20,
      "text": "حَتَّىٰٓ إِذَا جَآءُو قَالَ أَكَذَّبۡتُم بِـَٔايَٰتِي وَلَمۡ تُحِيطُواْ بِهَا عِلۡمًا أَمَّاذَا كُنتُمۡ تَعۡمَلُونَ"
    },
    {
      "id": 3244,
      "number": 85,
      "verse_key": "27:85",
      "hizb_number": 39,
      "rub_el_hizb_number": 154,
      "ruku_number": 333,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 384,
      "juz_number": 20,
      "text": "وَوَقَعَ ٱلۡقَوۡلُ عَلَيۡهِم بِمَا ظَلَمُواْ فَهُمۡ لَا يَنطِقُونَ"
    },
    {
      "id": 3245,
      "number": 86,
      "verse_key": "27:86",
      "hizb_number": 39,
      "rub_el_hizb_number": 154,
      "ruku_number": 333,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 384,
      "juz_number": 20,
      "text": "أَلَمۡ يَرَوۡاْ أَنَّا جَعَلۡنَا ٱلَّيۡلَ لِيَسۡكُنُواْ فِيهِ وَٱلنَّهَارَ مُبۡصِرًاۚ إِنَّ فِي ذَٰلِكَ لَأٓيَٰتٖ لِّقَوۡمٖ يُؤۡمِنُونَ"
    },
    {
      "id": 3246,
      "number": 87,
      "verse_key": "27:87",
      "hizb_number": 39,
      "rub_el_hizb_number": 154,
      "ruku_number": 333,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 384,
      "juz_number": 20,
      "text": "وَيَوۡمَ يُنفَخُ فِي ٱلصُّورِ فَفَزِعَ مَن فِي ٱلسَّمَٰوَٰتِ وَمَن فِي ٱلۡأَرۡضِ إِلَّا مَن شَآءَ ٱللَّهُۚ وَكُلٌّ أَتَوۡهُ دَٰخِرِينَ"
    },
    {
      "id": 3247,
      "number": 88,
      "verse_key": "27:88",
      "hizb_number": 39,
      "rub_el_hizb_number": 154,
      "ruku_number": 333,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 384,
      "juz_number": 20,
      "text": "وَتَرَى ٱلۡجِبَالَ تَحۡسَبُهَا جَامِدَةٗ وَهِيَ تَمُرُّ مَرَّ ٱلسَّحَابِۚ صُنۡعَ ٱللَّهِ ٱلَّذِيٓ أَتۡقَنَ كُلَّ شَيۡءٍۚ إِنَّهُۥ خَبِيرُۢ بِمَا تَفۡعَلُونَ"
    },
    {
      "id": 3248,
      "number": 89,
      "verse_key": "27:89",
      "hizb_number": 39,
      "rub_el_hizb_number": 154,
      "ruku_number": 333,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 385,
      "juz_number": 20,
      "text": "مَن جَآءَ بِٱلۡحَسَنَةِ فَلَهُۥ خَيۡرٞ مِّنۡهَا وَهُم مِّن فَزَعٖ يَوۡمَئِذٍ ءَامِنُونَ"
    },
    {
      "id": 3168,
      "number": 9,
      "verse_key": "27:9",
      "hizb_number": 38,
      "rub_el_hizb_number": 151,
      "ruku_number": 327,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 377,
      "juz_number": 19,
      "text": "يَٰمُوسَىٰٓ إِنَّهُۥٓ أَنَا ٱللَّهُ ٱلۡعَزِيزُ ٱلۡحَكِيمُ"
    },
    {
      "id": 3249,
      "number": 90,
      "verse_key": "27:90",
      "hizb_number": 39,
      "rub_el_hizb_number": 154,
      "ruku_number": 333,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 385,
      "juz_number": 20,
      "text": "وَمَن جَآءَ بِٱلسَّيِّئَةِ فَكُبَّتۡ وُجُوهُهُمۡ فِي ٱلنَّارِ هَلۡ تُجۡزَوۡنَ إِلَّا مَا كُنتُمۡ تَعۡمَلُونَ"
    },
    {
      "id": 3250,
      "number": 91,
      "verse_key": "27:91",
      "hizb_number": 39,
      "rub_el_hizb_number": 154,
      "ruku_number": 333,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 385,
      "juz_number": 20,
      "text": "إِنَّمَآ أُمِرۡتُ أَنۡ أَعۡبُدَ رَبَّ هَٰذِهِ ٱلۡبَلۡدَةِ ٱلَّذِي حَرَّمَهَا وَلَهُۥ كُلُّ شَيۡءٖۖ وَأُمِرۡتُ أَنۡ أَكُونَ مِنَ ٱلۡمُسۡلِمِينَ"
    },
    {
      "id": 3251,
      "number": 92,
      "verse_key": "27:92",
      "hizb_number": 39,
      "rub_el_hizb_number": 154,
      "ruku_number": 333,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 385,
      "juz_number": 20,
      "text": "وَأَنۡ أَتۡلُوَاْ ٱلۡقُرۡءَانَۖ فَمَنِ ٱهۡتَدَىٰ فَإِنَّمَا يَهۡتَدِي لِنَفۡسِهِۦۖ وَمَن ضَلَّ فَقُلۡ إِنَّمَآ أَنَا۠ مِنَ ٱلۡمُنذِرِينَ"
    },
    {
      "id": 3252,
      "number": 93,
      "verse_key": "27:93",
      "hizb_number": 39,
      "rub_el_hizb_number": 154,
      "ruku_number": 333,
      "manzil_number": 5,
      "sajdah_number": null,
      "page_number": 385,
      "juz_number": 20,
      "text": "وَقُلِ ٱلۡحَمۡدُ لِلَّهِ سَيُرِيكُمۡ ءَايَٰتِهِۦ فَتَعۡرِفُونَهَاۚ وَمَا رَبُّكَ بِغَٰفِلٍ عَمَّا تَعۡمَلُونَ"
    }
  ]
}