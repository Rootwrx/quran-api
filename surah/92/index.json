{
  "verses": [
    {
      "id": 6059,
      "number": 1,
      "verse_key": "92:1",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 536,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 595,
      "juz_number": 30,
      "text": "وَٱلَّيۡلِ إِذَا يَغۡشَىٰ"
    },
    {
      "id": 6068,
      "number": 10,
      "verse_key": "92:10",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 536,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 595,
      "juz_number": 30,
      "text": "فَسَنُيَسِّرُهُۥ لِلۡعُسۡرَىٰ"
    },
    {
      "id": 6069,
      "number": 11,
      "verse_key": "92:11",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 536,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 595,
      "juz_number": 30,
      "text": "وَمَا يُغۡنِي عَنۡهُ مَالُهُۥٓ إِذَا تَرَدَّىٰٓ"
    },
    {
      "id": 6070,
      "number": 12,
      "verse_key": "92:12",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 536,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 595,
      "juz_number": 30,
      "text": "إِنَّ عَلَيۡنَا لَلۡهُدَىٰ"
    },
    {
      "id": 6071,
      "number": 13,
      "verse_key": "92:13",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 536,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 595,
      "juz_number": 30,
      "text": "وَإِنَّ لَنَا لَلۡأٓخِرَةَ وَٱلۡأُولَىٰ"
    },
    {
      "id": 6072,
      "number": 14,
      "verse_key": "92:14",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 536,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 595,
      "juz_number": 30,
      "text": "فَأَنذَرۡتُكُمۡ نَارٗا تَلَظَّىٰ"
    },
    {
      "id": 6073,
      "number": 15,
      "verse_key": "92:15",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 536,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 596,
      "juz_number": 30,
      "text": "لَا يَصۡلَىٰهَآ إِلَّا ٱلۡأَشۡقَى"
    },
    {
      "id": 6074,
      "number": 16,
      "verse_key": "92:16",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 536,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 596,
      "juz_number": 30,
      "text": "ٱلَّذِي كَذَّبَ وَتَوَلَّىٰ"
    },
    {
      "id": 6075,
      "number": 17,
      "verse_key": "92:17",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 536,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 596,
      "juz_number": 30,
      "text": "وَسَيُجَنَّبُهَا ٱلۡأَتۡقَى"
    },
    {
      "id": 6076,
      "number": 18,
      "verse_key": "92:18",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 536,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 596,
      "juz_number": 30,
      "text": "ٱلَّذِي يُؤۡتِي مَالَهُۥ يَتَزَكَّىٰ"
    },
    {
      "id": 6077,
      "number": 19,
      "verse_key": "92:19",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 536,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 596,
      "juz_number": 30,
      "text": "وَمَا لِأَحَدٍ عِندَهُۥ مِن نِّعۡمَةٖ تُجۡزَىٰٓ"
    },
    {
      "id": 6060,
      "number": 2,
      "verse_key": "92:2",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 536,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 595,
      "juz_number": 30,
      "text": "وَٱلنَّهَارِ إِذَا تَجَلَّىٰ"
    },
    {
      "id": 6078,
      "number": 20,
      "verse_key": "92:20",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 536,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 596,
      "juz_number": 30,
      "text": "إِلَّا ٱبۡتِغَآءَ وَجۡهِ رَبِّهِ ٱلۡأَعۡلَىٰ"
    },
    {
      "id": 6079,
      "number": 21,
      "verse_key": "92:21",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 536,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 596,
      "juz_number": 30,
      "text": "وَلَسَوۡفَ يَرۡضَىٰ"
    },
    {
      "id": 6061,
      "number": 3,
      "verse_key": "92:3",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 536,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 595,
      "juz_number": 30,
      "text": "وَمَا خَلَقَ ٱلذَّكَرَ وَٱلۡأُنثَىٰٓ"
    },
    {
      "id": 6062,
      "number": 4,
      "verse_key": "92:4",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 536,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 595,
      "juz_number": 30,
      "text": "إِنَّ سَعۡيَكُمۡ لَشَتَّىٰ"
    },
    {
      "id": 6063,
      "number": 5,
      "verse_key": "92:5",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 536,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 595,
      "juz_number": 30,
      "text": "فَأَمَّا مَنۡ أَعۡطَىٰ وَٱتَّقَىٰ"
    },
    {
      "id": 6064,
      "number": 6,
      "verse_key": "92:6",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 536,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 595,
      "juz_number": 30,
      "text": "وَصَدَّقَ بِٱلۡحُسۡنَىٰ"
    },
    {
      "id": 6065,
      "number": 7,
      "verse_key": "92:7",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 536,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 595,
      "juz_number": 30,
      "text": "فَسَنُيَسِّرُهُۥ لِلۡيُسۡرَىٰ"
    },
    {
      "id": 6066,
      "number": 8,
      "verse_key": "92:8",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 536,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 595,
      "juz_number": 30,
      "text": "وَأَمَّا مَنۢ بَخِلَ وَٱسۡتَغۡنَىٰ"
    },
    {
      "id": 6067,
      "number": 9,
      "verse_key": "92:9",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 536,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 595,
      "juz_number": 30,
      "text": "وَكَذَّبَ بِٱلۡحُسۡنَىٰ"
    }
  ]
}