{
  "verses": [
    {
      "id": 4676,
      "number": 1,
      "verse_key": "51:1",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 520,
      "juz_number": 26,
      "text": "وَٱلذَّٰرِيَٰتِ ذَرۡوٗا"
    },
    {
      "id": 4685,
      "number": 10,
      "verse_key": "51:10",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "text": "قُتِلَ ٱلۡخَرَّٰصُونَ"
    },
    {
      "id": 4686,
      "number": 11,
      "verse_key": "51:11",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "text": "ٱلَّذِينَ هُمۡ فِي غَمۡرَةٖ سَاهُونَ"
    },
    {
      "id": 4687,
      "number": 12,
      "verse_key": "51:12",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "text": "يَسۡـَٔلُونَ أَيَّانَ يَوۡمُ ٱلدِّينِ"
    },
    {
      "id": 4688,
      "number": 13,
      "verse_key": "51:13",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "text": "يَوۡمَ هُمۡ عَلَى ٱلنَّارِ يُفۡتَنُونَ"
    },
    {
      "id": 4689,
      "number": 14,
      "verse_key": "51:14",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "text": "ذُوقُواْ فِتۡنَتَكُمۡ هَٰذَا ٱلَّذِي كُنتُم بِهِۦ تَسۡتَعۡجِلُونَ"
    },
    {
      "id": 4690,
      "number": 15,
      "verse_key": "51:15",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "text": "إِنَّ ٱلۡمُتَّقِينَ فِي جَنَّٰتٖ وَعُيُونٍ"
    },
    {
      "id": 4691,
      "number": 16,
      "verse_key": "51:16",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "text": "ءَاخِذِينَ مَآ ءَاتَىٰهُمۡ رَبُّهُمۡۚ إِنَّهُمۡ كَانُواْ قَبۡلَ ذَٰلِكَ مُحۡسِنِينَ"
    },
    {
      "id": 4692,
      "number": 17,
      "verse_key": "51:17",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "text": "كَانُواْ قَلِيلٗا مِّنَ ٱلَّيۡلِ مَا يَهۡجَعُونَ"
    },
    {
      "id": 4693,
      "number": 18,
      "verse_key": "51:18",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "text": "وَبِٱلۡأَسۡحَارِ هُمۡ يَسۡتَغۡفِرُونَ"
    },
    {
      "id": 4694,
      "number": 19,
      "verse_key": "51:19",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "text": "وَفِيٓ أَمۡوَٰلِهِمۡ حَقّٞ لِّلسَّآئِلِ وَٱلۡمَحۡرُومِ"
    },
    {
      "id": 4677,
      "number": 2,
      "verse_key": "51:2",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 520,
      "juz_number": 26,
      "text": "فَٱلۡحَٰمِلَٰتِ وِقۡرٗا"
    },
    {
      "id": 4695,
      "number": 20,
      "verse_key": "51:20",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "text": "وَفِي ٱلۡأَرۡضِ ءَايَٰتٞ لِّلۡمُوقِنِينَ"
    },
    {
      "id": 4696,
      "number": 21,
      "verse_key": "51:21",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "text": "وَفِيٓ أَنفُسِكُمۡۚ أَفَلَا تُبۡصِرُونَ"
    },
    {
      "id": 4697,
      "number": 22,
      "verse_key": "51:22",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "text": "وَفِي ٱلسَّمَآءِ رِزۡقُكُمۡ وَمَا تُوعَدُونَ"
    },
    {
      "id": 4698,
      "number": 23,
      "verse_key": "51:23",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "text": "فَوَرَبِّ ٱلسَّمَآءِ وَٱلۡأَرۡضِ إِنَّهُۥ لَحَقّٞ مِّثۡلَ مَآ أَنَّكُمۡ تَنطِقُونَ"
    },
    {
      "id": 4699,
      "number": 24,
      "verse_key": "51:24",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "text": "هَلۡ أَتَىٰكَ حَدِيثُ ضَيۡفِ إِبۡرَٰهِيمَ ٱلۡمُكۡرَمِينَ"
    },
    {
      "id": 4700,
      "number": 25,
      "verse_key": "51:25",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "text": "إِذۡ دَخَلُواْ عَلَيۡهِ فَقَالُواْ سَلَٰمٗاۖ قَالَ سَلَٰمٞ قَوۡمٞ مُّنكَرُونَ"
    },
    {
      "id": 4701,
      "number": 26,
      "verse_key": "51:26",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "text": "فَرَاغَ إِلَىٰٓ أَهۡلِهِۦ فَجَآءَ بِعِجۡلٖ سَمِينٖ"
    },
    {
      "id": 4702,
      "number": 27,
      "verse_key": "51:27",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "text": "فَقَرَّبَهُۥٓ إِلَيۡهِمۡ قَالَ أَلَا تَأۡكُلُونَ"
    },
    {
      "id": 4703,
      "number": 28,
      "verse_key": "51:28",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "text": "فَأَوۡجَسَ مِنۡهُمۡ خِيفَةٗۖ قَالُواْ لَا تَخَفۡۖ وَبَشَّرُوهُ بِغُلَٰمٍ عَلِيمٖ"
    },
    {
      "id": 4704,
      "number": 29,
      "verse_key": "51:29",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "text": "فَأَقۡبَلَتِ ٱمۡرَأَتُهُۥ فِي صَرَّةٖ فَصَكَّتۡ وَجۡهَهَا وَقَالَتۡ عَجُوزٌ عَقِيمٞ"
    },
    {
      "id": 4678,
      "number": 3,
      "verse_key": "51:3",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 520,
      "juz_number": 26,
      "text": "فَٱلۡجَٰرِيَٰتِ يُسۡرٗا"
    },
    {
      "id": 4705,
      "number": 30,
      "verse_key": "51:30",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "text": "قَالُواْ كَذَٰلِكِ قَالَ رَبُّكِۖ إِنَّهُۥ هُوَ ٱلۡحَكِيمُ ٱلۡعَلِيمُ"
    },
    {
      "id": 4706,
      "number": 31,
      "verse_key": "51:31",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 522,
      "juz_number": 27,
      "text": "۞ قَالَ فَمَا خَطۡبُكُمۡ أَيُّهَا ٱلۡمُرۡسَلُونَ"
    },
    {
      "id": 4707,
      "number": 32,
      "verse_key": "51:32",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 522,
      "juz_number": 27,
      "text": "قَالُوٓاْ إِنَّآ أُرۡسِلۡنَآ إِلَىٰ قَوۡمٖ مُّجۡرِمِينَ"
    },
    {
      "id": 4708,
      "number": 33,
      "verse_key": "51:33",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 522,
      "juz_number": 27,
      "text": "لِنُرۡسِلَ عَلَيۡهِمۡ حِجَارَةٗ مِّن طِينٖ"
    },
    {
      "id": 4709,
      "number": 34,
      "verse_key": "51:34",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 522,
      "juz_number": 27,
      "text": "مُّسَوَّمَةً عِندَ رَبِّكَ لِلۡمُسۡرِفِينَ"
    },
    {
      "id": 4710,
      "number": 35,
      "verse_key": "51:35",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 522,
      "juz_number": 27,
      "text": "فَأَخۡرَجۡنَا مَن كَانَ فِيهَا مِنَ ٱلۡمُؤۡمِنِينَ"
    },
    {
      "id": 4711,
      "number": 36,
      "verse_key": "51:36",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 522,
      "juz_number": 27,
      "text": "فَمَا وَجَدۡنَا فِيهَا غَيۡرَ بَيۡتٖ مِّنَ ٱلۡمُسۡلِمِينَ"
    },
    {
      "id": 4712,
      "number": 37,
      "verse_key": "51:37",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 522,
      "juz_number": 27,
      "text": "وَتَرَكۡنَا فِيهَآ ءَايَةٗ لِّلَّذِينَ يَخَافُونَ ٱلۡعَذَابَ ٱلۡأَلِيمَ"
    },
    {
      "id": 4713,
      "number": 38,
      "verse_key": "51:38",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 522,
      "juz_number": 27,
      "text": "وَفِي مُوسَىٰٓ إِذۡ أَرۡسَلۡنَٰهُ إِلَىٰ فِرۡعَوۡنَ بِسُلۡطَٰنٖ مُّبِينٖ"
    },
    {
      "id": 4714,
      "number": 39,
      "verse_key": "51:39",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 522,
      "juz_number": 27,
      "text": "فَتَوَلَّىٰ بِرُكۡنِهِۦ وَقَالَ سَٰحِرٌ أَوۡ مَجۡنُونٞ"
    },
    {
      "id": 4679,
      "number": 4,
      "verse_key": "51:4",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 520,
      "juz_number": 26,
      "text": "فَٱلۡمُقَسِّمَٰتِ أَمۡرًا"
    },
    {
      "id": 4715,
      "number": 40,
      "verse_key": "51:40",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 522,
      "juz_number": 27,
      "text": "فَأَخَذۡنَٰهُ وَجُنُودَهُۥ فَنَبَذۡنَٰهُمۡ فِي ٱلۡيَمِّ وَهُوَ مُلِيمٞ"
    },
    {
      "id": 4716,
      "number": 41,
      "verse_key": "51:41",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 522,
      "juz_number": 27,
      "text": "وَفِي عَادٍ إِذۡ أَرۡسَلۡنَا عَلَيۡهِمُ ٱلرِّيحَ ٱلۡعَقِيمَ"
    },
    {
      "id": 4717,
      "number": 42,
      "verse_key": "51:42",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 522,
      "juz_number": 27,
      "text": "مَا تَذَرُ مِن شَيۡءٍ أَتَتۡ عَلَيۡهِ إِلَّا جَعَلَتۡهُ كَٱلرَّمِيمِ"
    },
    {
      "id": 4718,
      "number": 43,
      "verse_key": "51:43",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 522,
      "juz_number": 27,
      "text": "وَفِي ثَمُودَ إِذۡ قِيلَ لَهُمۡ تَمَتَّعُواْ حَتَّىٰ حِينٖ"
    },
    {
      "id": 4719,
      "number": 44,
      "verse_key": "51:44",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 522,
      "juz_number": 27,
      "text": "فَعَتَوۡاْ عَنۡ أَمۡرِ رَبِّهِمۡ فَأَخَذَتۡهُمُ ٱلصَّٰعِقَةُ وَهُمۡ يَنظُرُونَ"
    },
    {
      "id": 4720,
      "number": 45,
      "verse_key": "51:45",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 522,
      "juz_number": 27,
      "text": "فَمَا ٱسۡتَطَٰعُواْ مِن قِيَامٖ وَمَا كَانُواْ مُنتَصِرِينَ"
    },
    {
      "id": 4721,
      "number": 46,
      "verse_key": "51:46",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 522,
      "juz_number": 27,
      "text": "وَقَوۡمَ نُوحٖ مِّن قَبۡلُۖ إِنَّهُمۡ كَانُواْ قَوۡمٗا فَٰسِقِينَ"
    },
    {
      "id": 4722,
      "number": 47,
      "verse_key": "51:47",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 522,
      "juz_number": 27,
      "text": "وَٱلسَّمَآءَ بَنَيۡنَٰهَا بِأَيۡيْدٖ وَإِنَّا لَمُوسِعُونَ"
    },
    {
      "id": 4723,
      "number": 48,
      "verse_key": "51:48",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 522,
      "juz_number": 27,
      "text": "وَٱلۡأَرۡضَ فَرَشۡنَٰهَا فَنِعۡمَ ٱلۡمَٰهِدُونَ"
    },
    {
      "id": 4724,
      "number": 49,
      "verse_key": "51:49",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 522,
      "juz_number": 27,
      "text": "وَمِن كُلِّ شَيۡءٍ خَلَقۡنَا زَوۡجَيۡنِ لَعَلَّكُمۡ تَذَكَّرُونَ"
    },
    {
      "id": 4680,
      "number": 5,
      "verse_key": "51:5",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 520,
      "juz_number": 26,
      "text": "إِنَّمَا تُوعَدُونَ لَصَادِقٞ"
    },
    {
      "id": 4725,
      "number": 50,
      "verse_key": "51:50",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 522,
      "juz_number": 27,
      "text": "فَفِرُّوٓاْ إِلَى ٱللَّهِۖ إِنِّي لَكُم مِّنۡهُ نَذِيرٞ مُّبِينٞ"
    },
    {
      "id": 4726,
      "number": 51,
      "verse_key": "51:51",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 522,
      "juz_number": 27,
      "text": "وَلَا تَجۡعَلُواْ مَعَ ٱللَّهِ إِلَٰهًا ءَاخَرَۖ إِنِّي لَكُم مِّنۡهُ نَذِيرٞ مُّبِينٞ"
    },
    {
      "id": 4727,
      "number": 52,
      "verse_key": "51:52",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 523,
      "juz_number": 27,
      "text": "كَذَٰلِكَ مَآ أَتَى ٱلَّذِينَ مِن قَبۡلِهِم مِّن رَّسُولٍ إِلَّا قَالُواْ سَاحِرٌ أَوۡ مَجۡنُونٌ"
    },
    {
      "id": 4728,
      "number": 53,
      "verse_key": "51:53",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 523,
      "juz_number": 27,
      "text": "أَتَوَاصَوۡاْ بِهِۦۚ بَلۡ هُمۡ قَوۡمٞ طَاغُونَ"
    },
    {
      "id": 4729,
      "number": 54,
      "verse_key": "51:54",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 523,
      "juz_number": 27,
      "text": "فَتَوَلَّ عَنۡهُمۡ فَمَآ أَنتَ بِمَلُومٖ"
    },
    {
      "id": 4730,
      "number": 55,
      "verse_key": "51:55",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 523,
      "juz_number": 27,
      "text": "وَذَكِّرۡ فَإِنَّ ٱلذِّكۡرَىٰ تَنفَعُ ٱلۡمُؤۡمِنِينَ"
    },
    {
      "id": 4731,
      "number": 56,
      "verse_key": "51:56",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 523,
      "juz_number": 27,
      "text": "وَمَا خَلَقۡتُ ٱلۡجِنَّ وَٱلۡإِنسَ إِلَّا لِيَعۡبُدُونِ"
    },
    {
      "id": 4732,
      "number": 57,
      "verse_key": "51:57",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 523,
      "juz_number": 27,
      "text": "مَآ أُرِيدُ مِنۡهُم مِّن رِّزۡقٖ وَمَآ أُرِيدُ أَن يُطۡعِمُونِ"
    },
    {
      "id": 4733,
      "number": 58,
      "verse_key": "51:58",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 523,
      "juz_number": 27,
      "text": "إِنَّ ٱللَّهَ هُوَ ٱلرَّزَّاقُ ذُو ٱلۡقُوَّةِ ٱلۡمَتِينُ"
    },
    {
      "id": 4734,
      "number": 59,
      "verse_key": "51:59",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 523,
      "juz_number": 27,
      "text": "فَإِنَّ لِلَّذِينَ ظَلَمُواْ ذَنُوبٗا مِّثۡلَ ذَنُوبِ أَصۡحَٰبِهِمۡ فَلَا يَسۡتَعۡجِلُونِ"
    },
    {
      "id": 4681,
      "number": 6,
      "verse_key": "51:6",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 520,
      "juz_number": 26,
      "text": "وَإِنَّ ٱلدِّينَ لَوَٰقِعٞ"
    },
    {
      "id": 4735,
      "number": 60,
      "verse_key": "51:60",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 523,
      "juz_number": 27,
      "text": "فَوَيۡلٞ لِّلَّذِينَ كَفَرُواْ مِن يَوۡمِهِمُ ٱلَّذِي يُوعَدُونَ"
    },
    {
      "id": 4682,
      "number": 7,
      "verse_key": "51:7",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "text": "وَٱلسَّمَآءِ ذَاتِ ٱلۡحُبُكِ"
    },
    {
      "id": 4683,
      "number": 8,
      "verse_key": "51:8",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "text": "إِنَّكُمۡ لَفِي قَوۡلٖ مُّخۡتَلِفٖ"
    },
    {
      "id": 4684,
      "number": 9,
      "verse_key": "51:9",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "text": "يُؤۡفَكُ عَنۡهُ مَنۡ أُفِكَ"
    }
  ]
}