      "ruku_number": 544,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 599,
      "juz_number": 30,
      "text": "وَٱلۡعَٰدِيَٰتِ ضَبۡحٗا"
    },
    {
      "id": 6148,
      "number": 2,
      "verse_key": "100:2",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 544,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 599,
      "juz_number": 30,
      "text": "فَٱلۡمُورِيَٰتِ قَدۡحٗا"
    },
    {
      "id": 6149,
      "number": 3,
      "verse_key": "100:3",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 544,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 599,
      "juz_number": 30,
      "text": "فَٱلۡمُغِيرَٰتِ صُبۡحٗا"
    },
    {
      "id": 6150,
      "number": 4,
      "verse_key": "100:4",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 544,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 599,
      "juz_number": 30,
      "text": "فَأَثَرۡنَ بِهِۦ نَقۡعٗا"
    },
    {
      "id": 6151,
      "number": 5,
      "verse_key": "100:5",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 544,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 599,
      "juz_number": 30,
      "text": "فَوَسَطۡنَ بِهِۦ جَمۡعًا"
    },
    {
      "id": 6152,
      "number": 6,
      "verse_key": "100:6",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 544,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 599,
      "juz_number": 30,
      "text": "إِنَّ ٱلۡإِنسَٰنَ لِرَبِّهِۦ لَكَنُودٞ"
    },
    {
      "id": 6153,
      "number": 7,
      "verse_key": "100:7",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 544,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 599,
      "juz_number": 30,
      "text": "وَإِنَّهُۥ عَلَىٰ ذَٰلِكَ لَشَهِيدٞ"
    },
    {
      "id": 6154,
      "number": 8,
      "verse_key": "100:8",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 544,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 599,
      "juz_number": 30,
      "text": "وَإِنَّهُۥ لِحُبِّ ٱلۡخَيۡرِ لَشَدِيدٌ"
    },
    {
      "id": 6155,
      "number": 9,
      "verse_key": "100:9",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 544,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 599,
      "juz_number": 30,
      "text": "۞ أَفَلَا يَعۡلَمُ إِذَا بُعۡثِرَ مَا فِي ٱلۡقُبُورِ"
    },
    {
      "id": 6156,
      "number": 10,
      "verse_key": "100:10",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 544,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 600,
      "juz_number": 30,
      "text": "وَحُصِّلَ مَا فِي ٱلصُّدُورِ"
    },
    {
      "id": 6157,
      "number": 11,
      "verse_key": "100:11",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 544,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 600,
      "juz_number": 30,
      "text": "إِنَّ رَبَّهُم بِهِمۡ يَوۡمَئِذٖ لَّخَبِيرُۢ"
    }
  ]
}