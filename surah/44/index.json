{
  "verses": [
    {
      "id": 4415,
      "number": 1,
      "verse_key": "44:1",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 496,
      "juz_number": 25,
      "text": "حمٓ"
    },
    {
      "id": 4424,
      "number": 10,
      "verse_key": "44:10",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 496,
      "juz_number": 25,
      "text": "فَٱرۡتَقِبۡ يَوۡمَ تَأۡتِي ٱلسَّمَآءُ بِدُخَانٖ مُّبِينٖ"
    },
    {
      "id": 4425,
      "number": 11,
      "verse_key": "44:11",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 496,
      "juz_number": 25,
      "text": "يَغۡشَى ٱلنَّاسَۖ هَٰذَا عَذَابٌ أَلِيمٞ"
    },
    {
      "id": 4426,
      "number": 12,
      "verse_key": "44:12",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 496,
      "juz_number": 25,
      "text": "رَّبَّنَا ٱكۡشِفۡ عَنَّا ٱلۡعَذَابَ إِنَّا مُؤۡمِنُونَ"
    },
    {
      "id": 4427,
      "number": 13,
      "verse_key": "44:13",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 496,
      "juz_number": 25,
      "text": "أَنَّىٰ لَهُمُ ٱلذِّكۡرَىٰ وَقَدۡ جَآءَهُمۡ رَسُولٞ مُّبِينٞ"
    },
    {
      "id": 4428,
      "number": 14,
      "verse_key": "44:14",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 496,
      "juz_number": 25,
      "text": "ثُمَّ تَوَلَّوۡاْ عَنۡهُ وَقَالُواْ مُعَلَّمٞ مَّجۡنُونٌ"
    },
    {
      "id": 4429,
      "number": 15,
      "verse_key": "44:15",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 496,
      "juz_number": 25,
      "text": "إِنَّا كَاشِفُواْ ٱلۡعَذَابِ قَلِيلًاۚ إِنَّكُمۡ عَآئِدُونَ"
    },
    {
      "id": 4430,
      "number": 16,
      "verse_key": "44:16",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 496,
      "juz_number": 25,
      "text": "يَوۡمَ نَبۡطِشُ ٱلۡبَطۡشَةَ ٱلۡكُبۡرَىٰٓ إِنَّا مُنتَقِمُونَ"
    },
    {
      "id": 4431,
      "number": 17,
      "verse_key": "44:17",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 496,
      "juz_number": 25,
      "text": "۞ وَلَقَدۡ فَتَنَّا قَبۡلَهُمۡ قَوۡمَ فِرۡعَوۡنَ وَجَآءَهُمۡ رَسُولٞ كَرِيمٌ"
    },
    {
      "id": 4432,
      "number": 18,
      "verse_key": "44:18",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 496,
      "juz_number": 25,
      "text": "أَنۡ أَدُّوٓاْ إِلَيَّ عِبَادَ ٱللَّهِۖ إِنِّي لَكُمۡ رَسُولٌ أَمِينٞ"
    },
    {
      "id": 4433,
      "number": 19,
      "verse_key": "44:19",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 497,
      "juz_number": 25,
      "text": "وَأَن لَّا تَعۡلُواْ عَلَى ٱللَّهِۖ إِنِّيٓ ءَاتِيكُم بِسُلۡطَٰنٖ مُّبِينٖ"
    },
    {
      "id": 4416,
      "number": 2,
      "verse_key": "44:2",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 496,
      "juz_number": 25,
      "text": "وَٱلۡكِتَٰبِ ٱلۡمُبِينِ"
    },
    {
      "id": 4434,
      "number": 20,
      "verse_key": "44:20",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 497,
      "juz_number": 25,
      "text": "وَإِنِّي عُذۡتُ بِرَبِّي وَرَبِّكُمۡ أَن تَرۡجُمُونِ"
    },
    {
      "id": 4435,
      "number": 21,
      "verse_key": "44:21",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 497,
      "juz_number": 25,
      "text": "وَإِن لَّمۡ تُؤۡمِنُواْ لِي فَٱعۡتَزِلُونِ"
    },
    {
      "id": 4436,
      "number": 22,
      "verse_key": "44:22",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 497,
      "juz_number": 25,
      "text": "فَدَعَا رَبَّهُۥٓ أَنَّ هَٰٓؤُلَآءِ قَوۡمٞ مُّجۡرِمُونَ"
    },
    {
      "id": 4437,
      "number": 23,
      "verse_key": "44:23",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 497,
      "juz_number": 25,
      "text": "فَأَسۡرِ بِعِبَادِي لَيۡلًا إِنَّكُم مُّتَّبَعُونَ"
    },
    {
      "id": 4438,
      "number": 24,
      "verse_key": "44:24",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 497,
      "juz_number": 25,
      "text": "وَٱتۡرُكِ ٱلۡبَحۡرَ رَهۡوًاۖ إِنَّهُمۡ جُندٞ مُّغۡرَقُونَ"
    },
    {
      "id": 4439,
      "number": 25,
      "verse_key": "44:25",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 497,
      "juz_number": 25,
      "text": "كَمۡ تَرَكُواْ مِن جَنَّٰتٖ وَعُيُونٖ"
    },
    {
      "id": 4440,
      "number": 26,
      "verse_key": "44:26",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 497,
      "juz_number": 25,
      "text": "وَزُرُوعٖ وَمَقَامٖ كَرِيمٖ"
    },
    {
      "id": 4441,
      "number": 27,
      "verse_key": "44:27",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 497,
      "juz_number": 25,
      "text": "وَنَعۡمَةٖ كَانُواْ فِيهَا فَٰكِهِينَ"
    },
    {
      "id": 4442,
      "number": 28,
      "verse_key": "44:28",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 497,
      "juz_number": 25,
      "text": "كَذَٰلِكَۖ وَأَوۡرَثۡنَٰهَا قَوۡمًا ءَاخَرِينَ"
    },
    {
      "id": 4443,
      "number": 29,
      "verse_key": "44:29",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 497,
      "juz_number": 25,
      "text": "فَمَا بَكَتۡ عَلَيۡهِمُ ٱلسَّمَآءُ وَٱلۡأَرۡضُ وَمَا كَانُواْ مُنظَرِينَ"
    },
    {
      "id": 4417,
      "number": 3,
      "verse_key": "44:3",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 496,
      "juz_number": 25,
      "text": "إِنَّآ أَنزَلۡنَٰهُ فِي لَيۡلَةٖ مُّبَٰرَكَةٍۚ إِنَّا كُنَّا مُنذِرِينَ"
    },
    {
      "id": 4444,
      "number": 30,
      "verse_key": "44:30",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 434,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 497,
      "juz_number": 25,
      "text": "وَلَقَدۡ نَجَّيۡنَا بَنِيٓ إِسۡرَٰٓءِيلَ مِنَ ٱلۡعَذَابِ ٱلۡمُهِينِ"
    },
    {
      "id": 4445,
      "number": 31,
      "verse_key": "44:31",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 434,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 497,
      "juz_number": 25,
      "text": "مِن فِرۡعَوۡنَۚ إِنَّهُۥ كَانَ عَالِيٗا مِّنَ ٱلۡمُسۡرِفِينَ"
    },
    {
      "id": 4446,
      "number": 32,
      "verse_key": "44:32",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 434,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 497,
      "juz_number": 25,
      "text": "وَلَقَدِ ٱخۡتَرۡنَٰهُمۡ عَلَىٰ عِلۡمٍ عَلَى ٱلۡعَٰلَمِينَ"
    },
    {
      "id": 4447,
      "number": 33,
      "verse_key": "44:33",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 434,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 497,
      "juz_number": 25,
      "text": "وَءَاتَيۡنَٰهُم مِّنَ ٱلۡأٓيَٰتِ مَا فِيهِ بَلَٰٓؤٞاْ مُّبِينٌ"
    },
    {
      "id": 4448,
      "number": 34,
      "verse_key": "44:34",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 434,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 497,
      "juz_number": 25,
      "text": "إِنَّ هَٰٓؤُلَآءِ لَيَقُولُونَ"
    },
    {
      "id": 4449,
      "number": 35,
      "verse_key": "44:35",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 434,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 497,
      "juz_number": 25,
      "text": "إِنۡ هِيَ إِلَّا مَوۡتَتُنَا ٱلۡأُولَىٰ وَمَا نَحۡنُ بِمُنشَرِينَ"
    },
    {
      "id": 4450,
      "number": 36,
      "verse_key": "44:36",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 434,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 497,
      "juz_number": 25,
      "text": "فَأۡتُواْ بِـَٔابَآئِنَآ إِن كُنتُمۡ صَٰدِقِينَ"
    },
    {
      "id": 4451,
      "number": 37,
      "verse_key": "44:37",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 434,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 497,
      "juz_number": 25,
      "text": "أَهُمۡ خَيۡرٌ أَمۡ قَوۡمُ تُبَّعٖ وَٱلَّذِينَ مِن قَبۡلِهِمۡ أَهۡلَكۡنَٰهُمۡۚ إِنَّهُمۡ كَانُواْ مُجۡرِمِينَ"
    },
    {
      "id": 4452,
      "number": 38,
      "verse_key": "44:38",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 434,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 497,
      "juz_number": 25,
      "text": "وَمَا خَلَقۡنَا ٱلسَّمَٰوَٰتِ وَٱلۡأَرۡضَ وَمَا بَيۡنَهُمَا لَٰعِبِينَ"
    },
    {
      "id": 4453,
      "number": 39,
      "verse_key": "44:39",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 434,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 497,
      "juz_number": 25,
      "text": "مَا خَلَقۡنَٰهُمَآ إِلَّا بِٱلۡحَقِّ وَلَٰكِنَّ أَكۡثَرَهُمۡ لَا يَعۡلَمُونَ"
    },
    {
      "id": 4418,
      "number": 4,
      "verse_key": "44:4",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 496,
      "juz_number": 25,
      "text": "فِيهَا يُفۡرَقُ كُلُّ أَمۡرٍ حَكِيمٍ"
    },
    {
      "id": 4454,
      "number": 40,
      "verse_key": "44:40",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 434,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 498,
      "juz_number": 25,
      "text": "إِنَّ يَوۡمَ ٱلۡفَصۡلِ مِيقَٰتُهُمۡ أَجۡمَعِينَ"
    },
    {
      "id": 4455,
      "number": 41,
      "verse_key": "44:41",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 434,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 498,
      "juz_number": 25,
      "text": "يَوۡمَ لَا يُغۡنِي مَوۡلًى عَن مَّوۡلٗى شَيۡـٔٗا وَلَا هُمۡ يُنصَرُونَ"
    },
    {
      "id": 4456,
      "number": 42,
      "verse_key": "44:42",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 434,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 498,
      "juz_number": 25,
      "text": "إِلَّا مَن رَّحِمَ ٱللَّهُۚ إِنَّهُۥ هُوَ ٱلۡعَزِيزُ ٱلرَّحِيمُ"
    },
    {
      "id": 4457,
      "number": 43,
      "verse_key": "44:43",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 435,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 498,
      "juz_number": 25,
      "text": "إِنَّ شَجَرَتَ ٱلزَّقُّومِ"
    },
    {
      "id": 4458,
      "number": 44,
      "verse_key": "44:44",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 435,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 498,
      "juz_number": 25,
      "text": "طَعَامُ ٱلۡأَثِيمِ"
    },
    {
      "id": 4459,
      "number": 45,
      "verse_key": "44:45",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 435,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 498,
      "juz_number": 25,
      "text": "كَٱلۡمُهۡلِ يَغۡلِي فِي ٱلۡبُطُونِ"
    },
    {
      "id": 4460,
      "number": 46,
      "verse_key": "44:46",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 435,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 498,
      "juz_number": 25,
      "text": "كَغَلۡيِ ٱلۡحَمِيمِ"
    },
    {
      "id": 4461,
      "number": 47,
      "verse_key": "44:47",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 435,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 498,
      "juz_number": 25,
      "text": "خُذُوهُ فَٱعۡتِلُوهُ إِلَىٰ سَوَآءِ ٱلۡجَحِيمِ"
    },
    {
      "id": 4462,
      "number": 48,
      "verse_key": "44:48",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 435,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 498,
      "juz_number": 25,
      "text": "ثُمَّ صُبُّواْ فَوۡقَ رَأۡسِهِۦ مِنۡ عَذَابِ ٱلۡحَمِيمِ"
    },
    {
      "id": 4463,
      "number": 49,
      "verse_key": "44:49",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 435,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 498,
      "juz_number": 25,
      "text": "ذُقۡ إِنَّكَ أَنتَ ٱلۡعَزِيزُ ٱلۡكَرِيمُ"
    },
    {
      "id": 4419,
      "number": 5,
      "verse_key": "44:5",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 496,
      "juz_number": 25,
      "text": "أَمۡرٗا مِّنۡ عِندِنَآۚ إِنَّا كُنَّا مُرۡسِلِينَ"
    },
    {
      "id": 4464,
      "number": 50,
      "verse_key": "44:50",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 435,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 498,
      "juz_number": 25,
      "text": "إِنَّ هَٰذَا مَا كُنتُم بِهِۦ تَمۡتَرُونَ"
    },
    {
      "id": 4465,
      "number": 51,
      "verse_key": "44:51",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 435,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 498,
      "juz_number": 25,
      "text": "إِنَّ ٱلۡمُتَّقِينَ فِي مَقَامٍ أَمِينٖ"
    },
    {
      "id": 4466,
      "number": 52,
      "verse_key": "44:52",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 435,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 498,
      "juz_number": 25,
      "text": "فِي جَنَّٰتٖ وَعُيُونٖ"
    },
    {
      "id": 4467,
      "number": 53,
      "verse_key": "44:53",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 435,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 498,
      "juz_number": 25,
      "text": "يَلۡبَسُونَ مِن سُندُسٖ وَإِسۡتَبۡرَقٖ مُّتَقَٰبِلِينَ"
    },
    {
      "id": 4468,
      "number": 54,
      "verse_key": "44:54",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 435,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 498,
      "juz_number": 25,
      "text": "كَذَٰلِكَ وَزَوَّجۡنَٰهُم بِحُورٍ عِينٖ"
    },
    {
      "id": 4469,
      "number": 55,
      "verse_key": "44:55",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 435,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 498,
      "juz_number": 25,
      "text": "يَدۡعُونَ فِيهَا بِكُلِّ فَٰكِهَةٍ ءَامِنِينَ"
    },
    {
      "id": 4470,
      "number": 56,
      "verse_key": "44:56",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 435,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 498,
      "juz_number": 25,
      "text": "لَا يَذُوقُونَ فِيهَا ٱلۡمَوۡتَ إِلَّا ٱلۡمَوۡتَةَ ٱلۡأُولَىٰۖ وَوَقَىٰهُمۡ عَذَابَ ٱلۡجَحِيمِ"
    },
    {
      "id": 4471,
      "number": 57,
      "verse_key": "44:57",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 435,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 498,
      "juz_number": 25,
      "text": "فَضۡلٗا مِّن رَّبِّكَۚ ذَٰلِكَ هُوَ ٱلۡفَوۡزُ ٱلۡعَظِيمُ"
    },
    {
      "id": 4472,
      "number": 58,
      "verse_key": "44:58",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 435,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 498,
      "juz_number": 25,
      "text": "فَإِنَّمَا يَسَّرۡنَٰهُ بِلِسَانِكَ لَعَلَّهُمۡ يَتَذَكَّرُونَ"
    },
    {
      "id": 4473,
      "number": 59,
      "verse_key": "44:59",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 435,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 498,
      "juz_number": 25,
      "text": "فَٱرۡتَقِبۡ إِنَّهُم مُّرۡتَقِبُونَ"
    },
    {
      "id": 4420,
      "number": 6,
      "verse_key": "44:6",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 496,
      "juz_number": 25,
      "text": "رَحۡمَةٗ مِّن رَّبِّكَۚ إِنَّهُۥ هُوَ ٱلسَّمِيعُ ٱلۡعَلِيمُ"
    },
    {
      "id": 4421,
      "number": 7,
      "verse_key": "44:7",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 496,
      "juz_number": 25,
      "text": "رَبِّ ٱلسَّمَٰوَٰتِ وَٱلۡأَرۡضِ وَمَا بَيۡنَهُمَآۖ إِن كُنتُم مُّوقِنِينَ"
    },
    {
      "id": 4422,
      "number": 8,
      "verse_key": "44:8",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 496,
      "juz_number": 25,
      "text": "لَآ إِلَٰهَ إِلَّا هُوَ يُحۡيِۦ وَيُمِيتُۖ رَبُّكُمۡ وَرَبُّ ءَابَآئِكُمُ ٱلۡأَوَّلِينَ"
    },
    {
      "id": 4423,
      "number": 9,
      "verse_key": "44:9",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 496,
      "juz_number": 25,
      "text": "بَلۡ هُمۡ فِي شَكّٖ يَلۡعَبُونَ"
    }
  ]
}