{
  "verses": [
    {
      "id": 5932,
      "number": 1,
      "verse_key": "86:1",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 530,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 591,
      "juz_number": 30,
      "text": "وَٱلسَّمَآءِ وَٱلطَّارِقِ"
    },
    {
      "id": 5941,
      "number": 10,
      "verse_key": "86:10",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 530,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 591,
      "juz_number": 30,
      "text": "فَمَا لَهُۥ مِن قُوَّةٖ وَلَا نَاصِرٖ"
    },
    {
      "id": 5942,
      "number": 11,
      "verse_key": "86:11",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 530,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 591,
      "juz_number": 30,
      "text": "وَٱلسَّمَآءِ ذَاتِ ٱلرَّجۡعِ"
    },
    {
      "id": 5943,
      "number": 12,
      "verse_key": "86:12",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 530,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 591,
      "juz_number": 30,
      "text": "وَٱلۡأَرۡضِ ذَاتِ ٱلصَّدۡعِ"
    },
    {
      "id": 5944,
      "number": 13,
      "verse_key": "86:13",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 530,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 591,
      "juz_number": 30,
      "text": "إِنَّهُۥ لَقَوۡلٞ فَصۡلٞ"
    },
    {
      "id": 5945,
      "number": 14,
      "verse_key": "86:14",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 530,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 591,
      "juz_number": 30,
      "text": "وَمَا هُوَ بِٱلۡهَزۡلِ"
    },
    {
      "id": 5946,
      "number": 15,
      "verse_key": "86:15",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 530,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 591,
      "juz_number": 30,
      "text": "إِنَّهُمۡ يَكِيدُونَ كَيۡدٗا"
    },
    {
      "id": 5947,
      "number": 16,
      "verse_key": "86:16",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 530,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 591,
      "juz_number": 30,
      "text": "وَأَكِيدُ كَيۡدٗا"
    },
    {
      "id": 5948,
      "number": 17,
      "verse_key": "86:17",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 530,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 591,
      "juz_number": 30,
      "text": "فَمَهِّلِ ٱلۡكَٰفِرِينَ أَمۡهِلۡهُمۡ رُوَيۡدَۢا"
    },
    {
      "id": 5933,
      "number": 2,
      "verse_key": "86:2",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 530,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 591,
      "juz_number": 30,
      "text": "وَمَآ أَدۡرَىٰكَ مَا ٱلطَّارِقُ"
    },
    {
      "id": 5934,
      "number": 3,
      "verse_key": "86:3",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 530,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 591,
      "juz_number": 30,
      "text": "ٱلنَّجۡمُ ٱلثَّاقِبُ"
    },
    {
      "id": 5935,
      "number": 4,
      "verse_key": "86:4",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 530,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 591,
      "juz_number": 30,
      "text": "إِن كُلُّ نَفۡسٖ لَّمَّا عَلَيۡهَا حَافِظٞ"
    },
    {
      "id": 5936,
      "number": 5,
      "verse_key": "86:5",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 530,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 591,
      "juz_number": 30,
      "text": "فَلۡيَنظُرِ ٱلۡإِنسَٰنُ مِمَّ خُلِقَ"
    },
    {
      "id": 5937,
      "number": 6,
      "verse_key": "86:6",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 530,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 591,
      "juz_number": 30,
      "text": "خُلِقَ مِن مَّآءٖ دَافِقٖ"
    },
    {
      "id": 5938,
      "number": 7,
      "verse_key": "86:7",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 530,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 591,
      "juz_number": 30,
      "text": "يَخۡرُجُ مِنۢ بَيۡنِ ٱلصُّلۡبِ وَٱلتَّرَآئِبِ"
    },
    {
      "id": 5939,
      "number": 8,
      "verse_key": "86:8",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 530,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 591,
      "juz_number": 30,
      "text": "إِنَّهُۥ عَلَىٰ رَجۡعِهِۦ لَقَادِرٞ"
    },
    {
      "id": 5940,
      "number": 9,
      "verse_key": "86:9",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 530,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 591,
      "juz_number": 30,
      "text": "يَوۡمَ تُبۡلَى ٱلسَّرَآئِرُ"
    }
  ]
}