{
  "verses": [
    {
      "id": 5324,
      "number": 1,
      "verse_key": "69:1",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 566,
      "juz_number": 29,
      "text": "ٱلۡحَآقَّةُ"
    },
    {
      "id": 5325,
      "number": 2,
      "verse_key": "69:2",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 566,
      "juz_number": 29,
      "text": "مَا ٱلۡحَآقَّةُ"
    },
    {
      "id": 5326,
      "number": 3,
      "verse_key": "69:3",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 566,
      "juz_number": 29,
      "text": "وَمَآ أَدۡرَىٰكَ مَا ٱلۡحَآقَّةُ"
    },
    {
      "id": 5327,
      "number": 4,
      "verse_key": "69:4",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 566,
      "juz_number": 29,
      "text": "كَذَّبَتۡ ثَمُودُ وَعَادُۢ بِٱلۡقَارِعَةِ"
    },
    {
      "id": 5328,
      "number": 5,
      "verse_key": "69:5",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 566,
      "juz_number": 29,
      "text": "فَأَمَّا ثَمُودُ فَأُهۡلِكُواْ بِٱلطَّاغِيَةِ"
    },
    {
      "id": 5329,
      "number": 6,
      "verse_key": "69:6",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 566,
      "juz_number": 29,
      "text": "وَأَمَّا عَادٞ فَأُهۡلِكُواْ بِرِيحٖ صَرۡصَرٍ عَاتِيَةٖ"
    },
    {
      "id": 5330,
      "number": 7,
      "verse_key": "69:7",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 566,
      "juz_number": 29,
      "text": "سَخَّرَهَا عَلَيۡهِمۡ سَبۡعَ لَيَالٖ وَثَمَٰنِيَةَ أَيَّامٍ حُسُومٗاۖ فَتَرَى ٱلۡقَوۡمَ فِيهَا صَرۡعَىٰ كَأَنَّهُمۡ أَعۡجَازُ نَخۡلٍ خَاوِيَةٖ"
    },
    {
      "id": 5331,
      "number": 8,
      "verse_key": "69:8",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 566,
      "juz_number": 29,
      "text": "فَهَلۡ تَرَىٰ لَهُم مِّنۢ بَاقِيَةٖ"
    },
    {
      "id": 5332,
      "number": 9,
      "verse_key": "69:9",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 567,
      "juz_number": 29,
      "text": "وَجَآءَ فِرۡعَوۡنُ وَمَن قَبۡلَهُۥ وَٱلۡمُؤۡتَفِكَٰتُ بِٱلۡخَاطِئَةِ"
    },
    {
      "id": 5333,
      "number": 10,
      "verse_key": "69:10",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 567,
      "juz_number": 29,
      "text": "فَعَصَوۡاْ رَسُولَ رَبِّهِمۡ فَأَخَذَهُمۡ أَخۡذَةٗ رَّابِيَةً"
    },
    {
      "id": 5334,
      "number": 11,
      "verse_key": "69:11",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 567,
      "juz_number": 29,
      "text": "إِنَّا لَمَّا طَغَا ٱلۡمَآءُ حَمَلۡنَٰكُمۡ فِي ٱلۡجَارِيَةِ"
    },
    {
      "id": 5335,
      "number": 12,
      "verse_key": "69:12",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 567,
      "juz_number": 29,
      "text": "لِنَجۡعَلَهَا لَكُمۡ تَذۡكِرَةٗ وَتَعِيَهَآ أُذُنٞ وَٰعِيَةٞ"
    },
    {
      "id": 5336,
      "number": 13,
      "verse_key": "69:13",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 567,
      "juz_number": 29,
      "text": "فَإِذَا نُفِخَ فِي ٱلصُّورِ نَفۡخَةٞ وَٰحِدَةٞ"
    },
    {
      "id": 5337,
      "number": 14,
      "verse_key": "69:14",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 567,
      "juz_number": 29,
      "text": "وَحُمِلَتِ ٱلۡأَرۡضُ وَٱلۡجِبَالُ فَدُكَّتَا دَكَّةٗ وَٰحِدَةٗ"
    },
    {
      "id": 5338,
      "number": 15,
      "verse_key": "69:15",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 567,
      "juz_number": 29,
      "text": "فَيَوۡمَئِذٖ وَقَعَتِ ٱلۡوَاقِعَةُ"
    },
    {
      "id": 5339,
      "number": 16,
      "verse_key": "69:16",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 567,
      "juz_number": 29,
      "text": "وَٱنشَقَّتِ ٱلسَّمَآءُ فَهِيَ يَوۡمَئِذٖ وَاهِيَةٞ"
    },
    {
      "id": 5340,
      "number": 17,
      "verse_key": "69:17",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 567,
      "juz_number": 29,
      "text": "وَٱلۡمَلَكُ عَلَىٰٓ أَرۡجَآئِهَاۚ وَيَحۡمِلُ عَرۡشَ رَبِّكَ فَوۡقَهُمۡ يَوۡمَئِذٖ ثَمَٰنِيَةٞ"
    },
    {
      "id": 5341,
      "number": 18,
      "verse_key": "69:18",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 567,
      "juz_number": 29,
      "text": "يَوۡمَئِذٖ تُعۡرَضُونَ لَا تَخۡفَىٰ مِنكُمۡ خَافِيَةٞ"
    },
    {
      "id": 5342,
      "number": 19,
      "verse_key": "69:19",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 567,
      "juz_number": 29,
      "text": "فَأَمَّا مَنۡ أُوتِيَ كِتَٰبَهُۥ بِيَمِينِهِۦ فَيَقُولُ هَآؤُمُ ٱقۡرَءُواْ كِتَٰبِيَهۡ"
    },
    {
      "id": 5343,
      "number": 20,
      "verse_key": "69:20",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 567,
      "juz_number": 29,
      "text": "إِنِّي ظَنَنتُ أَنِّي مُلَٰقٍ حِسَابِيَهۡ"
    },
    {
      "id": 5344,
      "number": 21,
      "verse_key": "69:21",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 567,
      "juz_number": 29,
      "text": "فَهُوَ فِي عِيشَةٖ رَّاضِيَةٖ"
    },
    {
      "id": 5345,
      "number": 22,
      "verse_key": "69:22",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 567,
      "juz_number": 29,
      "text": "فِي جَنَّةٍ عَالِيَةٖ"
    },
    {
      "id": 5346,
      "number": 23,
      "verse_key": "69:23",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 567,
      "juz_number": 29,
      "text": "قُطُوفُهَا دَانِيَةٞ"
    },
    {
      "id": 5347,
      "number": 24,
      "verse_key": "69:24",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 567,
      "juz_number": 29,
      "text": "كُلُواْ وَٱشۡرَبُواْ هَنِيٓـَٔۢا بِمَآ أَسۡلَفۡتُمۡ فِي ٱلۡأَيَّامِ ٱلۡخَالِيَةِ"
    },
    {
      "id": 5348,
      "number": 25,
      "verse_key": "69:25",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 567,
      "juz_number": 29,
      "text": "وَأَمَّا مَنۡ أُوتِيَ كِتَٰبَهُۥ بِشِمَالِهِۦ فَيَقُولُ يَٰلَيۡتَنِي لَمۡ أُوتَ كِتَٰبِيَهۡ"
    },
    {
      "id": 5349,
      "number": 26,
      "verse_key": "69:26",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 567,
      "juz_number": 29,
      "text": "وَلَمۡ أَدۡرِ مَا حِسَابِيَهۡ"
    },
    {
      "id": 5350,
      "number": 27,
      "verse_key": "69:27",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 567,
      "juz_number": 29,
      "text": "يَٰلَيۡتَهَا كَانَتِ ٱلۡقَاضِيَةَ"
    },
    {
      "id": 5351,
      "number": 28,
      "verse_key": "69:28",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 567,
      "juz_number": 29,
      "text": "مَآ أَغۡنَىٰ عَنِّي مَالِيَهۡۜ"
    },
    {
      "id": 5352,
      "number": 29,
      "verse_key": "69:29",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 567,
      "juz_number": 29,
      "text": "هَلَكَ عَنِّي سُلۡطَٰنِيَهۡ"
    },
    {
      "id": 5353,
      "number": 30,
      "verse_key": "69:30",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 567,
      "juz_number": 29,
      "text": "خُذُوهُ فَغُلُّوهُ"
    },
    {
      "id": 5354,
      "number": 31,
      "verse_key": "69:31",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 567,
      "juz_number": 29,
      "text": "ثُمَّ ٱلۡجَحِيمَ صَلُّوهُ"
    },
    {
      "id": 5355,
      "number": 32,
      "verse_key": "69:32",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 567,
      "juz_number": 29,
      "text": "ثُمَّ فِي سِلۡسِلَةٖ ذَرۡعُهَا سَبۡعُونَ ذِرَاعٗا فَٱسۡلُكُوهُ"
    },
    {
      "id": 5356,
      "number": 33,
      "verse_key": "69:33",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 567,
      "juz_number": 29,
      "text": "إِنَّهُۥ كَانَ لَا يُؤۡمِنُ بِٱللَّهِ ٱلۡعَظِيمِ"
    },
    {
      "id": 5357,
      "number": 34,
      "verse_key": "69:34",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 567,
      "juz_number": 29,
      "text": "وَلَا يَحُضُّ عَلَىٰ طَعَامِ ٱلۡمِسۡكِينِ"
    },
    {
      "id": 5358,
      "number": 35,
      "verse_key": "69:35",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 568,
      "juz_number": 29,
      "text": "فَلَيۡسَ لَهُ ٱلۡيَوۡمَ هَٰهُنَا حَمِيمٞ"
    },
    {
      "id": 5359,
      "number": 36,
      "verse_key": "69:36",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 568,
      "juz_number": 29,
      "text": "وَلَا طَعَامٌ إِلَّا مِنۡ غِسۡلِينٖ"
    },
    {
      "id": 5360,
      "number": 37,
      "verse_key": "69:37",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 568,
      "juz_number": 29,
      "text": "لَّا يَأۡكُلُهُۥٓ إِلَّا ٱلۡخَٰطِـُٔونَ"
    },
    {
      "id": 5361,
      "number": 38,
      "verse_key": "69:38",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 503,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 568,
      "juz_number": 29,
      "text": "فَلَآ أُقۡسِمُ بِمَا تُبۡصِرُونَ"
    },
    {
      "id": 5362,
      "number": 39,
      "verse_key": "69:39",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 503,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 568,
      "juz_number": 29,
      "text": "وَمَا لَا تُبۡصِرُونَ"
    },
    {
      "id": 5363,
      "number": 40,
      "verse_key": "69:40",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 503,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 568,
      "juz_number": 29,
      "text": "إِنَّهُۥ لَقَوۡلُ رَسُولٖ كَرِيمٖ"
    },
    {
      "id": 5364,
      "number": 41,
      "verse_key": "69:41",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 503,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 568,
      "juz_number": 29,
      "text": "وَمَا هُوَ بِقَوۡلِ شَاعِرٖۚ قَلِيلٗا مَّا تُؤۡمِنُونَ"
    },
    {
      "id": 5365,
      "number": 42,
      "verse_key": "69:42",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 503,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 568,
      "juz_number": 29,
      "text": "وَلَا بِقَوۡلِ كَاهِنٖۚ قَلِيلٗا مَّا تَذَكَّرُونَ"
    },
    {
      "id": 5366,
      "number": 43,
      "verse_key": "69:43",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 503,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 568,
      "juz_number": 29,
      "text": "تَنزِيلٞ مِّن رَّبِّ ٱلۡعَٰلَمِينَ"
    },
    {
      "id": 5367,
      "number": 44,
      "verse_key": "69:44",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 503,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 568,
      "juz_number": 29,
      "text": "وَلَوۡ تَقَوَّلَ عَلَيۡنَا بَعۡضَ ٱلۡأَقَاوِيلِ"
    },
    {
