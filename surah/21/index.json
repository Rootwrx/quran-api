{
  "verses": [
    {
      "id": 2484,
      "number": 1,
      "verse_key": "21:1",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 278,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 322,
      "juz_number": 17,
      "text": "ٱقۡتَرَبَ لِلنَّاسِ حِسَابُهُمۡ وَهُمۡ فِي غَفۡلَةٖ مُّعۡرِضُونَ"
    },
    {
      "id": 2493,
      "number": 10,
      "verse_key": "21:10",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 278,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 322,
      "juz_number": 17,
      "text": "لَقَدۡ أَنزَلۡنَآ إِلَيۡكُمۡ كِتَٰبٗا فِيهِ ذِكۡرُكُمۡۚ أَفَلَا تَعۡقِلُونَ"
    },
    {
      "id": 2583,
      "number": 100,
      "verse_key": "21:100",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 284,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 330,
      "juz_number": 17,
      "text": "لَهُمۡ فِيهَا زَفِيرٞ وَهُمۡ فِيهَا لَا يَسۡمَعُونَ"
    },
    {
      "id": 2584,
      "number": 101,
      "verse_key": "21:101",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 284,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 330,
      "juz_number": 17,
      "text": "إِنَّ ٱلَّذِينَ سَبَقَتۡ لَهُم مِّنَّا ٱلۡحُسۡنَىٰٓ أُوْلَٰٓئِكَ عَنۡهَا مُبۡعَدُونَ"
    },
    {
      "id": 2585,
      "number": 102,
      "verse_key": "21:102",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 284,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 331,
      "juz_number": 17,
      "text": "لَا يَسۡمَعُونَ حَسِيسَهَاۖ وَهُمۡ فِي مَا ٱشۡتَهَتۡ أَنفُسُهُمۡ خَٰلِدُونَ"
    },
    {
      "id": 2586,
      "number": 103,
      "verse_key": "21:103",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 284,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 331,
      "juz_number": 17,
      "text": "لَا يَحۡزُنُهُمُ ٱلۡفَزَعُ ٱلۡأَكۡبَرُ وَتَتَلَقَّىٰهُمُ ٱلۡمَلَٰٓئِكَةُ هَٰذَا يَوۡمُكُمُ ٱلَّذِي كُنتُمۡ تُوعَدُونَ"
    },
    {
      "id": 2587,
      "number": 104,
      "verse_key": "21:104",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 284,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 331,
      "juz_number": 17,
      "text": "يَوۡمَ نَطۡوِي ٱلسَّمَآءَ كَطَيِّ ٱلسِّجِلِّ لِلۡكُتُبِۚ كَمَا بَدَأۡنَآ أَوَّلَ خَلۡقٖ نُّعِيدُهُۥۚ وَعۡدًا عَلَيۡنَآۚ إِنَّا كُنَّا فَٰعِلِينَ"
    },
    {
      "id": 2588,
      "number": 105,
      "verse_key": "21:105",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 284,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 331,
      "juz_number": 17,
      "text": "وَلَقَدۡ كَتَبۡنَا فِي ٱلزَّبُورِ مِنۢ بَعۡدِ ٱلذِّكۡرِ أَنَّ ٱلۡأَرۡضَ يَرِثُهَا عِبَادِيَ ٱلصَّٰلِحُونَ"
    },
    {
      "id": 2589,
      "number": 106,
      "verse_key": "21:106",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 284,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 331,
      "juz_number": 17,
      "text": "إِنَّ فِي هَٰذَا لَبَلَٰغٗا لِّقَوۡمٍ عَٰبِدِينَ"
    },
    {
      "id": 2590,
      "number": 107,
      "verse_key": "21:107",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 284,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 331,
      "juz_number": 17,
      "text": "وَمَآ أَرۡسَلۡنَٰكَ إِلَّا رَحۡمَةٗ لِّلۡعَٰلَمِينَ"
    },
    {
      "id": 2591,
      "number": 108,
      "verse_key": "21:108",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 284,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 331,
      "juz_number": 17,
      "text": "قُلۡ إِنَّمَا يُوحَىٰٓ إِلَيَّ أَنَّمَآ إِلَٰهُكُمۡ إِلَٰهٞ وَٰحِدٞۖ فَهَلۡ أَنتُم مُّسۡلِمُونَ"
    },
    {
      "id": 2592,
      "number": 109,
      "verse_key": "21:109",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 284,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 331,
      "juz_number": 17,
      "text": "فَإِن تَوَلَّوۡاْ فَقُلۡ ءَاذَنتُكُمۡ عَلَىٰ سَوَآءٖۖ وَإِنۡ أَدۡرِيٓ أَقَرِيبٌ أَم بَعِيدٞ مَّا تُوعَدُونَ"
    },
    {
      "id": 2494,
      "number": 11,
      "verse_key": "21:11",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 323,
      "juz_number": 17,
      "text": "وَكَمۡ قَصَمۡنَا مِن قَرۡيَةٖ كَانَتۡ ظَالِمَةٗ وَأَنشَأۡنَا بَعۡدَهَا قَوۡمًا ءَاخَرِينَ"
    },
    {
      "id": 2593,
      "number": 110,
      "verse_key": "21:110",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 284,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 331,
      "juz_number": 17,
      "text": "إِنَّهُۥ يَعۡلَمُ ٱلۡجَهۡرَ مِنَ ٱلۡقَوۡلِ وَيَعۡلَمُ مَا تَكۡتُمُونَ"
    },
    {
      "id": 2594,
      "number": 111,
      "verse_key": "21:111",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 284,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 331,
      "juz_number": 17,
      "text": "وَإِنۡ أَدۡرِي لَعَلَّهُۥ فِتۡنَةٞ لَّكُمۡ وَمَتَٰعٌ إِلَىٰ حِينٖ"
    },
    {
      "id": 2595,
      "number": 112,
      "verse_key": "21:112",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 284,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 331,
      "juz_number": 17,
      "text": "قَٰلَ رَبِّ ٱحۡكُم بِٱلۡحَقِّۗ وَرَبُّنَا ٱلرَّحۡمَٰنُ ٱلۡمُسۡتَعَانُ عَلَىٰ مَا تَصِفُونَ"
    },
    {
      "id": 2495,
      "number": 12,
      "verse_key": "21:12",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 323,
      "juz_number": 17,
      "text": "فَلَمَّآ أَحَسُّواْ بَأۡسَنَآ إِذَا هُم مِّنۡهَا يَرۡكُضُونَ"
    },
    {
      "id": 2496,
      "number": 13,
      "verse_key": "21:13",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 323,
      "juz_number": 17,
      "text": "لَا تَرۡكُضُواْ وَٱرۡجِعُوٓاْ إِلَىٰ مَآ أُتۡرِفۡتُمۡ فِيهِ وَمَسَٰكِنِكُمۡ لَعَلَّكُمۡ تُسۡـَٔلُونَ"
    },
    {
      "id": 2497,
      "number": 14,
      "verse_key": "21:14",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 323,
      "juz_number": 17,
      "text": "قَالُواْ يَٰوَيۡلَنَآ إِنَّا كُنَّا ظَٰلِمِينَ"
    },
    {
      "id": 2498,
      "number": 15,
      "verse_key": "21:15",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 323,
      "juz_number": 17,
      "text": "فَمَا زَالَت تِّلۡكَ دَعۡوَىٰهُمۡ حَتَّىٰ جَعَلۡنَٰهُمۡ حَصِيدًا خَٰمِدِينَ"
    },
    {
      "id": 2499,
      "number": 16,
      "verse_key": "21:16",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 323,
      "juz_number": 17,
      "text": "وَمَا خَلَقۡنَا ٱلسَّمَآءَ وَٱلۡأَرۡضَ وَمَا بَيۡنَهُمَا لَٰعِبِينَ"
    },
    {
      "id": 2500,
      "number": 17,
      "verse_key": "21:17",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 323,
      "juz_number": 17,
      "text": "لَوۡ أَرَدۡنَآ أَن نَّتَّخِذَ لَهۡوٗا لَّٱتَّخَذۡنَٰهُ مِن لَّدُنَّآ إِن كُنَّا فَٰعِلِينَ"
    },
    {
      "id": 2501,
      "number": 18,
      "verse_key": "21:18",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 323,
      "juz_number": 17,
      "text": "بَلۡ نَقۡذِفُ بِٱلۡحَقِّ عَلَى ٱلۡبَٰطِلِ فَيَدۡمَغُهُۥ فَإِذَا هُوَ زَاهِقٞۚ وَلَكُمُ ٱلۡوَيۡلُ مِمَّا تَصِفُونَ"
    },
    {
      "id": 2502,
      "number": 19,
      "verse_key": "21:19",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 323,
      "juz_number": 17,
      "text": "وَلَهُۥ مَن فِي ٱلسَّمَٰوَٰتِ وَٱلۡأَرۡضِۚ وَمَنۡ عِندَهُۥ لَا يَسۡتَكۡبِرُونَ عَنۡ عِبَادَتِهِۦ وَلَا يَسۡتَحۡسِرُونَ"
    },
    {
      "id": 2485,
      "number": 2,
      "verse_key": "21:2",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 278,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 322,
      "juz_number": 17,
      "text": "مَا يَأۡتِيهِم مِّن ذِكۡرٖ مِّن رَّبِّهِم مُّحۡدَثٍ إِلَّا ٱسۡتَمَعُوهُ وَهُمۡ يَلۡعَبُونَ"
    },
    {
      "id": 2503,
      "number": 20,
      "verse_key": "21:20",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 323,
      "juz_number": 17,
      "text": "يُسَبِّحُونَ ٱلَّيۡلَ وَٱلنَّهَارَ لَا يَفۡتُرُونَ"
    },
    {
      "id": 2504,
      "number": 21,
      "verse_key": "21:21",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 323,
      "juz_number": 17,
      "text": "أَمِ ٱتَّخَذُوٓاْ ءَالِهَةٗ مِّنَ ٱلۡأَرۡضِ هُمۡ يُنشِرُونَ"
    },
    {
      "id": 2505,
      "number": 22,
      "verse_key": "21:22",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 323,
      "juz_number": 17,
      "text": "لَوۡ كَانَ فِيهِمَآ ءَالِهَةٌ إِلَّا ٱللَّهُ لَفَسَدَتَاۚ فَسُبۡحَٰنَ ٱللَّهِ رَبِّ ٱلۡعَرۡشِ عَمَّا يَصِفُونَ"
    },
    {
      "id": 2506,
      "number": 23,
      "verse_key": "21:23",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 323,
      "juz_number": 17,
      "text": "لَا يُسۡـَٔلُ عَمَّا يَفۡعَلُ وَهُمۡ يُسۡـَٔلُونَ"
    },
    {
      "id": 2507,
      "number": 24,
      "verse_key": "21:24",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 323,
      "juz_number": 17,
      "text": "أَمِ ٱتَّخَذُواْ مِن دُونِهِۦٓ ءَالِهَةٗۖ قُلۡ هَاتُواْ بُرۡهَٰنَكُمۡۖ هَٰذَا ذِكۡرُ مَن مَّعِيَ وَذِكۡرُ مَن قَبۡلِيۚ بَلۡ أَكۡثَرُهُمۡ لَا يَعۡلَمُونَ ٱلۡحَقَّۖ فَهُم مُّعۡرِضُونَ"
    },
    {
      "id": 2508,
      "number": 25,
      "verse_key": "21:25",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 324,
      "juz_number": 17,
      "text": "وَمَآ أَرۡسَلۡنَا مِن قَبۡلِكَ مِن رَّسُولٍ إِلَّا نُوحِيٓ إِلَيۡهِ أَنَّهُۥ لَآ إِلَٰهَ إِلَّآ أَنَا۠ فَٱعۡبُدُونِ"
    },
    {
      "id": 2509,
      "number": 26,
      "verse_key": "21:26",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 324,
      "juz_number": 17,
      "text": "وَقَالُواْ ٱتَّخَذَ ٱلرَّحۡمَٰنُ وَلَدٗاۗ سُبۡحَٰنَهُۥۚ بَلۡ عِبَادٞ مُّكۡرَمُونَ"
    },
    {
      "id": 2510,
      "number": 27,
      "verse_key": "21:27",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 324,
      "juz_number": 17,
      "text": "لَا يَسۡبِقُونَهُۥ بِٱلۡقَوۡلِ وَهُم بِأَمۡرِهِۦ يَعۡمَلُونَ"
    },
    {
      "id": 2511,
      "number": 28,
      "verse_key": "21:28",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 324,
      "juz_number": 17,
      "text": "يَعۡلَمُ مَا بَيۡنَ أَيۡدِيهِمۡ وَمَا خَلۡفَهُمۡ وَلَا يَشۡفَعُونَ إِلَّا لِمَنِ ٱرۡتَضَىٰ وَهُم مِّنۡ خَشۡيَتِهِۦ مُشۡفِقُونَ"
    },
    {
      "id": 2512,
      "number": 29,
      "verse_key": "21:29",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 324,
      "juz_number": 17,
      "text": "۞ وَمَن يَقُلۡ مِنۡهُمۡ إِنِّيٓ إِلَٰهٞ مِّن دُونِهِۦ فَذَٰلِكَ نَجۡزِيهِ جَهَنَّمَۚ كَذَٰلِكَ نَجۡزِي ٱلظَّٰلِمِينَ"
    },
    {
      "id": 2486,
      "number": 3,
      "verse_key": "21:3",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 278,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 322,
      "juz_number": 17,
      "text": "لَاهِيَةٗ قُلُوبُهُمۡۗ وَأَسَرُّواْ ٱلنَّجۡوَى ٱلَّذِينَ ظَلَمُواْ هَلۡ هَٰذَآ إِلَّا بَشَرٞ مِّثۡلُكُمۡۖ أَفَتَأۡتُونَ ٱلسِّحۡرَ وَأَنتُمۡ تُبۡصِرُونَ"
    },
    {
      "id": 2513,
      "number": 30,
      "verse_key": "21:30",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 280,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 324,
      "juz_number": 17,
      "text": "أَوَلَمۡ يَرَ ٱلَّذِينَ كَفَرُوٓاْ أَنَّ ٱلسَّمَٰوَٰتِ وَٱلۡأَرۡضَ كَانَتَا رَتۡقٗا فَفَتَقۡنَٰهُمَاۖ وَجَعَلۡنَا مِنَ ٱلۡمَآءِ كُلَّ شَيۡءٍ حَيٍّۚ أَفَلَا يُؤۡمِنُونَ"
    },
    {
      "id": 2514,
      "number": 31,
      "verse_key": "21:31",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 280,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 324,
      "juz_number": 17,
      "text": "وَجَعَلۡنَا فِي ٱلۡأَرۡضِ رَوَٰسِيَ أَن تَمِيدَ بِهِمۡ وَجَعَلۡنَا فِيهَا فِجَاجٗا سُبُلٗا لَّعَلَّهُمۡ يَهۡتَدُونَ"
    },
    {
      "id": 2515,
      "number": 32,
      "verse_key": "21:32",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 280,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 324,
      "juz_number": 17,
      "text": "وَجَعَلۡنَا ٱلسَّمَآءَ سَقۡفٗا مَّحۡفُوظٗاۖ وَهُمۡ عَنۡ ءَايَٰتِهَا مُعۡرِضُونَ"
    },
    {
      "id": 2516,
      "number": 33,
      "verse_key": "21:33",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 280,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 324,
      "juz_number": 17,
      "text": "وَهُوَ ٱلَّذِي خَلَقَ ٱلَّيۡلَ وَٱلنَّهَارَ وَٱلشَّمۡسَ وَٱلۡقَمَرَۖ كُلّٞ فِي فَلَكٖ يَسۡبَحُونَ"
    },
    {
      "id": 2517,
      "number": 34,
      "verse_key": "21:34",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 280,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 324,
      "juz_number": 17,
      "text": "وَمَا جَعَلۡنَا لِبَشَرٖ مِّن قَبۡلِكَ ٱلۡخُلۡدَۖ أَفَإِيْن مِّتَّ فَهُمُ ٱلۡخَٰلِدُونَ"
    },
    {
      "id": 2518,
      "number": 35,
      "verse_key": "21:35",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 280,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 324,
      "juz_number": 17,
      "text": "كُلُّ نَفۡسٖ ذَآئِقَةُ ٱلۡمَوۡتِۗ وَنَبۡلُوكُم بِٱلشَّرِّ وَٱلۡخَيۡرِ فِتۡنَةٗۖ وَإِلَيۡنَا تُرۡجَعُونَ"
    },
    {
      "id": 2519,
      "number": 36,
      "verse_key": "21:36",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 280,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 325,
      "juz_number": 17,
      "text": "وَإِذَا رَءَاكَ ٱلَّذِينَ كَفَرُوٓاْ إِن يَتَّخِذُونَكَ إِلَّا هُزُوًا أَهَٰذَا ٱلَّذِي يَذۡكُرُ ءَالِهَتَكُمۡ وَهُم بِذِكۡرِ ٱلرَّحۡمَٰنِ هُمۡ كَٰفِرُونَ"
    },
    {
      "id": 2520,
      "number": 37,
      "verse_key": "21:37",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 280,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 325,
      "juz_number": 17,
      "text": "خُلِقَ ٱلۡإِنسَٰنُ مِنۡ عَجَلٖۚ سَأُوْرِيكُمۡ ءَايَٰتِي فَلَا تَسۡتَعۡجِلُونِ"
    },
    {
      "id": 2521,
      "number": 38,
      "verse_key": "21:38",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 280,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 325,
      "juz_number": 17,
      "text": "وَيَقُولُونَ مَتَىٰ هَٰذَا ٱلۡوَعۡدُ إِن كُنتُمۡ صَٰدِقِينَ"
    },
    {
      "id": 2522,
      "number": 39,
      "verse_key": "21:39",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 280,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 325,
      "juz_number": 17,
      "text": "لَوۡ يَعۡلَمُ ٱلَّذِينَ كَفَرُواْ حِينَ لَا يَكُفُّونَ عَن وُجُوهِهِمُ ٱلنَّارَ وَلَا عَن ظُهُورِهِمۡ وَلَا هُمۡ يُنصَرُونَ"
    },
    {
      "id": 2487,
      "number": 4,
      "verse_key": "21:4",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 278,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 322,
      "juz_number": 17,
      "text": "قَالَ رَبِّي يَعۡلَمُ ٱلۡقَوۡلَ فِي ٱلسَّمَآءِ وَٱلۡأَرۡضِۖ وَهُوَ ٱلسَّمِيعُ ٱلۡعَلِيمُ"
    },
    {
      "id": 2523,
      "number": 40,
      "verse_key": "21:40",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 280,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 325,
      "juz_number": 17,
      "text": "بَلۡ تَأۡتِيهِم بَغۡتَةٗ فَتَبۡهَتُهُمۡ فَلَا يَسۡتَطِيعُونَ رَدَّهَا وَلَا هُمۡ يُنظَرُونَ"
    },
    {
      "id": 2524,
      "number": 41,
      "verse_key": "21:41",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 280,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 325,
      "juz_number": 17,
      "text": "وَلَقَدِ ٱسۡتُهۡزِئَ بِرُسُلٖ مِّن قَبۡلِكَ فَحَاقَ بِٱلَّذِينَ سَخِرُواْ مِنۡهُم مَّا كَانُواْ بِهِۦ يَسۡتَهۡزِءُونَ"
    },
    {
      "id": 2525,
      "number": 42,
      "verse_key": "21:42",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 281,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 325,
      "juz_number": 17,
      "text": "قُلۡ مَن يَكۡلَؤُكُم بِٱلَّيۡلِ وَٱلنَّهَارِ مِنَ ٱلرَّحۡمَٰنِۚ بَلۡ هُمۡ عَن ذِكۡرِ رَبِّهِم مُّعۡرِضُونَ"
    },
    {
      "id": 2526,
      "number": 43,
      "verse_key": "21:43",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 281,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 325,
      "juz_number": 17,
      "text": "أَمۡ لَهُمۡ ءَالِهَةٞ تَمۡنَعُهُم مِّن دُونِنَاۚ لَا يَسۡتَطِيعُونَ نَصۡرَ أَنفُسِهِمۡ وَلَا هُم مِّنَّا يُصۡحَبُونَ"
    },
    {
      "id": 2527,
      "number": 44,
      "verse_key": "21:44",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 281,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 325,
      "juz_number": 17,
      "text": "بَلۡ مَتَّعۡنَا هَٰٓؤُلَآءِ وَءَابَآءَهُمۡ حَتَّىٰ طَالَ عَلَيۡهِمُ ٱلۡعُمُرُۗ أَفَلَا يَرَوۡنَ أَنَّا نَأۡتِي ٱلۡأَرۡضَ نَنقُصُهَا مِنۡ أَطۡرَافِهَآۚ أَفَهُمُ ٱلۡغَٰلِبُونَ"
    },
    {
      "id": 2528,
      "number": 45,
      "verse_key": "21:45",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 281,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 326,
      "juz_number": 17,
      "text": "قُلۡ إِنَّمَآ أُنذِرُكُم بِٱلۡوَحۡيِۚ وَلَا يَسۡمَعُ ٱلصُّمُّ ٱلدُّعَآءَ إِذَا مَا يُنذَرُونَ"
    },
    {
      "id": 2529,
      "number": 46,
      "verse_key": "21:46",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 281,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 326,
      "juz_number": 17,
      "text": "وَلَئِن مَّسَّتۡهُمۡ نَفۡحَةٞ مِّنۡ عَذَابِ رَبِّكَ لَيَقُولُنَّ يَٰوَيۡلَنَآ إِنَّا كُنَّا ظَٰلِمِينَ"
    },
    {
      "id": 2530,
      "number": 47,
      "verse_key": "21:47",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 281,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 326,
      "juz_number": 17,
      "text": "وَنَضَعُ ٱلۡمَوَٰزِينَ ٱلۡقِسۡطَ لِيَوۡمِ ٱلۡقِيَٰمَةِ فَلَا تُظۡلَمُ نَفۡسٞ شَيۡـٔٗاۖ وَإِن كَانَ مِثۡقَالَ حَبَّةٖ مِّنۡ خَرۡدَلٍ أَتَيۡنَا بِهَاۗ وَكَفَىٰ بِنَا حَٰسِبِينَ"
    },
    {
      "id": 2531,
      "number": 48,
      "verse_key": "21:48",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 281,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 326,
      "juz_number": 17,
      "text": "وَلَقَدۡ ءَاتَيۡنَا مُوسَىٰ وَهَٰرُونَ ٱلۡفُرۡقَانَ وَضِيَآءٗ وَذِكۡرٗا لِّلۡمُتَّقِينَ"
    },
    {
      "id": 2532,
      "number": 49,
      "verse_key": "21:49",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 281,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 326,
      "juz_number": 17,
      "text": "ٱلَّذِينَ يَخۡشَوۡنَ رَبَّهُم بِٱلۡغَيۡبِ وَهُم مِّنَ ٱلسَّاعَةِ مُشۡفِقُونَ"
    },
    {
      "id": 2488,
      "number": 5,
      "verse_key": "21:5",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 278,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 322,
      "juz_number": 17,
      "text": "بَلۡ قَالُوٓاْ أَضۡغَٰثُ أَحۡلَٰمِۭ بَلِ ٱفۡتَرَىٰهُ بَلۡ هُوَ شَاعِرٞ فَلۡيَأۡتِنَا بِـَٔايَةٖ كَمَآ أُرۡسِلَ ٱلۡأَوَّلُونَ"
    },
    {
      "id": 2533,
      "number": 50,
      "verse_key": "21:50",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 281,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 326,
      "juz_number": 17,
      "text": "وَهَٰذَا ذِكۡرٞ مُّبَارَكٌ أَنزَلۡنَٰهُۚ أَفَأَنتُمۡ لَهُۥ مُنكِرُونَ"
    },
    {
      "id": 2534,
      "number": 51,
      "verse_key": "21:51",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 326,
      "juz_number": 17,
      "text": "۞ وَلَقَدۡ ءَاتَيۡنَآ إِبۡرَٰهِيمَ رُشۡدَهُۥ مِن قَبۡلُ وَكُنَّا بِهِۦ عَٰلِمِينَ"
    },
    {
      "id": 2535,
      "number": 52,
      "verse_key": "21:52",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 326,
      "juz_number": 17,
      "text": "إِذۡ قَالَ لِأَبِيهِ وَقَوۡمِهِۦ مَا هَٰذِهِ ٱلتَّمَاثِيلُ ٱلَّتِيٓ أَنتُمۡ لَهَا عَٰكِفُونَ"
    },
    {
      "id": 2536,
      "number": 53,
      "verse_key": "21:53",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 326,
      "juz_number": 17,
      "text": "قَالُواْ وَجَدۡنَآ ءَابَآءَنَا لَهَا عَٰبِدِينَ"
    },
    {
      "id": 2537,
      "number": 54,
      "verse_key": "21:54",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 326,
      "juz_number": 17,
      "text": "قَالَ لَقَدۡ كُنتُمۡ أَنتُمۡ وَءَابَآؤُكُمۡ فِي ضَلَٰلٖ مُّبِينٖ"
    },
    {
      "id": 2538,
      "number": 55,
      "verse_key": "21:55",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 326,
      "juz_number": 17,
      "text": "قَالُوٓاْ أَجِئۡتَنَا بِٱلۡحَقِّ أَمۡ أَنتَ مِنَ ٱللَّٰعِبِينَ"
    },
    {
      "id": 2539,
      "number": 56,
      "verse_key": "21:56",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 326,
      "juz_number": 17,
      "text": "قَالَ بَل رَّبُّكُمۡ رَبُّ ٱلسَّمَٰوَٰتِ وَٱلۡأَرۡضِ ٱلَّذِي فَطَرَهُنَّ وَأَنَا۠ عَلَىٰ ذَٰلِكُم مِّنَ ٱلشَّٰهِدِينَ"
    },
    {
      "id": 2540,
      "number": 57,
      "verse_key": "21:57",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 326,
      "juz_number": 17,
      "text": "وَتَٱللَّهِ لَأَكِيدَنَّ أَصۡنَٰمَكُم بَعۡدَ أَن تُوَلُّواْ مُدۡبِرِينَ"
    },
    {
      "id": 2541,
      "number": 58,
      "verse_key": "21:58",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 327,
      "juz_number": 17,
      "text": "فَجَعَلَهُمۡ جُذَٰذًا إِلَّا كَبِيرٗا لَّهُمۡ لَعَلَّهُمۡ إِلَيۡهِ يَرۡجِعُونَ"
    },
    {
      "id": 2542,
      "number": 59,
      "verse_key": "21:59",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 327,
      "juz_number": 17,
      "text": "قَالُواْ مَن فَعَلَ هَٰذَا بِـَٔالِهَتِنَآ إِنَّهُۥ لَمِنَ ٱلظَّٰلِمِينَ"
    },
    {
      "id": 2489,
      "number": 6,
      "verse_key": "21:6",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 278,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 322,
      "juz_number": 17,
      "text": "مَآ ءَامَنَتۡ قَبۡلَهُم مِّن قَرۡيَةٍ أَهۡلَكۡنَٰهَآۖ أَفَهُمۡ يُؤۡمِنُونَ"
    },
    {
      "id": 2543,
      "number": 60,
      "verse_key": "21:60",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 327,
      "juz_number": 17,
      "text": "قَالُواْ سَمِعۡنَا فَتٗى يَذۡكُرُهُمۡ يُقَالُ لَهُۥٓ إِبۡرَٰهِيمُ"
    },
    {
      "id": 2544,
      "number": 61,
      "verse_key": "21:61",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 327,
      "juz_number": 17,
      "text": "قَالُواْ فَأۡتُواْ بِهِۦ عَلَىٰٓ أَعۡيُنِ ٱلنَّاسِ لَعَلَّهُمۡ يَشۡهَدُونَ"
    },
    {
      "id": 2545,
      "number": 62,
      "verse_key": "21:62",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 327,
      "juz_number": 17,
      "text": "قَالُوٓاْ ءَأَنتَ فَعَلۡتَ هَٰذَا بِـَٔالِهَتِنَا يَٰٓإِبۡرَٰهِيمُ"
    },
    {
      "id": 2546,
      "number": 63,
      "verse_key": "21:63",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 327,
      "juz_number": 17,
      "text": "قَالَ بَلۡ فَعَلَهُۥ كَبِيرُهُمۡ هَٰذَا فَسۡـَٔلُوهُمۡ إِن كَانُواْ يَنطِقُونَ"
    },
    {
      "id": 2547,
      "number": 64,
      "verse_key": "21:64",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 327,
      "juz_number": 17,
      "text": "فَرَجَعُوٓاْ إِلَىٰٓ أَنفُسِهِمۡ فَقَالُوٓاْ إِنَّكُمۡ أَنتُمُ ٱلظَّٰلِمُونَ"
    },
    {
      "id": 2548,
      "number": 65,
      "verse_key": "21:65",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 327,
      "juz_number": 17,
      "text": "ثُمَّ نُكِسُواْ عَلَىٰ رُءُوسِهِمۡ لَقَدۡ عَلِمۡتَ مَا هَٰٓؤُلَآءِ يَنطِقُونَ"
    },
    {
      "id": 2549,
      "number": 66,
      "verse_key": "21:66",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 327,
      "juz_number": 17,
      "text": "قَالَ أَفَتَعۡبُدُونَ مِن دُونِ ٱللَّهِ مَا لَا يَنفَعُكُمۡ شَيۡـٔٗا وَلَا يَضُرُّكُمۡ"
    },
    {
      "id": 2550,
      "number": 67,
      "verse_key": "21:67",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 327,
      "juz_number": 17,
      "text": "أُفّٖ لَّكُمۡ وَلِمَا تَعۡبُدُونَ مِن دُونِ ٱللَّهِۚ أَفَلَا تَعۡقِلُونَ"
    },
    {
      "id": 2551,
      "number": 68,
      "verse_key": "21:68",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 327,
      "juz_number": 17,
      "text": "قَالُواْ حَرِّقُوهُ وَٱنصُرُوٓاْ ءَالِهَتَكُمۡ إِن كُنتُمۡ فَٰعِلِينَ"
    },
    {
      "id": 2552,
      "number": 69,
      "verse_key": "21:69",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 327,
      "juz_number": 17,
      "text": "قُلۡنَا يَٰنَارُ كُونِي بَرۡدٗا وَسَلَٰمًا عَلَىٰٓ إِبۡرَٰهِيمَ"
    },
    {
      "id": 2490,
      "number": 7,
      "verse_key": "21:7",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 278,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 322,
      "juz_number": 17,
      "text": "وَمَآ أَرۡسَلۡنَا قَبۡلَكَ إِلَّا رِجَالٗا نُّوحِيٓ إِلَيۡهِمۡۖ فَسۡـَٔلُوٓاْ أَهۡلَ ٱلذِّكۡرِ إِن كُنتُمۡ لَا تَعۡلَمُونَ"
    },
    {
      "id": 2553,
      "number": 70,
      "verse_key": "21:70",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 327,
      "juz_number": 17,
      "text": "وَأَرَادُواْ بِهِۦ كَيۡدٗا فَجَعَلۡنَٰهُمُ ٱلۡأَخۡسَرِينَ"
    },
    {
      "id": 2554,
      "number": 71,
      "verse_key": "21:71",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 327,
      "juz_number": 17,
      "text": "وَنَجَّيۡنَٰهُ وَلُوطًا إِلَى ٱلۡأَرۡضِ ٱلَّتِي بَٰرَكۡنَا فِيهَا لِلۡعَٰلَمِينَ"
    },
    {
      "id": 2555,
      "number": 72,
      "verse_key": "21:72",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 327,
      "juz_number": 17,
      "text": "وَوَهَبۡنَا لَهُۥٓ إِسۡحَٰقَ وَيَعۡقُوبَ نَافِلَةٗۖ وَكُلّٗا جَعَلۡنَا صَٰلِحِينَ"
    },
    {
      "id": 2556,
      "number": 73,
      "verse_key": "21:73",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 328,
      "juz_number": 17,
      "text": "وَجَعَلۡنَٰهُمۡ أَئِمَّةٗ يَهۡدُونَ بِأَمۡرِنَا وَأَوۡحَيۡنَآ إِلَيۡهِمۡ فِعۡلَ ٱلۡخَيۡرَٰتِ وَإِقَامَ ٱلصَّلَوٰةِ وَإِيتَآءَ ٱلزَّكَوٰةِۖ وَكَانُواْ لَنَا عَٰبِدِينَ"
    },
    {
      "id": 2557,
      "number": 74,
      "verse_key": "21:74",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 328,
      "juz_number": 17,
      "text": "وَلُوطًا ءَاتَيۡنَٰهُ حُكۡمٗا وَعِلۡمٗا وَنَجَّيۡنَٰهُ مِنَ ٱلۡقَرۡيَةِ ٱلَّتِي كَانَت تَّعۡمَلُ ٱلۡخَبَٰٓئِثَۚ إِنَّهُمۡ كَانُواْ قَوۡمَ سَوۡءٖ فَٰسِقِينَ"
    },
    {
      "id": 2558,
      "number": 75,
      "verse_key": "21:75",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 328,
      "juz_number": 17,
      "text": "وَأَدۡخَلۡنَٰهُ فِي رَحۡمَتِنَآۖ إِنَّهُۥ مِنَ ٱلصَّٰلِحِينَ"
    },
    {
      "id": 2559,
      "number": 76,
      "verse_key": "21:76",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 328,
      "juz_number": 17,
      "text": "وَنُوحًا إِذۡ نَادَىٰ مِن قَبۡلُ فَٱسۡتَجَبۡنَا لَهُۥ فَنَجَّيۡنَٰهُ وَأَهۡلَهُۥ مِنَ ٱلۡكَرۡبِ ٱلۡعَظِيمِ"
    },
    {
      "id": 2560,
      "number": 77,
      "verse_key": "21:77",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 328,
      "juz_number": 17,
      "text": "وَنَصَرۡنَٰهُ مِنَ ٱلۡقَوۡمِ ٱلَّذِينَ كَذَّبُواْ بِـَٔايَٰتِنَآۚ إِنَّهُمۡ كَانُواْ قَوۡمَ سَوۡءٖ فَأَغۡرَقۡنَٰهُمۡ أَجۡمَعِينَ"
    },
    {
      "id": 2561,
      "number": 78,
      "verse_key": "21:78",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 328,
      "juz_number": 17,
      "text": "وَدَاوُۥدَ وَسُلَيۡمَٰنَ إِذۡ يَحۡكُمَانِ فِي ٱلۡحَرۡثِ إِذۡ نَفَشَتۡ فِيهِ غَنَمُ ٱلۡقَوۡمِ وَكُنَّا لِحُكۡمِهِمۡ شَٰهِدِينَ"
    },
    {
      "id": 2562,
      "number": 79,
      "verse_key": "21:79",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 328,
      "juz_number": 17,
      "text": "فَفَهَّمۡنَٰهَا سُلَيۡمَٰنَۚ وَكُلًّا ءَاتَيۡنَا حُكۡمٗا وَعِلۡمٗاۚ وَسَخَّرۡنَا مَعَ دَاوُۥدَ ٱلۡجِبَالَ يُسَبِّحۡنَ وَٱلطَّيۡرَۚ وَكُنَّا فَٰعِلِينَ"
    },
    {
      "id": 2491,
      "number": 8,
      "verse_key": "21:8",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 278,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 322,
      "juz_number": 17,
      "text": "وَمَا جَعَلۡنَٰهُمۡ جَسَدٗا لَّا يَأۡكُلُونَ ٱلطَّعَامَ وَمَا كَانُواْ خَٰلِدِينَ"
    },
    {
      "id": 2563,
      "number": 80,
      "verse_key": "21:80",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 328,
      "juz_number": 17,
      "text": "وَعَلَّمۡنَٰهُ صَنۡعَةَ لَبُوسٖ لَّكُمۡ لِتُحۡصِنَكُم مِّنۢ بَأۡسِكُمۡۖ فَهَلۡ أَنتُمۡ شَٰكِرُونَ"
    },
    {
      "id": 2564,
      "number": 81,
      "verse_key": "21:81",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 328,
      "juz_number": 17,
      "text": "وَلِسُلَيۡمَٰنَ ٱلرِّيحَ عَاصِفَةٗ تَجۡرِي بِأَمۡرِهِۦٓ إِلَى ٱلۡأَرۡضِ ٱلَّتِي بَٰرَكۡنَا فِيهَاۚ وَكُنَّا بِكُلِّ شَيۡءٍ عَٰلِمِينَ"
    },
    {
      "id": 2565,
      "number": 82,
      "verse_key": "21:82",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 329,
      "juz_number": 17,
      "text": "وَمِنَ ٱلشَّيَٰطِينِ مَن يَغُوصُونَ لَهُۥ وَيَعۡمَلُونَ عَمَلٗا دُونَ ذَٰلِكَۖ وَكُنَّا لَهُمۡ حَٰفِظِينَ"
    },
    {
      "id": 2566,
      "number": 83,
      "verse_key": "21:83",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 329,
      "juz_number": 17,
      "text": "۞ وَأَيُّوبَ إِذۡ نَادَىٰ رَبَّهُۥٓ أَنِّي مَسَّنِيَ ٱلضُّرُّ وَأَنتَ أَرۡحَمُ ٱلرَّٰحِمِينَ"
    },
    {
      "id": 2567,
      "number": 84,
      "verse_key": "21:84",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 329,
      "juz_number": 17,
      "text": "فَٱسۡتَجَبۡنَا لَهُۥ فَكَشَفۡنَا مَا بِهِۦ مِن ضُرّٖۖ وَءَاتَيۡنَٰهُ أَهۡلَهُۥ وَمِثۡلَهُم مَّعَهُمۡ رَحۡمَةٗ مِّنۡ عِندِنَا وَذِكۡرَىٰ لِلۡعَٰبِدِينَ"
    },
    {
      "id": 2568,
      "number": 85,
      "verse_key": "21:85",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 329,
      "juz_number": 17,
      "text": "وَإِسۡمَٰعِيلَ وَإِدۡرِيسَ وَذَا ٱلۡكِفۡلِۖ كُلّٞ مِّنَ ٱلصَّٰبِرِينَ"
    },
    {
      "id": 2569,
      "number": 86,
      "verse_key": "21:86",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 329,
      "juz_number": 17,
      "text": "وَأَدۡخَلۡنَٰهُمۡ فِي رَحۡمَتِنَآۖ إِنَّهُم مِّنَ ٱلصَّٰلِحِينَ"
    },
    {
      "id": 2570,
      "number": 87,
      "verse_key": "21:87",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 329,
      "juz_number": 17,
      "text": "وَذَا ٱلنُّونِ إِذ ذَّهَبَ مُغَٰضِبٗا فَظَنَّ أَن لَّن نَّقۡدِرَ عَلَيۡهِ فَنَادَىٰ فِي ٱلظُّلُمَٰتِ أَن لَّآ إِلَٰهَ إِلَّآ أَنتَ سُبۡحَٰنَكَ إِنِّي كُنتُ مِنَ ٱلظَّٰلِمِينَ"
    },
    {
      "id": 2571,
      "number": 88,
      "verse_key": "21:88",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 329,
      "juz_number": 17,
      "text": "فَٱسۡتَجَبۡنَا لَهُۥ وَنَجَّيۡنَٰهُ مِنَ ٱلۡغَمِّۚ وَكَذَٰلِكَ نُـۨجِي ٱلۡمُؤۡمِنِينَ"
    },
    {
      "id": 2572,
      "number": 89,
      "verse_key": "21:89",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 329,
      "juz_number": 17,
      "text": "وَزَكَرِيَّآ إِذۡ نَادَىٰ رَبَّهُۥ رَبِّ لَا تَذَرۡنِي فَرۡدٗا وَأَنتَ خَيۡرُ ٱلۡوَٰرِثِينَ"
    },
    {
      "id": 2492,
      "number": 9,
      "verse_key": "21:9",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 278,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 322,
      "juz_number": 17,
      "text": "ثُمَّ صَدَقۡنَٰهُمُ ٱلۡوَعۡدَ فَأَنجَيۡنَٰهُمۡ وَمَن نَّشَآءُ وَأَهۡلَكۡنَا ٱلۡمُسۡرِفِينَ"
    },
    {
      "id": 2573,
      "number": 90,
      "verse_key": "21:90",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 329,
      "juz_number": 17,
      "text": "فَٱسۡتَجَبۡنَا لَهُۥ وَوَهَبۡنَا لَهُۥ يَحۡيَىٰ وَأَصۡلَحۡنَا لَهُۥ زَوۡجَهُۥٓۚ إِنَّهُمۡ كَانُواْ يُسَٰرِعُونَ فِي ٱلۡخَيۡرَٰتِ وَيَدۡعُونَنَا رَغَبٗا وَرَهَبٗاۖ وَكَانُواْ لَنَا خَٰشِعِينَ"
    },
    {
      "id": 2574,
      "number": 91,
      "verse_key": "21:91",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 330,
      "juz_number": 17,
      "text": "وَٱلَّتِيٓ أَحۡصَنَتۡ فَرۡجَهَا فَنَفَخۡنَا فِيهَا مِن رُّوحِنَا وَجَعَلۡنَٰهَا وَٱبۡنَهَآ ءَايَةٗ لِّلۡعَٰلَمِينَ"
    },
    {
      "id": 2575,
      "number": 92,
      "verse_key": "21:92",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 330,
      "juz_number": 17,
      "text": "إِنَّ هَٰذِهِۦٓ أُمَّتُكُمۡ أُمَّةٗ وَٰحِدَةٗ وَأَنَا۠ رَبُّكُمۡ فَٱعۡبُدُونِ"
    },
    {
      "id": 2576,
      "number": 93,
      "verse_key": "21:93",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 330,
      "juz_number": 17,
      "text": "وَتَقَطَّعُوٓاْ أَمۡرَهُم بَيۡنَهُمۡۖ كُلٌّ إِلَيۡنَا رَٰجِعُونَ"
    },
    {
      "id": 2577,
      "number": 94,
      "verse_key": "21:94",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 284,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 330,
      "juz_number": 17,
      "text": "فَمَن يَعۡمَلۡ مِنَ ٱلصَّٰلِحَٰتِ وَهُوَ مُؤۡمِنٞ فَلَا كُفۡرَانَ لِسَعۡيِهِۦ وَإِنَّا لَهُۥ كَٰتِبُونَ"
    },
    {
      "id": 2578,
      "number": 95,
      "verse_key": "21:95",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 284,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 330,
      "juz_number": 17,
      "text": "وَحَرَٰمٌ عَلَىٰ قَرۡيَةٍ أَهۡلَكۡنَٰهَآ أَنَّهُمۡ لَا يَرۡجِعُونَ"
    },
    {
      "id": 2579,
      "number": 96,
      "verse_key": "21:96",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 284,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 330,
      "juz_number": 17,
      "text": "حَتَّىٰٓ إِذَا فُتِحَتۡ يَأۡجُوجُ وَمَأۡجُوجُ وَهُم مِّن كُلِّ حَدَبٖ يَنسِلُونَ"
    },
    {
      "id": 2580,
      "number": 97,
      "verse_key": "21:97",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 284,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 330,
      "juz_number": 17,
      "text": "وَٱقۡتَرَبَ ٱلۡوَعۡدُ ٱلۡحَقُّ فَإِذَا هِيَ شَٰخِصَةٌ أَبۡصَٰرُ ٱلَّذِينَ كَفَرُواْ يَٰوَيۡلَنَا قَدۡ كُنَّا فِي غَفۡلَةٖ مِّنۡ هَٰذَا بَلۡ كُنَّا ظَٰلِمِينَ"
    },
    {
      "id": 2581,
      "number": 98,
      "verse_key": "21:98",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 284,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 330,
      "juz_number": 17,
      "text": "إِنَّكُمۡ وَمَا تَعۡبُدُونَ مِن دُونِ ٱللَّهِ حَصَبُ جَهَنَّمَ أَنتُمۡ لَهَا وَٰرِدُونَ"
    },
    {
      "id": 2582,
      "number": 99,
      "verse_key": "21:99",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 284,
      "manzil_number": 4,
      "sajdah_number": null,
      "page_number": 330,
      "juz_number": 17,
      "text": "لَوۡ كَانَ هَٰٓؤُلَآءِ ءَالِهَةٗ مَّا وَرَدُوهَاۖ وَكُلّٞ فِيهَا خَٰلِدُونَ"
    }
  ]
}